{
  if (((regionInfo.getStartKey().getLength() == 0) || (regionInfo.getStartKey().compareTo(row) <= 0)) && ((regionInfo.getEndKey().getLength() == 0) || (regionInfo.getEndKey().compareTo(row) > 0))) {
  }
 else {
    throw new WrongRegionException("Requested row out of range for " + "HRegion " + regionInfo.getRegionName() + ", startKey='"+ regionInfo.getStartKey()+ "', getEndKey()='"+ regionInfo.getEndKey()+ "', row='"+ row+ "'");
  }
}
