{
  checkReadOnly();
  HStoreKey origin=new HStoreKey(row,column,ts);
  Set<HStoreKey> keys=getKeys(origin,versions);
  if (keys.size() > 0) {
    TreeMap<HStoreKey,byte[]> edits=new TreeMap<HStoreKey,byte[]>(new HStoreKey.HStoreKeyComparator());
    for (    HStoreKey key : keys) {
      edits.put(key,HLogEdit.DELETED_BYTES);
    }
    update(edits);
  }
}
