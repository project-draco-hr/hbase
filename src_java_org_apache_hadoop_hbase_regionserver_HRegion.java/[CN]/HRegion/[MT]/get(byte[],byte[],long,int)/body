{
  splitsAndClosesLock.readLock().lock();
  try {
    if (this.closed.get()) {
      throw new IOException("Region " + this + " closed");
    }
    checkRow(row);
    checkColumn(column);
    HStoreKey key=new HStoreKey(row,column,timestamp);
    return getStore(column).get(key,numVersions);
  }
  finally {
    splitsAndClosesLock.readLock().unlock();
  }
}
