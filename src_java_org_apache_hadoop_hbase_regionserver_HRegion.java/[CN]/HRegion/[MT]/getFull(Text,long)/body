{
  HStoreKey key=new HStoreKey(row,ts);
  obtainRowLock(row);
  try {
    TreeMap<Text,Cell> result=new TreeMap<Text,Cell>();
    for (    Text colFamily : stores.keySet()) {
      HStore targetStore=stores.get(colFamily);
      targetStore.getFull(key,result);
    }
    return result;
  }
  finally {
    releaseRowLock(row);
  }
}
