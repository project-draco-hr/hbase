{
  checkColumn(key.getColumn());
  checkReadOnly();
  TreeMap<HStoreKey,byte[]> targets=this.targetColumns.get(lockid);
  if (targets == null) {
    targets=new TreeMap<HStoreKey,byte[]>(new HStoreKey.HStoreKeyWritableComparator());
    this.targetColumns.put(lockid,targets);
  }
  targets.put(key,val);
}
