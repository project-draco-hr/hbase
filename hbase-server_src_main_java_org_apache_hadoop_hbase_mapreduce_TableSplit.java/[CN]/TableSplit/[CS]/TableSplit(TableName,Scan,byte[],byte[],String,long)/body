{
  this.tableName=tableName;
  try {
    this.scan=(null == scan) ? "" : TableMapReduceUtil.convertScanToString(scan);
  }
 catch (  IOException e) {
    LOG.warn("Failed to convert Scan to String",e);
  }
  this.startRow=startRow;
  this.endRow=endRow;
  this.regionLocation=location;
  this.length=length;
}
