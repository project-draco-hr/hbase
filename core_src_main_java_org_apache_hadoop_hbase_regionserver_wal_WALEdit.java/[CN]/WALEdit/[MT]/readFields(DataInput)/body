{
  kvs=new ArrayList<KeyValue>();
  int versionOrLength=in.readInt();
  if (versionOrLength == VERSION_2) {
    int numEdits=in.readInt();
    for (int idx=0; idx < numEdits; idx++) {
      KeyValue kv=new KeyValue();
      kv.readFields(in);
      this.add(kv);
    }
  }
 else {
    KeyValue kv=new KeyValue();
    kv.readFields(versionOrLength,in);
    this.add(kv);
  }
}
