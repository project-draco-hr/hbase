{
  if (firstRowCell == null || this.prefix == null)   return true;
  int length=firstRowCell.getRowLength();
  if (length < prefix.length)   return true;
  int cmp=Bytes.compareTo(firstRowCell.getRowArray(),firstRowCell.getRowOffset(),this.prefix.length,this.prefix,0,this.prefix.length);
  if ((!isReversed() && cmp > 0) || (isReversed() && cmp < 0)) {
    passedPrefix=true;
  }
  filterRow=(cmp != 0);
  return filterRow;
}
