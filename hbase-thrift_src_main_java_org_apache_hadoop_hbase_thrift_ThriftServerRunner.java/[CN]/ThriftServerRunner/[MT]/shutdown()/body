{
  if (tserver != null) {
    tserver.stop();
    tserver=null;
  }
  if (httpServer != null) {
    try {
      httpServer.stop();
      httpServer=null;
    }
 catch (    Exception e) {
      LOG.error("Problem encountered in shutting down HTTP server " + e.getCause());
    }
    httpServer=null;
  }
}
