{
  try {
    localRegion.abort(localLockId);
  }
 catch (  IOException iex) {
    if (iex instanceof RemoteException) {
      try {
        iex=RemoteExceptionHandler.decodeRemoteException((RemoteException)iex);
      }
 catch (      IOException x) {
        iex=x;
      }
    }
    LOG.error(iex);
  }
}
