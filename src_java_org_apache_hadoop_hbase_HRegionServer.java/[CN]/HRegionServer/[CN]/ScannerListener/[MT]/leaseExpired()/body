{
  LOG.info("Scanner " + this.scannerName + " lease expired");
  HInternalScannerInterface s=null;
synchronized (scanners) {
    s=scanners.remove(this.scannerName);
  }
  if (s != null) {
    try {
      s.close();
    }
 catch (    IOException e) {
      LOG.error("Closing scanner",e);
    }
  }
}
