{
  try {
    this.workerThread.join();
  }
 catch (  InterruptedException iex) {
  }
  try {
    this.logRollerThread.join();
  }
 catch (  InterruptedException iex) {
  }
  try {
    this.cacheFlusherThread.join();
  }
 catch (  InterruptedException iex) {
  }
  this.leases.close();
  try {
    this.server.join();
  }
 catch (  InterruptedException iex) {
  }
  LOG.info("server stopped at: " + address.toString());
}
