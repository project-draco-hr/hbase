{
  HRegion region=getRegion(regionName);
  if (region == null) {
    throw new IOException("Not serving region " + regionName);
  }
  if (LOG.isDebugEnabled()) {
    LOG.debug("get " + row.toString() + ", "+ column.toString());
  }
  BytesWritable results=region.get(row,column);
  if (results != null) {
    return results;
  }
  return null;
}
