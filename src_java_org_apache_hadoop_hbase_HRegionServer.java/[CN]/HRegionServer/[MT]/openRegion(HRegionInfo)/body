{
  HRegion region=onlineRegions.get(regionInfo.getRegionName());
  if (region == null) {
    try {
      region=new HRegion(new Path(this.conf.get(HConstants.HBASE_DIR)),this.log,FileSystem.get(conf),conf,regionInfo,null,this.cacheFlusher);
    }
 catch (    IOException e) {
      LOG.error("error opening region " + regionInfo.getRegionName(),e);
      reportClose(region);
      return;
    }
    this.lock.writeLock().lock();
    try {
      this.log.setSequenceNumber(region.getMinSequenceId());
      this.onlineRegions.put(region.getRegionName(),region);
    }
  finally {
      this.lock.writeLock().unlock();
    }
    reportOpen(region);
  }
}
