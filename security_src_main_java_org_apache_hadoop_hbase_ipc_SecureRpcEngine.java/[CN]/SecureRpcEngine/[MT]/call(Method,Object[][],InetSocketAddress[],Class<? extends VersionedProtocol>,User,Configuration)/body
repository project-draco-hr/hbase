{
  Invocation[] invocations=new Invocation[params.length];
  for (int i=0; i < params.length; i++)   invocations[i]=new Invocation(method,params[i]);
  SecureClient client=CLIENTS.getClient(conf);
  try {
    Writable[] wrappedValues=client.call(invocations,addrs,protocol,ticket);
    if (method.getReturnType() == Void.TYPE) {
      return null;
    }
    Object[] values=(Object[])Array.newInstance(method.getReturnType(),wrappedValues.length);
    for (int i=0; i < values.length; i++)     if (wrappedValues[i] != null)     values[i]=((HbaseObjectWritable)wrappedValues[i]).get();
    return values;
  }
  finally {
    CLIENTS.stopClient(client);
  }
}
