{
  LinkedList<Call> responseQueue=call.connection.responseQueue;
synchronized (responseQueue) {
    Iterator<Call> iter=responseQueue.listIterator(0);
    while (iter.hasNext()) {
      call=iter.next();
      if (now > call.timestamp + PURGE_INTERVAL) {
        closeConnection(call.connection);
        break;
      }
    }
  }
}
