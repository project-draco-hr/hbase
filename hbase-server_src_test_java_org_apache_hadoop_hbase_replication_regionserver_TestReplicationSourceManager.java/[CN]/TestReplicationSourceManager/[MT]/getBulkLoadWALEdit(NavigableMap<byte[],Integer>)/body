{
  Map<byte[],List<Path>> storeFiles=new HashMap<>(1);
  List<Path> p=new ArrayList<>(1);
  p.add(new Path(Bytes.toString(f1)));
  storeFiles.put(f1,p);
  scope.put(f1,1);
  p=new ArrayList<>(1);
  p.add(new Path(Bytes.toString(f2)));
  storeFiles.put(f2,p);
  BulkLoadDescriptor desc=ProtobufUtil.toBulkLoadDescriptor(hri.getTable(),ByteStringer.wrap(hri.getEncodedNameAsBytes()),storeFiles,1);
  WALEdit logEdit=WALEdit.createBulkLoadEvent(hri,desc);
  return logEdit;
}
