{
  Collection<SortedSet<String>> sets=logZnodesMap.values();
  if (sets.size() > 1) {
    throw new RuntimeException("unexpected size of logZnodesMap: " + sets.size());
  }
  if (sets.size() == 1) {
    SortedSet<String> s=sets.iterator().next();
    for (    String file : files) {
      if (!s.contains(file)) {
        return 0;
      }
    }
    return 1;
  }
  return 0;
}
