{
  HTableDescriptor htd=createTableDescriptor(getName(),0,3,HConstants.FOREVER,true);
  HRegion region=createNewHRegion(htd,null,null);
  long ts=System.currentTimeMillis();
  Put p=new Put(T1,ts);
  p.add(c0,c0,T1);
  region.put(p);
  p=new Put(T1,ts + 1);
  p.add(c0,c0,T2);
  region.put(p);
  p=new Put(T1,ts + 2);
  p.add(c0,c0,T3);
  region.put(p);
  p=new Put(T1,ts + 4);
  p.add(c0,c0,T4);
  region.put(p);
  Delete d=new Delete(T1,ts + 2,null);
  region.delete(d,null,true);
  assertEquals(3,countDeleteMarkers(region));
  Get g=new Get(T1);
  g.setMaxVersions();
  g.setTimeRange(0L,ts + 2);
  Result r=region.get(g,null);
  checkResult(r,c0,c0,T2,T1);
  region.flushcache();
  r=region.get(g,null);
  checkResult(r,c0,c0,T2);
  region.compactStores(true);
  region.compactStores(true);
  assertEquals(1,countDeleteMarkers(region));
  r=region.get(g,null);
  checkResult(r,c0,c0,T2);
  g.setTimeRange(0L,ts + 4);
  r=region.get(g,null);
  assertTrue(r.isEmpty());
  p=new Put(T1,ts + 5);
  p.add(c0,c0,T5);
  region.put(p);
  p=new Put(T1,ts + 6);
  p.add(c0,c0,T6);
  region.put(p);
  p=new Put(T1,ts);
  p.add(c0,c0,T1);
  region.put(p);
  r=region.get(g,null);
  assertTrue(r.isEmpty());
  region.flushcache();
  region.compactStores(true);
  region.compactStores(true);
  region.put(p);
  r=region.get(g,null);
  checkResult(r,c0,c0,T1);
  assertEquals(0,countDeleteMarkers(region));
  region.close();
  region.getLog().closeAndDelete();
}
