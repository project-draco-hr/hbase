{
  if (status != Constants.FLAG_EXIT) {
    Shell shell=new Shell();
    List<String> argList=new ArrayList<String>();
    argList.add(String.valueOf(Constants.FLAG_RELAUNCH));
    if (Shell.HTML_OPTION != null)     argList.add(Shell.HTML_OPTION);
    if (Shell.IP != null && Shell.PORT != -1)     argList.add("--master:" + Shell.IP + ":"+ Shell.PORT);
    try {
      shell.main(argList.toArray(new String[]{}));
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
  }
}
