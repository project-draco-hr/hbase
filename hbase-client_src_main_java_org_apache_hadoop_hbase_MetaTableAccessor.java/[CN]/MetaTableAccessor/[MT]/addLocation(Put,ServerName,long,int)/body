{
  long now=EnvironmentEdgeManager.currentTime();
  p.addImmutable(getCatalogFamily(),getServerColumn(replicaId),now,Bytes.toBytes(sn.getHostAndPort()));
  p.addImmutable(getCatalogFamily(),getStartCodeColumn(replicaId),now,Bytes.toBytes(sn.getStartcode()));
  p.addImmutable(getCatalogFamily(),getSeqNumColumn(replicaId),now,Bytes.toBytes(openSeqNum));
  return p;
}
