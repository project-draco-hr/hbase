{
  p.addImmutable(HConstants.CATALOG_FAMILY,getServerColumn(replicaId),Bytes.toBytes(sn.getHostAndPort()));
  p.addImmutable(HConstants.CATALOG_FAMILY,getStartCodeColumn(replicaId),Bytes.toBytes(sn.getStartcode()));
  p.addImmutable(HConstants.CATALOG_FAMILY,getSeqNumColumn(replicaId),Bytes.toBytes(openSeqNum));
  return p;
}
