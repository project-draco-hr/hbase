{
  RowResult results=scanner.next();
  if (results == null) {
    return false;
  }
 else {
    key.setRow(results.getRow());
    values.clear();
    for (    Map.Entry<Text,Cell> entry : results.entrySet()) {
      values.put(entry.getKey(),entry.getValue().getValue());
    }
    return true;
  }
}
