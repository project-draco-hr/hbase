{
  Map<byte[],Cell> result=region.getFull(row,null,timestamp,null);
  Cell cell_value=result.get(column);
  if (value == null) {
    assertEquals(column.toString() + " at timestamp " + timestamp,null,cell_value);
  }
 else {
    if (cell_value == null) {
      fail(column.toString() + " at timestamp " + timestamp+ "\" was expected to be \""+ value+ " but was null");
    }
    if (cell_value != null) {
      assertEquals(column.toString() + " at timestamp " + timestamp,value,new String(cell_value.getValue()));
    }
  }
}
