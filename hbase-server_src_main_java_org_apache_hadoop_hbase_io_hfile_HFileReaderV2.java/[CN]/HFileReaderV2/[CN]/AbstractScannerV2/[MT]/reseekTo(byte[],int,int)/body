{
  int compared;
  if (isSeeked()) {
    compared=compareKey(reader.getComparator(),key,offset,length);
    if (compared < 1) {
      return compared;
    }
 else {
      if (this.nextIndexedKey != null && (this.nextIndexedKey == HConstants.NO_NEXT_INDEXED_KEY || reader.getComparator().compareFlatKey(key,offset,length,nextIndexedKey,0,nextIndexedKey.length) < 0)) {
        return loadBlockAndSeekToKey(this.block,this.nextIndexedKey,false,key,offset,length,false);
      }
    }
  }
  return seekTo(key,offset,length,false);
}
