{
  HFileBlockIndex.BlockIndexReader indexReader=reader.getDataBlockIndexReader();
  BlockWithScanInfo blockWithScanInfo=indexReader.loadDataBlockWithScanInfo(key,offset,length,block,cacheBlocks,pread,isCompaction,getEffectiveDataBlockEncoding());
  if (blockWithScanInfo == null || blockWithScanInfo.getHFileBlock() == null) {
    return -1;
  }
  return loadBlockAndSeekToKey(blockWithScanInfo.getHFileBlock(),blockWithScanInfo.getNextIndexedKey(),rewind,key,offset,length,false);
}
