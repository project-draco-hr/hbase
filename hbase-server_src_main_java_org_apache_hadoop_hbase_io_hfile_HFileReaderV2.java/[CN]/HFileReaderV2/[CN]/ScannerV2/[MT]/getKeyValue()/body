{
  if (!isSeeked())   return null;
  KeyValue ret=new KeyValue(blockBuffer.array(),blockBuffer.arrayOffset() + blockBuffer.position(),KEY_VALUE_LEN_SIZE + currKeyLen + currValueLen,currKeyLen);
  if (this.reader.shouldIncludeMemstoreTS()) {
    ret.setMvccVersion(currMemstoreTS);
  }
  return ret;
}
