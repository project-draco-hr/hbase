{
  HTableDescriptor htd=getHTableDescriptor("testMigrateHRegionInfoFromVersion0toVersion1");
  HRegionInfo090x ninety=new HRegionInfo090x(htd,HConstants.EMPTY_START_ROW,HConstants.EMPTY_END_ROW);
  byte[] bytes=Writables.getBytes(ninety);
  HRegionInfo hri=Writables.getHRegionInfo(bytes);
  Assert.assertEquals(hri.getTableNameAsString(),ninety.getTableDesc().getNameAsString());
  Assert.assertEquals(HRegionInfo.VERSION,hri.getVersion());
}
