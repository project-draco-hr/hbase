{
  new HTable(conf,".META.");
  int port=cluster.getMaster().getInfoServer().getPort();
  assertHasExpectedContent(new URL("http://localhost:" + port + "/index.html"),"master-status");
  port=cluster.getRegionServerThreads().get(0).getRegionServer().getInfoServer().getPort();
  assertHasExpectedContent(new URL("http://localhost:" + port + "/index.html"),"rs-status");
}
