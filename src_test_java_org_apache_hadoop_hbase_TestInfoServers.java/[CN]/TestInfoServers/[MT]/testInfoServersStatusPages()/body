{
  new HTable(UTIL.getConfiguration(),".META.");
  int port=UTIL.getHbaseCluster().getMaster().getInfoServer().getPort();
  assertHasExpectedContent(new URL("http://localhost:" + port + "/master-status"),"META");
  port=UTIL.getHbaseCluster().getRegionServerThreads().get(0).getRegionServer().getInfoServer().getPort();
  assertHasExpectedContent(new URL("http://localhost:" + port + "/rs-status"),"META");
}
