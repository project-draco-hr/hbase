{
  LOG.info(tagLine + ": " + rc.getThreadsCount()+ " threads"+ (initialThreadsCount > 0 ? " (was " + initialThreadsCount + "), " : ", ")+ rc.getOpenFileDescriptorCount()+ " file descriptors"+ (initialFileHandlesCount > 0 ? " (was " + initialFileHandlesCount + "). " : " ")+ (initialThreadsCount > 0 && rc.getThreadsCount() > initialThreadsCount ? " -thread leak?- " : "")+ (initialFileHandlesCount > 0 && rc.getOpenFileDescriptorCount() > initialFileHandlesCount ? " -file handle leak?- " : ""));
}
