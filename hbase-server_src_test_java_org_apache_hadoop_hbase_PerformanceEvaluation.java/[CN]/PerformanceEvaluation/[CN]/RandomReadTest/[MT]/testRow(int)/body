{
  Get get=new Get(getRandomRow(this.rand,opts.totalRows));
  get.addColumn(FAMILY_NAME,QUALIFIER_NAME);
  if (opts.multiGet > 0) {
    this.gets.add(get);
    if (this.gets.size() == opts.multiGet) {
      this.table.get(this.gets);
      this.gets.clear();
    }
  }
 else {
    this.table.get(get);
  }
}
