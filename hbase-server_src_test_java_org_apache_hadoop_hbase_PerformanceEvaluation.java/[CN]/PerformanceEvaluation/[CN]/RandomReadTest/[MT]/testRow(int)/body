{
  if (i % everyN == 0) {
    Get get=new Get(getRandomRow(this.rand,this.totalRows));
    get.addColumn(FAMILY_NAME,QUALIFIER_NAME);
    long start=System.nanoTime();
    this.table.get(get);
    if (this.reportLatency) {
      times[idx++]=(float)((System.nanoTime() - start) / 1000000.0);
    }
  }
}
