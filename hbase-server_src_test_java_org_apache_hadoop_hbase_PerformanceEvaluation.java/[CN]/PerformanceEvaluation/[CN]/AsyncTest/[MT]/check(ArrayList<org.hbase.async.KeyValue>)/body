{
  if (!CHECK_READS) {
    return;
  }
  if (row.size() != 1) {
    throw new IOException((row.isEmpty() ? "No" : "Multiple (" + row.size() + ')') + " KeyValue found in row");
  }
 else   if (row.get(0).value().length != VALUE_LENGTH) {
    throw new IOException("Invalid value length (found: " + row.get(0).value().length + ", expected: "+ VALUE_LENGTH+ ") in row \""+ new String(row.get(0).key())+ '"');
  }
}
