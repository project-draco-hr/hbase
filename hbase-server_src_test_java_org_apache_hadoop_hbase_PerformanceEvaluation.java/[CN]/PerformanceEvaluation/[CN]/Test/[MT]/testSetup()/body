{
  this.connection=HConnectionManager.createConnection(conf);
  this.table=connection.getTable(opts.tableName);
  this.table.setAutoFlush(opts.autoFlush,true);
  String metricName=testName + "-Client-" + Thread.currentThread().getName()+ "-testRowTime";
  latency=metricsRegistry.newHistogram(PerformanceEvaluation.class,metricName);
}
