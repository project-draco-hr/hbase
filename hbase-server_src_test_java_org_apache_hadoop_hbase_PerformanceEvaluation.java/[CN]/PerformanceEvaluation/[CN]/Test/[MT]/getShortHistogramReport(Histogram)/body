{
  Snapshot sn=h.getSnapshot();
  return "mean=" + DOUBLE_FORMAT.format(h.mean()) + ", min="+ DOUBLE_FORMAT.format(h.min())+ ", max="+ DOUBLE_FORMAT.format(h.max())+ ", stdDev="+ DOUBLE_FORMAT.format(h.stdDev())+ ", 95th="+ DOUBLE_FORMAT.format(sn.get95thPercentile())+ ", 99th="+ DOUBLE_FORMAT.format(sn.get99thPercentile());
}
