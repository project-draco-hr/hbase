{
  final int size=readRawVarint32();
  if (size > 0 && size <= (limit - pos)) {
    final ByteString result=immutable && enableAliasing ? ByteString.wrap(buffer,pos,size) : ByteString.copyFrom(buffer,pos,size);
    pos+=size;
    return result;
  }
  if (size == 0) {
    return ByteString.EMPTY;
  }
  return ByteString.wrap(readRawBytes(size));
}
