{
  ExecCoprocessorRequest request=RequestConverter.buildExecCoprocessorRequest(regionName,exec);
  try {
    ExecCoprocessorResponse response=client.execCoprocessor(null,request);
    Object value=ProtobufUtil.toObject(response.getValue());
    return new ExecResult(regionName,value);
  }
 catch (  ServiceException se) {
    throw getRemoteException(se);
  }
}
