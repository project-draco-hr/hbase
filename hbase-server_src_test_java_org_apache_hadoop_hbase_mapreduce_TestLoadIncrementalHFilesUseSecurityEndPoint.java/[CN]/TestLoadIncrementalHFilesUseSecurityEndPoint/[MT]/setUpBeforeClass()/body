{
  util.getConfiguration().setInt(LoadIncrementalHFiles.MAX_FILES_PER_REGION_PER_FAMILY,MAX_FILES_PER_REGION_PER_FAMILY);
  util.getConfiguration().set(CoprocessorHost.REGION_COPROCESSOR_CONF_KEY,"org.apache.hadoop.hbase.security.access.SecureBulkLoadEndpoint");
  util.startMiniCluster();
  setupNamespace();
}
