{
  boolean bypass=false;
  for (  WALEnvironment env : coprocessors) {
    if (env.getInstance() instanceof org.apache.hadoop.hbase.coprocessor.WALObserver) {
      ((org.apache.hadoop.hbase.coprocessor.WALObserver)env.getInstance()).preWALWrite(env,info,logKey,logEdit);
      bypass|=env.shouldBypass();
      if (env.shouldComplete()) {
        break;
      }
    }
  }
  return bypass;
}
