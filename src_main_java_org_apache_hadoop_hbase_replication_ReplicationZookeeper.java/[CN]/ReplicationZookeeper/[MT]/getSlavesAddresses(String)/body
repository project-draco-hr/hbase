{
  if (this.peerClusters.size() == 0) {
    return new ArrayList<ServerName>(0);
  }
  ReplicationPeer peer=this.peerClusters.get(peerClusterId);
  if (peer == null) {
    return new ArrayList<ServerName>(0);
  }
  peer.setRegionServers(fetchSlavesAddresses(peer.getZkw()));
  return peer.getRegionServers();
}
