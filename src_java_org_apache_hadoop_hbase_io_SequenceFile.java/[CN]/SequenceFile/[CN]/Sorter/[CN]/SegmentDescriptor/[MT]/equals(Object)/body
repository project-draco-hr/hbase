{
  if (!(o instanceof SegmentDescriptor)) {
    return false;
  }
  SegmentDescriptor that=(SegmentDescriptor)o;
  if (this.segmentLength == that.segmentLength && this.segmentOffset == that.segmentOffset && this.segmentPathName.toString().equals(that.segmentPathName.toString())) {
    return true;
  }
  return false;
}
