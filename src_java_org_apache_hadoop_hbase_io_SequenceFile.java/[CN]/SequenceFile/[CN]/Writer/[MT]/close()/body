{
  keySerializer.close();
  uncompressedValSerializer.close();
  if (compressedValSerializer != null) {
    compressedValSerializer.close();
  }
  CodecPool.returnCompressor(compressor);
  compressor=null;
  if (out != null) {
    if (ownOutputStream) {
      out.close();
    }
 else {
      out.flush();
    }
    out=null;
  }
}
