{
  if (theBloom == null) {
    theBloom=bloom;
  }
  if (theBloom.limit() != byteSize) {
    throw new IllegalArgumentException("Bloom does not match expected size:" + " theBloom.limit()=" + theBloom.limit() + ", byteSize="+ byteSize);
  }
  return BloomFilterUtil.contains(buf,offset,length,theBloom,0,(int)byteSize,hash,hashCount);
}
