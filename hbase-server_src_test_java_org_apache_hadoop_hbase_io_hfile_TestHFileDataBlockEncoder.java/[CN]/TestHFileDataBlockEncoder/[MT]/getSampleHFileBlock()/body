{
  ByteBuffer keyValues=RedundantKVGenerator.convertKvToByteBuffer(generator.generateTestKeyValues(60),includesMemstoreTS);
  int size=keyValues.limit();
  ByteBuffer buf=ByteBuffer.allocate(size + HConstants.HFILEBLOCK_HEADER_SIZE);
  buf.position(HConstants.HFILEBLOCK_HEADER_SIZE);
  keyValues.rewind();
  buf.put(keyValues);
  HFileBlock b=new HFileBlock(BlockType.DATA,size,size,-1,buf,HFileBlock.FILL_HEADER,0,includesMemstoreTS,HFileReaderV2.MAX_MINOR_VERSION,0,ChecksumType.NULL.getCode(),0);
  return b;
}
