{
  validate();
  oprot.writeStructBegin(STRUCT_DESC);
  if (this.table != null) {
    oprot.writeFieldBegin(TABLE_FIELD_DESC);
    oprot.writeBinary(this.table);
    oprot.writeFieldEnd();
  }
  if (this.scan != null) {
    oprot.writeFieldBegin(SCAN_FIELD_DESC);
    this.scan.write(oprot);
    oprot.writeFieldEnd();
  }
  oprot.writeFieldStop();
  oprot.writeStructEnd();
}
