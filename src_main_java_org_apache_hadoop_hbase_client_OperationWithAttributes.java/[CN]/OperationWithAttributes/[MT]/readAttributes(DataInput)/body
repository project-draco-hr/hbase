{
  int numAttributes=in.readInt();
  if (numAttributes > 0) {
    this.attributes=new HashMap<String,byte[]>();
    for (int i=0; i < numAttributes; i++) {
      String name=WritableUtils.readString(in);
      byte[] value=Bytes.readByteArray(in);
      this.attributes.put(name,value);
    }
  }
}
