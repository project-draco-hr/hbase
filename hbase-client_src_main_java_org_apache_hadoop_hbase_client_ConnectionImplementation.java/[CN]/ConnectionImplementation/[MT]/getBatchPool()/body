{
  if (batchPool == null) {
synchronized (this) {
      if (batchPool == null) {
        this.batchPool=getThreadPool(conf.getInt("hbase.hconnection.threads.max",256),conf.getInt("hbase.hconnection.threads.core",Runtime.getRuntime().availableProcessors()),"-shared-",null);
        this.cleanupPool=true;
      }
    }
  }
  return this.batchPool;
}
