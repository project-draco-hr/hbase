{
  return new ClonedSeekerState(currentBuffer,keyBuffer,currentKey.getRowLength(),currentKey.getFamilyOffset(),currentKey.getFamilyLength(),keyLength,currentKey.getQualifierOffset(),currentKey.getQualifierLength(),currentKey.getTimestamp(),currentKey.getTypeByte(),valueLength,valueOffset,memstoreTS,tagsOffset,tagsLength,tagCompressionContext,tagsBuffer);
}
