  if (this.columns == null) {
    sb.append("null");
  }
 else {
    sb.append(this.columns);
  }
  first=false;
  if (isSetAttributes()) {
    if (!first)     sb.append(", ");
    sb.append("attributes:");
    if (this.attributes == null) {
      sb.append("null");
