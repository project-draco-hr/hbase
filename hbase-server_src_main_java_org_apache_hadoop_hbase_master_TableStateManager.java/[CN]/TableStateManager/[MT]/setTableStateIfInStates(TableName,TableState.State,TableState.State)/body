{
  lock.writeLock().lock();
  try {
    TableState currentState=readMetaState(tableName);
    if (currentState == null) {
      throw new TableNotFoundException(tableName);
    }
    if (currentState.inStates(states)) {
      udpateMetaState(tableName,newState);
      return true;
    }
 else {
      return false;
    }
  }
  finally {
    lock.writeLock().unlock();
  }
}
