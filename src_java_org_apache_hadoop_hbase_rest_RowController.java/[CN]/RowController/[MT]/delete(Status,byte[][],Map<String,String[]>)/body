{
  RowModel innerModel=getModel();
  byte[] tableName;
  byte[] rowName;
  tableName=pathSegments[0];
  rowName=pathSegments[2];
  if (queryMap.size() == 0) {
    innerModel.delete(tableName,rowName);
  }
 else {
    innerModel.delete(tableName,rowName,this.getColumnsFromQueryMap(queryMap));
  }
  s.setOK();
  s.respond();
}
