{
  if (key == null) {
    throw new NullPointerException("Key may not be null");
  }
  if (!membershipTest(key)) {
    throw new IllegalArgumentException("Key is not a member");
  }
  int[] h=hash.hash(key);
  hash.clear();
  for (int i=0; i < nbHash; i++) {
    if (vector[h[i]] >= 1) {
      vector[h[i]]--;
    }
  }
}
