{
  StringBuilder res=new StringBuilder();
  for (int i=0; i < vectorSize; i++) {
    if (i > 0) {
      res.append(" ");
    }
    int wordNum=i >> 4;
    int bucketShift=(i & 0x0f) << 2;
    long bucketMask=15L << bucketShift;
    long bucketValue=(buckets[wordNum] & bucketMask) >>> bucketShift;
    res.append(bucketValue);
  }
  return res.toString();
}
