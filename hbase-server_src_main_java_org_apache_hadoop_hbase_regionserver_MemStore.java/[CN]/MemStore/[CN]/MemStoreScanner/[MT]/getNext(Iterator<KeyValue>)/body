{
  long readPoint=MultiVersionConsistencyControl.getThreadReadPoint();
  KeyValue v=null;
  try {
    while (it.hasNext()) {
      v=it.next();
      if (v.getMvccVersion() <= readPoint) {
        return v;
      }
    }
    return null;
  }
  finally {
    if (v != null) {
      if (it == snapshotIt) {
        snapshotItRow=v;
      }
 else {
        kvsetItRow=v;
      }
    }
  }
}
