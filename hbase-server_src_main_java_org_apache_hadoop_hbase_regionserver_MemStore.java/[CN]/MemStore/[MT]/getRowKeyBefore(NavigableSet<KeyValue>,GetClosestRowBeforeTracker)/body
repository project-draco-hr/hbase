{
  KeyValue firstOnRow=state.getTargetKey();
  for (Member p=memberOfPreviousRow(set,state,firstOnRow); p != null; p=memberOfPreviousRow(p.set,state,firstOnRow)) {
    if (!state.isTargetTable(p.kv))     break;
    if (!state.isBetterCandidate(p.kv))     break;
    firstOnRow=new KeyValue(p.kv.getRowArray(),p.kv.getRowOffset(),p.kv.getRowLength(),HConstants.LATEST_TIMESTAMP);
    if (walkForwardInSingleRow(p.set,firstOnRow,state))     break;
  }
}
