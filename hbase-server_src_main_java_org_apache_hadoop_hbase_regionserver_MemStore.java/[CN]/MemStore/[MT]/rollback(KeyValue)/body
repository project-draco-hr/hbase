{
  KeyValue found=this.snapshot.get(kv);
  if (found != null && found.getMvccVersion() == kv.getMvccVersion()) {
    this.snapshot.remove(kv);
  }
  found=this.kvset.get(kv);
  if (found != null && found.getMvccVersion() == kv.getMvccVersion()) {
    removeFromKVSet(kv);
    long s=heapSizeChange(kv,true);
    this.size.addAndGet(-s);
  }
}
