{
  String registryClass=this.conf.get("hbase.client.registry.impl",ZooKeeperRegistry.class.getName());
  Registry registry=null;
  try {
    registry=(Registry)Class.forName(registryClass).newInstance();
  }
 catch (  Throwable t) {
    throw new IOException(t);
  }
  registry.init(this);
  return registry;
}
