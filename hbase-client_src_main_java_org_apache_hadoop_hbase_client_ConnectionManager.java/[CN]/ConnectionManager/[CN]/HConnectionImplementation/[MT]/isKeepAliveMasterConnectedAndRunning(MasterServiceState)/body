{
  if (mss.getStub() == null) {
    return false;
  }
  try {
    return mss.isMasterRunning();
  }
 catch (  UndeclaredThrowableException e) {
    LOG.info("Master connection is not running anymore",e.getUndeclaredThrowable());
    return false;
  }
catch (  ServiceException se) {
    LOG.warn("Checking master connection",se);
    return false;
  }
}
