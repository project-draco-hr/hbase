{
  if (this.closed) {
    return;
  }
  delayedClosing.stop("Closing connection");
  closeMaster();
  shutdownBatchPool();
  this.closed=true;
  closeZooKeeperWatcher();
  this.stubs.clear();
  if (clusterStatusListener != null) {
    clusterStatusListener.close();
  }
  if (rpcClient != null) {
    rpcClient.stop();
  }
}
