{
  if (this.master == null) {
    throw new MasterNotRunningException("master has been shut down");
  }
  try {
    this.master.enableTable(tableName);
  }
 catch (  RemoteException e) {
    throw RemoteExceptionHandler.decodeRemoteException(e);
  }
  for (int tries=0; (tries < numRetries) && (!isTableEnabled(tableName)); tries++) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("Sleep. Waiting for all regions to be enabled from " + Bytes.toString(tableName));
    }
    try {
      Thread.sleep(getPauseTime(tries));
    }
 catch (    InterruptedException e) {
    }
    if (LOG.isDebugEnabled()) {
      LOG.debug("Wake. Waiting for all regions to be enabled from " + Bytes.toString(tableName));
    }
  }
  if (!isTableEnabled(tableName))   throw new IOException("unable to enable table " + Bytes.toString(tableName));
  LOG.info("Enabled table " + Bytes.toString(tableName));
}
