{
  TransactionState transactionState=transactionManager.beginTransaction();
  Result row1_A=table.get(transactionState,new Get(ROW1).addColumn(COL_A));
  table.put(transactionState,new Put(ROW2).add(FAMILY,QUAL_A,row1_A.getValue(COL_A)));
  table.put(transactionState,new Put(ROW3).add(FAMILY,QUAL_A,row1_A.getValue(COL_A)));
  return transactionState;
}
