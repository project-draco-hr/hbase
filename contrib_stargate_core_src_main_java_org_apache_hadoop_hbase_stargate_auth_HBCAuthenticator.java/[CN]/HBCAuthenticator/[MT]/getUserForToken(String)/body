{
  String name=conf.get("stargate.auth.token." + token);
  if (name == null) {
    return null;
  }
  boolean admin=conf.getBoolean("stargate.auth.user." + name + ".admin",false);
  boolean disabled=conf.getBoolean("stargate.auth.user." + name + ".disabled",false);
  return new User(name,token,admin,disabled);
}
