{
  String className=conf.get(STORE_ENGINE_CLASS_KEY,DEFAULT_STORE_ENGINE_CLASS.getName());
  try {
    return ReflectionUtils.instantiateWithCustomCtor(className,new Class[]{Configuration.class,Store.class,KVComparator.class},new Object[]{conf,store,kvComparator});
  }
 catch (  Exception e) {
    throw new IOException("Unable to load configured store engine '" + className + "'",e);
  }
}
