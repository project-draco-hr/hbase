{
  int count=0;
  for (char c='a'; c <= 'c'; c++) {
    byte[] row=Bytes.toBytes("ab" + c);
    int i, j;
    long ts=System.currentTimeMillis();
    for (i=0; i < 100; i++) {
      byte[] b=Bytes.toBytes(String.format("%10d",i));
      for (j=0; j < 100; j++) {
        Put put=new Put(row);
        put.setDurability(Durability.SKIP_WAL);
        put.add(COLUMNS[rng.nextInt(COLUMNS.length)],b,++ts,b);
        region.put(put);
        count++;
      }
    }
  }
  return count;
}
