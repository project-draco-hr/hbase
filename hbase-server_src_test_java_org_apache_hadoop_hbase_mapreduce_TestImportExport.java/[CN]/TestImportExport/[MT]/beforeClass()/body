{
  UTIL.getConfiguration().setStrings(CoprocessorHost.REGION_COPROCESSOR_CONF_KEY,ExportEndpoint.class.getName());
  UTIL.getConfiguration().setInt(HConstants.REGION_SERVER_HIGH_PRIORITY_HANDLER_COUNT,10);
  UTIL.startMiniCluster();
  FileSystem fs=FileSystem.get(UTIL.getConfiguration());
  FQ_OUTPUT_DIR=new Path(OUTPUT_DIR).makeQualified(fs.getUri(),fs.getWorkingDirectory()).toString();
}
