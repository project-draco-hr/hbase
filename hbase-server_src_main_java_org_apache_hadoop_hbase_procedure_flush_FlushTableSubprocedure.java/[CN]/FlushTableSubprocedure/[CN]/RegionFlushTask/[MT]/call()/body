{
  LOG.debug("Starting region operation on " + region);
  region.startRegionOperation();
  try {
    LOG.debug("Flush region " + region.toString() + " started...");
    region.flushcache();
  }
  finally {
    LOG.debug("Closing region operation on " + region);
    region.closeRegionOperation();
  }
  return null;
}
