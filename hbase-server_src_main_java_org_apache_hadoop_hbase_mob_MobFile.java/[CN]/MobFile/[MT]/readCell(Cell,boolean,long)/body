{
  Cell result=null;
  StoreFileScanner scanner=null;
  List<StoreFile> sfs=new ArrayList<StoreFile>();
  sfs.add(sf);
  try {
    List<StoreFileScanner> sfScanners=StoreFileScanner.getScannersForStoreFiles(sfs,cacheMobBlocks,true,false,null,readPt);
    if (!sfScanners.isEmpty()) {
      scanner=sfScanners.get(0);
      if (scanner.seek(search)) {
        result=scanner.peek();
      }
    }
  }
  finally {
    if (scanner != null) {
      scanner.close();
    }
  }
  return result;
}
