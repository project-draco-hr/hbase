{
  this.lock.readLock().lock();
  try {
    List<HStoreKey> results;
synchronized (memcache) {
      results=getKeys(this.memcache,origin,versions,deletes,now,columnPattern);
    }
synchronized (snapshot) {
      results.addAll(results.size(),getKeys(snapshot,origin,versions == HConstants.ALL_VERSIONS ? versions : (versions - results.size()),deletes,now,columnPattern));
    }
    return results;
  }
  finally {
    this.lock.readLock().unlock();
  }
}
