{
  this.lock.writeLock().lock();
  try {
    if (this.snapshot != ss) {
      throw new UnexpectedException("Current snapshot is " + this.snapshot + ", was passed "+ ss);
    }
    if (!ss.isEmpty()) {
      this.snapshot=createMap(this.comparator);
    }
  }
  finally {
    this.lock.writeLock().unlock();
  }
}
