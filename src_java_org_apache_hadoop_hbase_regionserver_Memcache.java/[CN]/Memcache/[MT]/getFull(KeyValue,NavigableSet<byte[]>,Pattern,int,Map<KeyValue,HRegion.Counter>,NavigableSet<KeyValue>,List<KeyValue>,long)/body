{
  this.lock.readLock().lock();
  try {
    if (getFull(this.memcache,key,columns,columnPattern,numVersions,versionsCount,deletes,results,now)) {
      return true;
    }
    return getFull(this.snapshot,key,columns,columnPattern,numVersions,versionsCount,deletes,results,now);
  }
  finally {
    this.lock.readLock().unlock();
  }
}
