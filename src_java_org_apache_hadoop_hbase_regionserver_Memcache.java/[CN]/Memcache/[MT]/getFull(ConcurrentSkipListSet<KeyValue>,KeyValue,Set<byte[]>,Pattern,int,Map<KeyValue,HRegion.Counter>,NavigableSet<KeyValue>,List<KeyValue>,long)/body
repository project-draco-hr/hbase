{
  boolean hasEnough=false;
  if (target == null) {
    return hasEnough;
  }
  NavigableSet<KeyValue> tailset=set.tailSet(target);
  if (tailset == null || tailset.isEmpty()) {
    return hasEnough;
  }
  for (  KeyValue kv : tailset) {
    if (target.isEmptyColumn() ? !this.comparator.matchingRows(target,kv) : !this.comparator.matchingRowColumn(target,kv)) {
      break;
    }
    if (!Store.getFullCheck(this.comparator,target,kv,columns,columnPattern)) {
      continue;
    }
    if (Store.doKeyValue(kv,versions,versionCounter,columns,deletes,now,this.ttl,keyvalues,tailset)) {
      hasEnough=true;
      break;
    }
  }
  return hasEnough;
}
