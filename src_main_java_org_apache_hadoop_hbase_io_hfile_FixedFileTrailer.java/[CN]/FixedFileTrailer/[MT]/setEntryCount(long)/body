{
  if (majorVersion == 1) {
    int intEntryCount=(int)Math.min(Integer.MAX_VALUE,newEntryCount);
    if (intEntryCount != newEntryCount) {
      LOG.info("Warning: entry count is " + newEntryCount + " but writing "+ intEntryCount+ " into the version "+ majorVersion+ " trailer");
    }
    entryCount=intEntryCount;
    return;
  }
  entryCount=newEntryCount;
}
