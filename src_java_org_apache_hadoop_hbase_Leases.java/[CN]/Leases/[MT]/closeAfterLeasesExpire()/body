{
synchronized (this.leases) {
    while (this.leases.size() > 0) {
      LOG.info(Thread.currentThread().getName() + " " + Integer.toString(leases.size())+ " lease(s) "+ "outstanding. Waiting for them to expire.");
      try {
        this.leases.wait(this.leaseCheckFrequency);
      }
 catch (      InterruptedException e) {
      }
    }
  }
  close();
}
