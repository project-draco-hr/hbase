{
  HTableDescriptor htd=new HTableDescriptor("hank",10);
  htd.addFamily(new Text("hankfamily:"));
  htd.addFamily(new Text("hankotherfamily:"));
  assertEquals("Table descriptor",htd.toString(),"name: hank, maxVersions: 10, families: [hankfamily:, hankotherfamily:]");
  HRegionInfo hri=new HRegionInfo(-1,htd,new Text(),new Text("10"));
  assertEquals("HRegionInfo","regionname: hank__-1, startKey: <>, tableDesc: {name: hank, " + "maxVersions: 10, families: [hankfamily:, hankotherfamily:]}",hri.toString());
}
