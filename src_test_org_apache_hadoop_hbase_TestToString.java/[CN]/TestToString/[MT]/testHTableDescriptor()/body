{
  HTableDescriptor htd=HTableDescriptor.rootTableDesc;
  System.out.println(htd.toString());
  assertEquals("Table descriptor","name: -ROOT-, families: {info:={name: " + "info, max versions: 1, compression: NONE, in memory: false, " + "block cache enabled: false, max length: 2147483647, "+ "time to live: FOREVER, bloom filter: none}}",htd.toString());
}
