{
  return "reader=" + this.name + (!isFileInfoLoaded() ? "" : ", compression=" + this.compressAlgo.getName() + ", firstKey="+ Bytes.toString(getFirstKey())+ ", lastKey="+ Bytes.toString(getLastKey())+ ", avgKeyLen="+ this.avgKeyLen+ ", avgValueLen="+ this.avgValueLen+ ", entries="+ this.trailer.entryCount+ ", length="+ this.fileSize);
}
