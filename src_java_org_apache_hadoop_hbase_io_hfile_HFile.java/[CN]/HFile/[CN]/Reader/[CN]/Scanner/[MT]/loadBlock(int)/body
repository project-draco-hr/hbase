{
  if (block == null) {
    block=reader.readBlock(bloc,cacheBlocks);
    currBlock=bloc;
    blockFetches++;
  }
 else {
    if (bloc != currBlock) {
      block=reader.readBlock(bloc,cacheBlocks);
      currBlock=bloc;
      blockFetches++;
    }
 else {
      block.rewind();
      block.position(DATABLOCKMAGIC.length);
    }
  }
}
