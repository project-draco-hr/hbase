{
  this.outputStream=ostream;
  this.closeOutputStream=false;
  this.blocksize=blocksize;
  this.comparator=c == null ? Bytes.BYTES_RAWCOMPARATOR : c;
  this.name=this.outputStream.toString();
  this.compressAlgo=Compression.getCompressionAlgorithmByName(compress == null ? Compression.Algorithm.NONE.getName() : compress);
}
