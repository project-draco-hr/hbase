{
  checkKey(key);
  checkValue(value);
  checkBlockBoundary();
  this.out.writeInt(key.length);
  this.keylength+=key.length;
  this.out.writeInt(value.length);
  this.valuelength+=valuelength;
  this.out.write(key);
  if (value.length > 0) {
    this.out.write(value);
  }
  if (this.firstKey == null)   this.firstKey=key;
  this.lastKey=key;
  this.entryCount++;
}
