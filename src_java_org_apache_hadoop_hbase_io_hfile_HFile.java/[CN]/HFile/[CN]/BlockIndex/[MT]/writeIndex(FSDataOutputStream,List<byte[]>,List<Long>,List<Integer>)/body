{
  long pos=o.getPos();
  if (keys.size() > 0) {
    o.write(INDEXBLOCKMAGIC);
    for (int i=0; i < keys.size(); ++i) {
      o.writeLong(offsets.get(i).longValue());
      o.writeInt(sizes.get(i).intValue());
      byte[] key=keys.get(i);
      Bytes.writeByteArray(o,key);
    }
  }
  return pos;
}
