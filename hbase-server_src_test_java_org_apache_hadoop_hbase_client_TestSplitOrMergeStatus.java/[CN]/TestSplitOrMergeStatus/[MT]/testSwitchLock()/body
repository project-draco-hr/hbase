{
  Admin admin=TEST_UTIL.getAdmin();
  admin.setSplitOrMergeEnabled(false,false,false,MasterSwitchType.SPLIT,MasterSwitchType.MERGE);
  try {
    admin.setSplitOrMergeEnabled(false,false,true,MasterSwitchType.SPLIT,MasterSwitchType.MERGE);
    fail();
  }
 catch (  IOException e) {
    LOG.info("",e);
  }
  admin.releaseSplitOrMergeLockAndRollback();
  try {
    admin.setSplitOrMergeEnabled(true,false,true,MasterSwitchType.SPLIT,MasterSwitchType.MERGE);
  }
 catch (  IOException e) {
    fail();
  }
  admin.close();
}
