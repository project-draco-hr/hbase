{
  while (currentNumberOfTask == tasksDone.get()) {
    try {
synchronized (this.tasksDone) {
        this.tasksDone.wait(100);
      }
    }
 catch (    InterruptedException e) {
      throw new InterruptedIOException("#" + id + ", interrupted."+ " currentNumberOfTask="+ currentNumberOfTask+ ",  tableName="+ tableName+ ", tasksDone="+ tasksDone.get());
    }
  }
}
