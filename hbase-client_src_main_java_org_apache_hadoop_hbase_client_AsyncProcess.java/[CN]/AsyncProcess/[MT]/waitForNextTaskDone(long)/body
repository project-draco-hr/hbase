{
  while (currentNumberOfTask == tasksDone.get()) {
    try {
synchronized (this.tasksDone) {
        this.tasksDone.wait(100);
      }
    }
 catch (    InterruptedException e) {
      throw new InterruptedIOException("Interrupted." + " currentNumberOfTask=" + currentNumberOfTask + ",  tableName="+ Bytes.toString(tableName)+ ", tasksDone="+ tasksDone.get());
    }
  }
}
