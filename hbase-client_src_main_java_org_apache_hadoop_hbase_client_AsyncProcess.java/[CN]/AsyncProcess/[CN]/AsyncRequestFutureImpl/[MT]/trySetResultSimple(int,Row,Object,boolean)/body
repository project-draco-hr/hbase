{
  Object resObj=null;
  if (!isReplicaGet(row)) {
    if (isFromReplica) {
      throw new AssertionError("Unexpected stale result for " + row);
    }
    results[index]=result;
  }
 else {
synchronized (replicaResultLock) {
      if ((resObj=results[index]) == null) {
        if (isFromReplica) {
          throw new AssertionError("Unexpected stale result for " + row);
        }
        results[index]=result;
      }
    }
  }
  return (resObj == null || !(resObj instanceof ReplicaResultState)) ? null : (ReplicaResultState)resObj;
}
