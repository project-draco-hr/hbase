{
  try {
    waitUntilDone(Long.MAX_VALUE);
  }
 catch (  InterruptedException iex) {
    throw new InterruptedIOException(iex.getMessage());
  }
 finally {
    if (callsInProgress != null) {
      for (      MultiServerCallable<Row> clb : callsInProgress) {
        clb.cancel();
      }
    }
  }
}
