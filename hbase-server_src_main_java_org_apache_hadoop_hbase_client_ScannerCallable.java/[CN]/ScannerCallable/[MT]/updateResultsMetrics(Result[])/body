{
  if (this.scanMetrics == null || rrs == null) {
    return;
  }
  for (  Result rr : rrs) {
    if (rr.getBytes() != null) {
      this.scanMetrics.countOfBytesInResults.inc(rr.getBytes().getLength());
      if (isRegionServerRemote) {
        this.scanMetrics.countOfBytesInRemoteResults.inc(rr.getBytes().getLength());
      }
    }
  }
}
