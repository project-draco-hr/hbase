{
  LOG.debug(zkw.prefix("Forcing an existing unassigned node for " + region.getEncodedName() + " to an OFFLINE state"));
  RegionTransitionData data=new RegionTransitionData(EventType.M2ZK_REGION_OFFLINE,region.getRegionName(),serverName);
synchronized (zkw.getNodes()) {
    String node=getNodeName(zkw,region.getEncodedName());
    zkw.getNodes().add(node);
    ZKUtil.setData(zkw,node,data.getBytes());
  }
}
