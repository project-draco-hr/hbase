{
  if (userProvider.isHBaseSecurityEnabled()) {
    if (userToken != null && !hasForwardedToken) {
      try {
        userToken.cancel(this.fs.getConf());
      }
 catch (      Exception e) {
        LOG.warn("Failed to cancel HDFS delegation token: " + userToken,e);
      }
    }
    this.userToken=null;
    this.fs=null;
  }
}
