{
  final HConnection connection=this;
  return new Callable<MultiResponse>(){
    public MultiResponse call() throws IOException {
      return getRegionServerWithoutRetries(new ServerCallable<MultiResponse>(connection,tableName,null){
        public MultiResponse call() throws IOException {
          return server.multi(multi);
        }
        @Override public void connect(        boolean reload) throws IOException {
          server=connection.getHRegionConnection(loc.getHostname(),loc.getPort());
        }
      }
);
    }
  }
;
}
