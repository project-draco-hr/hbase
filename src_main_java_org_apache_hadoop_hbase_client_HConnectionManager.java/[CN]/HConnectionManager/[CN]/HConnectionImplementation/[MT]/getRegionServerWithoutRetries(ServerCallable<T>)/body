{
  try {
    callable.beforeCall();
    callable.connect(false);
    return callable.call();
  }
 catch (  Throwable t) {
    Throwable t2=translateException(t);
    if (t2 instanceof IOException) {
      throw (IOException)t2;
    }
 else {
      throw new RuntimeException(t2);
    }
  }
 finally {
    callable.afterCall();
  }
}
