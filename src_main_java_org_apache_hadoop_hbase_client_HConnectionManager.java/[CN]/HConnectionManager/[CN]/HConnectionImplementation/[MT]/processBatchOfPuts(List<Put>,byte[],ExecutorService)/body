{
  Object[] results=new Object[list.size()];
  try {
    processBatch((List)list,tableName,pool,results);
  }
 catch (  InterruptedException e) {
    throw new IOException(e);
  }
 finally {
    for (int i=results.length - 1; i >= 0; i--) {
      if (results[i] instanceof Result) {
        list.remove(i);
      }
    }
  }
}
