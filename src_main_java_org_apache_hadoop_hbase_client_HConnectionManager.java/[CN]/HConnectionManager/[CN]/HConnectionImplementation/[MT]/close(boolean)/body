{
  if (master != null) {
    if (stopProxy) {
      HBaseRPC.stopProxy(master);
    }
    master=null;
    masterChecked=false;
  }
  if (stopProxy) {
    for (    HRegionInterface i : servers.values()) {
      HBaseRPC.stopProxy(i);
    }
  }
  if (this.zooKeeper != null) {
    LOG.info("Closed zookeeper sessionid=0x" + Long.toHexString(this.zooKeeper.getZooKeeper().getSessionId()));
    this.zooKeeper.close();
    this.zooKeeper=null;
  }
  this.closed=true;
}
