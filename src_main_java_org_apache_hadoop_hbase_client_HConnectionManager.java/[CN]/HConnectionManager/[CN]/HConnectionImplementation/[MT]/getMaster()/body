{
synchronized (this.masterAndZKLock) {
    canCloseMaster=false;
    try {
      return getKeepAliveMaster();
    }
 catch (    MasterNotRunningException e) {
      throw e;
    }
catch (    IOException e) {
      throw new ZooKeeperConnectionException("Can't create a connection to master",e);
    }
  }
}
