{
  if (zooKeeper == null) {
    try {
      this.zooKeeper=new ZooKeeperWatcher(conf,ZKUtil.getZooKeeperClusterKey(conf),this);
      LOG.debug("zkw created, sessionid=0x" + Long.toHexString(this.zooKeeper.getZooKeeper().getSessionId()));
    }
 catch (    IOException e) {
      throw new ZooKeeperConnectionException(e);
    }
  }
  return zooKeeper;
}
