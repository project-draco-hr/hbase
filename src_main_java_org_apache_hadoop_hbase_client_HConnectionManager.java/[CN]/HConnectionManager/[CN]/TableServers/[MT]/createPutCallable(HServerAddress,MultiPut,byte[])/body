{
  final HConnection connection=this;
  return new Callable<MultiPutResponse>(){
    public MultiPutResponse call() throws IOException {
      return getRegionServerWithoutRetries(new ServerCallable<MultiPutResponse>(connection,tableName,null){
        public MultiPutResponse call() throws IOException {
          MultiPutResponse resp=server.multiPut(puts);
          resp.request=puts;
          return resp;
        }
        @Override public void instantiateServer(        boolean reload) throws IOException {
          server=connection.getHRegionConnection(address);
        }
      }
);
    }
  }
;
}
