{
  if (master != null) {
    if (stopProxy) {
      HBaseRPC.stopProxy(master);
    }
    master=null;
    masterChecked=false;
  }
  if (stopProxy) {
    for (    HRegionInterface i : servers.values()) {
      HBaseRPC.stopProxy(i);
    }
  }
  if (zooKeeper != null) {
    zooKeeper.close();
    zooKeeper=null;
  }
}
