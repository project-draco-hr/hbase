{
  final String m="--master=" + this.conf.get(HConstants.MASTER_ADDRESS);
  LOG.info("Creating table");
  assertEquals("create table",0,this.client.doCommandLine(new String[]{m,"createTable",getName(),"family:","1"}));
  assertEquals("list tables",0,this.client.doCommandLine(new String[]{m,"listTables"}));
  assertEquals("delete table",0,this.client.doCommandLine(new String[]{m,"deleteTable",getName()}));
}
