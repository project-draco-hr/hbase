{
  try {
    long procId=master.modifyTable(ProtobufUtil.toTableName(req.getTableName()),HTableDescriptor.convert(req.getTableSchema()),req.getNonceGroup(),req.getNonce());
    return ModifyTableResponse.newBuilder().setProcId(procId).build();
  }
 catch (  IOException ioe) {
    throw new ServiceException(ioe);
  }
}
