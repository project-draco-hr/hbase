{
  Entry e=entries[bucketIndex];
  Entry newE=new Entry(hash,key,value,e,tailPtr);
  entries[bucketIndex]=newE;
  if (size == 0) {
    headPtr=newE;
    tailPtr=newE;
  }
 else {
    newE.setPrevPtr(tailPtr);
    tailPtr.setNextPtr(newE);
    tailPtr=newE;
  }
  if (size++ >= threshold) {
    growTable(2 * entries.length);
  }
  return newE.heapSize();
}
