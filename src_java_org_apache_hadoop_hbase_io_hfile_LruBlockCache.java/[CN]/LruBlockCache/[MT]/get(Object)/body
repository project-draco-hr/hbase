{
  String key=(String)k;
  checkKey(key);
  int hash=hash(key);
  int i=hashIndex(hash,entries.length);
  Entry e=entries[i];
  while (true) {
    if (e == null) {
      missCount++;
      return null;
    }
    if (e.hash == hash && isEqual(key,e.key)) {
      hitCount++;
      updateLru(e);
      return e.value;
    }
    e=e.next;
  }
}
