{
  super(conf,storeConfigInfo);
  try {
    compactionPolicyPerWindow=ReflectionUtils.instantiateWithCustomCtor(comConf.getCompactionPolicyForTieredWindow(),new Class[]{Configuration.class,StoreConfigInformation.class},new Object[]{conf,storeConfigInfo});
  }
 catch (  Exception e) {
    throw new IOException("Unable to load configured compaction policy '" + comConf.getCompactionPolicyForTieredWindow() + "'",e);
  }
}
