{
  StringBuilder path=new StringBuilder();
  path.append('/');
  path.append("checkandput");
  path.append('/');
  path.append(table);
  path.append('/');
  path.append(row);
  path.append('/');
  path.append(column);
  return checkAndPutValueXML(path.toString(),table,row,column,valueToCheck,valueToPut);
}
