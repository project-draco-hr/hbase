{
  super.setUp();
  client=new Client(new Cluster().add("localhost",testServletPort));
  admin=new HBaseAdmin(conf);
  if (admin.tableExists(TABLE)) {
    return;
  }
  HTableDescriptor htd=new HTableDescriptor(TABLE);
  htd.addFamily(new HColumnDescriptor(KeyValue.parseColumn(Bytes.toBytes(COLUMN_1))[0]));
  htd.addFamily(new HColumnDescriptor(KeyValue.parseColumn(Bytes.toBytes(COLUMN_2))[0]));
  admin.createTable(htd);
}
