{
  String regionName=hri.getEncodedName();
  RegionState regionState=regionStates.get(regionName);
  if (regionState != null) {
    LOG.warn("Tried to create a state of a region already in RegionStates " + hri + ", used existing state: "+ regionState+ ", ignored new state: state=OFFLINE, server=null");
  }
 else {
    regionState=new RegionState(hri,State.OFFLINE);
    regionStates.put(regionName,regionState);
  }
  return regionState;
}
