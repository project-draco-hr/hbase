{
  Long pollResult=null;
  lock.lock();
  try {
    if (queueSize == 0) {
      waitCond.await();
      if (queueSize == 0) {
        return null;
      }
    }
    pollResult=doPoll(serverFairQ.poll());
    if (pollResult == null) {
      pollResult=doPoll(tableFairQ.poll());
    }
  }
 catch (  InterruptedException e) {
    Thread.currentThread().interrupt();
  }
 finally {
    lock.unlock();
  }
  return pollResult;
}
