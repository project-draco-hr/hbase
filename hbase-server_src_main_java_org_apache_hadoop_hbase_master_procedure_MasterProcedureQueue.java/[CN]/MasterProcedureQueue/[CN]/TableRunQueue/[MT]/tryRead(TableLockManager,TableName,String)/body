{
synchronized (this) {
    if (wlock) {
      return false;
    }
    tableLock=lockManager.readLock(tableName,purpose);
    try {
      tableLock.acquire();
    }
 catch (    IOException e) {
      LOG.error("failed acquire read lock on " + tableName,e);
      tableLock=null;
      return false;
    }
    rlock++;
  }
  return true;
}
