{
  super(fs,dirName,new HStoreKey.HStoreKeyWritableComparator(hri),conf,false);
  this.dirName=dirName;
  this.blockCacheEnabled=blockCacheEnabled;
  open(fs,dirName,new HStoreKey.HStoreKeyWritableComparator(hri),conf);
  midKey();
  reset();
  HStoreKey key=new HStoreKey();
  super.next(key,new ImmutableBytesWritable());
  key.setHRegionInfo(hri);
  this.firstKey=key;
  reset();
  key=new HStoreKey();
  super.finalKey(key);
  key.setHRegionInfo(hri);
  this.finalKey=key;
}
