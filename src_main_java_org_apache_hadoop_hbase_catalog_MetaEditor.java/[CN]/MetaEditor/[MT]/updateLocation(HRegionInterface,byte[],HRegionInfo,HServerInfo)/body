{
  Put put=new Put(regionInfo.getRegionName());
  addLocation(put,serverInfo);
  server.put(catalogRegionName,put);
  LOG.info("Updated row " + regionInfo.getRegionNameAsString() + " in region "+ Bytes.toString(catalogRegionName)+ " with "+ "server="+ serverInfo.getHostnamePort()+ ", "+ "startcode="+ serverInfo.getStartCode());
}
