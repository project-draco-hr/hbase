{
  if (this.currentRegion.getRegionInfo().getTableName().getNameAsString().equals("testShouldFailSplitIfZNodeDoesNotExistDueToPrevRollBack")) {
    try {
      if (!secondSplit) {
        callRollBack=true;
        latch.await();
      }
    }
 catch (    InterruptedException e) {
    }
  }
  super.transitionZKNode(server,services,a,b);
  if (this.currentRegion.getRegionInfo().getTableName().getNameAsString().equals("testShouldFailSplitIfZNodeDoesNotExistDueToPrevRollBack")) {
    firstSplitCompleted=true;
  }
}
