{
  if (columnOffset != null) {
    if (count >= limit) {
      return ReturnCode.NEXT_ROW;
    }
    byte[] buffer=v.getBuffer();
    if (buffer == null) {
      return ReturnCode.SEEK_NEXT_USING_HINT;
    }
    int cmp=0;
    if (count == 0) {
      cmp=Bytes.compareTo(buffer,v.getQualifierOffset(),v.getQualifierLength(),this.columnOffset,0,this.columnOffset.length);
    }
    if (cmp < 0) {
      return ReturnCode.SEEK_NEXT_USING_HINT;
    }
 else {
      count++;
      return ReturnCode.INCLUDE_AND_NEXT_COL;
    }
  }
 else {
    if (count >= offset + limit) {
      return ReturnCode.NEXT_ROW;
    }
    ReturnCode code=count < offset ? ReturnCode.NEXT_COL : ReturnCode.INCLUDE_AND_NEXT_COL;
    count++;
    return code;
  }
}
