{
  byte isInitialized=memoizedIsInitialized;
  if (isInitialized != -1)   return isInitialized == 1;
  for (int i=0; i < getRegionInfoCount(); i++) {
    if (!getRegionInfo(i).isInitialized()) {
      memoizedIsInitialized=0;
      return false;
    }
  }
  memoizedIsInitialized=1;
  return true;
}
