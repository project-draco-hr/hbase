{
  try {
    HTable localMeta=new HTable(conf,HConstants.META_TABLE_NAME);
    Configuration otherConf=HBaseConfiguration.create(conf);
    otherConf.set(HConstants.ZOOKEEPER_QUORUM,"127.0.0.1");
    HTable ipMeta=new HTable(otherConf,HConstants.META_TABLE_NAME);
    localMeta.exists(new Get(HConstants.LAST_ROW));
    ipMeta.exists(new Get(HConstants.LAST_ROW));
    assertFalse(HConnectionManager.getConnection(conf).getZooKeeperWatcher() == HConnectionManager.getConnection(otherConf).getZooKeeperWatcher());
    assertFalse(HConnectionManager.getConnection(conf).getZooKeeperWatcher().getQuorum().equals(HConnectionManager.getConnection(otherConf).getZooKeeperWatcher().getQuorum()));
  }
 catch (  Exception e) {
    e.printStackTrace();
    fail();
  }
}
