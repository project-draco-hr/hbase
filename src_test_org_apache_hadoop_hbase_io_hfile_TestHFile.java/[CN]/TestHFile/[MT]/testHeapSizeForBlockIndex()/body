{
  ClassSize cs=null;
  Class cl=null;
  long expected=0L;
  long actual=0L;
  try {
    cs=new ClassSize();
  }
 catch (  Exception e) {
  }
  cl=BlockIndex.class;
  expected=cs.estimateBase(cl,false);
  BlockIndex bi=new BlockIndex(Bytes.BYTES_RAWCOMPARATOR);
  actual=bi.heapSize();
  actual-=32;
  if (expected != actual) {
    cs.estimateBase(cl,true);
    assertEquals(expected,actual);
  }
}
