{
  boolean interrupted=false;
synchronized (readWaiters) {
    while (readPoint.get() < e.getWriteNumber()) {
      try {
        readWaiters.wait(0);
      }
 catch (      InterruptedException ie) {
        interrupted=true;
      }
    }
  }
  if (interrupted) {
    Thread.currentThread().interrupt();
  }
}
