{
  HTable table=new HTable(util.getConfiguration(),TEST_TABLE);
  Map<byte[],Object> results=table.coprocessorExec(PingProtocol.class,ROW_A,ROW_C,new Batch.Call<PingProtocol,Object>(){
    public Object call(    PingProtocol instance){
      instance.noop();
      return null;
    }
  }
);
  assertEquals("Should have results from three regions",3,results.size());
  for (  Object v : results.values()) {
    assertNull(v);
  }
}
