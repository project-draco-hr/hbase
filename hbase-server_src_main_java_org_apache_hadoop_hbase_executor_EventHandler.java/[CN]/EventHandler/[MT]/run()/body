{
  TraceScope chunk=Trace.startSpan(this.getClass().getSimpleName(),parent);
  try {
    if (getListener() != null)     getListener().beforeProcess(this);
    process();
    if (getListener() != null)     getListener().afterProcess(this);
  }
 catch (  Throwable t) {
    handleException(t);
  }
 finally {
    chunk.close();
  }
}
