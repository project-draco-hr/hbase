{
  byte[] values={'A','B','C','D'};
  List<Put> puts=new ArrayList<>();
  for (int i=0; i < values.length; i++) {
    for (int j=0; j < values.length; j++) {
      Put put=new Put(new byte[]{values[i],values[j]});
      put.add(Bytes.toBytes("fam"),new byte[]{},new byte[]{values[i],values[j]});
      puts.add(put);
    }
  }
  tbl.put(puts);
}
