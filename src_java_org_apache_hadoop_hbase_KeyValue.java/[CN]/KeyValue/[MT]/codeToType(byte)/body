{
  if (b == Put.getCode()) {
    return Put;
  }
 else   if (b == Delete.getCode()) {
    return Delete;
  }
 else   if (b == DeleteColumn.getCode()) {
    return DeleteColumn;
  }
 else   if (b == DeleteFamily.getCode()) {
    return DeleteFamily;
  }
 else   if (b == Maximum.getCode()) {
    return Maximum;
  }
  throw new RuntimeException("Unknown code " + b);
}
