{
  for (  byte[] regionName : emptyRows) {
    try {
      HRegion.removeRegionFromMETA(s,metaRegionName,regionName);
      LOG.warn("Removed region: " + Bytes.toString(regionName) + " from meta region: "+ Bytes.toString(metaRegionName)+ " because HRegionInfo was empty");
    }
 catch (    IOException e) {
      LOG.error("deleting region: " + Bytes.toString(regionName) + " from meta region: "+ Bytes.toString(metaRegionName),e);
    }
  }
}
