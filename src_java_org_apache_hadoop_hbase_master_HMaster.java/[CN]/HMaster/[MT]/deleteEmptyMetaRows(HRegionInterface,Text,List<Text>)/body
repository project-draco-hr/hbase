{
  for (  Text regionName : emptyRows) {
    try {
      HRegion.removeRegionFromMETA(server,metaRegionName,regionName);
      LOG.warn("Removed region: " + regionName + " from meta region: "+ metaRegionName+ " because HRegionInfo was empty");
    }
 catch (    IOException e) {
      LOG.error("deleting region: " + regionName + " from meta region: "+ metaRegionName,e);
    }
  }
}
