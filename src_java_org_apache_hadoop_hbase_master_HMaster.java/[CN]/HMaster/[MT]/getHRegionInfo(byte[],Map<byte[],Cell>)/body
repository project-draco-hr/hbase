{
  Cell regioninfo=map.get(COL_REGIONINFO);
  if (regioninfo == null) {
    StringBuilder sb=new StringBuilder();
    for (    byte[] e : map.keySet()) {
      if (sb.length() > 0) {
        sb.append(", ");
      }
      sb.append(Bytes.toString(e));
    }
    LOG.warn(Bytes.toString(COL_REGIONINFO) + " is empty for row: " + Bytes.toString(row)+ "; has keys: "+ sb.toString());
    return null;
  }
  return Writables.getHRegionInfo(regioninfo.getValue());
}
