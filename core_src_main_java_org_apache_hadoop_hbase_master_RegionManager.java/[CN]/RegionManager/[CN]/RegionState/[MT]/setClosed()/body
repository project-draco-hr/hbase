{
  if (!pendingClose && !pendingOpen && !closing) {
    throw new IllegalStateException("Cannot set a region to be closed if it was not already marked as" + " pending close, pending open or closing. State: " + toString());
  }
  this.unassigned=false;
  this.pendingOpen=false;
  this.open=false;
  this.closing=false;
  this.pendingClose=false;
  this.closed=true;
}
