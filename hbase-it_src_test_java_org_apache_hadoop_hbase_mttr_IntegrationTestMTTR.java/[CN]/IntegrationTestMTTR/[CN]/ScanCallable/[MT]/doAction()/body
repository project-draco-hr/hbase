{
  ResultScanner rs=null;
  try {
    Scan s=new Scan();
    s.setBatch(2);
    s.addFamily(FAMILY);
    s.setFilter(new KeyOnlyFilter());
    s.setMaxVersions(1);
    rs=table.getScanner(s);
    Result result=rs.next();
    return rs != null && result != null && result.size() > 0;
  }
  finally {
    if (rs != null) {
      rs.close();
    }
  }
}
