{
  MetricsTag tag=new MetricsTag(name,description,value);
  if (!override) {
    MetricsTag existing=tagsMap.putIfAbsent(name,tag);
    if (existing != null) {
      throw new MetricsException("Tag " + name + " already exists!");
    }
    return this;
  }
  tagsMap.put(name,tag);
  return this;
}
