{
  if (!(obj instanceof HServerInfo)) {
    return false;
  }
  HServerInfo that=(HServerInfo)obj;
  if (!this.serverAddress.equals(that.serverAddress)) {
    return false;
  }
  if (this.infoPort != that.infoPort) {
    return false;
  }
  if (this.startCode != that.startCode) {
    return false;
  }
  return true;
}
