{
  StringBuilder sb=new StringBuilder("THRegionLocation(");
  boolean first=true;
  sb.append("serverName:");
  if (this.serverName == null) {
    sb.append("null");
  }
 else {
    sb.append(this.serverName);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("regionInfo:");
  if (this.regionInfo == null) {
    sb.append("null");
  }
 else {
    sb.append(this.regionInfo);
  }
  first=false;
  sb.append(")");
  return sb.toString();
}
