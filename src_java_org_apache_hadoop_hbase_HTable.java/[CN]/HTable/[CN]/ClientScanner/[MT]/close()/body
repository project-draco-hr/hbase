{
  checkClosed();
  if (this.scannerId != -1L) {
    try {
      this.server.close(this.scannerId);
    }
 catch (    IOException e) {
      if (e instanceof RemoteException) {
        e=RemoteExceptionHandler.decodeRemoteException((RemoteException)e);
      }
      if (!(e instanceof NotServingRegionException)) {
        throw e;
      }
    }
    this.scannerId=-1L;
  }
  this.server=null;
  this.closed=true;
}
