{
  if (rows.isEmpty()) {
    return;
  }
  HTableInterface table=null;
  try {
    table=new HTable(tableName,this.sharedHtableCon,this.sharedThreadPool);
    table.batch(rows);
  }
 catch (  InterruptedException ix) {
    throw new IOException(ix);
  }
 finally {
    if (table != null) {
      table.close();
    }
  }
}
