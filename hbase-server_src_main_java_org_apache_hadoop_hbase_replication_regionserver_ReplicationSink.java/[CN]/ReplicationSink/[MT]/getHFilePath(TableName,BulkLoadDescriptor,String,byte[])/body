{
  return new StringBuilder(100).append(table.getNamespaceAsString()).append(Path.SEPARATOR).append(table.getQualifierAsString()).append(Path.SEPARATOR).append(Bytes.toString(bld.getEncodedRegionName().toByteArray())).append(Path.SEPARATOR).append(Bytes.toString(family)).append(Path.SEPARATOR).append(storeFile).toString();
}
