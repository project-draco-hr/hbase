{
  if (!isBlockCacheEnabled()) {
    return "CacheConfig:disabled";
  }
  return "blockCache=" + getBlockCache() + ", cacheDataOnRead="+ shouldCacheDataOnRead()+ ", cacheDataOnWrite="+ shouldCacheDataOnWrite()+ ", cacheIndexesOnWrite="+ shouldCacheIndexesOnWrite()+ ", cacheBloomsOnWrite="+ shouldCacheBloomsOnWrite()+ ", cacheEvictOnClose="+ shouldEvictOnClose()+ ", cacheCompressed="+ shouldCacheCompressed()+ ", prefetchOnOpen="+ shouldPrefetchOnOpen();
}
