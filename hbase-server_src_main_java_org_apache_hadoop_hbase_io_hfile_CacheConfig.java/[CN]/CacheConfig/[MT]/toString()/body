{
  if (!isBlockCacheEnabled()) {
    return "CacheConfig:disabled";
  }
  return "CacheConfig:enabled " + "[cacheDataOnRead=" + shouldCacheDataOnRead() + "] "+ "[cacheDataOnWrite="+ shouldCacheDataOnWrite()+ "] "+ "[cacheIndexesOnWrite="+ shouldCacheIndexesOnWrite()+ "] "+ "[cacheBloomsOnWrite="+ shouldCacheBloomsOnWrite()+ "] "+ "[cacheEvictOnClose="+ shouldEvictOnClose()+ "] "+ "[cacheCompressed="+ shouldCacheCompressed()+ "]";
}
