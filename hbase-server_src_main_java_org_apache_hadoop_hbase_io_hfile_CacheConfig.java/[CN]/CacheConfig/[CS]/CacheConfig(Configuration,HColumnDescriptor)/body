{
  this(CacheConfig.instantiateBlockCache(conf),family.isBlockCacheEnabled(),family.isInMemory(),conf.getBoolean(CACHE_BLOCKS_ON_WRITE_KEY,DEFAULT_CACHE_DATA_ON_WRITE) || family.shouldCacheDataOnWrite(),conf.getBoolean(CACHE_INDEX_BLOCKS_ON_WRITE_KEY,DEFAULT_CACHE_INDEXES_ON_WRITE) || family.shouldCacheIndexesOnWrite(),conf.getBoolean(CACHE_BLOOM_BLOCKS_ON_WRITE_KEY,DEFAULT_CACHE_BLOOMS_ON_WRITE) || family.shouldCacheBloomsOnWrite(),conf.getBoolean(EVICT_BLOCKS_ON_CLOSE_KEY,DEFAULT_EVICT_ON_CLOSE) || family.shouldEvictBlocksOnClose(),conf.getBoolean(CACHE_DATA_BLOCKS_COMPRESSED_KEY,DEFAULT_COMPRESSED_CACHE));
}
