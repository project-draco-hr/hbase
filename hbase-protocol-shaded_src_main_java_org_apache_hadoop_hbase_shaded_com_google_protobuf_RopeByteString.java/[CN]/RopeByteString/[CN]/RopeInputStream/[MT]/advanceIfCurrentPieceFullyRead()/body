{
  if (currentPiece != null && currentPieceIndex == currentPieceSize) {
    currentPieceOffsetInRope+=currentPieceSize;
    currentPieceIndex=0;
    if (pieceIterator.hasNext()) {
      currentPiece=pieceIterator.next();
      currentPieceSize=currentPiece.size();
    }
 else {
      currentPiece=null;
      currentPieceSize=0;
    }
  }
}
