{
  if (store.getMemStoreSize() > this.flushSizeLowerBound) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("Column Family: " + store.getColumnFamilyName() + " of region "+ region+ " will be flushed because of memstoreSize("+ store.getMemStoreSize()+ ") is larger than lower bound("+ this.flushSizeLowerBound+ ")");
    }
    return true;
  }
  return region.shouldFlushStore(store);
}
