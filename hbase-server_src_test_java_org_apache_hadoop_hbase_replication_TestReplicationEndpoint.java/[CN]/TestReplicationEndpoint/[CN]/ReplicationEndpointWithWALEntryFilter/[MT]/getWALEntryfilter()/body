{
  return new ChainWALEntryFilter(super.getWALEntryfilter(),new WALEntryFilter(){
    @Override public Entry filter(    Entry entry){
      ArrayList<KeyValue> kvs=entry.getEdit().getKeyValues();
      int size=kvs.size();
      for (int i=size - 1; i >= 0; i--) {
        KeyValue kv=kvs.get(i);
        if (!Bytes.equals(kv.getRowArray(),kv.getRowOffset(),kv.getRowLength(),row,0,row.length)) {
          kvs.remove(i);
        }
      }
      return entry;
    }
  }
);
}
