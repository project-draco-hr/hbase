{
  TestHFileSeek testCase=new TestHFileSeek();
  MyOptions options=new MyOptions(argv);
  if (options.proceed == false) {
    return;
  }
  testCase.options=options;
  for (int i=0; i < options.trialCount; i++) {
    Log.info("Beginning trial " + (i + 1));
    testCase.setUp();
    testCase.testSeeks();
    testCase.tearDown();
  }
}
