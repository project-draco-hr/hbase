{
  SpanReceiver impl;
  try {
    Object o=ReflectionUtils.newInstance(implClass,conf);
    impl=(SpanReceiver)o;
  }
 catch (  SecurityException e) {
    throw new IOException(e);
  }
catch (  IllegalArgumentException e) {
    throw new IOException(e);
  }
catch (  RuntimeException e) {
    throw new IOException(e);
  }
  return impl;
}
