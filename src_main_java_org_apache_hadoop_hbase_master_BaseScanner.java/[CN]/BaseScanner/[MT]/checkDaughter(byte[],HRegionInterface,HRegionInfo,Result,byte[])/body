{
  HRegionInfo hri=getDaughterRegionInfo(rowContent,qualifier);
  boolean references=hasReferences(metaRegionName,srvr,parent,rowContent,hri,qualifier);
  if (!references)   return references;
  if (!verifyDaughterRowPresent(rowContent,qualifier,srvr,metaRegionName,hri,parent)) {
    addDaughterRowChecked(metaRegionName,srvr,parent.getRegionName(),hri,qualifier);
  }
  return references;
}
