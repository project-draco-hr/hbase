{
  boolean result=false;
  if (split == null) {
    return result;
  }
  Path tabledir=new Path(this.master.getRootDir(),split.getTableDesc().getNameAsString());
  for (  HColumnDescriptor family : split.getTableDesc().getFamilies()) {
    Path p=Store.getStoreHomedir(tabledir,split.getEncodedName(),family.getName());
    if (!this.master.getFileSystem().exists(p))     continue;
    FileStatus[] ps=this.master.getFileSystem().listStatus(p,new PathFilter(){
      public boolean accept(      Path path){
        return StoreFile.isReference(path);
      }
    }
);
    if (ps != null && ps.length > 0) {
      result=true;
      break;
    }
  }
  if (!result) {
    removeDaughterFromParent(metaRegionName,srvr,parent,split,qualifier);
  }
  return result;
}
