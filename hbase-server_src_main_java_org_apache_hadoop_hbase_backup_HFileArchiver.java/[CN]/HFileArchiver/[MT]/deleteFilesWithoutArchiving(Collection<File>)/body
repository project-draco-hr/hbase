{
  List<IOException> errors=new ArrayList<IOException>(0);
  for (  File file : files) {
    try {
      LOG.debug("Deleting region file:" + file);
      file.delete();
    }
 catch (    IOException e) {
      LOG.error("Failed to delete file:" + file);
      errors.add(e);
    }
  }
  if (errors.size() > 0) {
    throw MultipleIOException.createIOException(errors);
  }
}
