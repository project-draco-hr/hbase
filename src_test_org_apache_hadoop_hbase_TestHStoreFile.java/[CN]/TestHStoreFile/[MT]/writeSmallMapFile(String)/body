{
  Path path=new Path(DIR,name);
  MapFile.Writer writer=new MapFile.Writer(this.conf,fs,path.toString(),HStoreKey.class,ImmutableBytesWritable.class);
  try {
    for (char d=FIRST_CHAR; d <= LAST_CHAR; d++) {
      byte[] b=new byte[]{(byte)d};
      Text t=new Text(new String(b,HConstants.UTF8_ENCODING));
      writer.append(new HStoreKey(t,t,System.currentTimeMillis()),new ImmutableBytesWritable(t.getBytes()));
    }
  }
  finally {
    writer.close();
  }
  return path;
}
