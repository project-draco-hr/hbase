{
  Get get=new Get(row);
  get.addColumn(family,null);
  Result result=ht.get(get);
  assertSingleResult(result,row,family,null,value);
  get=new Get(row);
  get.addColumn(family,EMPTY);
  result=ht.get(get);
  assertSingleResult(result,row,family,EMPTY,value);
  get=new Get(row);
  get.addFamily(family);
  result=ht.get(get);
  assertSingleResult(result,row,family,EMPTY,value);
  get=new Get(row);
  result=ht.get(get);
  assertSingleResult(result,row,family,EMPTY,value);
}
