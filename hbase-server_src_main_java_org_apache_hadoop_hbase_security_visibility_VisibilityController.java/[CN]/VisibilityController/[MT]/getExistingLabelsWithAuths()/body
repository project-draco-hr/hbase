{
  Scan scan=new Scan();
  RegionScanner scanner=this.regionEnv.getRegion().getScanner(scan);
  List<List<Cell>> existingLabels=new ArrayList<List<Cell>>();
  try {
    while (true) {
      List<Cell> cells=new ArrayList<Cell>();
      scanner.next(cells);
      if (cells.isEmpty()) {
        break;
      }
      existingLabels.add(cells);
    }
  }
  finally {
    scanner.close();
  }
  return existingLabels;
}
