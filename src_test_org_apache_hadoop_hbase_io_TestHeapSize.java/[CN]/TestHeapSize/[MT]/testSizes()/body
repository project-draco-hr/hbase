{
  ClassSize cs=null;
  Class cl=null;
  long expected=0L;
  long actual=0L;
  try {
    cs=new ClassSize();
  }
 catch (  Exception e) {
  }
  cl=KeyValue.class;
  expected=cs.estimateBase(cl,false);
  KeyValue kv=new KeyValue();
  actual=kv.heapSize();
  if (expected != actual) {
    cs.estimateBase(cl,true);
    assertEquals(expected,actual);
  }
  cl=LruBlockCache.class;
  expected=cs.estimateBase(cl,false);
  LruBlockCache c=new LruBlockCache(1,1,200);
  actual=c.heapSize() - HeapSize.REFERENCE;
  if (expected != actual) {
    cs.estimateBase(cl,true);
    assertEquals(expected,actual);
  }
  cl=Put.class;
  expected=cs.estimateBase(cl,false);
  expected+=HeapSize.TREEMAP_SIZE;
  Put put=new Put(Bytes.toBytes(""));
  actual=put.heapSize();
  if (expected != actual) {
    cs.estimateBase(cl,true);
    assertEquals(expected,actual);
  }
}
