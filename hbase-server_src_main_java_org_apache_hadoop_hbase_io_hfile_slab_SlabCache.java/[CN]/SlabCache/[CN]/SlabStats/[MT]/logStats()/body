{
  AtomicLong[] fineGrainedStats=getUsage();
  for (int i=0; i < fineGrainedStats.length; i++) {
    if (fineGrainedStats[i].get() > 0) {
      SlabCache.LOG.info("From  " + StringUtils.humanReadableInt((long)getLowerBound(i)) + "- "+ StringUtils.humanReadableInt((long)getUpperBound(i))+ ": "+ StringUtils.humanReadableInt(fineGrainedStats[i].get())+ " requests");
    }
  }
}
