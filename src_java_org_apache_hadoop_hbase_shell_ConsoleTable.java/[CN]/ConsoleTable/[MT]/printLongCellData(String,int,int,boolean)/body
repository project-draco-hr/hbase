{
  if (end > key.length()) {
    if (key.length() > interval && end - interval < key.length()) {
      out.printf("%-20s",key.substring(end - interval,key.length()));
    }
 else {
      out.printf("%-20s","");
    }
  }
 else {
    out.printf("%-20s",key.substring(end - interval,end));
  }
  out.print(" | ");
  if (newLine)   out.println();
}
