{
  this.log.info(getName() + " started");
  final AtomicLong sequenceId=new AtomicLong(1);
  try {
    for (int i=0; i < this.count; i++) {
      long now=System.currentTimeMillis();
      if (i % 10 == 0 && ((FSHLog)this.wal).getNumEntries() > 0) {
        this.wal.rollWriter();
      }
      WALEdit edit=new WALEdit();
      byte[] bytes=Bytes.toBytes(i);
      edit.add(new KeyValue(bytes,bytes,bytes,now,EMPTY_1K_ARRAY));
      this.wal.append(HRegionInfo.FIRST_META_REGIONINFO,HTableDescriptor.META_TABLEDESC.getTableName(),edit,now,HTableDescriptor.META_TABLEDESC,sequenceId);
    }
    String msg=getName() + " finished";
    if (isException())     this.log.info(msg,getException());
 else     this.log.info(msg);
  }
 catch (  Exception e) {
    this.e=e;
    log.info("Caught exception from Appender:" + getName(),e);
  }
}
