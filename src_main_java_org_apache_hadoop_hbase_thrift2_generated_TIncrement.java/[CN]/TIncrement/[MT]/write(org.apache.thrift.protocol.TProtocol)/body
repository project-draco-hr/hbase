{
  validate();
  oprot.writeStructBegin(STRUCT_DESC);
  if (this.row != null) {
    oprot.writeFieldBegin(ROW_FIELD_DESC);
    oprot.writeBinary(this.row);
    oprot.writeFieldEnd();
