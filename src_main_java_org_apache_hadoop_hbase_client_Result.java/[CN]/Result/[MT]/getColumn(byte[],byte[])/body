{
  List<KeyValue> result=new ArrayList<KeyValue>();
  KeyValue[] kvs=raw();
  if (kvs == null || kvs.length == 0) {
    return result;
  }
  int pos=binarySearch(kvs,family,qualifier);
  if (pos == -1) {
    return result;
  }
  for (int i=pos; i < kvs.length; i++) {
    KeyValue kv=kvs[i];
    if (kv.matchingColumn(family,qualifier)) {
      result.add(kv);
    }
 else {
      break;
    }
  }
  return result;
}
