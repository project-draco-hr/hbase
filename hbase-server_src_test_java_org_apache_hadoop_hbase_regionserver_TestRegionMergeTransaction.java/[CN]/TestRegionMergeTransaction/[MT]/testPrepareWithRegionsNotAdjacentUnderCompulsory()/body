{
  RegionMergeTransaction mt=new RegionMergeTransaction(region_a,region_c,true);
  RegionMergeTransaction spyMT=Mockito.spy(mt);
  doReturn(false).when(spyMT).hasMergeQualifierInMeta(null,region_a.getRegionName());
  doReturn(false).when(spyMT).hasMergeQualifierInMeta(null,region_c.getRegionName());
  assertTrue("Since focible is true, should merge two regions even if they are not adjacent",spyMT.prepare(null));
}
