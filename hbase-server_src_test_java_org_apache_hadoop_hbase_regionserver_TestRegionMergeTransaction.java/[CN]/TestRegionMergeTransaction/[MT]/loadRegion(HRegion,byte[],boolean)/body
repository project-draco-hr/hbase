{
  byte[] k=new byte[3];
  int rowCount=0;
  for (byte b1='a'; b1 <= 'z'; b1++) {
    for (byte b2='a'; b2 <= 'z'; b2++) {
      for (byte b3='a'; b3 <= 'z'; b3++) {
        k[0]=b1;
        k[1]=b2;
        k[2]=b3;
        if (!HRegion.rowIsInRange(r.getRegionInfo(),k)) {
          continue;
        }
        Put put=new Put(k);
        put.add(f,null,k);
        if (r.getWAL() == null)         put.setDurability(Durability.SKIP_WAL);
        r.put(put);
        rowCount++;
      }
    }
    if (flush) {
      r.flushcache();
    }
  }
  return rowCount;
}
