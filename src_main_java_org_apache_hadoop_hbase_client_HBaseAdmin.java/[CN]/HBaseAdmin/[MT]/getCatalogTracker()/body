{
  if (this.catalogTracker == null) {
    this.catalogTracker=new CatalogTracker(this.connection.getZooKeeperWatcher(),ServerConnectionManager.getConnection(conf),this,this.conf.getInt("hbase.admin.catalog.timeout",10 * 1000));
    try {
      this.catalogTracker.start();
    }
 catch (    InterruptedException e) {
      Thread.currentThread().interrupt();
      throw new IOException("Interrupted",e);
    }
  }
  return this.catalogTracker;
}
