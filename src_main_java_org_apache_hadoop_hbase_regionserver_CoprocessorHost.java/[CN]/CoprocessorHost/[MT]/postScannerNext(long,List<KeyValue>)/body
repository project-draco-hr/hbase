{
  try {
    coprocessorLock.readLock().lock();
    for (    Environment env : coprocessors) {
      if (env.impl instanceof RegionObserver) {
        results=((RegionObserver)env.impl).postScannerNext(env,scannerId,results);
      }
    }
    return results;
  }
  finally {
    coprocessorLock.readLock().unlock();
  }
}
