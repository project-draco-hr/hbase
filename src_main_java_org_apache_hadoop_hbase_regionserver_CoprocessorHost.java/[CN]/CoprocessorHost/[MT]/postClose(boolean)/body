{
  try {
    coprocessorLock.writeLock().lock();
    for (    Environment env : coprocessors) {
      if (env.impl instanceof RegionObserver) {
        ((RegionObserver)env.impl).postClose(env,abortRequested);
      }
      env.shutdown();
    }
  }
  finally {
    coprocessorLock.writeLock().unlock();
  }
}
