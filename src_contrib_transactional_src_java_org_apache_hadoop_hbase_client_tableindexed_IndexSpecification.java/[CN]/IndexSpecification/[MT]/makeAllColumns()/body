{
  this.allColumns=new byte[indexedColumns.length + (additionalColumns == null ? 0 : additionalColumns.length)][];
  System.arraycopy(indexedColumns,0,allColumns,0,indexedColumns.length);
  if (additionalColumns != null) {
    System.arraycopy(additionalColumns,0,allColumns,indexedColumns.length,additionalColumns.length);
  }
}
