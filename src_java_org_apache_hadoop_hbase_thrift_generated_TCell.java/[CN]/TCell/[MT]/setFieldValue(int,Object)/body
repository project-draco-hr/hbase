{
switch (fieldID) {
case VALUE:
    if (value == null) {
      unsetValue();
    }
 else {
      setValue((byte[])value);
    }
  break;
case TIMESTAMP:
if (value == null) {
  unsetTimestamp();
}
 else {
  setTimestamp((Long)value);
}
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
