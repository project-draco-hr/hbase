{
  byte row[]=Bytes.toBytes("row:" + rowIdx);
  byte column[]=Bytes.toBytes("column:" + colIdx);
  Filter filter=new TimestampsFilter(versions);
  Get get=new Get(row);
  get.addColumn(cf,column);
  get.setFilter(filter);
  get.setMaxVersions();
  Result result=ht.get(get);
  return result.raw();
}
