{
  Put p=new Put(ROW_BYTES);
  for (  int colId : colIds) {
    long ts=Long.MAX_VALUE;
    String qual=qualFromId(colId);
    allColIds.add(colId);
    KeyValue kv=KeyValueTestUtil.create(ROW,FAMILY,qual,ts,TestMultiColumnScanner.createValue(ROW,qual,ts));
    p.add(kv);
  }
  region.put(p);
  region.flushcache();
}
