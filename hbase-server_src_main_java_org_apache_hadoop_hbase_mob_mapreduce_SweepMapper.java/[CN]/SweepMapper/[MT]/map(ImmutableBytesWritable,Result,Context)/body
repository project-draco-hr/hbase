{
  if (columns == null) {
    return;
  }
  KeyValue[] kvList=columns.raw();
  if (kvList == null || kvList.length == 0) {
    return;
  }
  for (  KeyValue kv : kvList) {
    if (MobUtils.hasValidMobRefCellValue(kv)) {
      String fileName=MobUtils.getMobFileName(kv);
      context.write(new Text(fileName),kv);
    }
  }
}
