{
  if (columns == null) {
    return;
  }
  Cell[] cells=columns.rawCells();
  if (cells == null || cells.length == 0) {
    return;
  }
  for (  Cell c : cells) {
    if (MobUtils.hasValidMobRefCellValue(c)) {
      String fileName=MobUtils.getMobFileName(c);
      context.write(new Text(fileName),KeyValueUtil.ensureKeyValue(c));
    }
  }
}
