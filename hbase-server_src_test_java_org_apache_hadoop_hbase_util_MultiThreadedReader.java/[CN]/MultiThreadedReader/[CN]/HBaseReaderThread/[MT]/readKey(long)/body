{
  Get get=new Get(LoadTestKVGenerator.md5PrefixedKey(keyToRead).getBytes());
  get.addFamily(columnFamily);
  try {
    if (verbose) {
      LOG.info("[" + readerId + "] "+ "Querying key "+ keyToRead+ ", cf "+ Bytes.toStringBinary(columnFamily));
    }
    queryKey(get,random.nextInt(100) < verifyPercent);
  }
 catch (  IOException e) {
    numReadFailures.addAndGet(1);
    LOG.debug("[" + readerId + "] FAILED read, key = "+ (keyToRead + "")+ ", time from start: "+ (System.currentTimeMillis() - startTimeMs)+ " ms");
  }
  return get;
}
