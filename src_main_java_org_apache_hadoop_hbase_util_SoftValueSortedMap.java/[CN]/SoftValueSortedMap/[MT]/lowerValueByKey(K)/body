{
synchronized (sync) {
    checkReferences();
    Map.Entry<K,SoftValue<K,V>> entry=((NavigableMap<K,SoftValue<K,V>>)this.internalMap).lowerEntry(key);
    if (entry == null) {
      return null;
    }
    SoftValue<K,V> value=entry.getValue();
    if (value == null) {
      return null;
    }
    if (value.get() == null) {
      this.internalMap.remove(key);
      return null;
    }
    return value.get();
  }
}
