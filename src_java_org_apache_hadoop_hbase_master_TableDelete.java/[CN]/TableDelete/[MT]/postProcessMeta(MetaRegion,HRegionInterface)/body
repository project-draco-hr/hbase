{
  for (  HRegionInfo i : unservedRegions) {
    try {
      HRegion.removeRegionFromMETA(server,m.getRegionName(),i.getRegionName());
      HRegion.deleteRegion(this.master.fs,this.master.rootdir,i);
    }
 catch (    IOException e) {
      LOG.error("failed to delete region " + i.getRegionName(),RemoteExceptionHandler.checkIOException(e));
    }
  }
  FileUtil.fullyDelete(master.fs,new Path(master.rootdir,tableName.toString()));
}
