{
  Tool t=new Migrate();
  GenericOptionsParser hadoopOpts=new GenericOptionsParser(t.getConf(),args);
  int status=0;
  try {
    status=ToolRunner.run(t,hadoopOpts.getRemainingArgs());
  }
 catch (  Exception e) {
    LOG.error("exiting due to error",e);
    status=-1;
  }
  System.exit(status);
}
