{
  puts.clear();
  int mapSize=in.readInt();
  for (int i=0; i < mapSize; i++) {
    byte[] key=Bytes.readByteArray(in);
    int listSize=in.readInt();
    List<Put> ps=new ArrayList<Put>(listSize);
    for (int j=0; j < listSize; j++) {
      Put put=new Put();
      put.readFields(in);
      ps.add(put);
    }
    puts.put(key,ps);
  }
}
