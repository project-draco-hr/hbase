{
  KeyValue peek=this.peek();
  if (peek == null) {
    return false;
  }
  byte[] row=peek.getRow();
  results.add(peek);
  if (limit > 0 && (results.size() == limit)) {
    return true;
  }
  while (true) {
    if (this.peek() == null) {
      break;
    }
    if (!Bytes.equals(row,this.peek().getRow())) {
      break;
    }
    results.add(this.next());
    if (limit > 0 && (results.size() == limit)) {
      break;
    }
  }
  return true;
}
