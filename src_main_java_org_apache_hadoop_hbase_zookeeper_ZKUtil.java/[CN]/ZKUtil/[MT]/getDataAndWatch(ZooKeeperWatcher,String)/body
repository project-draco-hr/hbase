{
  try {
    byte[] data=zkw.getZooKeeper().getData(znode,zkw,null);
    LOG.debug(zkw.prefix("Retrieved " + data.length + " bytes of data from znode "+ znode+ " and set a watcher"));
    return data;
  }
 catch (  KeeperException.NoNodeException e) {
    LOG.debug(zkw.prefix("Unable to get data of znode " + znode + " "+ "because node does not exist (not an error)"));
    return null;
  }
catch (  KeeperException e) {
    LOG.warn(zkw.prefix("Unable to get data of znode " + znode),e);
    zkw.keeperException(e);
    return null;
  }
catch (  InterruptedException e) {
    LOG.warn(zkw.prefix("Unable to get data of znode " + znode),e);
    zkw.interruptedException(e);
    return null;
  }
}
