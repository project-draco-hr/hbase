{
  if (isReference()) {
    this.hdfsBlocksDistribution=computeRefFileHDFSBlockDistribution(this.fs,this.reference,this.referencePath);
  }
 else {
    FileStatus status=this.fs.getFileStatus(this.path);
    long length=status.getLen();
    this.hdfsBlocksDistribution=FSUtils.computeHDFSBlocksDistribution(this.fs,status,0,length);
  }
}
