{
  HRegionInfo hri=getHRegionInfo(r,HConstants.REGIONINFO_QUALIFIER);
  if (hri == null) {
    LOG.warn("No serialized HRegionInfo in " + r);
    return true;
  }
  byte[] value=r.getValue(HConstants.CATALOG_FAMILY,HConstants.SERVER_QUALIFIER);
  if (value == null)   return false;
  if (!Bytes.equals(daughter.getTableName(),hri.getTableName())) {
    return false;
  }
  if (!Bytes.equals(daughter.getStartKey(),hri.getStartKey())) {
    return false;
  }
  this.found=true;
  return false;
}
