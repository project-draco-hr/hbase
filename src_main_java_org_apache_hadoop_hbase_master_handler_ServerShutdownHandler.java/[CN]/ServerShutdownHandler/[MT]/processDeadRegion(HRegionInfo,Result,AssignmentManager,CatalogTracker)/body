{
  boolean disabled=assignmentManager.getZKTable().isDisabledTable(hri.getTableDesc().getNameAsString());
  if (disabled)   return false;
  if (hri.isOffline() && hri.isSplit()) {
    LOG.debug("Offlined and split region " + hri.getRegionNameAsString() + "; checking daughter presence");
    fixupDaughters(result,assignmentManager,catalogTracker);
    return false;
  }
  return true;
}
