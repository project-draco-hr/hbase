{
  ByteBuffer origBlock=newBlock.getBufferReadOnly();
  ByteBuffer encodedBlock=ByteBuffer.wrap(origBlock.array(),origBlock.arrayOffset() + HFileBlock.HEADER_SIZE + DataBlockEncodings.ID_SIZE,origBlock.limit() - HFileBlock.HEADER_SIZE - DataBlockEncodings.ID_SIZE).slice();
  return encodedBlock;
}
