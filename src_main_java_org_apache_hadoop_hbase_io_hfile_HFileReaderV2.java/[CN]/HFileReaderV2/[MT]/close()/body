{
  if (evictOnClose && blockCache != null) {
    int numEvicted=blockCache.evictBlocksByPrefix(name + HFile.CACHE_KEY_SEPARATOR);
    LOG.debug("On close of file " + name + " evicted "+ numEvicted+ " block(s)");
  }
  if (closeIStream && istream != null) {
    istream.close();
    istream=null;
  }
}
