{
  super.serializeStateData(stream);
  MasterProcedureProtos.DeleteColumnFamilyStateData.Builder deleteCFMsg=MasterProcedureProtos.DeleteColumnFamilyStateData.newBuilder().setUserInfo(MasterProcedureUtil.toProtoUserInfo(getUser())).setTableName(ProtobufUtil.toProtoTableName(tableName)).setColumnfamilyName(ByteStringer.wrap(familyName));
  if (unmodifiedHTableDescriptor != null) {
    deleteCFMsg.setUnmodifiedTableSchema(ProtobufUtil.convertToTableSchema(unmodifiedHTableDescriptor));
  }
  deleteCFMsg.build().writeDelimitedTo(stream);
}
