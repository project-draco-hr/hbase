{
  List<HRegionInfo> regions=MetaTableAccessor.getTableRegions(UTIL.getZooKeeperWatcher(),hConnection,TableName.META_TABLE_NAME);
  assertTrue(regions.size() >= 1);
  assertTrue(MetaTableAccessor.getTableRegionsAndLocations(UTIL.getZooKeeperWatcher(),hConnection,TableName.META_TABLE_NAME).size() >= 1);
}
