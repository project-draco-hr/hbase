{
  HLog.Reader in1, in2;
  in1=HLogFactory.createReader(fs,p1,conf);
  in2=HLogFactory.createReader(fs,p2,conf);
  HLog.Entry entry1;
  HLog.Entry entry2;
  while ((entry1=in1.next()) != null) {
    entry2=in2.next();
    if ((entry1.getKey().compareTo(entry2.getKey()) != 0) || (!entry1.getEdit().toString().equals(entry2.getEdit().toString()))) {
      return false;
    }
  }
  return true;
}
