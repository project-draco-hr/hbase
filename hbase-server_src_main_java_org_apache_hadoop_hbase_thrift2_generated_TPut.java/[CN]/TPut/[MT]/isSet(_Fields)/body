{
  if (field == null) {
    throw new IllegalArgumentException();
  }
switch (field) {
case ROW:
    return isSetRow();
case COLUMN_VALUES:
  return isSetColumnValues();
case TIMESTAMP:
return isSetTimestamp();
case ATTRIBUTES:
return isSetAttributes();
case DURABILITY:
return isSetDurability();
}
throw new IllegalStateException();
}
