{
  assertTrue("Expected row [" + Bytes.toString(row) + "] "+ "Got row ["+ Bytes.toString(key.getRow())+ "]",equals(row,key.getRow()));
  assertTrue("Expected family [" + Bytes.toString(family) + "] "+ "Got family ["+ Bytes.toString(key.getFamily())+ "]",equals(family,key.getFamily()));
  assertTrue("Expected qualifier [" + Bytes.toString(qualifier) + "] "+ "Got qualifier ["+ Bytes.toString(key.getQualifier())+ "]",equals(qualifier,key.getQualifier()));
  assertTrue("Expected value [" + value + "] "+ "Got value ["+ Bytes.toLong(key.getValue())+ "]",Bytes.toLong(key.getValue()) == value);
}
