{
  long start=System.currentTimeMillis();
  while (start + timeout > System.currentTimeMillis() && store.getNumberOfstorefiles() != count) {
    Thread.sleep(100);
  }
  System.out.println("start=" + start + ", now="+ System.currentTimeMillis()+ ", cur="+ store.getNumberOfstorefiles());
  assertEquals(count,store.getNumberOfstorefiles());
}
