{
  this.masterServices=masterServices;
  this.tableNamespaceManager=new TableNamespaceManager(masterServices);
  this.clusterSchemaOperationTimeoutInMillis=this.masterServices.getConfiguration().getInt(HBASE_MASTER_CLUSTER_SCHEMA_OPERATION_TIMEOUT_KEY,DEFAULT_HBASE_MASTER_CLUSTER_SCHEMA_OPERATION_TIMEOUT);
}
