{
  Text[] columns=null;
  try {
    if (this.columnList.contains("*")) {
      columns=hTable.getRow(new Text(this.rowKey)).keySet().toArray(new Text[]{});
    }
 else {
      List<Text> tmpList=new ArrayList<Text>();
      for (int i=0; i < this.columnList.size(); i++) {
        Text column=null;
        if (this.columnList.get(i).contains(":"))         column=new Text(this.columnList.get(i));
 else         column=new Text(this.columnList.get(i) + ":");
        tmpList.add(column);
      }
      columns=tmpList.toArray(new Text[]{});
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  return columns;
}
