{
  int row1=Bytes.toInt(table.get(new Get(ROW1).addColumn(COL_A)).getValue(COL_A));
  int row2=Bytes.toInt(table.get(new Get(ROW2).addColumn(COL_A)).getValue(COL_A));
  int row3=Bytes.toInt(table.get(new Get(ROW3).addColumn(COL_A)).getValue(COL_A));
  assertEquals(TOTAL_VALUE - 2 * numRuns,row1);
  assertEquals(numRuns,row2);
  assertEquals(numRuns,row3);
}
