{
  this.instance.clear();
  int entries=in.readInt();
  for (int i=0; i < entries; i++) {
    byte[] key=Bytes.readByteArray(in);
    Writable value=(Writable)ReflectionUtils.newInstance(getClass(in.readByte()),getConf());
    value.readFields(in);
    V v=(V)value;
    this.instance.put(key,v);
  }
}
