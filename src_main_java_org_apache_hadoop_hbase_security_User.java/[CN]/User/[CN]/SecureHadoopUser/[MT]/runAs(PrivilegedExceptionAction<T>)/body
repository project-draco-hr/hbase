{
  try {
    return (T)call(ugi,"doAs",new Class[]{PrivilegedExceptionAction.class},new Object[]{action});
  }
 catch (  IOException ioe) {
    throw ioe;
  }
catch (  InterruptedException ie) {
    throw ie;
  }
