{
  if (!this.done) {
    long deadlineTs=EnvironmentEdgeManager.currentTime() + unit.toMillis(timeout);
    try {
      this.result=waitProcedureResult(procId,deadlineTs);
    }
 catch (    IOException e) {
      this.exception=new ExecutionException(e);
    }
    this.done=true;
  }
  if (exception != null) {
    throw exception;
  }
  return result;
}
