{
  long commitTime=System.currentTimeMillis();
  THLogKey key=new THLogKey(regionInfo.getRegionName(),regionInfo.getTableDesc().getName(),-1,commitTime,THLogKey.TrxOp.OP,transactionId);
  for (  KeyValue value : convertToKeyValues(update)) {
    WALEdit e=new WALEdit();
    e.add(value);
    super.append(regionInfo,key,e,regionInfo.isMetaRegion());
  }
}
