{
  LOG.debug("start get writer");
  Text tableName=new Text(job.get(OUTPUT_TABLE));
  HClient client=null;
  try {
    client=new HClient(job);
    client.openTable(tableName);
  }
 catch (  Exception e) {
    LOG.error(e);
  }
  LOG.debug("end get writer");
  return new TableRecordWriter(client);
}
