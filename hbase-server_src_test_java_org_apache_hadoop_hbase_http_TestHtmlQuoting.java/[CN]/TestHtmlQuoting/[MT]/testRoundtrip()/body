{
  runRoundTrip("");
  runRoundTrip("<>&'\"");
  runRoundTrip("ab>cd<ef&ghi'\"");
  runRoundTrip("A string\n with no quotable chars in it!");
  runRoundTrip(null);
  StringBuilder buffer=new StringBuilder();
  for (char ch=0; ch < 127; ++ch) {
    buffer.append(ch);
  }
  runRoundTrip(buffer.toString());
}
