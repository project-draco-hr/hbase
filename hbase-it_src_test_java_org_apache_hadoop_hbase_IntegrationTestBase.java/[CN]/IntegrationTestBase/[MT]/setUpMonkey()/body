{
  util=getTestingUtil(getConf());
  MonkeyFactory fact=MonkeyFactory.getFactory(monkeyToUse);
  if (fact == null) {
    fact=MonkeyFactory.getFactory(util.isDistributedCluster() ? MonkeyFactory.CALM : MonkeyFactory.SLOW_DETERMINISTIC);
  }
  monkey=fact.setUtil(util).setTableName(getTablename()).setColumnFamilies(getColumnFamilies()).build();
  monkey.start();
}
