{
  super.setConf(conf);
  try {
    long cacheRefreshPeriod=conf.getLong(HFILE_CACHE_REFRESH_PERIOD_CONF_KEY,DEFAULT_HFILE_CACHE_REFRESH_PERIOD);
    FileSystem fs=FSUtils.getCurrentFileSystem(conf);
    Path rootDir=FSUtils.getRootDir(conf);
    cache=new SnapshotFileCache(fs,rootDir,cacheRefreshPeriod,cacheRefreshPeriod,"snapshot-hfile-cleaner-cache-refresher",new FSUtils.RegionDirFilter(fs));
  }
 catch (  IOException e) {
    LOG.error("Failed to create cleaner util",e);
  }
}
