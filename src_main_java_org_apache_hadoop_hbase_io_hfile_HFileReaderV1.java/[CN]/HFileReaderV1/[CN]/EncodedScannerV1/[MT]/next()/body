{
  if (blockBuffer == null) {
    throw new IOException("Next called on non-seeked scanner");
  }
  boolean ok=seeker.next();
  if (!ok) {
    if (currBlock + 1 >= reader.getDataBlockIndexReader().getRootBlockCount()) {
      currBlock=0;
      blockBuffer=null;
      return false;
    }
    loadBlock(currBlock + 1,false);
    ok=true;
  }
  return ok;
}
