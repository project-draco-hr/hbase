{
  List<HRegionInfo> serverRegions=this.servers.get(hsi);
  boolean removed=serverRegions.remove(hri);
  if (expected) {
    if (!removed) {
      LOG.warn(hri.getRegionNameAsString() + " not found on " + hsi+ "; unexpected");
    }
  }
 else {
    if (removed) {
      LOG.warn(hri.getRegionNameAsString() + " found on " + hsi+ "; unexpected");
    }
  }
}
