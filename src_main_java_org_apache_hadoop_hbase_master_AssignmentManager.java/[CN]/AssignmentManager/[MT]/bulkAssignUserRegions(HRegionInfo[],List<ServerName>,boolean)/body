{
  Map<ServerName,List<HRegionInfo>> bulkPlan=LoadBalancer.roundRobinAssignment(Arrays.asList(regions),servers);
  LOG.info("Bulk assigning " + regions.length + " region(s) "+ "round-robin across "+ servers.size()+ " server(s)");
  BulkAssigner ba=new GeneralBulkAssigner(this.master,bulkPlan,this);
  try {
    ba.bulkAssign(sync);
  }
 catch (  InterruptedException e) {
    throw new IOException("InterruptedException bulk assigning",e);
  }
  LOG.info("Bulk assigning done");
}
