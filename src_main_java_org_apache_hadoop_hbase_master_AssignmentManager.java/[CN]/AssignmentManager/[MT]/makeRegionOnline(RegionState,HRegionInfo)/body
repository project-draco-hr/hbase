{
  regionOnline(regionInfo,rs.serverName);
  LOG.info("The master has opened the region " + regionInfo.getRegionNameAsString() + " that was online on "+ rs.serverName);
  if (this.getZKTable().isDisablingOrDisabledTable(regionInfo.getTableNameAsString())) {
    debugLog(regionInfo,"Opened region " + regionInfo.getRegionNameAsString() + " but "+ "this table is disabled, triggering close of region");
    unassign(regionInfo);
  }
}
