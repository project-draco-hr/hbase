{
  if (path.startsWith(watcher.assignmentZNode)) {
    try {
      RegionTransitionData data=ZKAssign.getData(watcher,path);
      if (data == null) {
        return;
      }
      handleRegion(data);
    }
 catch (    KeeperException e) {
      master.abort("Unexpected ZK exception reading unassigned node data",e);
    }
  }
}
