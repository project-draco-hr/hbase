{
  Stat stat=new Stat();
  RegionTransitionData data=ZKAssign.getDataAndWatch(watcher,encodedRegionName,stat);
  if (data == null)   return false;
  HRegionInfo hri=regionInfo;
  if (hri == null) {
    Pair<HRegionInfo,ServerName> p=MetaReader.getRegion(catalogTracker,data.getRegionName());
    if (p == null)     return false;
    hri=p.getFirst();
  }
  processRegionsInTransition(data,hri,deadServers,stat.getVersion());
  return true;
}
