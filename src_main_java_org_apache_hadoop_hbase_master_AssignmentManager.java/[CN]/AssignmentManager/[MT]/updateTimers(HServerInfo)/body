{
  for (  Map.Entry<String,RegionPlan> e : this.regionPlans.entrySet()) {
    if (e.getValue().getDestination().equals(hsi)) {
      RegionState rs=null;
synchronized (this.regionsInTransition) {
        rs=this.regionsInTransition.get(e.getKey());
      }
      if (rs != null) {
synchronized (rs) {
          rs.update(rs.getState());
        }
      }
    }
  }
}
