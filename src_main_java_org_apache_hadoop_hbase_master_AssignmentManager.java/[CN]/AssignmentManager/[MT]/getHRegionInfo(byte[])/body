{
  Pair<HRegionInfo,ServerName> p=null;
  try {
    p=MetaReader.getRegion(catalogTracker,regionName);
    if (p == null)     return null;
    return p.getFirst();
  }
 catch (  IOException e) {
    master.abort("Aborting because error occoured while reading " + Bytes.toStringBinary(regionName) + " from .META.",e);
    return null;
  }
}
