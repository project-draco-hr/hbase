{
  SchemaConfigured sc=new SchemaConfigured(null,TABLE_NAME,CF_NAME);
  JSONStringer json=new JSONStringer();
  json.object();
  json.key("tableName");
  json.value(TABLE_NAME);
  json.key("cfName");
  json.value(CF_NAME);
  json.endObject();
  assertEquals(json.toString(),sc.schemaConfAsJSON());
}
