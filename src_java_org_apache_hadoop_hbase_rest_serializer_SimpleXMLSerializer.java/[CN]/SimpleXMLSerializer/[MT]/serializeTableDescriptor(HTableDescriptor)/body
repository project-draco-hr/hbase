{
  printer.print("<table>");
  printer.print("<name>");
  printer.print(tableDescriptor.getNameAsString());
  printer.print("</name>");
  printer.print("<columnfamilies>");
  for (  HColumnDescriptor column : tableDescriptor.getColumnFamilies()) {
    column.restSerialize(this);
  }
  printer.print("</columnfamilies>");
  printer.print("</table>");
  printer.flush();
}
