{
  printer.print("<status>");
  printer.print("<code>");
  printer.print(message.getStatusCode());
  printer.print("</code>");
  printer.print("<message>");
  printer.print(message.getMessage().toString());
  printer.print("</message>");
  printer.print("<error>");
  printer.print(message.getError());
  printer.print("</error>");
  printer.print("</status>");
  printer.flush();
}
