{
  try {
    final byte[] regionName=req.getRegion().getValue().toByteArray();
    RegionSpecifierType type=req.getRegion().getType();
    AssignRegionResponse arr=AssignRegionResponse.newBuilder().build();
    checkInitialized();
    if (type != RegionSpecifierType.REGION_NAME) {
      LOG.warn("assignRegion specifier type: expected: " + RegionSpecifierType.REGION_NAME + " actual: "+ type);
    }
    Pair<HRegionInfo,ServerName> pair=MetaReader.getRegion(this.catalogTracker,regionName);
    if (pair == null)     throw new UnknownRegionException(Bytes.toString(regionName));
    if (cpHost != null) {
      if (cpHost.preAssign(pair.getFirst())) {
        return arr;
      }
    }
    assignRegion(pair.getFirst());
    if (cpHost != null) {
      cpHost.postAssign(pair.getFirst());
    }
    return arr;
  }
 catch (  IOException ioe) {
    throw new ServiceException(ioe);
  }
}
