{
  try {
    MasterAdminProtos.GetTableDescriptorsByNamespaceResponse.Builder b=MasterAdminProtos.GetTableDescriptorsByNamespaceResponse.newBuilder();
    for (    HTableDescriptor htd : getTableDescriptorsByNamespace(request.getNamespaceName())) {
      b.addTableSchema(htd.convert());
    }
    return b.build();
  }
 catch (  IOException e) {
    throw new ServiceException(e);
  }
}
