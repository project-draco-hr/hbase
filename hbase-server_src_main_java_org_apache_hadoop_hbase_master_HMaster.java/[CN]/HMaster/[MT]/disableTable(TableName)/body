{
  checkInitialized();
  if (cpHost != null) {
    cpHost.preDisableTable(tableName);
  }
  LOG.info(getClientIdAuditPrefix() + " disable " + tableName);
  this.executorService.submit(new DisableTableHandler(this,tableName,catalogTracker,assignmentManager,tableLockManager,false).prepare());
  if (cpHost != null) {
    cpHost.postDisableTable(tableName);
  }
}
