{
  if (!this.isActiveMaster || this.stopped) {
    return true;
  }
  if (t != null && t instanceof KeeperException.SessionExpiredException) {
    try {
      LOG.info("Primary Master trying to recover from ZooKeeper session " + "expiry.");
      return !tryRecoveringExpiredZKSession();
    }
 catch (    Throwable newT) {
      LOG.error("Primary master encountered unexpected exception while " + "trying to recover from ZooKeeper session" + " expiry. Proceeding with server abort.",newT);
    }
  }
  return true;
}
