{
  checkInitialized();
  if (cpHost != null) {
    cpHost.preDeleteTable(tableName);
  }
  LOG.info(getClientIdAuditPrefix() + " delete " + tableName);
  ProcedurePrepareLatch latch=ProcedurePrepareLatch.createLatch();
  long procId=this.procedureExecutor.submitProcedure(new DeleteTableProcedure(procedureExecutor.getEnvironment(),tableName,latch));
  latch.await();
  if (cpHost != null) {
    cpHost.postDeleteTable(tableName);
  }
}
