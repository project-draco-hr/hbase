{
  checkInitialized();
  if (cpHost != null) {
    cpHost.preDeleteTable(tableName);
  }
  LOG.info(getClientIdAuditPrefix() + " delete " + tableName);
  this.service.submit(new DeleteTableHandler(tableName,this,this).prepare());
  if (cpHost != null) {
    cpHost.postDeleteTable(tableName);
  }
}
