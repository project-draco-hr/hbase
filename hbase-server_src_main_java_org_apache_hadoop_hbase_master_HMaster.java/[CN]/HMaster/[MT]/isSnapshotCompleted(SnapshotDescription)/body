{
  final Path snapshotDir=SnapshotDescriptionUtils.getCompletedSnapshotDir(snapshot,this.getMasterFileSystem().getRootDir());
  FileSystem fs=this.getMasterFileSystem().getFileSystem();
  try {
    return fs.exists(snapshotDir);
  }
 catch (  IOException e) {
    throw new ServiceException(e);
  }
}
