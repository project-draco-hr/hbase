{
  final String name="testGetRegion";
  LOG.info("Started " + name);
  Pair<HRegionInfo,ServerName> pair=MetaReader.getRegion(CT,Bytes.toBytes("nonexistent-region"));
  assertNull(pair);
  pair=MetaReader.getRegion(CT,HRegionInfo.FIRST_META_REGIONINFO.getRegionName());
  assertEquals(HRegionInfo.FIRST_META_REGIONINFO.getEncodedName(),pair.getFirst().getEncodedName());
  LOG.info("Finished " + name);
}
