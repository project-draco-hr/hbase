{
  Get get=new Get(row);
  Result result=meta.get(get);
  assertTrue(Bytes.equals(result.getValue(HConstants.CATALOG_FAMILY,MetaReader.getServerColumn(replicaId)),Bytes.toBytes(serverName.getHostAndPort())));
  assertTrue(Bytes.equals(result.getValue(HConstants.CATALOG_FAMILY,MetaReader.getStartCodeColumn(replicaId)),Bytes.toBytes(serverName.getStartcode())));
  if (checkSeqNum) {
    assertTrue(Bytes.equals(result.getValue(HConstants.CATALOG_FAMILY,MetaReader.getSeqNumColumn(replicaId)),Bytes.toBytes(seqNum)));
  }
}
