{
  this(name,calcNumWriters(handlerCount,readShare),calcNumReaders(handlerCount,readShare),calcNumWriters(numQueues,readShare),calcNumReaders(numQueues,readShare),scanShare,LinkedBlockingQueue.class,new Object[]{maxQueueLength},readQueueClass,ArrayUtils.addAll(new Object[]{maxQueueLength},readQueueInitArgs));
}
