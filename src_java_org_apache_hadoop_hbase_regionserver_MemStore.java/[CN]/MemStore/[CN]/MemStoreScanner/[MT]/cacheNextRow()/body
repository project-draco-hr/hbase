{
  SortedSet<KeyValue> keys;
  try {
    keys=this.kvs.tailSet(this.current);
  }
 catch (  Exception e) {
    close();
    return false;
  }
  if (keys == null || keys.isEmpty()) {
    close();
    return false;
  }
  this.current=null;
  byte[] row=keys.first().getRow();
  for (  KeyValue kv : keys) {
    if (comparator.compareRows(kv,row) != 0) {
      this.current=kv;
      break;
    }
    result.add(kv);
  }
  return true;
}
