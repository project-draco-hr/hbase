{
  int count=0;
  for (char c='a'; c <= 'c'; c++) {
    byte[] row=Bytes.toBytes("ab" + c);
    int i;
    for (i=0; i < 2500; i++) {
      byte[] b=Bytes.toBytes(String.format("%10d",i));
      Put put=new Put(row);
      put.add(COLUMNS[rng.nextInt(COLUMNS.length)],b,b);
      region.put(put);
      count++;
    }
  }
  return count;
}
