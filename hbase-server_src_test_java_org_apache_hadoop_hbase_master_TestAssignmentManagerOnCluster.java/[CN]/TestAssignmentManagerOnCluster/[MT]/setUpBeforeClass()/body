{
  conf.setClass(HConstants.HBASE_MASTER_LOADBALANCER_CLASS,MyLoadBalancer.class,LoadBalancer.class);
  conf.setClass(CoprocessorHost.REGION_COPROCESSOR_CONF_KEY,MyRegionObserver.class,RegionObserver.class);
  conf.setInt("hbase.assignment.maximum.attempts",3);
  conf.setInt("hbase.master.maximum.ping.server.attempts",3);
  conf.setInt("hbase.master.ping.server.retry.sleep.interval",1);
  TEST_UTIL.startMiniCluster(1,4,null,MyMaster.class,MyRegionServer.class);
  admin=TEST_UTIL.getHBaseAdmin();
}
