{
  this.store=store;
  matcher=new ScanQueryMatcher(scan,store.getFamily().getName(),null,store.ttl,store.comparator.getRawComparator(),store.versionsToReturn(scan.getMaxVersions()));
  for (  KeyValueScanner scanner : scanners) {
    scanner.seek(matcher.getStartKey());
  }
  heap=new KeyValueHeap(scanners,store.comparator);
}
