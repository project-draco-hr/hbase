{
  int size=coeff[0] + (coeff[1] * 4 + coeff[2]) * refSize;
  size=(int)alignSize(size);
  if (debug) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("Primitives " + coeff[0] + ", arrays "+ coeff[1]+ ", references(inlcuding "+ HeapSize.OBJECT+ ", for object overhead) "+ coeff[2]+ ", refSize "+ refSize+ ", size "+ size);
    }
  }
  return (size < minObjectSize) ? minObjectSize : size;
}
