{
  long size=coeff[0] + align(coeff[1] * ARRAY) + coeff[2] * REFERENCE;
  size=align(size);
  if (debug) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("Primitives " + coeff[0] + ", arrays "+ coeff[1]+ ", references(inlcuding "+ nrOfRefsPerObj+ ", for object overhead) "+ coeff[2]+ ", refSize "+ REFERENCE+ ", size "+ size);
    }
  }
  return size;
}
