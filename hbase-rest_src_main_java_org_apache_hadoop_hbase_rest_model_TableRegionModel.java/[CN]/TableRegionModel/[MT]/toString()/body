{
  StringBuilder sb=new StringBuilder();
  sb.append(getName());
  sb.append(" [\n  id=");
  sb.append(id);
  sb.append("\n  startKey='");
  sb.append(Bytes.toString(startKey));
  sb.append("'\n  endKey='");
  sb.append(Bytes.toString(endKey));
  if (location != null) {
    sb.append("'\n  location='");
    sb.append(location);
  }
  sb.append("'\n]\n");
  return sb.toString();
}
