{
  int index=-1;
  int count=0;
  for (  JVMClusterUtil.RegionServerThread rst : getRegionServerThreads()) {
    HRegionServer hrs=rst.getRegionServer();
    HRegion metaRegion=hrs.getOnlineRegion(regionName);
    if (metaRegion != null) {
      index=count;
      break;
    }
    count++;
  }
  return index;
}
