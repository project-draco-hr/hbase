{
  this.store=store;
  this.cacheBlocks=false;
  this.isGet=false;
  matcher=new ScanQueryMatcher(scan,store.getFamily().getName(),null,store.ttl,store.comparator.getRawComparator(),store.minVersions,store.versionsToReturn(scan.getMaxVersions()),retainDeletesInOutput);
  for (  KeyValueScanner scanner : scanners) {
    scanner.seek(matcher.getStartKey());
  }
  heap=new KeyValueHeap(scanners,store.comparator);
}
