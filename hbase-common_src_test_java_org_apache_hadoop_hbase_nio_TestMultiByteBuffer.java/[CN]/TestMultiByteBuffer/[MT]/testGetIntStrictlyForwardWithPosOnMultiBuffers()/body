{
  byte[] b=new byte[4];
  byte[] b1=new byte[4];
  ByteBuffer bb1=ByteBuffer.wrap(b);
  ByteBuffer bb2=ByteBuffer.wrap(b1);
  MultiByteBuffer mbb1=new MultiByteBuffer(bb1,bb2);
  mbb1.position(2);
  mbb1.putInt(4);
  mbb1.position(7);
  mbb1.put((byte)2);
  mbb1.position(0);
  mbb1.getIntStrictlyForward(4);
  byte res=mbb1.get(7);
  assertEquals((byte)2,res);
}
