{
  secondaryRegion.replayWALRegionEventMarker(RegionEventDescriptor.newBuilder().setTableName(UnsafeByteOperations.unsafeWrap(primaryRegion.getTableDesc().getTableName().getName())).setEncodedRegionName(UnsafeByteOperations.unsafeWrap(primaryRegion.getRegionInfo().getEncodedNameAsBytes())).setRegionName(UnsafeByteOperations.unsafeWrap(primaryRegion.getRegionInfo().getRegionName())).setEventType(EventType.REGION_OPEN).setServer(ProtobufUtil.toServerName(ServerName.valueOf("foo",1,1))).setLogSequenceNumber(Long.MAX_VALUE).addStores(StoreDescriptor.newBuilder().setFamilyName(UnsafeByteOperations.unsafeWrap(families[0])).setStoreHomeDir("/store_home_dir").addStoreFile("/foo").build()).build());
}
