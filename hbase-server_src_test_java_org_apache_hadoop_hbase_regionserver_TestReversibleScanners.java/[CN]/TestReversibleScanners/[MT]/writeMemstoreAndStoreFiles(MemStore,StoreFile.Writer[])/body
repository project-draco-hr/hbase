{
  Random rand=new Random();
  try {
    for (int i=0; i < ROWSIZE; i++) {
      for (int j=0; j < QUALSIZE; j++) {
        if (i % 2 == 0) {
          memstore.add(makeKV(i,j));
        }
 else {
          writers[(i + j) % writers.length].append(makeKV(i,j));
        }
      }
    }
  }
  finally {
    for (int i=0; i < writers.length; i++) {
      writers[i].close();
    }
  }
}
