{
  String tableName=this.hTableDescriptor.getNameAsString();
  try {
    LOG.info("Attemping to create the table " + tableName);
    handleCreateTable();
  }
 catch (  IOException e) {
    LOG.error("Error trying to create the table " + tableName,e);
  }
catch (  KeeperException e) {
    LOG.error("Error trying to create the table " + tableName,e);
  }
}
