{
  AccessTestAction getNamespaceAction=new AccessTestAction(){
    @Override public Object run() throws Exception {
      ACCESS_CONTROLLER.preGetNamespaceDescriptor(ObserverContext.createAndPrepare(CP_ENV,null),TEST_NAMESPACE);
      return null;
    }
  }
;
  verifyAllowed(getNamespaceAction,SUPERUSER,USER_GLOBAL_ADMIN,USER_NS_ADMIN);
  verifyDeniedWithException(getNamespaceAction,USER_GLOBAL_CREATE,USER_GLOBAL_WRITE,USER_GLOBAL_READ,USER_GLOBAL_EXEC,USER_NS_CREATE,USER_NS_WRITE,USER_NS_READ,USER_NS_EXEC,USER_TABLE_CREATE,USER_TABLE_WRITE);
}
