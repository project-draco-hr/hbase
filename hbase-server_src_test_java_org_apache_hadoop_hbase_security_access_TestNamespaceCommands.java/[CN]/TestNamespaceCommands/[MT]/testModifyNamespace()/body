{
  AccessTestAction modifyNamespace=new AccessTestAction(){
    public Object run() throws Exception {
      ACCESS_CONTROLLER.preModifyNamespace(ObserverContext.createAndPrepare(CP_ENV,null),NamespaceDescriptor.create(TEST_NAMESPACE).addConfiguration("abc","156").build());
      return null;
    }
  }
;
  verifyAllowed(modifyNamespace,SUPERUSER,USER_GLOBAL_ADMIN);
  verifyDeniedWithException(modifyNamespace,USER_GLOBAL_CREATE,USER_GLOBAL_WRITE,USER_GLOBAL_READ,USER_GLOBAL_EXEC,USER_NS_ADMIN,USER_NS_CREATE,USER_NS_WRITE,USER_NS_READ,USER_NS_EXEC);
}
