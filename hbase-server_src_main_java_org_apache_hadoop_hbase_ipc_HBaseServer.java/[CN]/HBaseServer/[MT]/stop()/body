{
  LOG.info("Stopping server on " + port);
  running=false;
  stopHandlers(handlers);
  stopHandlers(priorityHandlers);
  stopHandlers(replicationHandlers);
  listener.interrupt();
  listener.doStop();
  responder.interrupt();
  notifyAll();
  if (this.rpcMetrics != null) {
    this.rpcMetrics.shutdown();
  }
}
