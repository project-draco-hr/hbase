{
  LOG.info(getName() + ": starting");
  SERVER.set(HBaseServer.this);
  try {
    doRunLoop();
  }
  finally {
    LOG.info(getName() + ": stopping");
    try {
      writeSelector.close();
    }
 catch (    IOException ioe) {
      LOG.error(getName() + ": couldn't close write selector",ioe);
    }
  }
}
