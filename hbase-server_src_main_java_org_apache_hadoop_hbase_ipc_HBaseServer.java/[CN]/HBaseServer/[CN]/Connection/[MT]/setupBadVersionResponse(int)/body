{
  String errMsg="Server IPC version " + HConstants.CURRENT_VERSION + " cannot communicate with client version "+ clientVersion;
  ByteArrayOutputStream buffer=new ByteArrayOutputStream();
  if (clientVersion >= 3) {
    Call fakeCall=new Call(0,null,this,responder,0,null);
    setupResponse(buffer,fakeCall,Status.FATAL,VersionMismatch.class.getName(),errMsg);
    responder.doRespond(fakeCall);
  }
}
