{
  ZooKeeper zk=wrapper.getZooKeeper();
  try {
    byte[] data=zk.getData(usersZNode + "/" + token,null,null);
    if (data == null) {
      return null;
    }
    UserModel model=unmarshaller.unmarshalFromJSON(new ByteArrayInputStream(data),UserModel.class);
    return new User(model.name,token,model.admin,model.disabled);
  }
 catch (  KeeperException.NoNodeException e) {
    return null;
  }
catch (  Exception e) {
    throw new IOException(e);
  }
}
