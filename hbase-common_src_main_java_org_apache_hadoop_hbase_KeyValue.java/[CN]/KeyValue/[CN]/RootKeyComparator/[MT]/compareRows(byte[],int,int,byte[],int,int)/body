{
  int lmetaOffsetPlusDelimiter=loffset + META_LENGTH + 1;
  int leftFarDelimiter=getDelimiterInReverse(left,lmetaOffsetPlusDelimiter,llength - META_LENGTH - 1,HConstants.DELIMITER);
  int rmetaOffsetPlusDelimiter=roffset + META_LENGTH + 1;
  int rightFarDelimiter=getDelimiterInReverse(right,rmetaOffsetPlusDelimiter,rlength - META_LENGTH - 1,HConstants.DELIMITER);
  if (leftFarDelimiter < 0 && rightFarDelimiter >= 0) {
    return -1;
  }
 else   if (rightFarDelimiter < 0 && leftFarDelimiter >= 0) {
    return 1;
  }
 else   if (leftFarDelimiter < 0 && rightFarDelimiter < 0) {
    return 0;
  }
  int result=super.compareRows(left,lmetaOffsetPlusDelimiter,leftFarDelimiter - lmetaOffsetPlusDelimiter,right,rmetaOffsetPlusDelimiter,rightFarDelimiter - rmetaOffsetPlusDelimiter);
  if (result != 0) {
    return result;
  }
  leftFarDelimiter++;
  rightFarDelimiter++;
  result=compareRowid(left,leftFarDelimiter,llength - (leftFarDelimiter - loffset),right,rightFarDelimiter,rlength - (rightFarDelimiter - roffset));
  return result;
}
