{
  if (closed) {
    return false;
  }
  LabelledData[] values=null;
  do {
    values=server.next(scannerId,key);
  }
 while (values.length == 0 && nextScanner());
  for (int i=0; i < values.length; i++) {
    results.put(values[i].getLabel(),values[i].getData().get());
  }
  return values.length != 0;
}
