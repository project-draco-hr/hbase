{
  if (scanner != null) {
    scanner.close();
  }
  currentRegion+=1;
  if (currentRegion == regions.length) {
    close();
    return false;
  }
  try {
    server=getHRegionConnection(regions[currentRegion].serverAddress);
    scanner=server.openScanner(regions[currentRegion].regionInfo.regionName,columns,startRow);
  }
 catch (  IOException e) {
    close();
    throw e;
  }
  return true;
}
