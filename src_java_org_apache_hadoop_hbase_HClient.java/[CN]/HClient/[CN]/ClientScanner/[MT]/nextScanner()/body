{
  if (scannerId != -1L) {
    server.close(scannerId);
    scannerId=-1L;
  }
  currentRegion+=1;
  if (currentRegion == regions.length) {
    close();
    return false;
  }
  try {
    server=getHRegionConnection(regions[currentRegion].serverAddress);
    scannerId=server.openScanner(regions[currentRegion].regionInfo.regionName,columns,startRow);
  }
 catch (  IOException e) {
    close();
    throw e;
  }
  return true;
}
