{
  for (  String cmd : args) {
    if (cmd.equals("-h") || cmd.startsWith("--h")) {
      printUsage();
      return 0;
    }
    final String masterArgKey="--master=";
    if (cmd.startsWith(masterArgKey)) {
      this.conf.set(MASTER_ADDRESS,cmd.substring(masterArgKey.length()));
    }
  }
  int errCode=-1;
  try {
    locateRootRegion();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  return errCode;
}
