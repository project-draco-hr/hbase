{
  if (message != null && message.length() > 0) {
    System.err.println(message);
  }
  System.err.println("Usage: java " + this.getClass().getName() + " [options] deleteTable <name>");
  System.err.println("Example Usage:");
  System.err.println(" % java " + this.getClass().getName() + " deleteTable testtable");
}
