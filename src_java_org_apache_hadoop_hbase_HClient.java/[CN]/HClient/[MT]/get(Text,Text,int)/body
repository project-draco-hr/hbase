{
  TableInfo info=getTableInfo(row);
  BytesWritable[] values=getHRegionConnection(info.serverAddress).get(info.regionInfo.regionName,row,column,numVersions);
  ArrayList<byte[]> bytes=new ArrayList<byte[]>();
  for (int i=0; i < values.length; i++) {
    bytes.add(values[i].get());
  }
  return bytes.toArray(new byte[values.length][]);
}
