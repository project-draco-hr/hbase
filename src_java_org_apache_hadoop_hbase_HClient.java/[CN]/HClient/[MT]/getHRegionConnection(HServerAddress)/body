{
  HRegionInterface server=this.servers.get(regionServer.toString());
  if (server == null) {
    server=(HRegionInterface)RPC.waitForProxy(HRegionInterface.class,HRegionInterface.versionID,regionServer.getInetSocketAddress(),this.conf);
    this.servers.put(regionServer.toString(),server);
  }
  return server;
}
