{
  if (!hasFan()) {
    throw new IllegalStateException("This row node has no fan, so can't search it");
  }
  int fanIndexInBlock=Bytes.unsignedBinarySearch(block,fanOffset,fanOffset + fanOut,searchForByte);
  if (fanIndexInBlock >= 0) {
    return fanIndexInBlock - fanOffset;
  }
  return fanIndexInBlock + fanOffset + 1;
}
