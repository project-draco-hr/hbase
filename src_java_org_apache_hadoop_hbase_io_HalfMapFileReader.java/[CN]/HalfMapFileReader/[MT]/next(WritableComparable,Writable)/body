{
  if (firstNextCall) {
    firstNextCall=false;
    if (isEmpty()) {
      return false;
    }
    if (this.firstKey != null) {
      Writables.copyWritable(this.firstKey,key);
      WritableComparable nearest=getClosest(key,val);
      if (!key.equals(nearest)) {
        throw new IOException("Keys don't match and should: " + key.toString() + ", "+ nearest.toString());
      }
    }
    return true;
  }
  boolean result=super.next(key,val);
  if (!top && key.compareTo(midkey) >= 0) {
    result=false;
  }
  return result;
}
