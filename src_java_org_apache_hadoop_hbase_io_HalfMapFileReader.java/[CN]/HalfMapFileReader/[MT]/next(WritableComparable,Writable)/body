{
  if (firstNextCall) {
    firstNextCall=false;
    if (this.top) {
      WritableComparable nearest=getClosest(this.midkey,val);
      if (nearest != null) {
        Writables.copyWritable(nearest,key);
        return true;
      }
      return false;
    }
  }
  boolean result=super.next(key,val);
  if (!top && key.compareTo(midkey) >= 0) {
    result=false;
  }
  return result;
}
