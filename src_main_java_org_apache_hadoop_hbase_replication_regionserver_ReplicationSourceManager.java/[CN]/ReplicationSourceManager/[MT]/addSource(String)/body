{
  ReplicationSourceInterface src=getReplicationSource(this.conf,this.fs,this,stopper,replicating,id);
  src.setSourceEnabled(true);
synchronized (this.hlogs) {
    this.sources.add(src);
    if (this.hlogs.size() > 0) {
      this.zkHelper.addLogToList(this.hlogs.last(),this.sources.get(0).getPeerClusterZnode());
      src.enqueueLog(this.latestPath);
    }
  }
  src.startup();
  return src;
}
