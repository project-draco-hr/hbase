{
  if (!LOG.isDebugEnabled() || p == null) {
    return;
  }
  FileSystem fs=p.getFileSystem(conf);
  if (!fs.exists(p)) {
    return;
  }
  System.out.println(p);
  if (fs.isFile(p)) {
    return;
  }
  if (fs.getFileStatus(p).isDir()) {
    FileStatus[] fss=fs.listStatus(p);
    for (    FileStatus status : fss) {
      debugLsr(conf,status.getPath());
    }
  }
}
