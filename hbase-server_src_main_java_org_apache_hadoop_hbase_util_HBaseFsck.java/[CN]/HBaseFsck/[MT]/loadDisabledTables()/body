{
  HConnectionManager.execute(new HConnectable<Void>(getConf()){
    @Override public Void connect(    HConnection connection) throws IOException {
      TableName[] tables=connection.listTableNames();
      for (      TableName table : tables) {
        if (connection.getTableState(table).inStates(TableState.State.DISABLED,TableState.State.DISABLING)) {
          disabledTables.add(table);
        }
      }
      return null;
    }
  }
);
}
