{
  final String unknowntable="fubar";
  Exception exception=null;
  try {
    this.admin.compact(unknowntable);
  }
 catch (  IOException e) {
    exception=e;
  }
  assertTrue(exception instanceof TableNotFoundException);
  exception=null;
  try {
    this.admin.flush(unknowntable);
  }
 catch (  IOException e) {
    exception=e;
  }
  assertTrue(exception instanceof TableNotFoundException);
  exception=null;
  try {
    this.admin.split(unknowntable);
  }
 catch (  IOException e) {
    exception=e;
  }
  assertTrue(exception instanceof TableNotFoundException);
}
