{
  block.mark();
  block.position(Bytes.SIZEOF_INT);
  byte flag=block.get();
  int keyLength=ByteBufferUtils.readCompressedInt(block,1 + ((flag & KEY_SIZE_MASK) >>> KEY_SIZE_SHIFT));
  ByteBufferUtils.readCompressedInt(block,1 + ((flag & VALUE_SIZE_MASK) >>> VALUE_SIZE_SHIFT));
  int pos=block.position();
  block.reset();
  return ByteBuffer.wrap(block.array(),pos,keyLength).slice();
}
