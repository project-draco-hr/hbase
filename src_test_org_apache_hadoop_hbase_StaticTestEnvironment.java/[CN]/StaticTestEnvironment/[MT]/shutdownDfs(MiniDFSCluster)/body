{
  if (cluster != null) {
    try {
      FileSystem fs=cluster.getFileSystem();
      if (fs != null) {
        LOG.info("Shutting down FileSystem");
        fs.close();
      }
    }
 catch (    IOException e) {
      LOG.error("error closing file system",e);
    }
    LOG.info("Shutting down Mini DFS ");
    try {
      cluster.shutdown();
    }
 catch (    Exception e) {
    }
  }
}
