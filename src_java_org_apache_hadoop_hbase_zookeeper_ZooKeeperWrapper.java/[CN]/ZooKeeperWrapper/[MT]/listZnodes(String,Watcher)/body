{
  List<String> nodes=null;
  try {
    if (checkExistenceOf(znode)) {
      nodes=zooKeeper.getChildren(znode,watcher);
    }
  }
 catch (  KeeperException e) {
    LOG.warn("Failed to read " + znode + " znode in ZooKeeper: "+ e);
  }
catch (  InterruptedException e) {
    LOG.warn("Failed to read " + znode + " znode in ZooKeeper: "+ e);
  }
  return nodes;
}
