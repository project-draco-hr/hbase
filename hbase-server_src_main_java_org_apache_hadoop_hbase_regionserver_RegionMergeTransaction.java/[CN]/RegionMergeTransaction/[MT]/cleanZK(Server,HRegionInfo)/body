{
  try {
    if (!ZKAssign.deleteNode(server.getZooKeeper(),hri.getEncodedName(),RS_ZK_REQUEST_REGION_MERGE)) {
      ZKAssign.deleteNode(server.getZooKeeper(),hri.getEncodedName(),RS_ZK_REGION_MERGING);
    }
  }
 catch (  KeeperException.NoNodeException e) {
    LOG.warn("Failed cleanup zk node of " + hri.getRegionNameAsString(),e);
  }
catch (  KeeperException e) {
    server.abort("Failed cleanup zk node of " + hri.getRegionNameAsString(),e);
  }
}
