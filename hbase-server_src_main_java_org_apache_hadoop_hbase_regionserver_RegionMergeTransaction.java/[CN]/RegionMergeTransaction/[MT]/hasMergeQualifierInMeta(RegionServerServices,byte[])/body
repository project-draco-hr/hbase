{
  if (services == null)   return false;
  Pair<HRegionInfo,HRegionInfo> mergeRegions=MetaTableAccessor.getRegionsFromMergeQualifier(services.getShortCircuitConnection(),regionName);
  if (mergeRegions != null && (mergeRegions.getFirst() != null || mergeRegions.getSecond() != null)) {
    return true;
  }
  return false;
}
