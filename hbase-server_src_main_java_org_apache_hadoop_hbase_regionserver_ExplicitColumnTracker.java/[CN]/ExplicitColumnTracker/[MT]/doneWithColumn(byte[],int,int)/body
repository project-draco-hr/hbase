{
  while (this.column != null) {
    int compare=Bytes.compareTo(column.getBuffer(),column.getOffset(),column.getLength(),bytes,offset,length);
    resetTS();
    if (compare <= 0) {
      ++this.index;
      this.skipCount=0;
      if (done()) {
        this.column=null;
      }
 else {
        this.column=this.columns[this.index];
      }
      if (compare <= -1)       continue;
    }
    return;
  }
}
