{
  while (this.column != null) {
    int compare=Bytes.compareTo(column.getBuffer(),column.getOffset(),column.getLength(),bytes,offset,length);
    resetTS();
    if (compare == 0) {
      this.columns.remove(this.index);
      if (this.columns.size() == this.index) {
        this.column=null;
      }
 else {
        this.column=this.columns.get(this.index);
      }
      return;
    }
 else     if (compare <= -1) {
      if (++this.index != this.columns.size()) {
        this.column=this.columns.get(this.index);
      }
 else {
        this.column=null;
      }
    }
 else {
      return;
    }
  }
}
