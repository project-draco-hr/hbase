{
  while (this.column != null) {
    int compare=CellComparator.compareQualifiers(cell,column.getBuffer(),column.getOffset(),column.getLength());
    resetTS();
    if (compare >= 0) {
      ++this.index;
      if (done()) {
        this.column=null;
      }
 else {
        this.column=this.columns[this.index];
      }
      if (compare > 0)       continue;
    }
    return;
  }
}
