{
  HTable table=new HTable(this.conf,HConstants.ROOT_TABLE_NAME);
  try {
    HScannerInterface scanner=table.obtainScanner(HConstants.COLUMN_FAMILY_ARRAY,HConstants.EMPTY_START_ROW);
    for (    Map.Entry<HStoreKey,SortedMap<Text,byte[]>> e : scanner) {
      assertNotNull(e.getKey());
      assertNotNull(e.getValue());
    }
  }
  finally {
    table.close();
  }
}
