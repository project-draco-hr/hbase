{
  Put put=new Put(Bytes.toBytes(aput.row));
  for (  AColumnValue acv : aput.columnValues) {
    if (acv.timestamp != null) {
      put.add(Bytes.toBytes(acv.family),Bytes.toBytes(acv.qualifier),acv.timestamp,Bytes.toBytes(acv.value));
    }
 else {
      put.add(Bytes.toBytes(acv.family),Bytes.toBytes(acv.qualifier),Bytes.toBytes(acv.value));
    }
  }
  return put;
}
