    TEST_UTIL.getHBaseAdmin().flush(tableName);
    Scan s=new Scan();
    s.setAuthorizations(new Authorizations(SECRET,PRIVATE,CONFIDENTIAL));
    ResultScanner scanner=table.getScanner(s);
