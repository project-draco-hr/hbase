{
  if (null == this.admin) {
    try {
      this.admin=new HBaseAdmin(config);
    }
 catch (    Exception e) {
      LOG.error("Initial HBaseAdmin failed...",e);
      this.error=true;
    }
  }
  if (admin.isAborted()) {
    LOG.error("HBaseAdmin aborted");
    this.error=true;
  }
  return !this.error;
}
