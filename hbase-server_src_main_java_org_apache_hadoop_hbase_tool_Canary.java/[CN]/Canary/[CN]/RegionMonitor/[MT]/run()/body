{
  if (this.initAdmin()) {
    try {
      if (this.targets != null && this.targets.length > 0) {
        String[] tables=generateMonitorTables(this.targets);
        for (        String table : tables) {
          Canary.sniff(admin,sink,table);
        }
      }
 else {
        sniff();
      }
    }
 catch (    Exception e) {
      LOG.error("Run regionMonitor failed",e);
      this.error=true;
    }
  }
  this.done=true;
}
