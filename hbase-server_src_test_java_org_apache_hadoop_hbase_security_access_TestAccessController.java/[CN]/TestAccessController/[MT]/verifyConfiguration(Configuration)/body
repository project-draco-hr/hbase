{
  if (!(conf.get(CoprocessorHost.MASTER_COPROCESSOR_CONF_KEY).contains(AccessController.class.getName()) && conf.get(CoprocessorHost.REGION_COPROCESSOR_CONF_KEY).contains(AccessController.class.getName()) && conf.get(CoprocessorHost.REGIONSERVER_COPROCESSOR_CONF_KEY).contains(AccessController.class.getName()))) {
    throw new RuntimeException("AccessController is missing from a system coprocessor list");
  }
}
