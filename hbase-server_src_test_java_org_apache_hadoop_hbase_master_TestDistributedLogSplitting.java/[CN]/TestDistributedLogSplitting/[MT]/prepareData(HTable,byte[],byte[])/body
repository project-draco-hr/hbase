{
  t.setAutoFlush(false);
  byte[] k=new byte[3];
  for (byte b1='a'; b1 <= 'z'; b1++) {
    for (byte b2='a'; b2 <= 'z'; b2++) {
      for (byte b3='a'; b3 <= 'z'; b3++) {
        k[0]=b1;
        k[1]=b2;
        k[2]=b3;
        Put put=new Put(k);
        put.add(f,column,k);
        t.put(put);
      }
    }
  }
  t.flushCommits();
  for (byte b3='a'; b3 <= 'z'; b3++) {
    k[0]='a';
    k[1]='a';
    k[2]=b3;
    Delete del=new Delete(k);
    t.delete(del);
  }
  t.flushCommits();
}
