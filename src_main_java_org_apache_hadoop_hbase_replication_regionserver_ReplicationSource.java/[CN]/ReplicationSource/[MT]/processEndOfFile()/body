{
  this.pathLock.lock();
  try {
    if (this.queue.size() != 0) {
      this.currentPath=null;
      this.position=0;
      return true;
    }
 else     if (this.queueRecovered) {
      this.manager.closeRecoveredQueue(this);
      this.abort();
      return true;
    }
  }
  finally {
    this.pathLock.unlock();
  }
  return false;
}
