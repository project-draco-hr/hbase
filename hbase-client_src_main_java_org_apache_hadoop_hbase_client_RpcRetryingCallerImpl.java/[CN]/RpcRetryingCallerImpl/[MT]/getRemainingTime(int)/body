{
  if (callTimeout <= 0) {
    return 0;
  }
 else {
    if (callTimeout == Integer.MAX_VALUE)     return Integer.MAX_VALUE;
    int remainingTime=(int)(callTimeout - (EnvironmentEdgeManager.currentTime() - this.globalStartTime));
    if (remainingTime < 1) {
      remainingTime=1;
    }
    return remainingTime;
  }
}
