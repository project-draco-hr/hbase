{
  AtomicLong startPoint=new AtomicLong();
  while (!finished.get()) {
    MultiVersionConcurrencyControl.WriteEntry e=mvcc.beginMemstoreInsertWithSeqNum(startPoint.incrementAndGet());
    int sleepTime=rnd.nextInt(500);
    try {
      if (sleepTime > 0)       Thread.sleep(0,sleepTime * 1000);
    }
 catch (    InterruptedException e1) {
    }
    try {
      mvcc.completeMemstoreInsert(e);
    }
 catch (    RuntimeException ex) {
      System.out.println(ex.toString());
      ex.printStackTrace();
      status.set(false);
      return;
    }
  }
}
