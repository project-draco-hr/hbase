{
  if (this.metrics != null)   metrics.finishSync(timeInNanos / 1000000);
  if (timeInNanos > this.slowSyncNs) {
    String msg=new StringBuilder().append("Slow sync cost: ").append(timeInNanos / 1000).append(" ms, current pipeline: ").append(Arrays.toString(getPipeLine())).toString();
    Trace.addTimelineAnnotation(msg);
    LOG.info(msg);
  }
}
