{
  byte[][] regions=null;
  int logCount=getNumLogFiles();
  if (logCount > this.maxLogs && logCount > 0) {
synchronized (oldestSeqNumsLock) {
      regions=findMemstoresWithEditsEqualOrOlderThan(this.outputfiles.firstKey(),this.oldestUnflushedSeqNums);
    }
    if (regions != null) {
      StringBuilder sb=new StringBuilder();
      for (int i=0; i < regions.length; i++) {
        if (i > 0)         sb.append(", ");
        sb.append(Bytes.toStringBinary(regions[i]));
      }
      LOG.info("Too many hlogs: logs=" + logCount + ", maxlogs="+ this.maxLogs+ "; forcing flush of "+ regions.length+ " regions(s): "+ sb.toString());
    }
  }
  return regions;
}
