{
  try {
    if (this.closed) {
      return;
    }
    long txid=0;
synchronized (updateLock) {
      long now=EnvironmentEdgeManager.currentTimeMillis();
      WALEdit edit=completeCacheFlushLogEdit();
      HLogKey key=makeKey(encodedRegionName,tableName,logSeqId,System.currentTimeMillis(),HConstants.DEFAULT_CLUSTER_ID);
      logSyncerThread.append(new Entry(key,edit));
      txid=this.unflushedEntries.incrementAndGet();
      long took=EnvironmentEdgeManager.currentTimeMillis() - now;
      long len=0;
      for (      KeyValue kv : edit.getKeyValues()) {
        len+=kv.getLength();
      }
      this.metrics.finishAppend(took,len);
      this.numEntries.incrementAndGet();
    }
    this.sync(txid);
  }
  finally {
    this.lastSeqWritten.remove(getSnapshotName(encodedRegionName));
    this.cacheFlushLock.unlock();
  }
}
