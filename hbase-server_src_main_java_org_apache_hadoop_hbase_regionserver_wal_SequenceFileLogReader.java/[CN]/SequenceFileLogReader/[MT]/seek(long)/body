{
  if (compressionContext != null && emptyCompressionContext) {
    while (next() != null) {
      if (getPosition() == pos) {
        emptyCompressionContext=false;
        break;
      }
    }
  }
  try {
    reader.seek(pos);
  }
 catch (  IOException ioe) {
    throw addFileInfoToException(ioe);
  }
}
