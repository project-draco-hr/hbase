def __init__(self, d=None):
    self.isDelete = False
    self.column = None
    self.value = None
    if isinstance(d, dict):
        if ('isDelete' in d):
            self.isDelete = d['isDelete']
        if ('column' in d):
            self.column = d['column']
        if ('value' in d):
            self.value = d['value']
