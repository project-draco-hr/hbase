{
  int size=KeyValueUtil.length(cell);
  Preconditions.checkArgument(size >= 0,"negative size");
  if (size > maxAlloc) {
    return null;
  }
  Chunk c=null;
  int allocOffset=0;
  while (true) {
    c=getOrMakeChunk();
    allocOffset=c.alloc(size);
    if (allocOffset != -1) {
      break;
    }
    tryRetireChunk(c);
  }
  return KeyValueUtil.copyCellTo(cell,c.getData(),allocOffset);
}
