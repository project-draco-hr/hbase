{
  if (getSnapshotFormat(desc) == SnapshotManifestV1.DESCRIPTOR_VERSION) {
    Path rootDir=FSUtils.getRootDir(conf);
    LOG.info("Using old Snapshot Format");
    new FSTableDescriptors(fs,rootDir).createTableDescriptorForTableDirectory(workingDir,new TableDescriptor(htd,TableState.State.ENABLED),false);
  }
 else {
    LOG.debug("Convert to Single Snapshot Manifest");
    convertToV2SingleManifest();
  }
}
