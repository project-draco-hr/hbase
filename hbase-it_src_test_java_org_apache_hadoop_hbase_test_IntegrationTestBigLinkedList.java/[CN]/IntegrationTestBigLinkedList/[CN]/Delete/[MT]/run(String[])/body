{
  if (args.length != 1) {
    System.out.println("Usage : " + Delete.class.getSimpleName() + " <node to delete>");
    return 0;
  }
  long val=new BigInteger(args[0],16).longValue();
  org.apache.hadoop.hbase.client.Delete delete=new org.apache.hadoop.hbase.client.Delete(Bytes.toBytes(val));
  HTable table=new HTable(getConf(),getTableName(getConf()));
  table.delete(delete);
  table.flushCommits();
  table.close();
  System.out.println("Delete successful");
  return 0;
}
