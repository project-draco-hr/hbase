{
  List<KeyValue> kvs=new ArrayList<KeyValue>();
{
    Scan scan=new Scan(row,row);
    scan.addColumn(FAM,person);
    doScan(region,scan,kvs);
  }
  Scan scan=new Scan(row,row);
  for (  KeyValue kv : kvs) {
    byte[] friends=kv.getValue();
    for (    byte f : friends) {
      scan.addColumn(FAM,new byte[]{f});
    }
  }
  doScan(region,scan,kvs);
  result.clear();
  for (  KeyValue kv : kvs) {
    for (    byte b : kv.getValue()) {
      result.add((char)b + "");
    }
  }
}
