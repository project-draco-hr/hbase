{
  StringBuilder out=new StringBuilder();
  out.append("[");
  if (kvs != null) {
    for (    Cell kv : kvs) {
      byte[] col=CellUtil.getQualifierArray(kv);
      byte[] val=CellUtil.getValueArray(kv);
      if (Bytes.equals(col,COUNTER)) {
        out.append(Bytes.toStringBinary(col) + ":" + Bytes.toInt(val)+ " ");
      }
 else {
        out.append(Bytes.toStringBinary(col) + ":" + Bytes.toStringBinary(val)+ " ");
      }
    }
  }
  out.append("]");
  return out.toString();
}
