{
  for (int i=0; i < handlerCount; i++) {
    Thread t=new Thread(new Runnable(){
      @Override public void run(){
        consumerLoop(callQueue);
      }
    }
);
    t.setDaemon(true);
    t.setName(Strings.nullToEmpty(threadNamePrefix) + "RpcServer.handler=" + i+ ",port="+ port);
    t.start();
    handlers.add(t);
  }
}
