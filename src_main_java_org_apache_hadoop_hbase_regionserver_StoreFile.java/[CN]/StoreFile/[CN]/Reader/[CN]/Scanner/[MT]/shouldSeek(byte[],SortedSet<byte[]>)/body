{
  if (bloomFilter == null) {
    return true;
  }
  byte[] key;
switch (bloomFilterType) {
case ROW:
    key=row;
  break;
case ROWCOL:
if (columns.size() == 1) {
  byte[] col=columns.first();
  key=Bytes.add(row,col);
  break;
}
default :
return true;
}
try {
ByteBuffer bloom=getMetaBlock(BLOOM_FILTER_DATA_KEY,true);
if (bloom != null) {
return bloomFilter.contains(key,bloom);
}
}
 catch (IOException e) {
LOG.error("Error reading bloom filter data -- proceeding without",e);
bloomFilter=null;
}
catch (IllegalArgumentException e) {
LOG.error("Bad bloom filter data -- proceeding without",e);
bloomFilter=null;
}
return true;
}
