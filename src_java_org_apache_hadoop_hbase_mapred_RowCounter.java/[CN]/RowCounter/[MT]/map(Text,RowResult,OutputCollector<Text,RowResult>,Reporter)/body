{
  boolean content=false;
  for (  Map.Entry<Text,Cell> e : value.entrySet()) {
    Cell cell=e.getValue();
    if (cell != null && cell.getValue().length > 0) {
      content=true;
      break;
    }
  }
  if (!content) {
    return;
  }
  reporter.incrCounter(Counters.ROWS,1);
  output.collect(row,EMPTY_RESULT_VALUE);
}
