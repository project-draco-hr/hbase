{
  int listize=filters.size();
  for (int i=0; i < listize; i++) {
    if (filters.get(i).filterAllRemaining()) {
      if (operator == Operator.MUST_PASS_ALL) {
        return true;
      }
    }
 else {
      if (operator == Operator.MUST_PASS_ONE) {
        return false;
      }
    }
  }
  return operator == Operator.MUST_PASS_ONE;
}
