{
  Scan scan=new Scan();
  scan.addColumn(HConstants.CATALOG_FAMILY,HConstants.REGIONINFO_QUALIFIER);
  InternalScanner metaScanner=m.getScanner(scan);
  try {
    List<KeyValue> results=new ArrayList<KeyValue>();
    while (metaScanner.next(results)) {
      HRegionInfo info=null;
      for (      KeyValue kv : results) {
        if (kv.matchingColumn(HConstants.CATALOG_FAMILY,HConstants.REGIONINFO_QUALIFIER)) {
          info=Writables.getHRegionInfoOrNull(kv.getValue());
          if (info == null) {
            LOG.warn("region info is null for row " + Bytes.toString(kv.getRow()) + " in table "+ HConstants.META_TABLE_NAME);
          }
          break;
        }
      }
      if (!listener.processRow(info)) {
        break;
      }
      results.clear();
    }
  }
  finally {
    metaScanner.close();
  }
}
