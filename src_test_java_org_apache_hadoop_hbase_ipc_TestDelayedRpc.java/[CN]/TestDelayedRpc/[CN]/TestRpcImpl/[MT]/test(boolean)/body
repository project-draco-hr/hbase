{
  if (!delay) {
    return UNDELAYED;
  }
  final Delayable call=rpcServer.getCurrentCall();
  call.startDelay();
  new Thread(){
    public void run(){
      try {
        Thread.sleep(500);
        call.endDelay(DELAYED);
      }
 catch (      IOException e) {
        e.printStackTrace();
      }
catch (      InterruptedException e) {
        e.printStackTrace();
      }
    }
  }
.start();
  return 0xDEADBEEF;
}
