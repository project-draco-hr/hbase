{
  try {
    ArrayList<Result> a;
    ResultScanner s;
    Result r;
    a=new ArrayList<Result>();
    s=scannerMaster.getScanner(id);
    while ((r=s.next()) != null) {
      a.add(r);
    }
    scannerMaster.scannerClose(id);
    return a.toArray(new Result[0]);
  }
 catch (  IOException e) {
    throw new HBaseRestException(e);
  }
}
