{
  try {
    ArrayList<RowResult> a;
    Scanner s;
    RowResult r;
    a=new ArrayList<RowResult>();
    s=scannerMaster.getScanner(id);
    while ((r=s.next()) != null) {
      a.add(r);
    }
    scannerMaster.scannerClose(id);
    return a.toArray(new RowResult[0]);
  }
 catch (  IOException e) {
    throw new HBaseRestException(e);
  }
}
