{
  try {
    ArrayList<RowResult> a;
    Scanner s;
    RowResult r;
    a=new ArrayList<RowResult>();
    s=scannerMaster.getScanner(id);
    if (s == null) {
      throw new HBaseRestException("ScannerId: " + id + " is unavailable.  Please create a new scanner");
    }
    for (int i=0; i < numRows; i++) {
      if ((r=s.next()) != null) {
        a.add(r);
      }
 else {
        scannerMaster.scannerClose(id);
        break;
      }
    }
    return a.toArray(new RowResult[0]);
  }
 catch (  IOException e) {
    throw new HBaseRestException(e);
  }
}
