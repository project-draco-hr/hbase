{
  MetricMutableCounterLong counter=counters.get(counterName);
  if (counter == null) {
    MetricMutableCounterLong newCounter=new MetricMutableCounterLong(counterName,"",potentialStartingValue);
    counter=counters.putIfAbsent(counterName,newCounter);
    if (counter == null) {
      counter=newCounter;
    }
  }
  return counter;
}
