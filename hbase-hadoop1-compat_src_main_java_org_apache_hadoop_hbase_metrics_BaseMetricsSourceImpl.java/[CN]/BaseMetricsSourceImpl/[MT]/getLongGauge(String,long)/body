{
  MetricMutableGaugeLong gauge=gauges.get(gaugeName);
  if (gauge == null) {
    MetricMutableGaugeLong newGauge=new MetricMutableGaugeLong(gaugeName,"",potentialStartingValue);
    gauge=gauges.putIfAbsent(gaugeName,newGauge);
    if (gauge == null) {
      gauge=newGauge;
    }
  }
  return gauge;
}
