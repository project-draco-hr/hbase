{
  StringBuilder sb=new StringBuilder();
  toStringClassDetails(sb);
  if (procId != null) {
    sb.append(" id=");
    sb.append(getProcId());
  }
  if (hasParent()) {
    sb.append(" parent=");
    sb.append(getParentProcId());
  }
  if (hasOwner()) {
    sb.append(" owner=");
    sb.append(getOwner());
  }
  sb.append(" state=");
  sb.append(getState());
  return sb.toString();
}
