{
  if (count == 1) {
    super.releaseRowLock(lockId);
    return;
  }
  if (count == 2) {
    try {
      putCompleted=true;
      super.releaseRowLock(lockId);
      latch.await();
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
    }
  }
  if (count == 3) {
    super.releaseRowLock(lockId);
    try {
      Thread.sleep(1000);
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
    }
    latch.countDown();
  }
}
