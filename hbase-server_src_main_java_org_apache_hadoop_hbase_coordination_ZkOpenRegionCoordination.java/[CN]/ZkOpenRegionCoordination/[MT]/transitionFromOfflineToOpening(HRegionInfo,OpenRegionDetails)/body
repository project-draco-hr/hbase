{
  ZkOpenRegionDetails zkOrd=(ZkOpenRegionDetails)ord;
  final String encodedName=regionInfo.getEncodedName();
  try {
    zkOrd.setVersion(ZKAssign.transitionNode(watcher,regionInfo,zkOrd.getServerName(),EventType.M_ZK_REGION_OFFLINE,EventType.RS_ZK_REGION_OPENING,zkOrd.getVersionOfOfflineNode()));
  }
 catch (  KeeperException e) {
    LOG.error("Error transition from OFFLINE to OPENING for region=" + encodedName,e);
    zkOrd.setVersion(-1);
    return false;
  }
  boolean b=isGoodVersion(zkOrd);
  if (!b) {
    LOG.warn("Failed transition from OFFLINE to OPENING for region=" + encodedName);
  }
  return b;
}
