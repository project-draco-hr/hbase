{
switch (field) {
case TABLE_NAME:
    if (value == null) {
      unsetTableName();
    }
 else {
      setTableName((ByteBuffer)value);
    }
  break;
case ROW:
if (value == null) {
  unsetRow();
}
 else {
  setRow((ByteBuffer)value);
}
break;
case MUTATIONS:
if (value == null) {
unsetMutations();
}
 else {
setMutations((List<Mutation>)value);
}
break;
case TIMESTAMP:
if (value == null) {
unsetTimestamp();
}
 else {
setTimestamp((Long)value);
}
break;
case ATTRIBUTES:
if (value == null) {
unsetAttributes();
}
 else {
setAttributes((Map<ByteBuffer,ByteBuffer>)value);
}
break;
}
}
