{
  oprot_.writeMessageBegin(new org.apache.thrift.protocol.TMessage("scannerOpen",org.apache.thrift.protocol.TMessageType.CALL,++seqid_));
  scannerOpen_args args=new scannerOpen_args();
  args.setTableName(tableName);
  args.setStartRow(startRow);
  args.setColumns(columns);
  args.write(oprot_);
  oprot_.writeMessageEnd();
  oprot_.getTransport().flush();
}
