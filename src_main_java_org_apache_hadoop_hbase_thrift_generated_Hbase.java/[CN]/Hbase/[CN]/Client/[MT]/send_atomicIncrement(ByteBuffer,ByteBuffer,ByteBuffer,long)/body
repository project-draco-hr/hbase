{
  oprot_.writeMessageBegin(new org.apache.thrift.protocol.TMessage("atomicIncrement",org.apache.thrift.protocol.TMessageType.CALL,++seqid_));
  atomicIncrement_args args=new atomicIncrement_args();
  args.setTableName(tableName);
  args.setRow(row);
  args.setColumn(column);
  args.setValue(value);
  args.write(oprot_);
  oprot_.writeMessageEnd();
  oprot_.getTransport().flush();
}
