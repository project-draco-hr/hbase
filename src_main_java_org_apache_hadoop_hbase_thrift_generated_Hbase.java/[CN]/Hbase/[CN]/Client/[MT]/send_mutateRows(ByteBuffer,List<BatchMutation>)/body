{
  oprot_.writeMessageBegin(new org.apache.thrift.protocol.TMessage("mutateRows",org.apache.thrift.protocol.TMessageType.CALL,++seqid_));
  mutateRows_args args=new mutateRows_args();
  args.setTableName(tableName);
  args.setRowBatches(rowBatches);
