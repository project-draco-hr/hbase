{
  oprot_.writeMessageBegin(new org.apache.thrift.protocol.TMessage("getRows",org.apache.thrift.protocol.TMessageType.CALL,++seqid_));
  getRows_args args=new getRows_args();
  args.setTableName(tableName);
  args.setRows(rows);
  args.write(oprot_);
  oprot_.writeMessageEnd();
  oprot_.getTransport().flush();
}
