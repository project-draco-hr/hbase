{
  TMessage msg=iprot_.readMessageBegin();
  if (msg.type == TMessageType.EXCEPTION) {
    TApplicationException x=TApplicationException.read(iprot_);
    iprot_.readMessageEnd();
    throw x;
  }
  if (msg.seqid != seqid_) {
    throw new TApplicationException(TApplicationException.BAD_SEQUENCE_ID,"scannerOpenWithScan failed: out of sequence response");
  }
  scannerOpenWithScan_result result=new scannerOpenWithScan_result();
  result.read(iprot_);
  iprot_.readMessageEnd();
  if (result.isSetSuccess()) {
    return result.success;
  }
  if (result.io != null) {
    throw result.io;
  }
  throw new TApplicationException(TApplicationException.MISSING_RESULT,"scannerOpenWithScan failed: unknown result");
}
