{
  oprot_.writeMessageBegin(new org.apache.thrift.protocol.TMessage("deleteAll",org.apache.thrift.protocol.TMessageType.CALL,++seqid_));
  deleteAll_args args=new deleteAll_args();
  args.setTableName(tableName);
  args.setRow(row);
  args.setColumn(column);
  args.write(oprot_);
  oprot_.writeMessageEnd();
  oprot_.getTransport().flush();
}
