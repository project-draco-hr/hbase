{
  oprot_.writeMessageBegin(new org.apache.thrift.protocol.TMessage("mutateRowTs",org.apache.thrift.protocol.TMessageType.CALL,++seqid_));
  mutateRowTs_args args=new mutateRowTs_args();
  args.setTableName(tableName);
  args.setRow(row);
