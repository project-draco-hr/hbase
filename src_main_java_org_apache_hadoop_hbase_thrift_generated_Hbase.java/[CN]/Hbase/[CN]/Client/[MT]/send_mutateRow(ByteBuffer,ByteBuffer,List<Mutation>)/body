{
  oprot_.writeMessageBegin(new org.apache.thrift.protocol.TMessage("mutateRow",org.apache.thrift.protocol.TMessageType.CALL,++seqid_));
  mutateRow_args args=new mutateRow_args();
  args.setTableName(tableName);
  args.setRow(row);
  args.setMutations(mutations);
  args.write(oprot_);
  oprot_.writeMessageEnd();
  oprot_.getTransport().flush();
}
