{
switch (field) {
case TABLE_NAME:
    if (value == null) {
      unsetTableName();
    }
 else {
      setTableName((ByteBuffer)value);
    }
  break;
case ROW_BATCHES:
if (value == null) {
  unsetRowBatches();
}
 else {
  setRowBatches((List<BatchMutation>)value);
}
break;
case ATTRIBUTES:
if (value == null) {
unsetAttributes();
}
 else {
setAttributes((Map<ByteBuffer,ByteBuffer>)value);
}
break;
}
}
