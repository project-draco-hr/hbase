{
  if (this.regionThreads != null) {
synchronized (this.regionThreads) {
      for (      Thread t : this.regionThreads) {
        if (t.isAlive()) {
          try {
            t.join();
          }
 catch (          InterruptedException e) {
          }
        }
      }
    }
  }
  if (this.master != null && this.master.isAlive()) {
    try {
      this.master.join();
    }
 catch (    InterruptedException e) {
    }
  }
}
