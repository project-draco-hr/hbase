{
  ServerManager serverManager=masterServices.getServerManager();
  ServerLoad load=serverManager.getLoad(sn);
  if (load != null) {
    Map<byte[],RegionLoad> regionsLoad=load.getRegionsLoad();
    if (regionsLoad != null) {
      return regionsLoad.get(hri.getRegionName());
    }
  }
  return null;
}
