{
  super();
  try {
    this.server=null;
    this.log=null;
    this.tableName=null;
    this.value=null;
    String className=this.getClass().getName();
    StringBuilder v=new StringBuilder(className);
    while (v.length() < 1000) {
      v.append(className);
    }
    value=v.toString().getBytes(HConstants.UTF8_ENCODING);
  }
 catch (  Exception e) {
    LOG.fatal("error in constructor",e);
    throw e;
  }
}
