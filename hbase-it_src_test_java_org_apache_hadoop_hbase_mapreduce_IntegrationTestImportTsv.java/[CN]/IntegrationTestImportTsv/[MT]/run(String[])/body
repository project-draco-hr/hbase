{
  if (args.length != 0) {
    System.err.println(format("%s [genericOptions]",NAME));
    System.err.println("  Runs ImportTsv integration tests against a distributed cluster.");
    System.err.println();
    GenericOptionsParser.printGenericCommandUsage(System.err);
    return 1;
  }
  provisionCluster();
  testGenerateAndLoad();
  releaseCluster();
  return 0;
}
