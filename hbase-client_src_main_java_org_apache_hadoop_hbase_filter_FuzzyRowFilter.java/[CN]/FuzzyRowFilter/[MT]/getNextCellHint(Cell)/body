{
  byte[] nextRowKey=null;
  for (  Pair<byte[],byte[]> fuzzyData : fuzzyKeysData) {
    byte[] nextRowKeyCandidate=getNextForFuzzyRule(currentCell.getRowArray(),currentCell.getRowOffset(),currentCell.getRowLength(),fuzzyData.getFirst(),fuzzyData.getSecond());
    if (nextRowKeyCandidate == null) {
      continue;
    }
    if (nextRowKey == null || Bytes.compareTo(nextRowKeyCandidate,nextRowKey) < 0) {
      nextRowKey=nextRowKeyCandidate;
    }
  }
  if (nextRowKey == null) {
    throw new IllegalStateException("No next row key that satisfies fuzzy exists when" + " getNextKeyHint() is invoked." + " Filter: " + this.toString() + " currentKV: "+ KeyValueUtil.ensureKeyValue(currentCell).toString());
  }
  return KeyValueUtil.createFirstOnRow(nextRowKey);
}
