{
  byte[] result=Arrays.copyOf(fuzzyKeyBytes,length > fuzzyKeyBytes.length ? length : fuzzyKeyBytes.length);
  int toInc=-1;
  boolean increased=false;
  for (int i=0; i < result.length; i++) {
    if (i >= fuzzyKeyMeta.length || fuzzyKeyMeta[i] == 1) {
      result[i]=row[offset + i];
      if (!isMax(row[i])) {
        toInc=i;
      }
    }
 else     if (i < fuzzyKeyMeta.length && fuzzyKeyMeta[i] == 0) {
      if ((row[i + offset] & 0xFF) < (fuzzyKeyBytes[i] & 0xFF)) {
        increased=true;
        break;
      }
      if ((row[i + offset] & 0xFF) > (fuzzyKeyBytes[i] & 0xFF)) {
        break;
      }
    }
  }
  if (!increased) {
    if (toInc < 0) {
      return null;
    }
    result[toInc]++;
    for (int i=toInc + 1; i < result.length; i++) {
      if (i >= fuzzyKeyMeta.length || fuzzyKeyMeta[i] == 1) {
        result[i]=0;
      }
    }
  }
  return result;
}
