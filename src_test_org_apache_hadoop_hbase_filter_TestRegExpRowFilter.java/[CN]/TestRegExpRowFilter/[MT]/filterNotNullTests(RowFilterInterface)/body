{
  ((RegExpRowFilter)filter).setColumnFilter(new byte[]{LAST_CHAR},null);
  char secondToLast=(char)(LAST_CHAR - 1);
  char thirdToLast=(char)(LAST_CHAR - 2);
  colvalues.remove(new byte[]{(byte)secondToLast});
  assertTrue("Failed with last columnKey " + thirdToLast,filter.filterRow(colvalues));
  colvalues.put(new byte[]{(byte)secondToLast},new Cell(GOOD_BYTES,HConstants.LATEST_TIMESTAMP));
  assertFalse("Failed with last columnKey " + secondToLast,filter.filterRow(colvalues));
  colvalues.put(new byte[]{LAST_CHAR},new Cell(GOOD_BYTES,HConstants.LATEST_TIMESTAMP));
  assertTrue("Failed with last columnKey " + LAST_CHAR,filter.filterRow(colvalues));
  colvalues.put(new byte[]{LAST_CHAR},new Cell(HLogEdit.deleteBytes.get(),HConstants.LATEST_TIMESTAMP));
  assertFalse("Failed with last columnKey " + LAST_CHAR + " mapping to null.",filter.filterRow(colvalues));
}
