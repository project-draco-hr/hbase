{
  if (isMetaTable) {
    this.metaRegionName=HRegionInfo.ROOT_REGIONINFO.getRegionName();
    this.metaRegion=new MetaRegion(master.getRootRegionLocation(),this.metaRegionName,HConstants.EMPTY_START_ROW);
  }
 else {
    this.metaRegion=master.regionManager.getFirstMetaRegionForRegion(regionInfo);
    if (this.metaRegion != null) {
      this.metaRegionName=this.metaRegion.getRegionName();
    }
  }
  return this.metaRegion;
}
