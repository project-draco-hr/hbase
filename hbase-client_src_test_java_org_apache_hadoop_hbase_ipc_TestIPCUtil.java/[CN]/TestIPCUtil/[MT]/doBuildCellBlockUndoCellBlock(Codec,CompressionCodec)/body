{
  final int count=10;
  Cell[] cells=getCells(count);
  ByteBuffer bb=this.util.buildCellBlock(codec,compressor,CellUtil.createCellScanner(Arrays.asList(cells).iterator()));
  CellScanner scanner=this.util.createCellScanner(codec,compressor,bb.array(),0,bb.limit());
  int i=0;
  while (scanner.advance()) {
    i++;
  }
  assertEquals(count,i);
}
