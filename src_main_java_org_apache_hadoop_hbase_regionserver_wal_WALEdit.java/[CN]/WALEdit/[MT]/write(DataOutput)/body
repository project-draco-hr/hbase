{
  out.writeInt(VERSION_2);
  out.writeInt(kvs.size());
  for (  KeyValue kv : kvs) {
    kv.write(out);
  }
  if (scopes == null) {
    out.writeInt(0);
  }
 else {
    out.writeInt(scopes.size());
    for (    byte[] key : scopes.keySet()) {
      Bytes.writeByteArray(out,key);
      out.writeInt(scopes.get(key));
    }
  }
}
