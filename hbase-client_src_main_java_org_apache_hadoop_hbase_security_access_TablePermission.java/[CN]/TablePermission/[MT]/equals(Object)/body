{
  if (!(obj instanceof TablePermission)) {
    return false;
  }
  TablePermission other=(TablePermission)obj;
  if (!(table.equals(other.getTable()) && ((family == null && other.getFamily() == null) || Bytes.equals(family,other.getFamily())) && ((qualifier == null && other.getQualifier() == null) || Bytes.equals(qualifier,other.getQualifier()))&& ((namespace == null && other.getNamespace() == null) || namespace.equals(other.getNamespace())))) {
    return false;
  }
  return super.equals(other);
}
