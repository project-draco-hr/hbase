{
  if (!(this.editsSize.get() == 0)) {
    this.writer.close();
    if (this.reader != null) {
      this.reader.close();
    }
    this.fs.delete(this.path,true);
  }
  this.writer=HLog.createWriter(this.fs,this.path,this.conf);
  this.editsSize.set(0);
  this.position=0;
  LOG.debug("New replication log");
}
