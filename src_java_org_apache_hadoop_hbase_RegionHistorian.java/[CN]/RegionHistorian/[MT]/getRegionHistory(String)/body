{
  if (!isOnline()) {
    return null;
  }
  List<RegionHistoryInformation> informations=new ArrayList<RegionHistoryInformation>();
  try {
    for (    HistorianColumnKey keyEnu : HistorianColumnKey.values()) {
      byte[] columnKey=keyEnu.key;
      Cell[] cells=this.metaTable.get(Bytes.toBytes(regionName),columnKey,ALL_VERSIONS);
      if (cells != null) {
        for (        Cell cell : cells) {
          informations.add(historian.new RegionHistoryInformation(cell.getTimestamp(),Bytes.toString(columnKey).split(":")[1],Bytes.toString(cell.getValue())));
        }
      }
    }
  }
 catch (  IOException ioe) {
    LOG.warn("Unable to retrieve region history",ioe);
  }
  Collections.sort(informations);
  return informations;
}
