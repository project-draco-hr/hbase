{
  if (closing.get() || closed.get()) {
    close();
    throw new NotServingRegionException(regionInfo.getRegionNameAsString() + " is closing=" + closing.get()+ " or closed="+ closed.get());
  }
  results.clear();
  boolean returnResult=nextInternal(limit);
  if (!returnResult && filter != null) {
    returnResult=filter.filterRow(results);
    if (returnResult || filter.filterRow()) {
      results.clear();
    }
  }
  outResults.addAll(results);
  resetFilters();
  if (isFilterDone()) {
    return false;
  }
  return returnResult;
}
