{
  if (this.scanner != null) {
    try {
      this.scanner.close();
      this.scanner=null;
    }
 catch (    IOException ex) {
      Log.warn("Exception while closing scanner",ex);
    }
  }
  if (this.region != null) {
    try {
      this.region.closeRegionOperation();
      this.region.close(true);
      this.region=null;
    }
 catch (    IOException ex) {
      Log.warn("Exception while closing region",ex);
    }
  }
}
