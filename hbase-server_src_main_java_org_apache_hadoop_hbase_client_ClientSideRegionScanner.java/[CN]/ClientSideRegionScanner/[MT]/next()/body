{
  values.clear();
  scanner.nextRaw(values,-1);
  if (values.isEmpty()) {
    return null;
  }
  Result result=Result.create(values);
  if (this.scanMetrics != null) {
    long resultSize=0;
    for (    Cell kv : values) {
      resultSize+=KeyValueUtil.ensureKeyValue(kv).getLength();
    }
    this.scanMetrics.countOfBytesInResults.addAndGet(resultSize);
  }
  return result;
}
