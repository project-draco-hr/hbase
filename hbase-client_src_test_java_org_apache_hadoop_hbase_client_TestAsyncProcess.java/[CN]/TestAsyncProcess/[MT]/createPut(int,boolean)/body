{
  Put p;
  if (!success) {
    p=new Put(FAILS);
  }
 else switch (regCnt) {
case 1:
    p=new Put(DUMMY_BYTES_1);
  break;
case 2:
p=new Put(DUMMY_BYTES_2);
break;
case 3:
p=new Put(DUMMY_BYTES_3);
break;
default :
throw new IllegalArgumentException("unknown " + regCnt);
}
p.add(DUMMY_BYTES_1,DUMMY_BYTES_1,DUMMY_BYTES_1);
return p;
}
