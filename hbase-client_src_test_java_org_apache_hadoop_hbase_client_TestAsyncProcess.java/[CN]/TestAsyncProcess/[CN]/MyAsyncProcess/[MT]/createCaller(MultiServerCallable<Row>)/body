{
  final MultiResponse mr=createMultiResponse(callable.getLocation(),callable.getMulti(),nbMultiResponse,nbActions);
  return new RpcRetryingCaller<MultiResponse>(conf){
    @Override public MultiResponse callWithoutRetries(    RetryingCallable<MultiResponse> callable) throws IOException, RuntimeException {
      try {
        Thread.sleep(1000);
      }
 catch (      InterruptedException e) {
      }
      return mr;
    }
  }
;
}
