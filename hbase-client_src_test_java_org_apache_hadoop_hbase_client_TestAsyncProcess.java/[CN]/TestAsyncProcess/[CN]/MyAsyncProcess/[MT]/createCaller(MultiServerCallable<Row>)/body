{
  callsCt.incrementAndGet();
  final MultiResponse mr=createMultiResponse(callable.getMulti(),nbMultiResponse,nbActions);
  return new RpcRetryingCaller<MultiResponse>(100,10){
    @Override public MultiResponse callWithoutRetries(    RetryingCallable<MultiResponse> callable,    int callTimeout) throws IOException, RuntimeException {
      try {
        Thread.sleep(1000);
      }
 catch (      InterruptedException e) {
      }
      return mr;
    }
  }
;
}
