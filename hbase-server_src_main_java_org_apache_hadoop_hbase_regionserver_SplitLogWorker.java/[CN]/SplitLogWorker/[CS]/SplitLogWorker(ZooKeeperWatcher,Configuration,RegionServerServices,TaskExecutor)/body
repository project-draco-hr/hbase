{
  super(watcher);
  this.server=server;
  this.serverName=server.getServerName();
  this.splitTaskExecutor=splitTaskExecutor;
  report_period=conf.getInt("hbase.splitlog.report.period",conf.getInt("hbase.splitlog.manager.timeout",SplitLogManager.DEFAULT_TIMEOUT) / 3);
  this.conf=conf;
  this.executorService=this.server.getExecutorService();
  this.maxConcurrentTasks=conf.getInt("hbase.regionserver.wal.max.splitters",DEFAULT_MAX_SPLITTERS);
}
