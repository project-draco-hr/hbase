{
  LinkedList<HTable> queue=tables.get(tableName);
  if (queue == null) {
    queue=new LinkedList<HTable>();
    tables.put(tableName,queue);
    return newHTable(tableName);
  }
  HTable table;
synchronized (queue) {
    table=queue.poll();
  }
  if (table == null) {
    return newHTable(tableName);
  }
  return table;
}
