{
  if (checkTagPresence) {
    if (results.size() > 0) {
      Result result=results.get(0);
      CellScanner cellScanner=result.cellScanner();
      if (cellScanner.advance()) {
        Cell cell=cellScanner.current();
        tags=Tag.asList(cell.getTagsArray(),cell.getTagsOffset(),cell.getTagsLength());
      }
    }
  }
  return hasMore;
}
