{
  basePath=testUtil.getDataTestDir(TestClassFinder.class.getSimpleName()).toString();
  if (!basePath.endsWith("/")) {
    basePath+="/";
  }
  File testDir=new File(basePath);
  if (testDir.exists()) {
    deleteTestDir();
  }
  assertTrue(testDir.mkdirs());
  Log.info("Using new, clean directory=" + testDir);
}
