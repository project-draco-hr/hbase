{
  Map<Long,StoreFile> map=this.store.getStorefiles().descendingMap();
  List<StoreFileScanner> sfScanners=StoreFileScanner.getScannersForStoreFiles(map.values(),cacheBlocks,isGet);
  List<KeyValueScanner> scanners=new ArrayList<KeyValueScanner>(sfScanners.size() + 1);
  scanners.addAll(sfScanners);
  scanners.addAll(this.store.memstore.getScanners());
  return scanners;
}
