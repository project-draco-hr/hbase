{
  this.lock.writeLock().lock();
  try {
    ArrayList<StoreFile> result=new ArrayList<StoreFile>(storefiles.values());
    this.storefiles.clear();
    for (    StoreFile f : result) {
      f.closeReader();
    }
    LOG.debug("closed " + this.storeNameStr);
    return result;
  }
  finally {
    this.lock.writeLock().unlock();
  }
}
