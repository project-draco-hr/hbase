{
  StoreFile storeFile=null;
  try {
    storeFile=new StoreFile(this.fs,path,this.conf,this.cacheConf,this.family.getBloomFilterType());
    storeFile.createReader();
  }
 catch (  IOException e) {
    LOG.error("Failed to open store file : " + path + ", keeping it in tmp location",e);
    throw e;
  }
 finally {
    if (storeFile != null) {
      storeFile.closeReader();
    }
  }
}
