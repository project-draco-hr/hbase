{
  m_row.clear();
  HStoreKey tKey=key;
  boolean hasMore=m_scanner.next(tKey,m_row);
  if (hasMore) {
    if (m_endRow.getLength() > 0 && (tKey.getRow().compareTo(m_endRow) >= 0)) {
      hasMore=false;
    }
 else {
      for (      Map.Entry<Text,byte[]> e : m_row.entrySet()) {
        value.put(e.getKey(),new ImmutableBytesWritable(e.getValue()));
      }
    }
  }
  return hasMore;
}
