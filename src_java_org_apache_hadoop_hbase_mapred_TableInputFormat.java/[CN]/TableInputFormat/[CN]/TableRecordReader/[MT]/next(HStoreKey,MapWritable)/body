{
  this.m_row.clear();
  HStoreKey tKey=key;
  boolean hasMore=this.m_scanner.next(tKey,this.m_row);
  if (hasMore) {
    value.clear();
    for (    Map.Entry<Text,byte[]> e : this.m_row.entrySet()) {
      value.put(e.getKey(),new ImmutableBytesWritable(e.getValue()));
    }
  }
  return hasMore;
}
