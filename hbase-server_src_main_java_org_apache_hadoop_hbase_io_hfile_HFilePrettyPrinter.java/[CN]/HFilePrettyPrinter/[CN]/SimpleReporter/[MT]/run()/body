{
  for (  Map.Entry<String,SortedMap<MetricName,Metric>> entry : getMetricsRegistry().groupedMetrics(MetricPredicate.ALL).entrySet()) {
    try {
      for (      Map.Entry<MetricName,Metric> subEntry : entry.getValue().entrySet()) {
        out.print("   " + subEntry.getKey().getName());
        out.println(':');
        subEntry.getValue().processWith(this,subEntry.getKey(),out);
      }
    }
 catch (    Exception e) {
      e.printStackTrace(out);
    }
  }
}
