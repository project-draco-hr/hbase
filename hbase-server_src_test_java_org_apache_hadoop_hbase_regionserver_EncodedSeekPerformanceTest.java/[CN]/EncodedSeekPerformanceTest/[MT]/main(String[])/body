{
  if (args.length < 1) {
    printUsage();
    System.exit(-1);
  }
  Path path=new Path(args[0]);
  List<HFileDataBlockEncoder> encoders=new ArrayList<HFileDataBlockEncoder>();
  for (  DataBlockEncoding encodingAlgo : DataBlockEncoding.values()) {
    encoders.add(new HFileDataBlockEncoderImpl(DataBlockEncoding.NONE,encodingAlgo));
  }
  EncodedSeekPerformanceTest utility=new EncodedSeekPerformanceTest();
  utility.runTests(path,encoders);
  System.exit(0);
}
