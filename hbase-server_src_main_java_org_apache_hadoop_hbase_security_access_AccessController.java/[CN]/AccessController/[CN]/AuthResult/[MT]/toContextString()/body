{
  return "(user=" + (user != null ? user.getName() : "UNKNOWN") + ", "+ "scope="+ (table == null ? "GLOBAL" : Bytes.toString(table))+ ", "+ "family="+ (family != null ? Bytes.toString(family) : "")+ ", "+ "qualifer="+ (qualifier != null ? Bytes.toString(qualifier) : "")+ ", "+ "action="+ (action != null ? action.toString() : "")+ ")";
}
