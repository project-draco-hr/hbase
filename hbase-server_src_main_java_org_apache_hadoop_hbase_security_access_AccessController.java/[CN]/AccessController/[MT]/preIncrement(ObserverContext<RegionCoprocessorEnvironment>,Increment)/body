{
  requireCoveringPermission("increment",c.getEnvironment(),increment.getRow(),increment.getFamilyCellMap(),increment.getTimeRange().getMax(),false,Action.WRITE);
  byte[] bytes=increment.getAttribute(AccessControlConstants.OP_ATTRIBUTE_ACL);
  if (bytes != null) {
    if (canPersistCellACLs) {
      addCellPermissions(bytes,increment.getFamilyCellMap());
    }
 else {
      throw new DoNotRetryIOException("Cell ACLs cannot be persisted");
    }
  }
  return null;
}
