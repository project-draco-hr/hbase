{
  requireCoveringPermission("checkAndPut",c.getEnvironment(),row,makeFamilyMap(family,qualifier),HConstants.LATEST_TIMESTAMP,false,Action.READ,Action.WRITE);
  byte[] bytes=put.getAttribute(AccessControlConstants.OP_ATTRIBUTE_ACL);
  if (bytes != null) {
    addCellPermissions(bytes,put.getFamilyCellMap());
  }
  return result;
}
