{
  requireCoveringPermission(OpType.APPEND,c.getEnvironment(),append.getRow(),append.getFamilyCellMap(),HConstants.LATEST_TIMESTAMP,Action.WRITE);
  byte[] bytes=append.getAttribute(AccessControlConstants.OP_ATTRIBUTE_ACL);
  if (bytes != null) {
    if (canPersistCellACLs) {
      addCellPermissions(bytes,append.getFamilyCellMap());
    }
 else {
      throw new DoNotRetryIOException("Cell ACLs cannot be persisted");
    }
  }
  return null;
}
