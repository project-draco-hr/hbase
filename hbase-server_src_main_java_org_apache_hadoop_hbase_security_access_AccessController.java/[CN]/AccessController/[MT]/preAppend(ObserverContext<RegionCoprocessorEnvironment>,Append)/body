{
  requireCoveringPermission("append",c.getEnvironment(),append.getRow(),append.getFamilyCellMap(),append.getTimeStamp(),false,Action.WRITE);
  byte[] bytes=append.getAttribute(AccessControlConstants.OP_ATTRIBUTE_ACL);
  if (bytes != null) {
    addCellPermissions(bytes,append.getFamilyCellMap());
  }
  return null;
}
