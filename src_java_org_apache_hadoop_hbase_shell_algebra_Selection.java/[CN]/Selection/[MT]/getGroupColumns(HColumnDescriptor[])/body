{
  String result=null;
  if (condition.containsKey(Constants.RELATIONAL_PROJECTION)) {
    result=getProjColumns();
  }
 else {
    result=getColumnStringArray(columns);
  }
  String[] groups=result.split(" ");
  for (int i=0; i < groups.length; i++) {
    projSet.add(groups[i]);
  }
  return result;
}
