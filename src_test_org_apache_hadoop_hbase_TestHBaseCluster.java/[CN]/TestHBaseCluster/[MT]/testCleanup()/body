{
  if (!initialized) {
    throw new IllegalStateException();
  }
  try {
    if (!failures) {
      client.deleteTable(desc.getName());
      try {
        Thread.sleep(60000);
      }
 catch (      InterruptedException e) {
      }
    }
  }
  finally {
    cluster.shutdown();
    client.close();
  }
}
