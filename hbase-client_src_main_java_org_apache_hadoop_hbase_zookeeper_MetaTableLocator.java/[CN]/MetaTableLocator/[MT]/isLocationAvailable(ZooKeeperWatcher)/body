{
  try {
    return ZKUtil.getData(zkw,zkw.metaServerZNode) != null;
  }
 catch (  KeeperException e) {
    LOG.error("ZK error trying to get hbase:meta from ZooKeeper");
    return false;
  }
catch (  InterruptedException e) {
    LOG.error("ZK error trying to get hbase:meta from ZooKeeper");
    return false;
  }
}
