{
  boolean result=false;
  while (true) {
    if (!keyIterators[i].hasNext()) {
      closeSubScanner(i);
      break;
    }
    HStoreKey hsk=keyIterators[i].next();
    if (hsk == null) {
      throw new NullPointerException("Unexpected null key");
    }
    if (hsk.getTimestamp() <= this.timestamp) {
      this.keys[i]=hsk;
      this.vals[i]=backingMaps[i].get(keys[i]);
      result=true;
      break;
    }
  }
  return result;
}
