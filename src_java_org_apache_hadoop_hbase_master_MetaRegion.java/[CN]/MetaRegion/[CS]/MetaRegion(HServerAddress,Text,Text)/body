{
  if (server == null) {
    throw new IllegalArgumentException("server cannot be null");
  }
  this.server=server;
  if (regionName == null) {
    throw new IllegalArgumentException("regionName cannot be null");
  }
  this.regionName=new Text(regionName);
  this.startKey=new Text();
  if (startKey != null) {
    this.startKey.set(startKey);
  }
}
