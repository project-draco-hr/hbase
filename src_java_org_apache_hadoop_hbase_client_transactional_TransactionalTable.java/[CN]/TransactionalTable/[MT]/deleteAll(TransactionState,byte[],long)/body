{
  super.getConnection().getRegionServerWithRetries(new TransactionalServerCallable<Boolean>(super.getConnection(),super.getTableName(),row,transactionState){
    public Boolean call() throws IOException {
      recordServer();
      getTransactionServer().deleteAll(transactionState.getTransactionId(),location.getRegionInfo().getRegionName(),row,ts);
      return null;
    }
  }
);
}
