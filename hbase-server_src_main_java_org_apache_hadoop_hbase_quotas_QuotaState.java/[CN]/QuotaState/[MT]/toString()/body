{
  StringBuilder builder=new StringBuilder();
  builder.append("QuotaState(ts=" + lastUpdate);
  if (isBypass()) {
    builder.append(" bypass");
  }
 else {
    if (globalLimiter != NoopQuotaLimiter.get()) {
      builder.append(" " + globalLimiter);
    }
  }
  builder.append(')');
  return builder.toString();
}
