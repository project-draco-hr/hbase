{
  if (closed) {
    return;
  }
  if (!insideCacheFlush) {
    throw new IOException("Impossible situation: inside " + "completeCacheFlush(), but 'insideCacheFlush' flag is false");
  }
  writer.append(new HLogKey(regionName,tableName,HLog.METAROW,logSeqId),new HLogEdit(HLog.METACOLUMN,COMPLETE_CACHEFLUSH,System.currentTimeMillis()));
  numEntries++;
  regionToLastFlush.put(regionName,logSeqId);
  insideCacheFlush=false;
  notifyAll();
}
