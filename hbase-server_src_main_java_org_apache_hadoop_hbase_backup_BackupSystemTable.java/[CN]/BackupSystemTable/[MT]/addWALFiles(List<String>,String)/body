{
  if (LOG.isDebugEnabled()) {
    LOG.debug("add WAL files to hbase:backup");
  }
  Table table=null;
  try {
    table=connection.getTable(tableName);
    List<Put> puts=BackupSystemTableHelper.createPutsForAddWALFiles(files,backupId);
    table.put(puts);
  }
  finally {
    if (table != null) {
      table.close();
    }
  }
}
