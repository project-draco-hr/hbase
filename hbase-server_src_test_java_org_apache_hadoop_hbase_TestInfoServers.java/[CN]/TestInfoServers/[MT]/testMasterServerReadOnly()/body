{
  String sTableName="testMasterServerReadOnly";
  byte[] tableName=Bytes.toBytes(sTableName);
  byte[] cf=Bytes.toBytes("d");
  UTIL.createTable(tableName,cf);
  new HTable(UTIL.getConfiguration(),tableName).close();
  int port=UTIL.getHBaseCluster().getMaster().getInfoServer().getPort();
  assertDoesNotContainContent(new URL("http://localhost:" + port + "/table.jsp?name="+ sTableName+ "&action=split&key="),"Table action request accepted");
  assertDoesNotContainContent(new URL("http://localhost:" + port + "/table.jsp?name="+ sTableName),"Actions:");
}
