{
  try {
    server.join();
  }
 catch (  InterruptedException iex) {
  }
  try {
    clientProcessorThread.join();
  }
 catch (  Exception iex) {
  }
  try {
    metaScannerThread.join();
  }
 catch (  Exception iex) {
  }
  try {
    rootScannerThread.join();
  }
 catch (  Exception iex) {
  }
  LOG.info("HMaster stopped");
}
