{
  long regionServerMsgInterval=conf.getLong("hbase.regionserver.msginterval",15 * 1000);
  long endTime=System.currentTimeMillis() + (regionServerMsgInterval * 3);
  while (endTime > System.currentTimeMillis() && this.serversToServerInfo.size() > 0) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("Waiting on regionservers: " + this.serversToServerInfo.values());
    }
    try {
      Thread.sleep(threadWakeFrequency);
    }
 catch (    InterruptedException e) {
    }
  }
}
