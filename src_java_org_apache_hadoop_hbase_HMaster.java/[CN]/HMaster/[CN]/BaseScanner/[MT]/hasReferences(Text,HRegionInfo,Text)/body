{
  boolean result=HRegion.hasReferences(fs,fs.makeQualified(dir),split);
  if (result) {
    return result;
  }
  if (LOG.isDebugEnabled()) {
    LOG.debug(split.getRegionName().toString() + " no longer has references to " + regionName.toString());
  }
  HTable t=new HTable(conf,this.tableName);
  try {
    HRegion.removeSplitFromMETA(t,regionName,column);
  }
  finally {
    t.close();
  }
  return result;
}
