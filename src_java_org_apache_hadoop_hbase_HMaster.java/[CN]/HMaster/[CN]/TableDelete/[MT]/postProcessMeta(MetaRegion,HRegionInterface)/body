{
  for (  HashSet<HRegionInfo> s : servedRegions.values()) {
    for (    HRegionInfo i : s) {
synchronized (serversToServerInfo) {
        regionsToDelete.add(i.getRegionName());
      }
    }
  }
  for (  HRegionInfo i : unservedRegions) {
    try {
      HRegion.deleteRegion(fs,dir,HRegionInfo.encodeRegionName(i.getRegionName()));
    }
 catch (    IOException e) {
      LOG.error("failed to delete region " + i.getRegionName(),RemoteExceptionHandler.checkIOException(e));
    }
  }
  super.postProcessMeta(m,server);
}
