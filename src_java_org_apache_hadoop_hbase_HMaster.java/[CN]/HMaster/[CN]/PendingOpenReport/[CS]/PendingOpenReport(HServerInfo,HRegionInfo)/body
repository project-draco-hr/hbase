{
  if (region.tableDesc.getName().equals(META_TABLE_NAME)) {
    this.rootRegion=true;
  }
 else {
    this.rootRegion=false;
  }
  this.regionName=region.regionName;
  try {
    this.serverAddress=info.getServerAddress().toString().getBytes(UTF8_ENCODING);
    this.startCode=String.valueOf(info.getStartCode()).getBytes(UTF8_ENCODING);
  }
 catch (  UnsupportedEncodingException e) {
    LOG.error(e);
  }
}
