{
  if (regionName.find(HGlobals.metaTableDesc.getName().toString()) == 0) {
    this.rootRegion=true;
  }
 else {
    this.rootRegion=false;
  }
  this.regionName=regionName;
  try {
    this.serverAddress=new BytesWritable(info.getServerAddress().toString().getBytes(UTF8_ENCODING));
    this.startCode=new BytesWritable(String.valueOf(info.getStartCode()).getBytes(UTF8_ENCODING));
  }
 catch (  UnsupportedEncodingException e) {
    e.printStackTrace();
  }
}
