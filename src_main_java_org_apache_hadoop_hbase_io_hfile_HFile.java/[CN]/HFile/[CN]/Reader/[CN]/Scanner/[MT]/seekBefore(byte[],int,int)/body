{
  int b=reader.blockContainingKey(key,offset,length);
  if (b < 0)   return false;
  if (this.reader.comparator.compare(reader.blockIndex.blockKeys[b],0,reader.blockIndex.blockKeys[b].length,key,offset,length) == 0) {
    if (b == 0) {
      return false;
    }
    b--;
  }
  loadBlock(b,true);
  blockSeek(key,offset,length,true);
  return true;
}
