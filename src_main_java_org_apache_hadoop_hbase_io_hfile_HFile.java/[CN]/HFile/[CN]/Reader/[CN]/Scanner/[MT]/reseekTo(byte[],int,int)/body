{
  if (this.block != null && this.currKeyLen != 0) {
    ByteBuffer bb=getKey();
    int compared=this.reader.comparator.compare(key,offset,length,bb.array(),bb.arrayOffset(),bb.limit());
    if (compared < 1) {
      return compared;
    }
  }
  int b=reader.blockContainingKey(key,offset,length);
  if (b < 0) {
    return -1;
  }
  loadBlock(b,false);
  return blockSeek(key,offset,length,false);
}
