{
  for (  String user : superUsers) {
    boolean hasAccess=false;
    if (!user.startsWith(AuthUtil.GROUP_PREFIX)) {
      for (      ACL acl : acls) {
        if (user.equals(acl.getId().getId()) && acl.getPerms() == Perms.ALL) {
          hasAccess=true;
          break;
        }
      }
      if (!hasAccess) {
        return false;
      }
    }
  }
  return true;
}
