{
  if (!CellUtil.matchingColumn(c,this.columnFamily,this.columnQualifier)) {
    return ReturnCode.INCLUDE;
  }
  if (comparator != null && doCompare(compareOp,comparator,c.getValueArray(),c.getValueOffset(),c.getValueLength()))   return ReturnCode.SKIP;
  stampSet.add(c.getTimestamp());
  if (dropDependentColumn) {
    return ReturnCode.SKIP;
  }
  return ReturnCode.INCLUDE;
}
