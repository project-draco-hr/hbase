{
  if (!path.equals(node))   return;
  try {
    byte[] data=ZKUtil.getDataAndWatch(watcher,node);
    if (data != null) {
      this.data=data;
      notifyAll();
    }
 else {
      nodeDeleted(path);
    }
  }
 catch (  KeeperException e) {
    if (abortable != null) {
      abortable.abort("Unexpected exception handling nodeCreated event",e);
    }
  }
}
