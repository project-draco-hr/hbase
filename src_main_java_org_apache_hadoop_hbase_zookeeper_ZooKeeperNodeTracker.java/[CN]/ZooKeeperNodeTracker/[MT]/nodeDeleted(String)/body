{
  if (path.equals(node)) {
    try {
      if (ZKUtil.watchAndCheckExists(watcher,node)) {
        nodeCreated(path);
      }
 else {
        this.data=null;
      }
    }
 catch (    KeeperException e) {
      if (abortable != null) {
        abortable.abort("Unexpected exception handling nodeDeleted event",e);
      }
    }
  }
}
