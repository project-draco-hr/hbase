{
  lock.lock();
  try {
    if (runnables.isEmpty()) {
      waitCond.await();
      if (!runnables.isEmpty()) {
        return runnables.pop();
      }
    }
 else {
      return runnables.pop();
    }
  }
 catch (  InterruptedException e) {
    Thread.currentThread().interrupt();
    return null;
  }
 finally {
    lock.unlock();
  }
  return null;
}
