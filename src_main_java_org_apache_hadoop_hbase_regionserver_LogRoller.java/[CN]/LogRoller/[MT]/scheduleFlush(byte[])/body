{
  boolean scheduled=false;
  HRegion r=this.services.getFromOnlineRegions(Bytes.toString(region));
  FlushRequester requester=null;
  if (r != null) {
    requester=this.services.getFlushRequester();
    if (requester != null) {
      requester.requestFlush(r);
      scheduled=true;
    }
  }
  if (!scheduled) {
    LOG.warn("Failed to schedule flush of " + Bytes.toString(region) + "r="+ r+ ", requester="+ requester);
  }
}
