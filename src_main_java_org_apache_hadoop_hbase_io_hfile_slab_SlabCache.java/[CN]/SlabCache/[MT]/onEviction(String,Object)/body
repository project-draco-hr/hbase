{
  stats.evict();
  while ((backingStore.remove(key)) == null) {
synchronized (notifier) {
      try {
        notifier.wait();
      }
 catch (      InterruptedException e) {
        LOG.warn("InterruptedException on the evicting thread: " + e);
      }
    }
  }
  stats.evicted();
synchronized (notifier) {
    notifier.notifyAll();
  }
}
