{
  KeyValue kv=KeyValueUtil.ensureKeyValue(cell);
  byte[] family=kv.getFamily();
  List<Cell> list=getCellList(family);
  int res=Bytes.compareTo(this.row,0,row.length,kv.getBuffer(),kv.getRowOffset(),kv.getRowLength());
  if (res != 0) {
    throw new WrongRowIOException("The row in " + kv.toString() + " doesn't match the original one "+ Bytes.toStringBinary(this.row));
  }
  list.add(kv);
  familyMap.put(family,list);
  return this;
}
