{
  try {
    super.tearDown();
    if (cluster != null) {
      cluster.shutdown();
    }
    if (dfs != null) {
      FileSystem fs=dfs.getFileSystem();
      try {
        dfs.shutdown();
      }
  finally {
        if (fs != null) {
          fs.close();
        }
      }
    }
  }
 catch (  Exception e) {
    LOG.fatal("error in tearDown",e);
    throw e;
  }
}
