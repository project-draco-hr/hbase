{
  RemoteIterator<LocatedFileStatus> it=fs.listFiles(rootDir,true);
  while (it.hasNext()) {
    LocatedFileStatus lfs=it.next();
    if (lfs.isDirectory()) {
      continue;
    }
    if (filter.accept(lfs.getPath())) {
      files.add(lfs.getPath().toString());
      LOG.info(lfs.getPath());
    }
  }
  return files;
}
