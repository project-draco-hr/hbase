{
  LOG.debug("Handling CLOSED event for " + regionInfo.getEncodedName());
  if (this.assignmentManager.getTableStateManager().isTableState(this.regionInfo.getTable(),ZooKeeperProtos.Table.State.DISABLED,ZooKeeperProtos.Table.State.DISABLING)) {
    assignmentManager.offlineDisabledRegion(regionInfo);
    return;
  }
  assignmentManager.getRegionStates().updateRegionState(regionInfo,RegionState.State.CLOSED);
  assignmentManager.removeClosedRegion(regionInfo);
  assignmentManager.assign(regionInfo,true);
}
