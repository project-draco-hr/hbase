{
  if (current == null) {
    return false;
  }
  heap.add(current);
  current=null;
  KeyValueScanner scanner;
  while ((scanner=heap.poll()) != null) {
    KeyValue topKey=scanner.peek();
    if (comparator.getComparator().compare(seekKey,topKey) <= 0) {
      current=scanner;
      return true;
    }
    if (!seekType.seek(scanner,seekKey,forward)) {
      scanner.close();
    }
 else {
      heap.add(scanner);
    }
  }
  return false;
}
