{
  StringBuilder sb=new StringBuilder(512);
  sb.append(getDescription());
  sb.append(": status=");
  sb.append(getStatus());
  sb.append(", state=");
  sb.append(getState());
  sb.append(", startTime=");
  sb.append(getStartTime());
  sb.append(", completionTime=");
  sb.append(getCompletionTimestamp());
  return sb.toString();
}
