{
  return "{" + HConstants.NAME + " => '"+ Bytes.toString(name)+ "', "+ HConstants.VERSIONS+ " => "+ maxVersions+ ", "+ COMPRESSION+ " => '"+ this.compressionType+ "', "+ IN_MEMORY+ " => "+ inMemory+ ", "+ BLOCKCACHE+ " => "+ blockCacheEnabled+ ", "+ LENGTH+ " => "+ maxValueLength+ ", "+ TTL+ " => "+ (timeToLive == HConstants.FOREVER ? "FOREVER" : Integer.toString(timeToLive))+ ", "+ BLOOMFILTER+ " => "+ (bloomFilterSpecified ? bloomFilter.toString() : CompressionType.NONE)+ "}";
}
