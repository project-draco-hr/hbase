{
  return "{name: " + Bytes.toString(name) + ", max versions: "+ maxVersions+ ", compression: "+ this.compressionType+ ", in memory: "+ inMemory+ ", block cache enabled: "+ blockCacheEnabled+ ", max length: "+ maxValueLength+ ", time to live: "+ (timeToLive == HConstants.FOREVER ? "FOREVER" : Integer.toString(timeToLive))+ ", bloom filter: "+ (bloomFilterSpecified ? bloomFilter.toString() : "none")+ "}";
}
