{
  String tmp=name.toString();
  return "{name: " + tmp.substring(0,tmp.length() - 1) + ", max versions: "+ maxVersions+ ", compression: "+ this.compressionType+ ", in memory: "+ inMemory+ ", block cache enabled: "+ blockCacheEnabled+ ", max length: "+ maxValueLength+ ", time to live: "+ (timeToLive == HConstants.FOREVER ? "FOREVER" : Integer.toString(timeToLive))+ ", bloom filter: "+ (bloomFilterSpecified ? bloomFilter.toString() : "none")+ "}";
}
