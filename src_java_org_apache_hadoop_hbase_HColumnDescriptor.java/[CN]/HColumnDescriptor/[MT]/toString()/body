{
  String compression="none";
switch (compressionType) {
case COMPRESSION_NONE:
    break;
case COMPRESSION_RECORD:
  compression="record";
break;
case COMPRESSION_BLOCK:
compression="block";
break;
default :
assert(false);
}
return "(" + name + ", max versions: "+ maxVersions+ ", compression: "+ compression+ ", in memory: "+ inMemory+ ", max value length: "+ maxValueLength+ ", bloom filter: "+ (bloomFilterSpecified ? bloomFilter.toString() : "none")+ ")";
}
