{
  ColumnTracker exp=new ExplicitColumnTracker(trackColumns,0,maxVersions,Long.MAX_VALUE);
  List<ScanQueryMatcher.MatchCode> result=new ArrayList<ScanQueryMatcher.MatchCode>();
  long timestamp=0;
  for (  byte[] col : scannerColumns) {
    result.add(exp.checkColumn(col,0,col.length,++timestamp,KeyValue.Type.Put.getCode()));
  }
  assertEquals(expected.size(),result.size());
  for (int i=0; i < expected.size(); i++) {
    assertEquals(expected.get(i),result.get(i));
    if (PRINT) {
      System.out.println("Expected " + expected.get(i) + ", actual "+ result.get(i));
    }
  }
}
