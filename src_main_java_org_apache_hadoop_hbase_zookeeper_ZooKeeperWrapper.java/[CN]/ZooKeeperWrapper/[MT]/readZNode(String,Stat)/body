{
  byte[] data;
  try {
    String fullyQualifiedZNodeName=getZNode(parentZNode,znodeName);
    data=zooKeeper.getData(fullyQualifiedZNodeName,this,stat);
  }
 catch (  InterruptedException e) {
    throw new IOException(e);
  }
catch (  KeeperException e) {
    throw new IOException(e);
  }
  return data;
}
