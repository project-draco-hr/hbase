{
  try {
    zooKeeper.exists(clusterStateZNode,this);
  }
 catch (  InterruptedException e) {
    LOG.warn("<" + instanceName + ">"+ "Failed to check on ZNode "+ clusterStateZNode,e);
  }
catch (  KeeperException e) {
    LOG.warn("<" + instanceName + ">"+ "Failed to check on ZNode "+ clusterStateZNode,e);
  }
}
