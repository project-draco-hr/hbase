{
  Stat stat=null;
  try {
    stat=zooKeeper.exists(path,false);
  }
 catch (  KeeperException e) {
    LOG.warn("<" + instanceName + ">"+ "checking existence of "+ path,e);
  }
catch (  InterruptedException e) {
    LOG.warn("<" + instanceName + ">"+ "checking existence of "+ path,e);
  }
  return stat != null;
}
