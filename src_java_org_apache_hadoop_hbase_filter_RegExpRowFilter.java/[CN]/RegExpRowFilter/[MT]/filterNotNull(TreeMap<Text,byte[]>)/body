{
  for (  Entry<Text,byte[]> col : columns.entrySet()) {
    if (nullColumns.contains(col.getKey()) && !Arrays.equals(HConstants.DELETE_BYTES.get(),col.getValue())) {
      return true;
    }
  }
  for (  Text col : equalsMap.keySet()) {
    if (!columns.containsKey(col)) {
      return true;
    }
  }
  return false;
}
