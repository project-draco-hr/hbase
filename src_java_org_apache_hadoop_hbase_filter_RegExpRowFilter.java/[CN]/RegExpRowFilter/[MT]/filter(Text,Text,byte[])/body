{
  if (filter(rowKey)) {
    return true;
  }
  if (filtersByColumnValue()) {
    byte[] filterValue=equalsMap.get(colKey);
    if (null != filterValue) {
      return !Arrays.equals(filterValue,data);
    }
  }
  if (nullColumns.contains(colKey)) {
    if (data != null && !Arrays.equals(HConstants.DELETE_BYTES.get(),data)) {
      return true;
    }
  }
  return false;
}
