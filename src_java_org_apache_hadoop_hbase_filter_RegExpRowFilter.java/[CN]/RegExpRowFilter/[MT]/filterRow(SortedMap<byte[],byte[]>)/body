{
  for (  Entry<byte[],byte[]> col : columns.entrySet()) {
    if (nullColumns.contains(col.getKey()) && !HLogEdit.isDeleted(col.getValue())) {
      return true;
    }
  }
  for (  byte[] col : equalsMap.keySet()) {
    if (!columns.containsKey(col)) {
      return true;
    }
  }
  return false;
}
