{
  boolean hasRowKeyPattern=in.readBoolean();
  if (hasRowKeyPattern) {
    rowKeyRegExp=in.readUTF();
  }
  equalsMap.clear();
  int size=in.readInt();
  for (int i=0; i < size; i++) {
    byte[] key=Bytes.readByteArray(in);
    int len=in.readInt();
    byte[] value=null;
    if (len >= 0) {
      value=new byte[len];
      in.readFully(value);
    }
    setColumnFilter(key,value);
  }
  nullColumns.clear();
  size=in.readInt();
  for (int i=0; i < size; i++) {
    setColumnFilter(Bytes.readByteArray(in),null);
  }
}
