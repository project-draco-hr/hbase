{
  if (filterRowKey(rowKey,roffset,rlength)) {
    return true;
  }
  byte[] colkey=null;
  if (filtersByColumnValue()) {
    colkey=getColKey(colunmName,coffset,clength);
    byte[] filterValue=equalsMap.get(colkey);
    if (null != filterValue) {
      return Bytes.compareTo(filterValue,0,filterValue.length,columnValue,voffset,vlength) != 0;
    }
  }
  if (colkey == null) {
    colkey=getColKey(colunmName,coffset,clength);
  }
  if (nullColumns.contains(colkey)) {
    if (columnValue != null) {
      return true;
    }
  }
  return false;
}
