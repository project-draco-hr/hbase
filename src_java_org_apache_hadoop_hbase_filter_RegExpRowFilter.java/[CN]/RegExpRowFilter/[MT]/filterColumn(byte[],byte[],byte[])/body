{
  if (filterRowKey(rowKey)) {
    return true;
  }
  if (filtersByColumnValue()) {
    byte[] filterValue=equalsMap.get(colKey);
    if (null != filterValue) {
      return !Arrays.equals(filterValue,data);
    }
  }
  if (nullColumns.contains(colKey)) {
    if (data != null && !HLogEdit.isDeleted(data)) {
      return true;
    }
  }
  return false;
}
