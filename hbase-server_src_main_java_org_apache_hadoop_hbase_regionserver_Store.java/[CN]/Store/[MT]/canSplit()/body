{
  this.lock.readLock().lock();
  try {
    for (    StoreFile sf : storefiles) {
      if (sf.isReference()) {
        if (LOG.isDebugEnabled()) {
          LOG.debug(sf + " is not splittable");
        }
        return false;
      }
    }
    return true;
  }
  finally {
    this.lock.readLock().unlock();
  }
}
