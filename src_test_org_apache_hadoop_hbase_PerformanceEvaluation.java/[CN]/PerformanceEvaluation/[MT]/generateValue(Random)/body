{
  StringBuilder val=new StringBuilder();
  while (val.length() < ROW_LENGTH) {
    val.append(Long.toString(r.nextLong()));
  }
  byte[] value=null;
  try {
    value=val.toString().getBytes(HConstants.UTF8_ENCODING);
  }
 catch (  UnsupportedEncodingException e) {
    assert(false);
  }
  return value;
}
