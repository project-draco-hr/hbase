{
  final String tableName=getName();
  final String columnFamily=tableName;
  createTable("create table " + tableName + " ("+ columnFamily+ ");",tableName,columnFamily);
  runCommand("insert into " + tableName + " ("+ columnFamily+ ") values ('"+ columnFamily+ "') where row='"+ columnFamily+ "';");
  runCommand("insert into " + tableName + " ("+ columnFamily+ ") values ('"+ columnFamily+ "') where row=\""+ columnFamily+ "\";");
  runCommand("insert into " + tableName + " ("+ columnFamily+ ") values (\""+ columnFamily+ "\") where row=\""+ columnFamily+ "\";");
  runCommand("select \"" + columnFamily + "\" from \""+ tableName+ "\" where row=\""+ columnFamily+ "\";");
  runCommand("delete \"" + columnFamily + ":\" from \""+ tableName+ "\" where row=\""+ columnFamily+ "\";");
}
