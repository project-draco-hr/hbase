{
  final String path="/" + TABLE + "/"+ ROW_4+ "/"+ COLUMN_1;
  Response response=client.put(path,Constants.MIMETYPE_BINARY,Bytes.toBytes(VALUE_4));
  assertEquals(response.getCode(),200);
  Thread.yield();
  response=client.get(path,Constants.MIMETYPE_JSON);
  assertEquals(response.getCode(),200);
  response=deleteRow(TABLE,ROW_4);
  assertEquals(response.getCode(),200);
  METRICS_ASSERT.assertCounterGt("requests",2l,RESTServlet.getInstance(conf).getMetrics().getSource());
  METRICS_ASSERT.assertCounterGt("successfulGet",0l,RESTServlet.getInstance(conf).getMetrics().getSource());
  METRICS_ASSERT.assertCounterGt("successfulPut",0l,RESTServlet.getInstance(conf).getMetrics().getSource());
  METRICS_ASSERT.assertCounterGt("successfulDelete",0l,RESTServlet.getInstance(conf).getMetrics().getSource());
}
