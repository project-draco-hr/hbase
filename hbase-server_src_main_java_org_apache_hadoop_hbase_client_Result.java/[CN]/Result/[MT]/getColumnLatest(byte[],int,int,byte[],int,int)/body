{
  KeyValue[] kvs=raw();
  if (kvs == null || kvs.length == 0) {
    return null;
  }
  int pos=binarySearch(kvs,family,foffset,flength,qualifier,qoffset,qlength);
  if (pos == -1) {
    return null;
  }
  KeyValue kv=kvs[pos];
  if (kv.matchingColumn(family,foffset,flength,qualifier,qoffset,qlength)) {
    return kv;
  }
  return null;
}
