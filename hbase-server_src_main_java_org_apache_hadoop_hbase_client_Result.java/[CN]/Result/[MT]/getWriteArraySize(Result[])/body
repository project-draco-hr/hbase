{
  long size=Bytes.SIZEOF_BYTE;
  if (results == null || results.length == 0) {
    size+=Bytes.SIZEOF_INT;
    return size;
  }
  size+=Bytes.SIZEOF_INT;
  size+=Bytes.SIZEOF_INT;
  for (  Result result : results) {
    size+=Bytes.SIZEOF_INT;
    if (result == null || result.isEmpty())     continue;
    for (    KeyValue kv : result.raw()) {
      size+=Bytes.SIZEOF_INT;
      size+=kv.getLength();
    }
  }
  return size;
}
