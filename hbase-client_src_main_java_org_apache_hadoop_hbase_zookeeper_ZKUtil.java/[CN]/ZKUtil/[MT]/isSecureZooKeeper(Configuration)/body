{
  try {
    javax.security.auth.login.Configuration testConfig=javax.security.auth.login.Configuration.getConfiguration();
    if (testConfig.getAppConfigurationEntry("Client") == null) {
      return false;
    }
  }
 catch (  Exception e) {
    return false;
  }
  return ("kerberos".equalsIgnoreCase(conf.get("hbase.security.authentication")) && conf.get("hbase.zookeeper.client.keytab.file") != null);
}
