{
  if (!LOG.isDebugEnabled())   return;
  LOG.debug(zkw.prefix("Retrieved " + ((data == null) ? 0 : data.length) + " byte(s) of data from znode "+ znode+ (watcherSet ? " and set watcher; " : "; data=")+ (data == null ? "null" : data.length == 0 ? "empty" : (znode.startsWith(zkw.assignmentZNode) ? ZKAssign.toString(data) : znode.startsWith(zkw.metaServerZNode) ? getServerNameOrEmptyString(data) : znode.startsWith(zkw.backupMasterAddressesZNode) ? getServerNameOrEmptyString(data) : StringUtils.abbreviate(Bytes.toStringBinary(data),32)))));
}
