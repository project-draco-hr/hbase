{
  assertFalse("Filtering on " + Bytes.toString(GOOD_ROW),filter.filterRowKey(GOOD_ROW,0,GOOD_ROW.length));
  assertTrue("Filtering on " + Bytes.toString(STOP_ROW),filter.filterRowKey(STOP_ROW,0,STOP_ROW.length));
  assertTrue("Filtering on " + Bytes.toString(PAST_STOP_ROW),filter.filterRowKey(PAST_STOP_ROW,0,PAST_STOP_ROW.length));
  assertFalse("Filtering on " + Bytes.toString(GOOD_ROW),filter.filterColumn(GOOD_ROW,0,GOOD_ROW.length,null,0,0,null,0,0));
  assertTrue("Filtering on " + Bytes.toString(STOP_ROW),filter.filterColumn(STOP_ROW,0,STOP_ROW.length,null,0,0,null,0,0));
  assertTrue("Filtering on " + Bytes.toString(PAST_STOP_ROW),filter.filterColumn(PAST_STOP_ROW,0,PAST_STOP_ROW.length,null,0,0,null,0,0));
  assertFalse("FilterAllRemaining",filter.filterAllRemaining());
  assertFalse("FilterNotNull",filter.filterRow((List<KeyValue>)null));
  assertFalse("Filter a null",filter.filterRowKey(null,0,0));
}
