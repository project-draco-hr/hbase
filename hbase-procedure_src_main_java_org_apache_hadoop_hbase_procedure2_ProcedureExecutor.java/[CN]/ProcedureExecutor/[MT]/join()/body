{
  boolean interrupted=false;
  for (int i=0; i < threads.length; ++i) {
    try {
      threads[i].join();
    }
 catch (    InterruptedException ex) {
      interrupted=true;
    }
  }
  if (interrupted) {
    Thread.currentThread().interrupt();
  }
  completed.clear();
  rollbackStack.clear();
  procedures.clear();
  nonceKeysToProcIdsMap.clear();
  waitingTimeout.clear();
  runnables.clear();
  lastProcId.set(-1);
}
