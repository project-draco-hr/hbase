{
  if (user == null) {
    return false;
  }
  Procedure proc=procedures.get(procId);
  if (proc != null) {
    return proc.getOwner().equals(user.getShortName());
  }
  ProcedureInfo procInfo=completed.get(procId);
  if (procInfo == null) {
    return false;
  }
  return ProcedureInfo.isProcedureOwner(procInfo,user);
}
