{
  while (isRunning()) {
    Long procId=runnables.poll();
    Procedure proc=procId != null ? procedures.get(procId) : null;
    if (proc == null)     continue;
    try {
      activeExecutorCount.incrementAndGet();
      execLoop(proc);
    }
  finally {
      activeExecutorCount.decrementAndGet();
    }
  }
}
