{
  ThreadPoolExecutor openAndCloseThreadPool=Threads.getBoundedCachedThreadPool(maxThreads,30L,TimeUnit.SECONDS,new ThreadFactory(){
    private int count=1;
    public Thread newThread(    Runnable r){
      Thread t=new Thread(r,threadNamePrefix + "-" + count++);
      return t;
    }
  }
);
  return openAndCloseThreadPool;
}
