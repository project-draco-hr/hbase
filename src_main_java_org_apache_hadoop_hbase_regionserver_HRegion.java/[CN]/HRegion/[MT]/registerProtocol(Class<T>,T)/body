{
  if (protocolHandlers.containsKey(protocol)) {
    LOG.error("Protocol " + protocol.getName() + " already registered, rejecting request from "+ handler);
    return false;
  }
  protocolHandlers.putInstance(protocol,handler);
  if (LOG.isDebugEnabled()) {
    LOG.debug("Registered protocol handler: region=" + Bytes.toStringBinary(getRegionName()) + " protocol="+ protocol.getName());
  }
  return true;
}
