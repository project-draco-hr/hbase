{
  for (  Store s : getStores().values()) {
    CompactionRequest cr=s.requestCompaction();
    if (cr != null) {
      try {
        compact(cr);
      }
  finally {
        s.finishRequest(cr);
      }
    }
    byte[] splitRow=s.checkSplit();
    if (splitRow != null) {
      return splitRow;
    }
  }
  return null;
}
