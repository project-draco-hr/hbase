{
  LOG.debug("Assigning tabledesc from .tableinfo for region = " + this.regionInfo.getRegionNameAsString());
  this.htableDescriptor=FSUtils.getTableDescriptor(tableDir,fs);
  if (this.htableDescriptor != null) {
    setHTableSpecificConf();
  }
 else {
    throw new IOException("Table description missing in " + ".tableinfo. Cannot create new region." + " current region is == "+ this.regionInfo.toString());
  }
}
