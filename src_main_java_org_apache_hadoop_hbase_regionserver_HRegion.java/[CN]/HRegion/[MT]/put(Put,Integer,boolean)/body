{
  checkReadOnly();
  checkResources();
  startRegionOperation();
  this.writeRequestsCount.increment();
  try {
    byte[] row=put.getRow();
    Integer lid=getLock(lockid,row,true);
    try {
      internalPut(put,put.getClusterId(),writeToWAL);
    }
  finally {
      if (lockid == null)       releaseRowLock(lid);
    }
  }
  finally {
    closeRegionOperation();
  }
}
