{
  HashedBytes rowKey=lockIds.remove(lockId);
  if (rowKey == null) {
    LOG.warn("Release unknown lockId: " + lockId);
    return;
  }
  CountDownLatch rowLatch=lockedRows.remove(rowKey);
  if (rowLatch == null) {
    LOG.error("Releases row not locked, lockId: " + lockId + " row: "+ rowKey);
    return;
  }
  rowLatch.countDown();
}
