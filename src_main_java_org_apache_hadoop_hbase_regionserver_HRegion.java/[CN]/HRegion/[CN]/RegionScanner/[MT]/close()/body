{
  if (coprocessorHost != null) {
    if (coprocessorHost.preScannerClose((InternalScanner)this)) {
      return;
    }
  }
  if (storeHeap != null) {
    storeHeap.close();
    storeHeap=null;
  }
  this.filterClosed=true;
  if (coprocessorHost != null) {
    coprocessorHost.postScannerClose((InternalScanner)this);
  }
}
