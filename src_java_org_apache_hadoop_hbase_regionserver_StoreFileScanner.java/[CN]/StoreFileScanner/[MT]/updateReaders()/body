{
  this.lock.writeLock().lock();
  try {
    ViableRow viableRow=getNextViableRow();
    openReaders(viableRow.getRow());
    LOG.debug("Replaced Scanner Readers at row " + (viableRow == null || viableRow.getRow() == null ? "null" : Bytes.toString(viableRow.getRow())));
  }
  finally {
    this.lock.writeLock().unlock();
  }
}
