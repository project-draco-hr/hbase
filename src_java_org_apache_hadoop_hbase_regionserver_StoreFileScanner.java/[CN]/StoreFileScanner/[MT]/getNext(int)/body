{
  boolean result=false;
  while (true) {
    if ((this.scanners[i].isSeeked() && !this.scanners[i].next()) || (!this.scanners[i].isSeeked() && !this.scanners[i].seekTo())) {
      closeSubScanner(i);
      break;
    }
    this.keys[i]=this.scanners[i].getKeyValue();
    if (isGoodKey(this.keys[i])) {
      result=true;
      break;
    }
  }
  return result;
}
