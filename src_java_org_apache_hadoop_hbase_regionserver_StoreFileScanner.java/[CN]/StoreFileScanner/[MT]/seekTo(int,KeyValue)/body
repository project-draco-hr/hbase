{
  if (firstKey == null) {
    if (!this.scanners[i].seekTo()) {
      closeSubScanner(i);
      return true;
    }
  }
 else {
    if (!Store.getClosest(this.scanners[i],firstKey)) {
      closeSubScanner(i);
      return true;
    }
  }
  this.keys[i]=this.scanners[i].getKeyValue();
  return isGoodKey(this.keys[i]);
}
