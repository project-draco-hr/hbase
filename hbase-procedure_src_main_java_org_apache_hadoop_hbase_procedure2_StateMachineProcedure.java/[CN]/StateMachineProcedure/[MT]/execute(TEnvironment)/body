{
  updateTimestamp();
  try {
    TState state=getCurrentState();
    if (stateCount == 0) {
      setNextState(getStateId(state));
    }
    if (executeFromState(env,state) == Flow.NO_MORE_STATE) {
      return null;
    }
    return (isWaiting() || isFailed()) ? null : new Procedure[]{this};
  }
  finally {
    updateTimestamp();
  }
}
