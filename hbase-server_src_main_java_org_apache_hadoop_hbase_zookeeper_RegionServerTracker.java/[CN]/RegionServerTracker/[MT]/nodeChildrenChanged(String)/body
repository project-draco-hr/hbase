{
  if (path.equals(watcher.rsZNode) && !server.isAborted() && !server.isStopped()) {
    try {
      List<String> servers=ZKUtil.listChildrenAndWatchThem(watcher,watcher.rsZNode);
      add(servers);
    }
 catch (    IOException e) {
      server.abort("Unexpected zk exception getting RS nodes",e);
    }
catch (    KeeperException e) {
      server.abort("Unexpected zk exception getting RS nodes",e);
    }
  }
}
