{
  HTable table=new HTable(conf,new Text(TABLE_NAME));
  Scanner scanner=table.getScanner(columns,HConstants.EMPTY_START_ROW);
  try {
    for (    RowResult r : scanner) {
      if (printResults) {
        LOG.info("row: " + r.getRow());
      }
      for (      Map.Entry<Text,Cell> e : r.entrySet()) {
        if (printResults) {
          LOG.info(" column: " + e.getKey() + " value: "+ new String(e.getValue().getValue(),HConstants.UTF8_ENCODING));
        }
      }
    }
  }
  finally {
    scanner.close();
  }
}
