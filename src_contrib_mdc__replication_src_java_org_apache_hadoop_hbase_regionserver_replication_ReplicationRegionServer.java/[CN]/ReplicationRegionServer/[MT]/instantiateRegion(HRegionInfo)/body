{
  HRegion r=new ReplicationRegion(HTableDescriptor.getTableDir(super.getRootDir(),regionInfo.getTableDesc().getName()),super.hlog,super.getFileSystem(),super.conf,regionInfo,super.getFlushRequester(),this.replicationSource);
  r.initialize(null,new Progressable(){
    public void progress(){
      addProcessingMessage(regionInfo);
    }
  }
);
  return r;
}
