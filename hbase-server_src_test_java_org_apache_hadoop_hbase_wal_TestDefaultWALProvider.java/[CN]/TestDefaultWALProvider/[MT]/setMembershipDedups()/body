{
  final Configuration localConf=new Configuration(conf);
  localConf.set(WALFactory.WAL_PROVIDER,DefaultWALProvider.class.getName());
  final WALFactory wals=new WALFactory(localConf,null,currentTest.getMethodName());
  try {
    final Set<WAL> seen=new HashSet<WAL>(1);
    final Random random=new Random();
    assertTrue("first attempt to add WAL from default provider should work.",seen.add(wals.getWAL(Bytes.toBytes(random.nextInt()))));
    for (int i=0; i < 1000; i++) {
      assertFalse("default wal provider is only supposed to return a single wal, which should " + "compare as .equals itself.",seen.add(wals.getWAL(Bytes.toBytes(random.nextInt()))));
    }
  }
  finally {
    wals.close();
  }
}
