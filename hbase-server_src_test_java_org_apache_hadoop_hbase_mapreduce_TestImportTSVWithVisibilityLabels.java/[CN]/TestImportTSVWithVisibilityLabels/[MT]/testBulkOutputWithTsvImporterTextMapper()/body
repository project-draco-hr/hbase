{
  String table="test-" + UUID.randomUUID();
  String FAMILY="FAM";
  Path bulkOutputPath=new Path(util.getDataTestDirOnTestFS(table),"hfiles");
  String[] args=new String[]{"-D" + ImportTsv.MAPPER_CONF_KEY + "=org.apache.hadoop.hbase.mapreduce.TsvImporterTextMapper","-D" + ImportTsv.COLUMNS_CONF_KEY + "=HBASE_ROW_KEY,FAM:A,FAM:B,HBASE_CELL_VISIBILITY","-D" + ImportTsv.SEPARATOR_CONF_KEY + "=\u001b","-D" + ImportTsv.BULK_OUTPUT_CONF_KEY + "="+ bulkOutputPath.toString(),table};
  String data="KEY\u001bVALUE4\u001bVALUE8\u001bsecret&private\n";
  doMROnTableTest(util,FAMILY,data,args,4);
  util.deleteTable(table);
}
