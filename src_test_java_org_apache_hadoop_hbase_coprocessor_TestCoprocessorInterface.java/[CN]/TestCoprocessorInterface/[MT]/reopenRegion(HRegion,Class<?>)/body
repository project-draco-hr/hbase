{
  HRegion r=new HRegion(closedRegion.getRegionDir(),closedRegion.getLog(),closedRegion.getFilesystem(),closedRegion.getConf(),closedRegion.getRegionInfo(),null);
  r.initialize();
  CoprocessorHost host=new CoprocessorHost(r,null,conf);
  r.setCoprocessorHost(host);
  host.load(implClass,Priority.USER);
  host.preOpen();
  host.postOpen();
  return r;
}
