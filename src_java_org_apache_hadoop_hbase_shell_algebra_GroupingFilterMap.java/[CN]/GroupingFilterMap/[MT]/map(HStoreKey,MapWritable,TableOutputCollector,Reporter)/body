{
  byte[][] keyVals=extractKeyValues(value);
  if (keyVals != null) {
    Text tKey=createGroupKey(keyVals);
    try {
      if (expressionParser.checkConstraints(value)) {
        output.collect(tKey,value);
      }
    }
 catch (    ParseException e) {
      throw new RuntimeException(e);
    }
  }
}
