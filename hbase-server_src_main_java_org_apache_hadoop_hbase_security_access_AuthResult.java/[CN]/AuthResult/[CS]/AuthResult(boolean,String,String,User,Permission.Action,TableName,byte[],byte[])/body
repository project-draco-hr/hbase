{
  this.allowed=allowed;
  this.request=request;
  this.reason=reason;
  this.user=user;
  this.table=table;
  this.family=family;
  this.qualifier=qualifier;
  this.action=action;
  this.families=null;
  this.namespace=null;
  this.params=new Params().setTableName(table).setFamily(family).setQualifier(qualifier);
}
