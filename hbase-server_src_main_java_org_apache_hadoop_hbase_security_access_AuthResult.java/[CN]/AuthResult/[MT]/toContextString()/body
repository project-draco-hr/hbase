{
  StringBuilder sb=new StringBuilder();
  sb.append("(user=").append(user != null ? user.getName() : "UNKNOWN").append(", ");
  sb.append("scope=").append(namespace != null ? namespace : table == null ? "GLOBAL" : table);
  if (namespace == null) {
    sb.append(", ").append("family=").append(toFamilyString()).append(", ");
  }
  sb.append("action=").append(action != null ? action.toString() : "").append(")");
  return sb.toString();
}
