{
  return admin.executeCallable(new MasterCallable<AbortProcedureResponse>(admin.getConnection()){
    @Override public AbortProcedureResponse call(    int callTimeout) throws ServiceException {
      PayloadCarryingRpcController controller=admin.getRpcControllerFactory().newController();
      controller.setCallTimeout(callTimeout);
      return master.abortProcedure(controller,request);
    }
  }
);
}
