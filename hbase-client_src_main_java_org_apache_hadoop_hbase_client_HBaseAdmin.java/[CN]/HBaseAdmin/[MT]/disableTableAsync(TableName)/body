{
  TableName.isLegalFullyQualifiedTableName(tableName.getName());
  DisableTableResponse response=executeCallable(new MasterCallable<DisableTableResponse>(getConnection()){
    @Override public DisableTableResponse call(    int callTimeout) throws ServiceException {
      PayloadCarryingRpcController controller=rpcControllerFactory.newController();
      controller.setCallTimeout(callTimeout);
      controller.setPriority(tableName);
      LOG.info("Started disable of " + tableName);
      DisableTableRequest req=RequestConverter.buildDisableTableRequest(tableName,ng.getNonceGroup(),ng.newNonce());
      return master.disableTable(controller,req);
    }
  }
);
  return new DisableTableFuture(this,tableName,response);
}
