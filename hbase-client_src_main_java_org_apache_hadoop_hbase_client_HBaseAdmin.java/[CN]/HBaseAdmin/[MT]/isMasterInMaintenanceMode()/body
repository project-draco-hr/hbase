{
  return executeCallable(new MasterCallable<IsInMaintenanceModeResponse>(getConnection()){
    @Override public IsInMaintenanceModeResponse call(    int callTimeout) throws ServiceException {
      PayloadCarryingRpcController controller=rpcControllerFactory.newController();
      controller.setCallTimeout(callTimeout);
      return master.isMasterInMaintenanceMode(controller,IsInMaintenanceModeRequest.newBuilder().build());
    }
  }
).getInMaintenanceMode();
}
