{
  DeleteColumnResponse response=executeCallable(new MasterCallable<DeleteColumnResponse>(getConnection()){
    @Override public DeleteColumnResponse call(    int callTimeout) throws ServiceException {
      PayloadCarryingRpcController controller=rpcControllerFactory.newController();
      controller.setCallTimeout(callTimeout);
      controller.setPriority(tableName);
      DeleteColumnRequest req=RequestConverter.buildDeleteColumnRequest(tableName,columnFamily,ng.getNonceGroup(),ng.newNonce());
      master.deleteColumn(controller,req);
      return null;
    }
  }
);
  return new DeleteColumnFamilyFuture(this,tableName,response);
}
