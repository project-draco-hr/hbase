{
  return executeCallable(new MasterCallable<TableName[]>(getConnection()){
    @Override public TableName[] call(    int callTimeout) throws ServiceException {
      PayloadCarryingRpcController controller=rpcControllerFactory.newController();
      controller.setCallTimeout(callTimeout);
      GetTableNamesRequest req=RequestConverter.buildGetTableNamesRequest(pattern,includeSysTables);
      return ProtobufUtil.getTableNameArray(master.getTableNames(controller,req).getTableNamesList());
    }
  }
);
}
