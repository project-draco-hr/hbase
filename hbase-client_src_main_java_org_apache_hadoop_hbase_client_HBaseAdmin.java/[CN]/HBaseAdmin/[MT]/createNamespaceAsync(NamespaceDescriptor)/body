{
  CreateNamespaceResponse response=executeCallable(new MasterCallable<CreateNamespaceResponse>(getConnection()){
    @Override public CreateNamespaceResponse call(    int callTimeout) throws Exception {
      return master.createNamespace(null,CreateNamespaceRequest.newBuilder().setNamespaceDescriptor(ProtobufUtil.toProtoNamespaceDescriptor(descriptor)).build());
    }
  }
);
  return new NamespaceFuture(this,descriptor.getName(),response.getProcId()){
    @Override public String getOperationType(){
      return "CREATE_NAMESPACE";
    }
  }
;
}
