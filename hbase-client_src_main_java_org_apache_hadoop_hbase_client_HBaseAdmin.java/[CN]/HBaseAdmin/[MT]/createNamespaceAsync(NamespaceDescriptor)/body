{
  CreateNamespaceResponse response=executeCallable(new MasterCallable<CreateNamespaceResponse>(getConnection(),getRpcControllerFactory()){
    @Override protected CreateNamespaceResponse call(    PayloadCarryingRpcController rpcController) throws Exception {
      return master.createNamespace(rpcController,CreateNamespaceRequest.newBuilder().setNamespaceDescriptor(ProtobufUtil.toProtoNamespaceDescriptor(descriptor)).build());
    }
  }
);
  return new NamespaceFuture(this,descriptor.getName(),response.getProcId()){
    @Override public String getOperationType(){
      return "CREATE_NAMESPACE";
    }
  }
;
}
