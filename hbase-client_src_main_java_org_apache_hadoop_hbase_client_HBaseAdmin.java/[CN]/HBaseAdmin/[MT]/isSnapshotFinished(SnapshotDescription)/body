{
  final HBaseProtos.SnapshotDescription snapshot=createHBaseProtosSnapshotDesc(snapshotDesc);
  return executeCallable(new MasterCallable<IsSnapshotDoneResponse>(getConnection(),getRpcControllerFactory()){
    @Override protected IsSnapshotDoneResponse call(    PayloadCarryingRpcController rpcController) throws Exception {
      return master.isSnapshotDone(rpcController,IsSnapshotDoneRequest.newBuilder().setSnapshot(snapshot).build());
    }
  }
).getDone();
}
