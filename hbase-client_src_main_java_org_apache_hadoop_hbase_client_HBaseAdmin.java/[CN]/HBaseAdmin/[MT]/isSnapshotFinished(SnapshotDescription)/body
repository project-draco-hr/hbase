{
  return executeCallable(new MasterCallable<IsSnapshotDoneResponse>(getConnection()){
    @Override public IsSnapshotDoneResponse call(    int callTimeout) throws ServiceException {
      PayloadCarryingRpcController controller=rpcControllerFactory.newController();
      controller.setCallTimeout(callTimeout);
      return master.isSnapshotDone(controller,IsSnapshotDoneRequest.newBuilder().setSnapshot(snapshot).build());
    }
  }
).getDone();
}
