{
  executeCallable(new MasterAdminCallable<Void>(getConnection()){
    @Override public Void call() throws Exception {
      masterAdmin.modifyNamespace(null,MasterAdminProtos.ModifyNamespaceRequest.newBuilder().setNamespaceDescriptor(ProtobufUtil.toProtoNamespaceDescriptor(descriptor)).build());
