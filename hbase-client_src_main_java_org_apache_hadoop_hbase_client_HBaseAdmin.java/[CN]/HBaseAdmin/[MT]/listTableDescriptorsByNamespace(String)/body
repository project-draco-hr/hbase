{
  return executeCallable(new MasterCallable<HTableDescriptor[]>(getConnection()){
    @Override public HTableDescriptor[] call(    int callTimeout) throws Exception {
      List<TableSchema> list=master.listTableDescriptorsByNamespace(null,ListTableDescriptorsByNamespaceRequest.newBuilder().setNamespaceName(name).build()).getTableSchemaList();
      HTableDescriptor[] res=new HTableDescriptor[list.size()];
      for (int i=0; i < list.size(); i++) {
        res[i]=HTableDescriptor.convert(list.get(i));
      }
      return res;
    }
  }
);
}
