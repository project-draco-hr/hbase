{
  return executeCallable(new MasterCallable<HTableDescriptor[]>(getConnection(),getRpcControllerFactory()){
    @Override protected HTableDescriptor[] call(    PayloadCarryingRpcController rpcController) throws Exception {
      List<TableSchema> list=master.listTableDescriptorsByNamespace(rpcController,ListTableDescriptorsByNamespaceRequest.newBuilder().setNamespaceName(name).build()).getTableSchemaList();
      HTableDescriptor[] res=new HTableDescriptor[list.size()];
      for (int i=0; i < list.size(); i++) {
        res[i]=ProtobufUtil.convertToHTableDesc(list.get(i));
      }
      return res;
    }
  }
);
}
