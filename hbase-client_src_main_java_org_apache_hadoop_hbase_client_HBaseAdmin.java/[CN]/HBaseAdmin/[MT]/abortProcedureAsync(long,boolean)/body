{
  Boolean abortProcResponse=executeCallable(new MasterCallable<AbortProcedureResponse>(getConnection()){
    @Override public AbortProcedureResponse call(    int callTimeout) throws ServiceException {
      PayloadCarryingRpcController controller=rpcControllerFactory.newController();
      controller.setCallTimeout(callTimeout);
      AbortProcedureRequest abortProcRequest=AbortProcedureRequest.newBuilder().setProcId(procId).build();
      return master.abortProcedure(controller,abortProcRequest);
    }
  }
).getIsProcedureAborted();
  AbortProcedureFuture abortProcFuture=new AbortProcedureFuture(this,procId,abortProcResponse);
  return abortProcFuture;
}
