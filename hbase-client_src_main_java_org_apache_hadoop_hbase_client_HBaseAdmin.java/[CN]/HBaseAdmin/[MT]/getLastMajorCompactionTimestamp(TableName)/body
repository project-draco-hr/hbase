{
  return executeCallable(new MasterCallable<Long>(getConnection()){
    @Override public Long call(    int callTimeout) throws ServiceException {
      PayloadCarryingRpcController controller=rpcControllerFactory.newController();
      controller.setCallTimeout(callTimeout);
      MajorCompactionTimestampRequest req=MajorCompactionTimestampRequest.newBuilder().setTableName(ProtobufUtil.toProtoTableName(tableName)).build();
      return master.getLastMajorCompactionTimestamp(controller,req).getCompactionTimestamp();
    }
  }
);
}
