{
  List<SnapshotDescription> snapshots=listSnapshots(pattern);
  for (  final SnapshotDescription snapshot : snapshots) {
    executeCallable(new MasterCallable<Void>(getConnection()){
      @Override public Void call() throws ServiceException {
        this.master.deleteSnapshot(null,DeleteSnapshotRequest.newBuilder().setSnapshot(snapshot).build());
        return null;
      }
    }
);
  }
}
