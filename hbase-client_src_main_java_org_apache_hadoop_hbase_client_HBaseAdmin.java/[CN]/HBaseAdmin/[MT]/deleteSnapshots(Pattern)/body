{
  List<SnapshotDescription> snapshots=listSnapshots(pattern);
  for (  final SnapshotDescription snapshot : snapshots) {
    execute(new MasterAdminCallable<Void>(){
      @Override public Void call() throws ServiceException {
        masterAdmin.deleteSnapshot(null,DeleteSnapshotRequest.newBuilder().setSnapshot(snapshot).build());
        return null;
      }
    }
);
  }
}
