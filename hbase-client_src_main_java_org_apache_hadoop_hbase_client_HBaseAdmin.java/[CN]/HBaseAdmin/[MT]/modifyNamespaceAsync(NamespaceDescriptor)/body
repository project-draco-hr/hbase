{
  ModifyNamespaceResponse response=executeCallable(new MasterCallable<ModifyNamespaceResponse>(getConnection(),getRpcControllerFactory()){
    @Override protected ModifyNamespaceResponse call(    PayloadCarryingRpcController rpcController) throws Exception {
      return master.modifyNamespace(rpcController,ModifyNamespaceRequest.newBuilder().setNamespaceDescriptor(ProtobufUtil.toProtoNamespaceDescriptor(descriptor)).build());
    }
  }
);
  return new NamespaceFuture(this,descriptor.getName(),response.getProcId()){
    @Override public String getOperationType(){
      return "MODIFY_NAMESPACE";
    }
  }
;
}
