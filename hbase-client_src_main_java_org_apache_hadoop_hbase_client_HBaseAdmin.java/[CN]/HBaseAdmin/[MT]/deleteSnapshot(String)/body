{
  HTableDescriptor.isLegalTableName(Bytes.toBytes(snapshotName));
  executeCallable(new MasterAdminCallable<Void>(getConnection()){
    @Override public Void call() throws ServiceException {
      masterAdmin.deleteSnapshot(null,DeleteSnapshotRequest.newBuilder().setSnapshot(SnapshotDescription.newBuilder().setName(snapshotName).build()).build());
      return null;
    }
  }
);
}
