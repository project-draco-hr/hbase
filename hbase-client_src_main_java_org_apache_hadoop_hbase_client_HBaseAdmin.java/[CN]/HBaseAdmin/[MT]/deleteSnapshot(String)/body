{
  TableName.isLegalFullyQualifiedTableName(Bytes.toBytes(snapshotName));
  executeCallable(new MasterCallable<Void>(getConnection(),getRpcControllerFactory()){
    @Override protected Void call(    PayloadCarryingRpcController rpcController) throws Exception {
      master.deleteSnapshot(rpcController,DeleteSnapshotRequest.newBuilder().setSnapshot(HBaseProtos.SnapshotDescription.newBuilder().setName(snapshotName).build()).build());
      return null;
    }
  }
);
}
