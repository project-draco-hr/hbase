{
  return executeCallable(new MasterCallable<Boolean>(getConnection()){
    @Override public Boolean call(    int callTimeout) throws ServiceException {
      PayloadCarryingRpcController controller=rpcControllerFactory.newController();
      controller.setCallTimeout(callTimeout);
      SetBalancerRunningRequest req=RequestConverter.buildSetBalancerRunningRequest(on,synchronous);
      return master.setBalancerRunning(controller,req).getPrevBalanceValue();
    }
  }
);
}
