{
  return executeCallable(new MasterCallable<boolean[]>(getConnection(),getRpcControllerFactory()){
    @Override protected boolean[] call(    PayloadCarryingRpcController rpcController) throws Exception {
      MasterProtos.SetSplitOrMergeEnabledResponse response=master.setSplitOrMergeEnabled(null,RequestConverter.buildSetSplitOrMergeEnabledRequest(enabled,synchronous,switchTypes));
      boolean[] result=new boolean[switchTypes.length];
      int i=0;
      for (      Boolean prevValue : response.getPrevValueList()) {
        result[i++]=prevValue;
      }
      return result;
    }
  }
);
}
