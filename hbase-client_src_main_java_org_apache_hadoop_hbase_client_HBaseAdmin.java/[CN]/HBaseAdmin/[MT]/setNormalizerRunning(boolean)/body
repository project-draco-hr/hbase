{
  return executeCallable(new MasterCallable<Boolean>(getConnection()){
    @Override public Boolean call(    int callTimeout) throws ServiceException {
      SetNormalizerRunningRequest req=RequestConverter.buildSetNormalizerRunningRequest(on);
      return master.setNormalizerRunning(null,req).getPrevNormalizerValue();
    }
  }
);
}
