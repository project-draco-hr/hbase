{
  executeCallable(new MasterCallable<Void>(getConnection(),getRpcControllerFactory()){
    @Override protected Void call(    PayloadCarryingRpcController rpcController) throws Exception {
      rpcController.setPriority(regionName);
      AssignRegionRequest request=RequestConverter.buildAssignRegionRequest(getRegionName(regionName));
      master.assignRegion(rpcController,request);
      return null;
    }
  }
);
}
