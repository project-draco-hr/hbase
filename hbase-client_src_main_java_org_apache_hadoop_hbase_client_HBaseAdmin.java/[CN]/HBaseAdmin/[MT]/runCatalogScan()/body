{
  return executeCallable(new MasterCallable<Integer>(getConnection()){
    @Override public Integer call(    int callTimeout) throws ServiceException {
      PayloadCarryingRpcController controller=rpcControllerFactory.newController();
      controller.setCallTimeout(callTimeout);
      return master.runCatalogScan(controller,RequestConverter.buildCatalogScanRequest()).getScanResult();
    }
  }
);
}
