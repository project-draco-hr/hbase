{
  return executeCallable(new MasterCallable<NamespaceDescriptor[]>(getConnection()){
    @Override public NamespaceDescriptor[] call(    int callTimeout) throws Exception {
      PayloadCarryingRpcController controller=rpcControllerFactory.newController();
      controller.setCallTimeout(callTimeout);
      List<HBaseProtos.NamespaceDescriptor> list=master.listNamespaceDescriptors(controller,ListNamespaceDescriptorsRequest.newBuilder().build()).getNamespaceDescriptorList();
      NamespaceDescriptor[] res=new NamespaceDescriptor[list.size()];
      for (int i=0; i < list.size(); i++) {
        res[i]=ProtobufUtil.toNamespaceDescriptor(list.get(i));
      }
      return res;
    }
  }
);
}
