{
  return executeCallable(new MasterAdminCallable<NamespaceDescriptor[]>(getConnection()){
    @Override public NamespaceDescriptor[] call() throws Exception {
      List<HBaseProtos.NamespaceDescriptor> list=masterAdmin.listNamespaceDescriptors(null,MasterAdminProtos.ListNamespaceDescriptorsRequest.newBuilder().build()).getNamespaceDescriptorList();
      NamespaceDescriptor[] res=new NamespaceDescriptor[list.size()];
      for (int i=0; i < list.size(); i++) {
        res[i]=ProtobufUtil.toNamespaceDescriptor(list.get(i));
      }
      return res;
    }
  }
);
}
