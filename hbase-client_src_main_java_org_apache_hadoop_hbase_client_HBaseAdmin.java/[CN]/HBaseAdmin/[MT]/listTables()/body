{
  return executeCallable(new MasterCallable<HTableDescriptor[]>(getConnection()){
    @Override public HTableDescriptor[] call(    int callTimeout) throws ServiceException {
      GetTableDescriptorsRequest req=RequestConverter.buildGetTableDescriptorsRequest((List<TableName>)null);
      return ProtobufUtil.getHTableDescriptorArray(master.getTableDescriptors(null,req));
    }
  }
);
}
