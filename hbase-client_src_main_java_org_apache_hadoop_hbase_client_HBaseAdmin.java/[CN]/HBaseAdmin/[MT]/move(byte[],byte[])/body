{
  executeCallable(new MasterCallable<Void>(getConnection()){
    @Override public Void call(    int callTimeout) throws ServiceException {
      PayloadCarryingRpcController controller=rpcControllerFactory.newController();
      controller.setCallTimeout(callTimeout);
      if (isMetaRegion(encodedRegionName)) {
        controller.setPriority(TableName.META_TABLE_NAME);
      }
      try {
        MoveRegionRequest request=RequestConverter.buildMoveRegionRequest(encodedRegionName,destServerName);
        master.moveRegion(controller,request);
      }
 catch (      DeserializationException de) {
        LOG.error("Could not parse destination server name: " + de);
        throw new ServiceException(new DoNotRetryIOException(de));
      }
      return null;
    }
  }
);
}
