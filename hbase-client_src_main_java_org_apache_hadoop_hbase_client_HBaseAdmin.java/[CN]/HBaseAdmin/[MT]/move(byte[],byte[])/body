{
  executeCallable(new MasterCallable<Void>(getConnection(),getRpcControllerFactory()){
    @Override protected Void call(    PayloadCarryingRpcController rpcController) throws Exception {
      rpcController.setPriority(encodedRegionName);
      MoveRegionRequest request=RequestConverter.buildMoveRegionRequest(encodedRegionName,destServerName);
      master.moveRegion(rpcController,request);
      return null;
    }
  }
);
}
