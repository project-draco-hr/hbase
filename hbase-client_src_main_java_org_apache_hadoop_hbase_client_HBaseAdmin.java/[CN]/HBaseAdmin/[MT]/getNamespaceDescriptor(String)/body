{
  return executeCallable(new MasterAdminCallable<NamespaceDescriptor>(getConnection()){
    @Override public NamespaceDescriptor call() throws Exception {
      return ProtobufUtil.toNamespaceDescriptor(masterAdmin.getNamespaceDescriptor(null,MasterAdminProtos.GetNamespaceDescriptorRequest.newBuilder().setNamespaceName(name).build()).getNamespaceDescriptor());
    }
  }
);
}
