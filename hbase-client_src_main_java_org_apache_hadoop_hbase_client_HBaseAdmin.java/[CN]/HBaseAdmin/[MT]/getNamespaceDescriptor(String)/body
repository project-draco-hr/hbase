{
  return executeCallable(new MasterCallable<NamespaceDescriptor>(getConnection(),getRpcControllerFactory()){
    @Override protected NamespaceDescriptor call(    PayloadCarryingRpcController rpcController) throws Exception {
      return ProtobufUtil.toNamespaceDescriptor(master.getNamespaceDescriptor(rpcController,GetNamespaceDescriptorRequest.newBuilder().setNamespaceName(name).build()).getNamespaceDescriptor());
    }
  }
);
}
