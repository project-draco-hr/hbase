{
  return executeCallable(new MasterCallable<NamespaceDescriptor>(getConnection()){
    @Override public NamespaceDescriptor call(    int callTimeout) throws Exception {
      PayloadCarryingRpcController controller=rpcControllerFactory.newController();
      controller.setCallTimeout(callTimeout);
      return ProtobufUtil.toNamespaceDescriptor(master.getNamespaceDescriptor(controller,GetNamespaceDescriptorRequest.newBuilder().setNamespaceName(name).build()).getNamespaceDescriptor());
    }
  }
);
}
