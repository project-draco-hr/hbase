{
  return executeCallable(new MasterAdminCallable<HTableDescriptor[]>(getConnection()){
    @Override public HTableDescriptor[] call() throws Exception {
      List<TableSchema> list=masterAdmin.getTableDescriptorsByNamespace(null,MasterAdminProtos.GetTableDescriptorsByNamespaceRequest.newBuilder().setNamespaceName(name).build()).getTableSchemaList();
      HTableDescriptor[] res=new HTableDescriptor[list.size()];
      for (int i=0; i < list.size(); i++) {
        res[i]=HTableDescriptor.convert(list.get(i));
      }
      return res;
    }
  }
);
}
