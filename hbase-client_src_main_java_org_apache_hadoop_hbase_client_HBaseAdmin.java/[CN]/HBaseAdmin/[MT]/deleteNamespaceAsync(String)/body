{
  DeleteNamespaceResponse response=executeCallable(new MasterCallable<DeleteNamespaceResponse>(getConnection()){
    @Override public DeleteNamespaceResponse call(    int callTimeout) throws Exception {
      return master.deleteNamespace(null,DeleteNamespaceRequest.newBuilder().setNamespaceName(name).build());
    }
  }
);
  return new NamespaceFuture(this,name,response.getProcId()){
    @Override public String getOperationType(){
      return "DELETE_NAMESPACE";
    }
  }
;
}
