{
  final byte[] toBeUnassigned=getRegionName(getRegionName);
  executeCallable(new MasterAdminCallable<Void>(getConnection()){
    @Override public Void call() throws ServiceException {
      UnassignRegionRequest request=RequestConverter.buildUnassignRegionRequest(toBeUnassigned,force);
      masterAdmin.unassignRegion(null,request);
      return null;
    }
  }
);
}
