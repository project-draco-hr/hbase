{
  HTable table=TEST_UTIL.createTable(Bytes.toBytes("testHTableExistsMethodMultipleRegionsSingleGet"),new byte[][]{FAMILY},1,new byte[]{0x00},new byte[]{(byte)0xff},255);
  Put put=new Put(ROW);
  put.add(FAMILY,QUALIFIER,VALUE);
  Get get=new Get(ROW);
  boolean exist=table.exists(get);
  assertEquals(exist,false);
  table.put(put);
  exist=table.exists(get);
  assertEquals(exist,true);
}
