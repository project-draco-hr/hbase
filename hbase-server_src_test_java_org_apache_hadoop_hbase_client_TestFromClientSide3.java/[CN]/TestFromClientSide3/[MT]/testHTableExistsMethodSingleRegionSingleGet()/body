{
  Table table=TEST_UTIL.createTable(Bytes.toBytes("testHTableExistsMethodSingleRegionSingleGet"),new byte[][]{FAMILY});
  Put put=new Put(ROW);
  put.add(FAMILY,QUALIFIER,VALUE);
  Get get=new Get(ROW);
  boolean exist=table.exists(get);
  assertEquals(exist,false);
  table.put(put);
  exist=table.exists(get);
  assertEquals(exist,true);
}
