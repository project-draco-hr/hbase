{
  conf.setBoolean(HBASE_SKIP_ERRORS,false);
  try {
    splitCorruptWALs(FaultySequenceFileLogReader.FailureType.BEGINNING);
  }
 catch (  IOException e) {
    LOG.debug("split with 'skip errors' set to 'false' correctly threw");
  }
  assertEquals("if skip.errors is false all files should remain in place",NUM_WRITERS + 1,fs.listStatus(WALDIR).length);
}
