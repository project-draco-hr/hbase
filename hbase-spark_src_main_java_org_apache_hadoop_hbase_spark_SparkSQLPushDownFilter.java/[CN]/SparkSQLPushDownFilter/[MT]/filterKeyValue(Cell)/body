{
  ColumnFilter filter=columnFamilyQualifierFilterMap.get(new ColumnFamilyQualifierMapKeyWrapper(c.getFamilyArray(),c.getFamilyOffset(),c.getFamilyLength(),c.getQualifierArray(),c.getQualifierOffset(),c.getQualifierLength()));
  if (filter == null) {
    return ReturnCode.INCLUDE;
  }
 else {
    if (filter.validate(c.getValueArray(),c.getValueOffset(),c.getValueLength())) {
      return ReturnCode.INCLUDE;
    }
  }
  return ReturnCode.NEXT_ROW;
}
