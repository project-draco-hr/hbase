{
switch (fieldID) {
case ROW:
    if (value == null) {
      unsetRow();
    }
 else {
      setRow((byte[])value);
    }
  break;
case MUTATIONS:
if (value == null) {
  unsetMutations();
}
 else {
  setMutations((List<Mutation>)value);
}
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
