{
  conf=TEST_UTIL.getConfiguration();
  fs=TEST_UTIL.getDFSCluster().getFileSystem();
  FileStatus[] entries=fs.listStatus(new Path("/"));
  for (  FileStatus dir : entries) {
    fs.delete(dir.getPath(),true);
  }
  seq=0;
