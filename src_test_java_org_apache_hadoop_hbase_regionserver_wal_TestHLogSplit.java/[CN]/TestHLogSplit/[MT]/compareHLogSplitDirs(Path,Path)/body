{
  FileStatus[] f1=fs.listStatus(p1);
  FileStatus[] f2=fs.listStatus(p2);
  for (int i=0; i < f1.length; i++) {
    if (!logsAreEqual(new Path(f1[i].getPath(),HConstants.HREGION_OLDLOGFILE_NAME),new Path(f2[i].getPath(),HConstants.HREGION_OLDLOGFILE_NAME))) {
      return -1;
    }
  }
  return 0;
}
