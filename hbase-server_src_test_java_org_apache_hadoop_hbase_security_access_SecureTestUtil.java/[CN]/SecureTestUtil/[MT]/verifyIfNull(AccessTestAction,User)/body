{
  for (  User user : users) {
    try {
      Object obj=user.runAs(action);
      if (obj != null) {
        fail("Non null results from action for user '" + user.getShortName() + "'");
      }
    }
 catch (    AccessDeniedException ade) {
      fail("Expected action to pass for user '" + user.getShortName() + "' but was denied");
    }
  }
}
