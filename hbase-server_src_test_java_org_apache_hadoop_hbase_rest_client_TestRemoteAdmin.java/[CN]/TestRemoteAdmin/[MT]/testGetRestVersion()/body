{
  VersionModel RETURNED_REST_VERSION=remoteAdmin.getRestVersion();
  System.out.print("Returned version is: " + RETURNED_REST_VERSION);
  assertTrue("Returned REST version did not contain info about rest.",RETURNED_REST_VERSION.toString().contains("rest"));
  assertTrue("Returned REST version did not contain info about the JVM.",RETURNED_REST_VERSION.toString().contains("JVM"));
  assertTrue("Returned REST version did not contain info about OS.",RETURNED_REST_VERSION.toString().contains("OS"));
}
