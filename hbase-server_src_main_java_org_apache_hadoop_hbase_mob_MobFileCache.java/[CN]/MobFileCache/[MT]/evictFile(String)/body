{
  if (isCacheEnabled) {
    IdLock.Entry lockEntry=null;
    try {
      lockEntry=keyLock.getLockEntry(fileName.hashCode());
      CachedMobFile evictedFile=map.remove(fileName);
      if (evictedFile != null) {
        evictedFile.close();
      }
    }
 catch (    IOException e) {
      LOG.error("Fail to evict the file " + fileName,e);
    }
 finally {
      if (lockEntry != null) {
        keyLock.releaseLockEntry(lockEntry);
      }
    }
  }
}
