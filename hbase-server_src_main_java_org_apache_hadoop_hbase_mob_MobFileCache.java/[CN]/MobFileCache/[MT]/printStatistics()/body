{
  long access=count.get() - lastAccess;
  long missed=miss.get() - lastMiss;
  long hitRate=(access - missed) * 100 / access;
  LOG.info("MobFileCache Statistics, access: " + access + ", miss: "+ missed+ ", hit: "+ (access - missed)+ ", hit rate: "+ ((access == 0) ? 0 : hitRate)+ "%");
  lastAccess+=access;
  lastMiss+=missed;
}
