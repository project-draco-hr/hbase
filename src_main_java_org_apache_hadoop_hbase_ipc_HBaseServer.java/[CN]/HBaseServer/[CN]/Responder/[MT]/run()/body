{
  LOG.info(getName() + ": starting");
  SERVER.set(HBaseServer.this);
  try {
    doRunLoop();
  }
  finally {
    LOG.info("Stopping " + this.getName());
    try {
      writeSelector.close();
    }
 catch (    IOException ioe) {
      LOG.error("Couldn't close write selector in " + this.getName(),ioe);
    }
  }
}
