{
  if (selector != null) {
    selector.wakeup();
    Thread.yield();
  }
  if (acceptChannel != null) {
    try {
      acceptChannel.socket().close();
    }
 catch (    IOException e) {
      LOG.info(getName() + ":Exception in closing listener socket. " + e);
    }
  }
  readPool.shutdownNow();
}
