{
  if (cacheSize < maxSize) {
    queue.add(entry);
    cacheSize+=entry.getValue().getLength();
  }
 else {
    BucketEntry head=queue.peek().getValue();
    if (entry.getValue().compareTo(head) > 0) {
      cacheSize+=entry.getValue().getLength();
      cacheSize-=head.getLength();
      if (cacheSize > maxSize) {
        queue.poll();
      }
 else {
        cacheSize+=head.getLength();
      }
      queue.add(entry);
    }
  }
}
