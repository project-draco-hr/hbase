{
  Pair<Long,ServerName> dest=movedRegions.get(encodedRegionName);
  if (dest != null) {
    if (dest.getFirst() > (EnvironmentEdgeManager.currentTimeMillis() - TIMEOUT_REGION_MOVED)) {
      return dest.getSecond();
    }
 else {
      movedRegions.remove(encodedRegionName);
    }
  }
  return null;
}
