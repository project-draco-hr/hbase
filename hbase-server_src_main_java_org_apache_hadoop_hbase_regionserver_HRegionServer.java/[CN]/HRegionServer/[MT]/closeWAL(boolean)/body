{
  if (this.hlogForMeta != null) {
    try {
      this.hlogForMeta.close();
    }
 catch (    Throwable e) {
      e=e instanceof RemoteException ? ((RemoteException)e).unwrapRemoteException() : e;
      LOG.error("Metalog close and delete failed",e);
    }
  }
  if (this.hlog != null) {
    try {
      if (delete) {
        hlog.closeAndDelete();
      }
 else {
        hlog.close();
      }
    }
 catch (    Throwable e) {
      e=e instanceof RemoteException ? ((RemoteException)e).unwrapRemoteException() : e;
      LOG.error("Close and delete failed",e);
    }
  }
}
