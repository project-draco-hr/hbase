{
  List<WALActionsListener> listeners=new ArrayList<WALActionsListener>();
  MetaLogRoller tmpLogRoller=new MetaLogRoller(this,this);
  String n=Thread.currentThread().getName();
  Threads.setDaemonThreadRunning(tmpLogRoller.getThread(),n + "MetaLogRoller",uncaughtExceptionHandler);
  this.metaHLogRoller=tmpLogRoller;
  tmpLogRoller=null;
  listeners.add(this.metaHLogRoller);
  return listeners;
}
