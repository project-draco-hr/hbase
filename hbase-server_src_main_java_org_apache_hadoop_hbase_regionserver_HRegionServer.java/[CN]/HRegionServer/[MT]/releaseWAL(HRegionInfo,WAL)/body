{
  if (regionInfo != null && regionInfo.isMetaTable() && regionInfo.getReplicaId() == HRegionInfo.DEFAULT_REPLICA_ID) {
    walFactory.closeMetaWAL(regionInfo.getEncodedNameAsBytes());
    LogRoller roller;
    if (regionInfo != null && regionInfo.isMetaTable() && regionInfo.getReplicaId() == HRegionInfo.DEFAULT_REPLICA_ID) {
      roller=metawalRoller.get();
      if (roller != null) {
        roller.removeWAL(wal);
      }
    }
  }
}
