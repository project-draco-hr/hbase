{
  if (!fsOk) {
    return false;
  }
  if (!(leases.isAlive() && cacheFlusher.isAlive() && walRoller.isAlive()&& this.compactionChecker.isAlive()&& this.periodicFlusher.isAlive())) {
    stop("One or more threads are no longer alive -- stop");
    return false;
  }
  final LogRoller metawalRoller=this.metawalRoller.get();
  if (metawalRoller != null && !metawalRoller.isAlive()) {
    stop("Meta WAL roller thread is no longer alive -- stop");
    return false;
  }
  return true;
}
