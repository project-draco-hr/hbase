{
  for (  HRegion r : this.instance.onlineRegions.values()) {
    if (r == null)     continue;
    for (    HStore s : r.getStores().values()) {
      try {
        if (s.needsCompaction()) {
          this.instance.compactSplitThread.requestCompaction(r,s,getName() + " requests compaction");
        }
 else         if (s.isMajorCompaction()) {
          if (majorCompactPriority == DEFAULT_PRIORITY || majorCompactPriority > r.getCompactPriority()) {
            this.instance.compactSplitThread.requestCompaction(r,s,getName() + " requests major compaction; use default priority");
          }
 else {
            this.instance.compactSplitThread.requestCompaction(r,s,getName() + " requests major compaction; use configured priority",this.majorCompactPriority);
          }
        }
      }
 catch (      IOException e) {
        LOG.warn("Failed major compaction check on " + r,e);
      }
    }
  }
}
