def __init__(self, d=None):
    self.tableName = None
    self.row = None
    self.timestamp = None
    if isinstance(d, dict):
        if ('tableName' in d):
            self.tableName = d['tableName']
        if ('row' in d):
            self.row = d['row']
        if ('timestamp' in d):
            self.timestamp = d['timestamp']
