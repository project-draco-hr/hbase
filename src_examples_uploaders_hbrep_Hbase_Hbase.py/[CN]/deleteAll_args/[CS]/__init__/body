def __init__(self, d=None):
    self.tableName = None
    self.row = None
    self.column = None
    if isinstance(d, dict):
        if ('tableName' in d):
            self.tableName = d['tableName']
        if ('row' in d):
            self.row = d['row']
        if ('column' in d):
            self.column = d['column']
