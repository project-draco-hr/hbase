def __init__(self, d=None):
    self.tableName = None
    self.startRow = None
    self.stopRow = None
    self.columns = None
    if isinstance(d, dict):
        if ('tableName' in d):
            self.tableName = d['tableName']
        if ('startRow' in d):
            self.startRow = d['startRow']
        if ('stopRow' in d):
            self.stopRow = d['stopRow']
        if ('columns' in d):
            self.columns = d['columns']
