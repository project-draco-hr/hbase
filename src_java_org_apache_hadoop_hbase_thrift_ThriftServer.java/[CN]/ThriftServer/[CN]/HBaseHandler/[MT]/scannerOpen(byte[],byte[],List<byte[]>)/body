{
  if (LOG.isDebugEnabled()) {
    LOG.debug("scannerOpen: table=" + new String(getText(tableName)) + ", start="+ new String(getText(startRow))+ ", columns="+ columns.toString());
  }
  try {
    HTable table=getTable(tableName);
    byte[][] columnsText=new byte[columns.size()][];
    for (int i=0; i < columns.size(); ++i) {
      columnsText[i]=getText(columns.get(i));
    }
    Scanner scanner=table.getScanner(columnsText,getText(startRow));
    return addScanner(scanner);
  }
 catch (  IOException e) {
    throw new IOError(e.getMessage());
  }
}
