{
  if (LOG.isDebugEnabled()) {
    LOG.debug("getVerTs: table=" + new String(tableName) + ", row="+ new String(row)+ ", col="+ new String(column)+ ", ts="+ timestamp+ ", numVers="+ numVersions);
  }
  try {
    HTable table=getTable(tableName);
    byte[][] values=table.get(getText(row),getText(column),timestamp,numVersions);
    if (values == null) {
      throw new NotFound();
    }
    return new ArrayList<byte[]>(Arrays.asList(values));
  }
 catch (  IOException e) {
    throw new IOError(e.getMessage());
  }
}
