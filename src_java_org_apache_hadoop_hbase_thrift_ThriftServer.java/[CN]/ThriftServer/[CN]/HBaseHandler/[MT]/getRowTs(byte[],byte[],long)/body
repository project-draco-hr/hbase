{
  if (LOG.isDebugEnabled()) {
    LOG.debug("getRowTs: table=" + new String(tableName) + ", row="+ new String(row)+ ", ts="+ timestamp);
  }
  try {
    HTable table=getTable(tableName);
    Map<byte[],Cell> values=table.getRow(getText(row),timestamp);
    TreeMap<byte[],byte[]> returnValues=new TreeMap<byte[],byte[]>(Bytes.BYTES_COMPARATOR);
    for (    Entry<byte[],Cell> e : values.entrySet()) {
      returnValues.put(e.getKey(),e.getValue().getValue());
    }
    return returnValues;
  }
 catch (  IOException e) {
    throw new IOError(e.getMessage());
  }
}
