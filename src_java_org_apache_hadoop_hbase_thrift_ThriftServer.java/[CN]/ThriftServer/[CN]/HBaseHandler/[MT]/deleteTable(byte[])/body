{
  if (LOG.isDebugEnabled()) {
    LOG.debug("deleteTable: table=" + new String(tableName));
  }
  try {
    byte[] tableStr=getText(tableName);
    if (!admin.tableExists(tableStr)) {
      throw new NotFound();
    }
    admin.deleteTable(tableStr);
  }
 catch (  IOException e) {
    throw new IOError(e.getMessage());
  }
}
