{
  if (LOG.isDebugEnabled()) {
    LOG.debug("deleteTable: table=" + new String(tableName));
  }
  try {
    if (!admin.tableExists(tableName)) {
      throw new IOError("table does not exist");
    }
    admin.deleteTable(tableName);
  }
 catch (  IOException e) {
    throw new IOError(e.getMessage());
  }
}
