{
  return "REGION => {" + HConstants.NAME + " => '"+ this.regionNameStr+ "', STARTKEY => '"+ Bytes.toStringBinary(this.startKey)+ "', ENDKEY => '"+ Bytes.toStringBinary(this.endKey)+ "', ENCODED => "+ getEncodedName()+ ","+ (isOffline() ? " OFFLINE => true," : "")+ (isSplit() ? " SPLIT => true," : "")+ " TABLE => {"+ this.tableDesc.toString()+ "}";
}
