{
  int offset=skipVintdByteArray(bb,0);
  byte firstByte=bb.get(offset);
  int vint=firstByte;
  int vintWidth=WritableUtils.decodeVIntSize(firstByte);
  if (vintWidth != 1) {
    vint=getBigVint(vintWidth,firstByte,bb.array(),bb.arrayOffset() + offset);
  }
  byte[] b=new byte[vint];
  System.arraycopy(bb.array(),bb.arrayOffset() + offset + vintWidth,b,0,vint);
  return b;
}
