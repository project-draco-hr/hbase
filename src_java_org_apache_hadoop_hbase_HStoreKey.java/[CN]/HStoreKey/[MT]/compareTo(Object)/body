{
  HStoreKey other=(HStoreKey)o;
  int result=this.row.compareTo(other.row);
  if (result == 0) {
    result=this.column.compareTo(other.column);
    if (result == 0) {
      if (this.timestamp < other.timestamp) {
        result=1;
      }
 else       if (this.timestamp > other.timestamp) {
        result=-1;
      }
    }
  }
  return result;
}
