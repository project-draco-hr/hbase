{
  int lmetaOffsetPlusDelimiter=loffset + 7;
  int leftFarDelimiter=getDelimiterInReverse(left,lmetaOffsetPlusDelimiter,llength - lmetaOffsetPlusDelimiter,HRegionInfo.DELIMITER);
  int rmetaOffsetPlusDelimiter=roffset + 7;
  int rightFarDelimiter=getDelimiterInReverse(right,rmetaOffsetPlusDelimiter,rlength - rmetaOffsetPlusDelimiter,HRegionInfo.DELIMITER);
  if (leftFarDelimiter < 0 && rightFarDelimiter >= 0) {
    return -1;
  }
 else   if (rightFarDelimiter < 0 && leftFarDelimiter >= 0) {
    return 1;
  }
 else   if (leftFarDelimiter < 0 && rightFarDelimiter < 0) {
    return 0;
  }
  int result=compareMetaRows(left,lmetaOffsetPlusDelimiter,leftFarDelimiter - lmetaOffsetPlusDelimiter,right,rmetaOffsetPlusDelimiter,rightFarDelimiter - rmetaOffsetPlusDelimiter);
  if (result != 0) {
    return result;
  }
  leftFarDelimiter++;
  rightFarDelimiter++;
  result=compareRowid(left,leftFarDelimiter,llength - leftFarDelimiter,right,rightFarDelimiter,rlength - rightFarDelimiter);
  return result;
}
