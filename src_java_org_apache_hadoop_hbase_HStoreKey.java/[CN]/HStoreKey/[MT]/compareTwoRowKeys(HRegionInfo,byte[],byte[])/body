{
  if (regionInfo != null && (regionInfo.isMetaRegion() || regionInfo.isRootRegion())) {
    byte[][] keysA=stripStartKeyMeta(rowA);
    byte[][] KeysB=stripStartKeyMeta(rowB);
    int rowCompare=Bytes.compareTo(keysA[0],KeysB[0]);
    if (rowCompare == 0)     rowCompare=Bytes.compareTo(keysA[1],KeysB[1]);
    return rowCompare;
  }
 else {
    return Bytes.compareTo(rowA,rowB);
  }
}
