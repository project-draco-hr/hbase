{
  Bytes.writeByteArray(out,this.column);
  if (this.val == null) {
    out.writeInt(0);
  }
 else {
    out.writeInt(this.val.length);
    out.write(this.val);
  }
  out.writeLong(timestamp);
  out.writeBoolean(isTransactionEntry);
  if (isTransactionEntry) {
    out.writeLong(transactionId);
    out.writeUTF(operation.name());
  }
}
