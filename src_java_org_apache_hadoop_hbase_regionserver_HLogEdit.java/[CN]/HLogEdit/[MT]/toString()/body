{
  String value="";
  try {
    value=(this.val.length > MAX_VALUE_LEN) ? new String(this.val,0,MAX_VALUE_LEN,HConstants.UTF8_ENCODING) + "..." : new String(getVal(),HConstants.UTF8_ENCODING);
  }
 catch (  UnsupportedEncodingException e) {
    throw new RuntimeException("UTF8 encoding not present?",e);
  }
  return "(" + Bytes.toString(getColumn()) + "/"+ getTimestamp()+ "/"+ value+ ")";
}
