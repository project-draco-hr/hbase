{
  try {
    super.setUp();
    this.desc=new HTableDescriptor("test");
    desc.addFamily(new HColumnDescriptor(CONTENTS,1,HColumnDescriptor.CompressionType.NONE,false,Integer.MAX_VALUE,new BloomFilterDescriptor(BloomFilterDescriptor.BLOOMFILTER,12499,4)));
    HBaseAdmin admin=new HBaseAdmin(conf);
    admin.createTable(desc);
    table=new HTable(conf,desc.getName());
    for (int i=0; i < 100; i++) {
      Text row=rows[i];
      String value=row.toString();
      long lockid=table.startUpdate(rows[i]);
      table.put(lockid,CONTENTS,value.getBytes(HConstants.UTF8_ENCODING));
      table.commit(lockid);
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
    fail();
  }
}
