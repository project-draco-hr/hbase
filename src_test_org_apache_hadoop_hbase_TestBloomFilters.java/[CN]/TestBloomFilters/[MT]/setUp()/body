{
  try {
    super.setUp();
    this.client=new HClient(conf);
    this.desc=new HTableDescriptor("test");
    desc.addFamily(new HColumnDescriptor(CONTENTS,1,HColumnDescriptor.CompressionType.NONE,false,Integer.MAX_VALUE,new BloomFilterDescriptor(BloomFilterDescriptor.BLOOMFILTER,12499,4)));
    client.createTable(desc);
    client.openTable(desc.getName());
    for (int i=0; i < 100; i++) {
      Text row=rows[i];
      String value=row.toString();
      long lockid=client.startUpdate(rows[i]);
      client.put(lockid,CONTENTS,value.getBytes(HConstants.UTF8_ENCODING));
      client.commit(lockid);
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
    fail();
  }
}
