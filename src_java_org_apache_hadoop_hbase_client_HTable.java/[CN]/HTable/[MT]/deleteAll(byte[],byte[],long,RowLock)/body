{
  connection.getRegionServerWithRetries(new ServerCallable<Boolean>(connection,tableName,row){
    public Boolean call() throws IOException {
      long lockId=-1L;
      if (rl != null) {
        lockId=rl.getLockId();
      }
      if (column != null) {
        this.server.deleteAll(location.getRegionInfo().getRegionName(),row,column,ts,lockId);
      }
 else {
        this.server.deleteAll(location.getRegionInfo().getRegionName(),row,ts,lockId);
      }
      return null;
    }
  }
);
}
