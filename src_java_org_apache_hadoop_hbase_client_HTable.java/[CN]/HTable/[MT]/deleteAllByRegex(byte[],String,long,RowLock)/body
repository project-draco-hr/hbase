{
  connection.getRegionServerWithRetries(new ServerCallable<Boolean>(connection,tableName,row){
    public Boolean call() throws IOException {
      long lockId=-1L;
      if (rl != null) {
        lockId=rl.getLockId();
      }
      this.server.deleteAllByRegex(location.getRegionInfo().getRegionName(),row,colRegex,ts,lockId);
      return null;
    }
  }
);
}
