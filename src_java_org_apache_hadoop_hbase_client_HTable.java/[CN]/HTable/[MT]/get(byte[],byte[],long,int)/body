{
  Get g=new Get(row);
  byte[][] fq=KeyValue.parseColumn(column);
  if (fq[1].length == 0) {
    g.addFamily(fq[0]);
  }
 else {
    g.addColumn(fq[0],fq[1]);
  }
  g.setMaxVersions(numVersions);
  g.setTimeRange(0,timestamp == HConstants.LATEST_TIMESTAMP ? timestamp : timestamp + 1);
  Result r=get(g);
  return r == null || r.size() <= 0 ? null : r.getCellValues();
}
