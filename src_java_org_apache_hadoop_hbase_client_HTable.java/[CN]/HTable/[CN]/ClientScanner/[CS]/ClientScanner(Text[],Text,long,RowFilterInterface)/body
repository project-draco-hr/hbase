{
  LOG.info("Creating scanner over " + tableName + " starting at key "+ startRow);
  this.closed=false;
  this.server=null;
  this.scannerId=-1L;
  this.columns=columns;
  this.startRow=startRow;
  this.scanTime=timestamp;
  this.filter=filter;
  if (filter != null) {
    filter.validate(columns);
  }
  nextScanner();
}
