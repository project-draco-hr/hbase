{
  if (LOG.isDebugEnabled()) {
    LOG.debug("Creating scanner over " + tableName + " starting at key '"+ startRow+ "'");
  }
  this.columns=columns;
  this.startRow=startRow;
  this.scanTime=timestamp;
  this.filter=filter;
  if (filter != null) {
    filter.validate(columns);
  }
  nextScanner();
}
