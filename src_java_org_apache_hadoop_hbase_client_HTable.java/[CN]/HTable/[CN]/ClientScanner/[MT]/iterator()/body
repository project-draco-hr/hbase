{
  return new Iterator<RowResult>(){
    RowResult next=null;
    public boolean hasNext(){
      if (next == null) {
        try {
          next=ClientScanner.this.next();
          return next != null;
        }
 catch (        IOException e) {
          throw new RuntimeException(e);
        }
      }
 else {
        return true;
      }
    }
    public RowResult next(){
      if (!hasNext()) {
        return null;
      }
      RowResult temp=next;
      next=null;
      return temp;
    }
    public void remove(){
      throw new UnsupportedOperationException();
    }
  }
;
}
