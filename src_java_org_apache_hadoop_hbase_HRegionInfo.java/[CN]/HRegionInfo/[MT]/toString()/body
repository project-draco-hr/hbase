{
  return "regionname: " + this.regionNameStr + ", startKey: <"+ Bytes.toString(this.startKey)+ ">, endKey: <"+ Bytes.toString(this.endKey)+ ">, encodedName: "+ getEncodedName()+ ","+ (isOffline() ? " offline: true," : "")+ (isSplit() ? " split: true," : "")+ " tableDesc: {"+ this.tableDesc.toString()+ "}";
}
