{
  byte[] bytes=null;
synchronized (encoder) {
    encoder.update(regionName.getBytes(),0,regionName.getLength());
    bytes=encoder.digest();
    encoder.reset();
  }
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < bytes.length; i++) {
    sb.append(bytes[i]);
  }
  return sb.toString();
}
