{
synchronized (this.cache) {
    if ((newState == ZooKeeperProtos.Table.State.DISABLING) && this.cache.get(tableName) != null && !isTableState(tableName,states) || (newState != ZooKeeperProtos.Table.State.DISABLING && !isTableState(tableName,states))) {
      return false;
    }
    try {
      setTableStateInZK(tableName,newState);
    }
 catch (    KeeperException e) {
      throw new CoordinatedStateException(e);
    }
    return true;
  }
}
