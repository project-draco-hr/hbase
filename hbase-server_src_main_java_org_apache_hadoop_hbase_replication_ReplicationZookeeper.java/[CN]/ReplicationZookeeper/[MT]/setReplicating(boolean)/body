{
  ZKUtil.createWithParents(this.zookeeper,ZKUtil.joinZNode(this.replicationZNode,this.replicationStateNodeName));
  byte[] stateBytes=(newState == true) ? ENABLED_ZNODE_BYTES : DISABLED_ZNODE_BYTES;
  ZKUtil.setData(this.zookeeper,ZKUtil.joinZNode(this.replicationZNode,this.replicationStateNodeName),stateBytes);
}
