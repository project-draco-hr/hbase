{
  if (!(obj instanceof Triple)) {
    return false;
  }
  Triple<?,?,?> otherTriple=(Triple<?,?,?>)obj;
  if (first != otherTriple.first && (first != null && !(first.equals(otherTriple.first))))   return false;
  if (second != otherTriple.second && (second != null && !(second.equals(otherTriple.second))))   return false;
  if (third != otherTriple.third && (third != null && !(third.equals(otherTriple.third))))   return false;
  return true;
}
