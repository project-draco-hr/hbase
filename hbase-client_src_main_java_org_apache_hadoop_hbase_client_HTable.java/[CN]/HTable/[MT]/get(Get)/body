{
  RegionServerCallable<Result> callable=new RegionServerCallable<Result>(this.connection,getTableName(),get.getRow()){
    public Result call() throws IOException {
      return ProtobufUtil.get(getStub(),getLocation().getRegionInfo().getRegionName(),get);
    }
  }
;
  return new RpcRetryingCaller<Result>().callWithRetries(callable,getConfiguration(),this.operationTimeout);
}
