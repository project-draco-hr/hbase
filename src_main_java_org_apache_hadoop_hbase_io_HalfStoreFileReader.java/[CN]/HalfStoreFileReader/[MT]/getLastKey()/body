{
  if (top) {
    return super.getLastKey();
  }
  HFileScanner scanner=getScanner(true,true);
  try {
    if (scanner.seekBefore(this.splitkey)) {
      return Bytes.toBytes(scanner.getKey());
    }
  }
 catch (  IOException e) {
    LOG.warn("Failed seekBefore " + Bytes.toStringBinary(this.splitkey),e);
  }
  return null;
}
