{
  if (src == null || tgt == null) {
    throw new IllegalArgumentException("Writables cannot be null");
  }
  byte[] bytes=getBytes(src);
  DataInputStream dis=new DataInputStream(new ByteArrayInputStream(bytes));
  try {
    tgt.readFields(dis);
  }
  finally {
    dis.close();
  }
  return tgt;
}
