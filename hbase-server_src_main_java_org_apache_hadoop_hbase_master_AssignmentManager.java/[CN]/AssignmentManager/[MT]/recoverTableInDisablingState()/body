{
  Set<String> disablingTables=ZKTable.getDisablingTables(watcher);
  if (disablingTables.size() != 0) {
    for (    String tableName : disablingTables) {
      LOG.info("The table " + tableName + " is in DISABLING state.  Hence recovering by moving the table"+ " to DISABLED state.");
      new DisableTableHandler(this.server,tableName.getBytes(),catalogTracker,this,true).process();
    }
  }
}
