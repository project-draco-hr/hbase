{
  if (this.tableStateManager.isTableState(region.getTable(),ZooKeeperProtos.Table.State.DISABLED,ZooKeeperProtos.Table.State.DISABLING) || replicasToClose.contains(region)) {
    LOG.info("Table " + region.getTable() + " is disabled or disabling;"+ " skipping assign of "+ region.getRegionNameAsString());
    offlineDisabledRegion(region);
    return true;
  }
  return false;
}
