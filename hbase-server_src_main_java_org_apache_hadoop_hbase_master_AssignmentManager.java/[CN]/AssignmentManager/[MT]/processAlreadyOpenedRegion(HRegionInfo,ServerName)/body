{
  LOG.debug("ALREADY_OPENED region " + region.getRegionNameAsString() + " to "+ sn);
  String encodedRegionName=region.getEncodedName();
  try {
    ZKAssign.deleteOfflineNode(watcher,encodedRegionName);
  }
 catch (  KeeperException.NoNodeException e) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("The unassigned node " + encodedRegionName + " doesnot exist.");
    }
  }
catch (  KeeperException e) {
    server.abort("Error deleting OFFLINED node in ZK for transition ZK node (" + encodedRegionName + ")",e);
  }
  regionStates.regionOnline(region,sn);
}
