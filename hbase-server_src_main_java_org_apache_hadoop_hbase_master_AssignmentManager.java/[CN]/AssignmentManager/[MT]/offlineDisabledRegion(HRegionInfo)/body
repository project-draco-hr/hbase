{
  LOG.debug("Table being disabled so deleting ZK node and removing from " + "regions in transition, skipping assignment of region " + regionInfo.getRegionNameAsString());
  try {
    if (!ZKAssign.deleteClosedNode(watcher,regionInfo.getEncodedName())) {
      ZKAssign.deleteOfflineNode(watcher,regionInfo.getEncodedName());
    }
  }
 catch (  KeeperException.NoNodeException nne) {
    LOG.debug("Tried to delete closed node for " + regionInfo + " but it "+ "does not exist so just offlining");
  }
catch (  KeeperException e) {
    this.server.abort("Error deleting CLOSED node in ZK",e);
  }
  regionOffline(regionInfo);
}
