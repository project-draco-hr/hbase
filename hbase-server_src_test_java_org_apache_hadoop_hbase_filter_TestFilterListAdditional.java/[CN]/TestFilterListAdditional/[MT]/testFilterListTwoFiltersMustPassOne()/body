{
  String prefix="Row C";
  HTable table=new HTable(TestFilterListAdditional.conf,tableName);
  assertTrue("Fail to create the table",admin.tableExists(tableName));
  FilterList flist=new FilterList(FilterList.Operator.MUST_PASS_ONE);
  flist.addFilter(new AlwaysNextColFilter());
  flist.addFilter(new PrefixFilter(prefix.getBytes()));
  Scan scan=new Scan();
  scan.setFilter(flist);
  ResultScanner scanner=table.getScanner(scan);
  for (  Result r : scanner) {
    assertTrue("The rowid of this row does not start with \"" + prefix + "\": "+ r.toString(),Bytes.toStringBinary(r.getRow()).startsWith(prefix));
  }
  scanner.close();
  table.close();
}
