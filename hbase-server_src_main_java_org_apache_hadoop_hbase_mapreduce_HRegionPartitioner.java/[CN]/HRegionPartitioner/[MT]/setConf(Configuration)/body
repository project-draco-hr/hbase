{
  this.conf=HBaseConfiguration.create(configuration);
  try {
    TableName tableName=TableName.valueOf(configuration.get(TableOutputFormat.OUTPUT_TABLE));
    this.table=new HTable(this.conf,tableName);
  }
 catch (  IOException e) {
    LOG.error(e);
  }
  try {
    this.startKeys=this.table.getStartKeys();
  }
 catch (  IOException e) {
    LOG.error(e);
  }
}
