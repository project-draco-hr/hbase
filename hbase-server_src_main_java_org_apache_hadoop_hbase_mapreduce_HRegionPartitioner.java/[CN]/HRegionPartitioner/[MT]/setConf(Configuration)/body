{
  this.conf=HBaseConfiguration.create(configuration);
  try {
    this.table=new HTable(this.conf,configuration.get(TableOutputFormat.OUTPUT_TABLE));
  }
 catch (  IOException e) {
    LOG.error(e);
  }
  try {
    this.startKeys=this.table.getStartKeys();
  }
 catch (  IOException e) {
    LOG.error(e);
  }
}
