{
  int count=0;
  try {
    while (count < maxOps) {
      Get get=new Get(Bytes.toBytes("row-" + count));
      for (      final HTable table : tables) {
        table.get(get);
      }
      count+=tables.length;
    }
  }
 catch (  ThrottlingException e) {
    LOG.error("get failed after nRetries=" + count,e);
  }
  return count;
}
