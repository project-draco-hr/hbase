{
  HTableInterface htable=htablePool.getTable(Bytes.toBytes(table));
  try {
    htable.put(AvroUtil.aputToPut(aput));
    return null;
  }
 catch (  IOException e) {
    AIOError ioe=new AIOError();
    ioe.message=new Utf8(e.getMessage());
    throw ioe;
  }
 finally {
    try {
      htablePool.putTable(htable);
    }
 catch (    IOException e) {
      AIOError ioe=new AIOError();
      ioe.message=new Utf8(e.getMessage());
      throw ioe;
    }
  }
}
