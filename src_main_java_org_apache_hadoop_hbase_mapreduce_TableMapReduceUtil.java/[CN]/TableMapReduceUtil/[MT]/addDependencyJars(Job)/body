{
  try {
    addDependencyJars(job.getConfiguration(),ZooKeeper.class,Function.class,job.getMapOutputKeyClass(),job.getMapOutputValueClass(),job.getOutputKeyClass(),job.getOutputValueClass(),job.getOutputFormatClass(),job.getPartitionerClass(),job.getCombinerClass());
