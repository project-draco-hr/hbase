{
  TimestampModel innerModel=getModel();
  byte[] tableName;
  byte[] rowName;
  long timestamp;
  tableName=pathSegments[0];
  rowName=pathSegments[2];
  timestamp=Bytes.toLong(pathSegments[3]);
  if (queryMap.size() == 0) {
    innerModel.delete(tableName,rowName,timestamp);
  }
 else {
    innerModel.delete(tableName,rowName,this.getColumnsFromQueryMap(queryMap),timestamp);
  }
  s.setAccepted();
  s.respond();
}
