{
  long meanTime=0;
  for (  long time : durationsInNanoSed) {
    meanTime+=time;
  }
  meanTime/=durationsInNanoSed.size();
  long standardDev=0;
  for (  long time : durationsInNanoSed) {
    standardDev+=(time - meanTime) * (time - meanTime);
  }
  standardDev=(long)Math.sqrt(standardDev / durationsInNanoSed.size());
  final double million=1000.0 * 1000.0 * 1000.0;
  double mbPerSec=(totalSize * million) / (1024.0 * 1024.0 * meanTime);
  double mbPerSecDev=(totalSize * million) / (1024.0 * 1024.0 * (meanTime - standardDev));
  System.out.println(String.format("  %s performance:%s %6.2f MB/s (+/- %.2f MB/s)",isDecompression ? "Decompression" : "Compression",isDecompression ? "" : "  ",mbPerSec,mbPerSecDev - mbPerSec));
}
