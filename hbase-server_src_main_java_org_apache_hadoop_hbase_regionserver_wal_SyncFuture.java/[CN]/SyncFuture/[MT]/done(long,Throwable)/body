{
  if (isDone())   return false;
  this.throwable=t;
  if (sequence < this.ringBufferSequence) {
    if (throwable == null) {
      this.throwable=new IllegalStateException("sequence=" + sequence + ", ringBufferSequence="+ this.ringBufferSequence);
    }
  }
  this.doneSequence=sequence;
  notify();
  return true;
}
