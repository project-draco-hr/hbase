{
  if (splitReporter == null) {
    return true;
  }
 else {
    long t=EnvironmentEdgeManager.currentTimeMillis();
    if ((t - last_report_at) > report_period) {
      last_report_at=t;
      if (this.splitReporter.progress() == false) {
        LOG.warn("Failed: reporter.progress asked us to terminate");
        return false;
      }
    }
    return true;
  }
}
