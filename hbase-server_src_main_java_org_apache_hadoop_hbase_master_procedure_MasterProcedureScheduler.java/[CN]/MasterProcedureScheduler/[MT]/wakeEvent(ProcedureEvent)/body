{
  final boolean isTraceEnabled=LOG.isTraceEnabled();
synchronized (event) {
    event.setReady(true);
    if (isTraceEnabled) {
      LOG.trace("Wake event " + event);
    }
    schedLock.lock();
    try {
      popEventWaitingObjects(event);
      if (queueSize > 1) {
        schedWaitCond.signalAll();
      }
 else       if (queueSize > 0) {
        schedWaitCond.signal();
      }
    }
  finally {
      schedLock.unlock();
    }
  }
}
