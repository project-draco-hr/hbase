{
  final boolean isTraceEnabled=LOG.isTraceEnabled();
synchronized (event) {
    event.setReady(true);
    if (isTraceEnabled) {
      LOG.trace("Wake event " + event);
    }
    schedLock.lock();
    try {
      final int waitingCount=popEventWaitingObjects(event);
      wakePollIfNeeded(waitingCount);
    }
  finally {
      schedLock.unlock();
    }
  }
}
