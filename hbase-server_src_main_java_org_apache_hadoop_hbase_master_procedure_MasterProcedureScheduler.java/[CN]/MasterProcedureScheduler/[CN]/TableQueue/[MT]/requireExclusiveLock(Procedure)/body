{
  TableProcedureInterface tpi=(TableProcedureInterface)proc;
switch (tpi.getTableOperationType()) {
case CREATE:
case DELETE:
case DISABLE:
case EDIT:
case ENABLE:
    return true;
case READ:
  return false;
default :
break;
}
throw new UnsupportedOperationException("unexpected type " + tpi.getTableOperationType());
}
