{
  long now=EnvironmentEdgeManager.currentTime();
  ScanQueryMatcher qm=new ScanQueryMatcher(scan,new ScanInfo(this.conf,fam2,10,1,ttl,KeepDeletedCells.FALSE,0,rowComparator),get.getFamilyMap().get(fam2),now - ttl,now);
  Cell kv=new KeyValue(row1,fam2,col2,1,data);
  Cell cell=CellUtil.createLastOnRowCol(kv);
  qm.setToNewRow(kv);
  MatchCode code=qm.match(cell);
  assertFalse(code.compareTo(MatchCode.SEEK_NEXT_COL) != 0);
}
