{
  java.lang.Object ref=substr_;
  if (ref instanceof String) {
    return (String)ref;
  }
 else {
    com.google.protobuf.ByteString bs=(com.google.protobuf.ByteString)ref;
    String s=bs.toStringUtf8();
    if (com.google.protobuf.Internal.isValidUtf8(bs)) {
      substr_=s;
    }
    return s;
  }
}
