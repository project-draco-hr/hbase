{
  if (value.heapSize() > maxBlockSize) {
    if (stats.failInsert() % 50 == 0) {
      LOG.warn(String.format("Trying to cache too large a block %s @ %,d is %,d which is larger than %,d",key.getHfileName(),key.getOffset(),value.heapSize(),DEFAULT_MAX_BLOCK_SIZE));
    }
  }
 else {
    cache.put(key,value);
  }
}
