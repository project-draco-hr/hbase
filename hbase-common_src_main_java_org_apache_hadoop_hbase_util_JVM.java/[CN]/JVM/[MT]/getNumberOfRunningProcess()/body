{
  if (!isUnix()) {
    return 0;
  }
  BufferedReader input=null;
  try {
    int count=0;
    Process p=Runtime.getRuntime().exec("ps -e");
    input=new BufferedReader(new InputStreamReader(p.getInputStream()));
    while (input.readLine() != null) {
      count++;
    }
    return count - 1;
  }
 catch (  IOException e) {
    return -1;
  }
 finally {
    if (input != null) {
      try {
        input.close();
      }
 catch (      IOException ignored) {
      }
    }
  }
}
