{
  String serviceName=this.connection.service != null ? this.connection.service.getDescriptorForType().getName() : "null";
  StringBuilder sb=new StringBuilder();
  sb.append("callId: ");
  sb.append(this.id);
  sb.append(" service: ");
  sb.append(serviceName);
  sb.append(" methodName: ");
  sb.append((this.md != null) ? this.md.getName() : "");
  sb.append(" size: ");
  sb.append(StringUtils.humanReadableInt(this.size));
  sb.append(" connection: ");
  sb.append(connection.toString());
  return sb.toString();
}
