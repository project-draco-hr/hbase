{
  if (started)   return;
  AuthenticationTokenSecretManager mgr=createSecretManager();
  if (mgr != null) {
    setSecretManager(mgr);
    mgr.start();
  }
  this.authManager=new ServiceAuthorizationManager();
  HBasePolicyProvider.init(conf,authManager);
  responder.start();
  listener.start();
  scheduler.start();
  started=true;
}
