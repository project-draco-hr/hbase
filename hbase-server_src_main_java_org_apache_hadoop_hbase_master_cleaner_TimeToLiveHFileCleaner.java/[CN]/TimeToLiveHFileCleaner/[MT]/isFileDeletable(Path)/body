{
  if (!instantiateFS()) {
    return false;
  }
  long time=0;
  long currentTime=EnvironmentEdgeManager.currentTimeMillis();
  try {
    FileStatus fStat=fs.getFileStatus(filePath);
    time=fStat.getModificationTime();
  }
 catch (  IOException e) {
    LOG.error("Unable to get modification time of file " + filePath.getName() + ", not deleting it.",e);
    return false;
  }
  long life=currentTime - time;
  LOG.debug("Life:" + life + ", ttl:"+ ttl+ ", current:"+ currentTime+ ", from: "+ time);
  if (life < 0) {
    LOG.warn("Found a log (" + filePath + ") newer than current time ("+ currentTime+ " < "+ time+ "), probably a clock skew");
    return false;
  }
  return life > ttl;
}
