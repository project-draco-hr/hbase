{
  if (args.length != 2) {
    usage();
    System.exit(1);
  }
  Configuration conf=new Configuration();
  Path path=new Path(args[0]);
  FileSystem fs=path.getFileSystem(conf);
  try {
    doSmokeTest(fs,path,args[1]);
  }
  finally {
    fs.delete(path,false);
  }
  System.out.println("SUCCESS");
}
