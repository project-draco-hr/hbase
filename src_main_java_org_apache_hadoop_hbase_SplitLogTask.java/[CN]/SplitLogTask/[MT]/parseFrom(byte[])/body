{
  ProtobufUtil.expectPBMagicPrefix(data);
  try {
    int prefixLen=ProtobufUtil.lengthOfPBMagic();
    ZooKeeperProtos.SplitLogTask slt=ZooKeeperProtos.SplitLogTask.newBuilder().mergeFrom(data,prefixLen,data.length - prefixLen).build();
    return new SplitLogTask(slt);
  }
 catch (  InvalidProtocolBufferException e) {
    throw new DeserializationException(Bytes.toStringBinary(data,0,64),e);
  }
}
