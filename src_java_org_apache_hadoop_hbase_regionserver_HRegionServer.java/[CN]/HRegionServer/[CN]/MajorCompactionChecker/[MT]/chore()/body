{
  Set<Integer> keys=this.instance.onlineRegions.keySet();
  for (  Integer i : keys) {
    HRegion r=this.instance.onlineRegions.get(i);
    try {
      if (r != null && r.isMajorCompaction()) {
        this.instance.compactSplitThread.compactionRequested(r,getName() + " requests major compaction");
      }
    }
 catch (    IOException e) {
      LOG.warn("Failed major compaction check on " + r,e);
    }
  }
}
