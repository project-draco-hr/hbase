{
  HRegion region=null;
  this.lock.readLock().lock();
  try {
    Integer key=Integer.valueOf(Bytes.hashCode(regionName));
    region=onlineRegions.get(key);
    if (region == null) {
      throw new NotServingRegionException(regionName);
    }
    return region;
  }
  finally {
    this.lock.readLock().unlock();
  }
}
