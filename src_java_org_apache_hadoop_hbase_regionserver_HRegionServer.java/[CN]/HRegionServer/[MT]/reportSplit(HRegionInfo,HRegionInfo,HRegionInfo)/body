{
  outboundMsgs.add(new HMsg(HMsg.Type.MSG_REPORT_SPLIT,oldRegion,(oldRegion.getRegionNameAsString() + " split; daughters: " + newRegionA.getRegionNameAsString()+ ", "+ newRegionB.getRegionNameAsString()).getBytes()));
  outboundMsgs.add(new HMsg(HMsg.Type.MSG_REPORT_OPEN,newRegionA));
  outboundMsgs.add(new HMsg(HMsg.Type.MSG_REPORT_OPEN,newRegionB));
}
