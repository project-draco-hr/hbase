{
  boolean stop=false;
  if (e instanceof OutOfMemoryError || (e.getCause() != null && e.getCause() instanceof OutOfMemoryError) || (e.getMessage() != null && e.getMessage().contains("java.lang.OutOfMemoryError"))) {
    LOG.fatal("OutOfMemoryError, aborting.",e);
    abort();
    stop=true;
  }
  return stop;
}
