{
  if (this.fsOk && fs != null) {
    try {
      FSUtils.checkFileSystemAvailable(fs);
    }
 catch (    IOException e) {
      LOG.fatal("Shutting down HRegionServer: file system not available",e);
      this.abortRequested=true;
      this.stopRequested.set(true);
      fsOk=false;
    }
  }
  return this.fsOk;
}
