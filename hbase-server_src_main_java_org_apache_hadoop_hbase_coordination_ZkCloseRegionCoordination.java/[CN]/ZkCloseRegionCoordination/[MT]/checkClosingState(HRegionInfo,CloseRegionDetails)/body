{
  ZkCloseRegionDetails zkCrd=(ZkCloseRegionDetails)crd;
  try {
    return zkCrd.isPublishStatusInZk() && !ZKAssign.checkClosingState(watcher,regionInfo,((ZkCloseRegionDetails)crd).getExpectedVersion());
  }
 catch (  KeeperException ke) {
    csm.getServer().abort("Unrecoverable exception while checking state with zk " + regionInfo.getRegionNameAsString() + ", still finishing close",ke);
    throw new RuntimeException(ke);
  }
}
