{
  out.writeUTF(this.methodName);
  out.writeInt(parameterClasses.length);
  for (int i=0; i < parameterClasses.length; i++) {
    HbaseObjectWritable.writeObject(out,parameters[i],parameters[i] != null ? parameters[i].getClass() : parameterClasses[i],conf);
    out.writeUTF(parameterClasses[i].getName());
  }
  Bytes.writeByteArray(out,referenceRow);
  out.writeUTF(protocol.getName());
}
