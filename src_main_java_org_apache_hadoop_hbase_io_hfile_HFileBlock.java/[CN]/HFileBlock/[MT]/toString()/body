{
  return "blockType=" + blockType + ", onDiskSizeWithoutHeader="+ onDiskSizeWithoutHeader+ ", uncompressedSizeWithoutHeader="+ uncompressedSizeWithoutHeader+ ", prevBlockOffset="+ prevBlockOffset+ ", dataBeginsWith="+ Bytes.toStringBinary(buf.array(),buf.arrayOffset() + headerSize(),Math.min(32,buf.limit() - buf.arrayOffset() - headerSize()))+ ", fileOffset="+ offset;
}
