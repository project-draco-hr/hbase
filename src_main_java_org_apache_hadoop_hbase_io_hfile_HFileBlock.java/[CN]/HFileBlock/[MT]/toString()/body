{
  return "blockType=" + blockType + ", onDiskSizeWithoutHeader="+ onDiskSizeWithoutHeader+ ", uncompressedSizeWithoutHeader="+ uncompressedSizeWithoutHeader+ ", prevBlockOffset="+ prevBlockOffset+ ", dataBeginsWith="+ Bytes.toStringBinary(buf.array(),buf.arrayOffset() + HEADER_SIZE,Math.min(32,buf.limit() - buf.arrayOffset() - HEADER_SIZE))+ ", fileOffset="+ offset;
}
