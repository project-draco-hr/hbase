{
  PrefetchedHeader prefetchedHeader=prefetchedHeaderForThread.get();
  prefetchedHeader.offset=offset + b.getOnDiskSizeWithHeader();
  int nextHeaderOffset=b.buf.arrayOffset() + HEADER_SIZE + b.uncompressedSizeWithoutHeader;
  System.arraycopy(b.buf.array(),nextHeaderOffset,prefetchedHeader.header,0,HEADER_SIZE);
}
