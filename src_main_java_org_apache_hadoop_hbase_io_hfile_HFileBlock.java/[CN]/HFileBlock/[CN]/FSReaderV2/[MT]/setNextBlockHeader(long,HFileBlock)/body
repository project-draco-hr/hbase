{
  PrefetchedHeader prefetchedHeader=prefetchedHeaderForThread.get();
  prefetchedHeader.offset=offset + b.getOnDiskSizeWithHeader();
  int nextHeaderOffset=b.buf.arrayOffset() + hdrSize + b.uncompressedSizeWithoutHeader+ b.totalChecksumBytes();
  System.arraycopy(b.buf.array(),nextHeaderOffset,prefetchedHeader.header,0,hdrSize);
}
