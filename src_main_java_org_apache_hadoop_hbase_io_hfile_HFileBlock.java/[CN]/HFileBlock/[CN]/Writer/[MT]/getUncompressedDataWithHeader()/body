{
  expectState(State.BLOCK_READY);
  if (compressAlgo == NONE)   return onDiskBytesWithHeader;
  if (!cacheOnWrite)   throw new IllegalStateException("Cache-on-write is turned off");
  if (uncompressedBytesWithHeader == null)   throw new NullPointerException();
  return uncompressedBytesWithHeader;
}
