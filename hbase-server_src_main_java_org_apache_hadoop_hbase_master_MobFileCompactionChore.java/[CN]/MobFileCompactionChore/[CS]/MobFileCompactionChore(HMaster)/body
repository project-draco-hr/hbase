{
  super(master.getServerName() + "-MobFileCompactChore",master.getConfiguration().getInt(MobConstants.MOB_FILE_COMPACTION_CHORE_PERIOD,MobConstants.DEFAULT_MOB_FILE_COMPACTION_CHORE_PERIOD),master);
  this.master=master;
