{
  try {
    VisibilityClient.addLabels(util.getConfiguration(),LABELS);
    VisibilityClient.setAuths(util.getConfiguration(),LABELS,User.getCurrent().getName());
    VisibilityClient.setAuths(util.getConfiguration(),new String[]{CONFIDENTIAL,TOPSECRET,SECRET,PRIVATE},ADMIN.getName());
    VisibilityClient.setAuths(util.getConfiguration(),new String[]{PUBLIC},NORMAL_USER.getName());
  }
 catch (  Throwable t) {
    throw new IOException(t);
  }
}
