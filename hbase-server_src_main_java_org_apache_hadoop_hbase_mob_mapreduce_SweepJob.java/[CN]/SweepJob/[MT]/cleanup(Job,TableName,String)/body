{
  if (job != null) {
    Path workingPath=new Path(job.getConfiguration().get(WORKING_DIR_KEY));
    try {
      fs.delete(workingPath,true);
    }
 catch (    IOException e) {
      LOG.warn("Failed to delete the working directory after sweeping store " + familyName + " in the table "+ tn.getNameAsString(),e);
    }
  }
}
