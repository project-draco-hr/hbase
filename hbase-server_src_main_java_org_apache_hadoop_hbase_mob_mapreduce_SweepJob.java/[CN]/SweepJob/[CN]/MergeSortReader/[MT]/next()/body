{
  IndexedResult result=results.poll();
  if (result != null) {
    SequenceFile.Reader reader=readers.get(result.getIndex());
    String key=(String)reader.next((String)null);
    if (key != null) {
      results.add(new IndexedResult(result.getIndex(),key));
    }
    return result.getValue();
  }
  return null;
}
