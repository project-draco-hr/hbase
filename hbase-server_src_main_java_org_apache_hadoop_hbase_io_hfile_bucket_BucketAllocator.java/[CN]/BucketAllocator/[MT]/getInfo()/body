{
  StringBuilder sb=new StringBuilder(1024);
  for (int i=0; i < buckets.length; ++i) {
    Bucket b=buckets[i];
    sb.append("    Bucket ").append(i).append(": ").append(b.itemAllocationSize());
    sb.append(" freeCount=").append(b.freeCount()).append(" used=").append(b.usedCount());
    sb.append('\n');
  }
  return sb.toString();
}
