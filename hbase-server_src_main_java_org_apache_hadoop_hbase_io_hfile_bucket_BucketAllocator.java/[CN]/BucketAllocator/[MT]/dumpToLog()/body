{
  logStatistics();
  StringBuilder sb=new StringBuilder();
  for (  Bucket b : buckets) {
    sb.append("Bucket:").append(b.baseOffset).append('\n');
    sb.append("  Size index: " + b.sizeIndex() + "; Free:"+ b.freeCount+ "; used:"+ b.usedCount+ "; freelist\n");
    for (int i=0; i < b.freeCount(); ++i)     sb.append(b.freeList[i]).append(',');
    sb.append('\n');
  }
  LOG.info(sb);
}
