{
  for (  Map.Entry<Text,byte[]> e : m.entrySet()) {
    outputter.startTag(COLUMN);
    doElement(outputter,"name",org.apache.hadoop.hbase.util.Base64.encodeBytes(e.getKey().getBytes()));
    doElement(outputter,"value",org.apache.hadoop.hbase.util.Base64.encodeBytes(e.getValue()));
    outputter.endTag();
  }
}
