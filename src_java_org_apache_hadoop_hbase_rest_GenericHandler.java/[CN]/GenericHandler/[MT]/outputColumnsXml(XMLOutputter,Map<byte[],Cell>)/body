{
  for (  Map.Entry<byte[],Cell> e : m.entrySet()) {
    outputter.startTag(COLUMN);
    doElement(outputter,"name",org.apache.hadoop.hbase.util.Base64.encodeBytes(e.getKey()));
    doElement(outputter,"value",org.apache.hadoop.hbase.util.Base64.encodeBytes(e.getValue().getValue()));
    outputter.endTag();
  }
}
