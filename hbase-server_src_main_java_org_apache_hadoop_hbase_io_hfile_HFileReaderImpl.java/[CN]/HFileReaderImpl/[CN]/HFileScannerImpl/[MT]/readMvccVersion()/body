{
  if (this.reader.shouldIncludeMemstoreTS()) {
    if (this.reader.isDecodeMemstoreTS()) {
      currMemstoreTS=Bytes.readAsVLong(blockBuffer.array(),blockBuffer.arrayOffset() + blockBuffer.position());
      currMemstoreTSLen=WritableUtils.getVIntSize(currMemstoreTS);
    }
 else {
      currMemstoreTS=0;
      currMemstoreTSLen=1;
    }
  }
}
