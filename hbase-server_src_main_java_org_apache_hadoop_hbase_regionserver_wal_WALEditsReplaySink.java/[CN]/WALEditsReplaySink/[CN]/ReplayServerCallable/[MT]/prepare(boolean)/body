{
  if (!reload)   return;
  boolean skip=false;
  for (  HLog.Entry entry : this.entries) {
    WALEdit edit=entry.getEdit();
    List<Cell> cells=edit.getCells();
    for (    Cell cell : cells) {
      setLocation(conn.locateRegion(tableName,cell.getRow()));
      skip=true;
      break;
    }
    if (skip)     break;
  }
}
