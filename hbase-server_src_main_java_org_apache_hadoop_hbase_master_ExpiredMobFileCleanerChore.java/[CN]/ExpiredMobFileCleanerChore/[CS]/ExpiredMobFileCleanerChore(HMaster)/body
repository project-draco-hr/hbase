{
  super(master.getServerName() + "-ExpiredMobFileCleanerChore",master,master.getConfiguration().getInt(MobConstants.MOB_CLEANER_PERIOD,MobConstants.DEFAULT_MOB_CLEANER_PERIOD));
  this.master=master;
  this.tableLockManager=master.getTableLockManager();
  cleaner=new ExpiredMobFileCleaner();
  cleaner.setConf(master.getConfiguration());
}
