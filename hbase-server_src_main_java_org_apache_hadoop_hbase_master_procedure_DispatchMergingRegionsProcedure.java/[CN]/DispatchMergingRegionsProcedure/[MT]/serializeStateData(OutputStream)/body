{
  super.serializeStateData(stream);
  MasterProcedureProtos.DispatchMergingRegionsStateData.Builder dispatchMergingRegionsMsg=MasterProcedureProtos.DispatchMergingRegionsStateData.newBuilder().setUserInfo(MasterProcedureUtil.toProtoUserInfo(user)).setTableName(ProtobufUtil.toProtoTableName(tableName)).setForcible(forcible);
  for (  HRegionInfo hri : regionsToMerge) {
    dispatchMergingRegionsMsg.addRegionInfo(HRegionInfo.convert(hri));
  }
  dispatchMergingRegionsMsg.build().writeDelimitedTo(stream);
}
