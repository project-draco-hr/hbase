{
  if (source.hasArray() && destination.hasArray()) {
    System.arraycopy(source.array(),sourceOffset + source.arrayOffset(),destination.array(),destination.position() + destination.arrayOffset(),length);
    skip(destination,length);
  }
 else {
    for (int i=0; i < length; ++i) {
      destination.put(source.get(sourceOffset + i));
    }
  }
}
