{
  for (int i=0; i < currentToken.length(); i++) {
    final char c=currentToken.charAt(i);
    if (('a' <= c && c <= 'z') || ('A' <= c && c <= 'Z') || ('0' <= c && c <= '9')|| (c == '_')|| (c == '.')) {
    }
 else {
      throw parseException("Expected identifier. Found '" + currentToken + "'");
    }
  }
  final String result=currentToken;
  nextToken();
  return result;
}
