{
  if (rc != 0) {
    Long retry_count=(Long)ctx;
    LOG.warn("rc=" + KeeperException.Code.get(rc) + " for "+ path+ " remaining retries="+ retry_count);
    if (retry_count == 0) {
      createRescanFailure();
    }
 else {
      createRescanNode(retry_count - 1);
    }
    return;
  }
  createRescanSuccess(name);
}
