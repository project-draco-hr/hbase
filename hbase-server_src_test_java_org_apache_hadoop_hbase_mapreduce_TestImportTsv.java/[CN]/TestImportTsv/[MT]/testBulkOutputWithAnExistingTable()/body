{
  String table="test-" + UUID.randomUUID();
  Path hfiles=new Path(util.getDataTestDirOnTestFS(table),"hfiles");
  String[] args=new String[]{"-D" + ImportTsv.COLUMNS_CONF_KEY + "=HBASE_ROW_KEY,FAM:A,FAM:B","-D" + ImportTsv.SEPARATOR_CONF_KEY + "=\u001b","-D" + ImportTsv.BULK_OUTPUT_CONF_KEY + "="+ hfiles.toString(),table};
  util.createTable(table,FAMILY);
  doMROnTableTest(util,FAMILY,null,args,3);
  util.deleteTable(table);
}
