{
  List<JVMClusterUtil.MasterThread> mts;
  while (!(mts=getMasterThreads()).isEmpty()) {
    for (    JVMClusterUtil.MasterThread mt : mts) {
      if (mt.getMaster().isActiveMaster() && mt.getMaster().isInitialized()) {
        return true;
      }
    }
    Thread.sleep(100);
  }
  return false;
}
