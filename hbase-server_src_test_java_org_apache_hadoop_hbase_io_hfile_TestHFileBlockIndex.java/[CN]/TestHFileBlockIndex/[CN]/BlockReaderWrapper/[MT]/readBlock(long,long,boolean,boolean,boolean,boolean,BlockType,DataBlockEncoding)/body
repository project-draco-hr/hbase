{
  if (offset == prevOffset && onDiskSize == prevOnDiskSize && pread == prevPread) {
    hitCount+=1;
    return prevBlock;
  }
  missCount+=1;
  prevBlock=realReader.readBlockData(offset,onDiskSize,-1,pread);
  prevOffset=offset;
  prevOnDiskSize=onDiskSize;
  prevPread=pread;
  return prevBlock;
}
