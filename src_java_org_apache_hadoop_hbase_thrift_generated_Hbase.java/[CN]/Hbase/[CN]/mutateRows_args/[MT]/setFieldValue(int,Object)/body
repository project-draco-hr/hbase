{
switch (fieldID) {
case TABLENAME:
    if (value == null) {
      unsetTableName();
    }
 else {
      setTableName((byte[])value);
    }
  break;
case ROWBATCHES:
if (value == null) {
  unsetRowBatches();
}
 else {
  setRowBatches((List<BatchMutation>)value);
}
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
