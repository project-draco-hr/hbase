{
switch (fieldID) {
case TABLENAME:
    if (value == null) {
      unsetTableName();
    }
 else {
      setTableName((byte[])value);
    }
  break;
case ROW:
if (value == null) {
  unsetRow();
}
 else {
  setRow((byte[])value);
}
break;
case MUTATIONS:
if (value == null) {
unsetMutations();
}
 else {
setMutations((List<Mutation>)value);
}
break;
case TIMESTAMP:
if (value == null) {
unsetTimestamp();
}
 else {
setTimestamp((Long)value);
}
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
