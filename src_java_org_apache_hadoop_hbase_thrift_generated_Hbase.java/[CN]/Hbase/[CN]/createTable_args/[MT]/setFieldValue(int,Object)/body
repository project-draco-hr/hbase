{
switch (fieldID) {
case TABLENAME:
    if (value == null) {
      unsetTableName();
    }
 else {
      setTableName((byte[])value);
    }
  break;
case COLUMNFAMILIES:
if (value == null) {
  unsetColumnFamilies();
}
 else {
  setColumnFamilies((List<ColumnDescriptor>)value);
}
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
