{
  oprot_.writeMessageBegin(new TMessage("atomicIncrement",TMessageType.CALL,seqid_));
  atomicIncrement_args args=new atomicIncrement_args();
  args.tableName=tableName;
  args.row=row;
  args.column=column;
  args.value=value;
  args.write(oprot_);
  oprot_.writeMessageEnd();
  oprot_.getTransport().flush();
}
