{
switch (field) {
case TABLE_NAME:
    if (value == null) {
      unsetTableName();
    }
 else {
      setTableName((byte[])value);
    }
  break;
case ROW:
if (value == null) {
  unsetRow();
}
 else {
  setRow((byte[])value);
}
break;
case COLUMNS:
if (value == null) {
unsetColumns();
}
 else {
setColumns((List<byte[]>)value);
}
break;
case TIMESTAMP:
if (value == null) {
unsetTimestamp();
}
 else {
setTimestamp((Long)value);
}
break;
}
}
