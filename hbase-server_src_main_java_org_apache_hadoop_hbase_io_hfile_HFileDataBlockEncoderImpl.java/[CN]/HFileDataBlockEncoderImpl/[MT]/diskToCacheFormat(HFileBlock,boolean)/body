{
  if (block.getBlockType() == BlockType.DATA) {
    if (!useEncodedScanner(isCompaction)) {
      return block;
    }
    return encodeDataBlock(block,inCache,block.doesIncludeMemstoreTS(),inCacheEncodeCtx);
  }
  if (block.getBlockType() == BlockType.ENCODED_DATA) {
    if (block.getDataBlockEncodingId() == onDisk.getId()) {
      return block;
    }
    throw new AssertionError("Expected on-disk data block encoding " + onDisk + ", got "+ block.getDataBlockEncoding());
  }
  return block;
}
