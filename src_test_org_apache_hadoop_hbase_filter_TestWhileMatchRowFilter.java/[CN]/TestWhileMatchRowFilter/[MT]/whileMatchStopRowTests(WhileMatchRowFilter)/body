{
  RowFilterInterface innerFilter=filter.getInternalFilter();
  String toTest;
  toTest="apples";
  assertFalse("filter: '" + toTest + "'",filter.filterRowKey(Bytes.toBytes(toTest)));
  byte[] toTestBytes=Bytes.toBytes(toTest);
  assertFalse("innerFilter: '" + toTest + "'",innerFilter.filterRowKey(toTestBytes,0,toTestBytes.length));
  toTest="tuna";
  toTestBytes=Bytes.toBytes(toTest);
  assertTrue("filter: '" + toTest + "'",filter.filterRowKey(toTestBytes));
  assertTrue("innerFilter: '" + toTest + "'",innerFilter.filterRowKey(toTestBytes,0,toTestBytes.length));
  assertTrue("filter: filterAllRemaining",filter.filterAllRemaining());
  assertFalse("innerFilter: filterAllRemaining pre-reset",innerFilter.filterAllRemaining());
  filter.reset();
  assertFalse("filter: filterAllRemaining post-reset",filter.filterAllRemaining());
  assertFalse("filter: filterNotNull",filter.filterRow((List<KeyValue>)null));
}
