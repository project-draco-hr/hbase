{
  RowFilterInterface innerFilter=filter.getInternalFilter();
  String toTest;
  toTest="apples";
  assertFalse("filter: '" + toTest + "'",filter.filter(new Text(toTest)));
  assertFalse("innerFilter: '" + toTest + "'",innerFilter.filter(new Text(toTest)));
  toTest="tuna";
  assertTrue("filter: '" + toTest + "'",filter.filter(new Text(toTest)));
  assertTrue("innerFilter: '" + toTest + "'",innerFilter.filter(new Text(toTest)));
  assertTrue("filter: filterAllRemaining",filter.filterAllRemaining());
  assertFalse("innerFilter: filterAllRemaining pre-reset",innerFilter.filterAllRemaining());
  filter.reset();
  assertFalse("filter: filterAllRemaining post-reset",filter.filterAllRemaining());
  assertFalse("filter: filterNotNull",filter.filterNotNull(null));
}
