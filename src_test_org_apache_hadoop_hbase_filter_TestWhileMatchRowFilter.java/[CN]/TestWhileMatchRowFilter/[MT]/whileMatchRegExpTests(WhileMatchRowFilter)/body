{
  RowFilterInterface innerFilter=filter.getInternalFilter();
  String toTest;
  toTest="regex_match";
  assertFalse("filter: '" + toTest + "'",filter.filterRowKey(Bytes.toBytes(toTest)));
  assertFalse("innerFilter: '" + toTest + "'",innerFilter.filterRowKey(Bytes.toBytes(toTest)));
  toTest="not_a_match";
  assertTrue("filter: '" + toTest + "'",filter.filterRowKey(Bytes.toBytes(toTest)));
  assertTrue("innerFilter: '" + toTest + "'",innerFilter.filterRowKey(Bytes.toBytes(toTest)));
  assertTrue("filter: filterAllRemaining",filter.filterAllRemaining());
  assertFalse("innerFilter: filterAllRemaining pre-reset",innerFilter.filterAllRemaining());
  filter.reset();
  assertFalse("filter: filterAllRemaining post-reset",filter.filterAllRemaining());
  toTest="asdf_regex_hjkl";
  assertFalse("filter: '" + toTest + "'",filter.filterColumn(Bytes.toBytes(toTest),null,null));
}
