{
  MutableGaugeLong gaugeInt=gauges.get(gaugeName);
  if (gaugeInt == null) {
    MutableGaugeLong newGauge=HBaseMetricsFactory.newGauge(gaugeName,"",potentialStartingValue);
    gaugeInt=gauges.putIfAbsent(gaugeName,newGauge);
    if (gaugeInt == null) {
      gaugeInt=newGauge;
    }
  }
  return gaugeInt;
}
