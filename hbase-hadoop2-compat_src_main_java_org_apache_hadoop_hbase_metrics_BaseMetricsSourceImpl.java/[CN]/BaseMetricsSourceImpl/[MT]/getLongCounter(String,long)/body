{
  MutableCounterLong counter=counters.get(counterName);
  if (counter == null) {
    MutableCounterLong newCounter=HBaseMetricsFactory.newCounter(counterName,"",potentialStartingValue);
    counter=counters.putIfAbsent(counterName,newCounter);
    if (counter == null) {
      counter=newCounter;
    }
  }
  return counter;
}
