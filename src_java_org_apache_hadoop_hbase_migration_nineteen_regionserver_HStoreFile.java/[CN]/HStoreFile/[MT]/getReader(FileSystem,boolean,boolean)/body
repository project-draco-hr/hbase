{
  if (isReference()) {
    return new HalfMapFileReader(fs,getMapFilePath(reference).toString(),conf,reference.getFileRegion(),reference.getMidkey(),bloomFilter,blockCacheEnabled,this.hri);
  }
  return new BloomFilterMapFile.Reader(fs,getMapFilePath().toString(),conf,bloomFilter,blockCacheEnabled,this.hri);
}
