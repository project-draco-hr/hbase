{
  boolean bypass=false;
  ObserverContext<WALCoprocessorEnvironment> ctx=null;
  for (  WALEnvironment env : coprocessors) {
    if (env.getInstance() instanceof org.apache.hadoop.hbase.coprocessor.WALObserver) {
      ctx=ObserverContext.createAndPrepare(env,ctx);
      ((org.apache.hadoop.hbase.coprocessor.WALObserver)env.getInstance()).preWALWrite(ctx,info,logKey,logEdit);
      bypass|=ctx.shouldBypass();
      if (ctx.shouldComplete()) {
        break;
      }
    }
  }
  return bypass;
}
