{
switch (value) {
case 0:
    return OPENED;
case 1:
  return FAILED_OPEN;
case 2:
return CLOSED;
case 3:
return READY_TO_SPLIT;
case 4:
return READY_TO_MERGE;
case 5:
return SPLIT_PONR;
case 6:
return MERGE_PONR;
case 7:
return SPLIT;
case 8:
return MERGED;
case 9:
return SPLIT_REVERTED;
case 10:
return MERGE_REVERTED;
default :
return null;
}
}
