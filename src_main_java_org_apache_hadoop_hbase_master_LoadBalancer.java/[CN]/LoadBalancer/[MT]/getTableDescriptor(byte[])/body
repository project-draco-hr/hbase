{
  HTableDescriptor tableDescriptor=null;
  try {
    if (this.services != null) {
      tableDescriptor=this.services.getTableDescriptors().get(Bytes.toString(tableName));
    }
  }
 catch (  TableExistsException tee) {
    LOG.debug("TableExistsException during getTableDescriptors." + " Current table name = " + tableName,tee);
  }
catch (  FileNotFoundException fnfe) {
    LOG.debug("FileNotFoundException during getTableDescriptors." + " Current table name = " + tableName,fnfe);
  }
  return tableDescriptor;
}
