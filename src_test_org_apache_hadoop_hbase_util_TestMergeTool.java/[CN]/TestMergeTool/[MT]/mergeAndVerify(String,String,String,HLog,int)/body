{
  Merge merger=new Merge(this.conf);
  LOG.info(msg);
  int errCode=ToolRunner.run(merger,new String[]{this.desc.getNameAsString(),regionName1,regionName2});
  assertTrue("'" + msg + "' failed",errCode == 0);
  HRegionInfo mergedInfo=merger.getMergedHRegionInfo();
  HRegion merged=HRegion.openHRegion(mergedInfo,this.rootdir,log,this.conf);
  verifyMerge(merged,upperbound);
  merged.close();
  LOG.info("Verified " + msg);
  return merged;
}
