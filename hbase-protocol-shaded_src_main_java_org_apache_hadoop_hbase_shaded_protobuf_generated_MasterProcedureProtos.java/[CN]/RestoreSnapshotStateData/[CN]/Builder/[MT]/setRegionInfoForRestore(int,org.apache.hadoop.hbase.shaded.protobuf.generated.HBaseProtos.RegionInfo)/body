{
  if (regionInfoForRestoreBuilder_ == null) {
    if (value == null) {
      throw new NullPointerException();
    }
    ensureRegionInfoForRestoreIsMutable();
    regionInfoForRestore_.set(index,value);
    onChanged();
  }
 else {
    regionInfoForRestoreBuilder_.setMessage(index,value);
  }
  return this;
}
