{
  this.tableLock=this.tableLockManager.writeLock(tableName,EventType.C_M_DISABLE_TABLE.toString());
  this.tableLock.acquire();
  boolean success=false;
  try {
    if (!MetaReader.tableExists(catalogTracker,this.tableNameStr)) {
      throw new TableNotFoundException(this.tableNameStr);
    }
    if (!skipTableStateCheck) {
      try {
        if (!this.assignmentManager.getZKTable().checkEnabledAndSetDisablingTable(this.tableNameStr)) {
          LOG.info("Table " + tableNameStr + " isn't enabled; skipping disable");
          throw new TableNotEnabledException(this.tableNameStr);
        }
      }
 catch (      KeeperException e) {
        throw new IOException("Unable to ensure that the table will be" + " disabling because of a ZooKeeper issue",e);
      }
    }
    success=true;
  }
  finally {
    if (!success) {
      releaseTableLock();
    }
  }
  return this;
}
