{
  try {
    LOG.info("Attempting to disable table " + this.tableName);
    MasterCoprocessorHost cpHost=((HMaster)this.server).getMasterCoprocessorHost();
    if (cpHost != null) {
      cpHost.preDisableTableAction(this.tableName);
    }
    handleDisableTable();
    if (cpHost != null) {
      cpHost.postCompletedDisableTableAction(this.tableName);
    }
  }
 catch (  IOException e) {
    LOG.error("Error trying to disable table " + this.tableName,e);
  }
 finally {
    releaseTableLock();
  }
}
