{
  checkOpen();
  super.getRequestCount().incrementAndGet();
  try {
    getTransactionalRegion(regionName).abort(transactionId);
  }
 catch (  NotServingRegionException e) {
    LOG.info("Got not serving region durring abort. Ignoring.");
  }
catch (  IOException e) {
    checkFileSystem();
    throw e;
  }
}
