{
  super.setConf(conf);
  try {
    fs=FileSystem.get(conf);
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
  conf.set(HConstants.CRYPTO_KEYPROVIDER_CONF_KEY,KeyProviderForTesting.class.getName());
