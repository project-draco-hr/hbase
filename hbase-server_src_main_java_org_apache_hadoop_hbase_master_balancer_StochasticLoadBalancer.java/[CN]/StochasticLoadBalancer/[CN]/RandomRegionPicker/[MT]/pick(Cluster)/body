{
  int thisServer=pickRandomServer(cluster);
  int otherServer=pickOtherRandomServer(cluster,thisServer);
  Pair<Integer,Integer> regions=pickRandomRegions(cluster,thisServer,otherServer);
  return new Pair<Pair<Integer,Integer>,Pair<Integer,Integer>>(new Pair<Integer,Integer>(thisServer,regions.getFirst()),new Pair<Integer,Integer>(otherServer,regions.getSecond()));
}
