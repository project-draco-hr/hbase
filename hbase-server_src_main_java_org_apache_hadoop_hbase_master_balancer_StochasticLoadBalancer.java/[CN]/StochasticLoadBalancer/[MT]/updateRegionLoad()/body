{
  loads.clear();
  for (  ServerName sn : clusterStatus.getServers()) {
    ServerLoad sl=clusterStatus.getLoad(sn);
    if (sl == null)     continue;
    for (    Entry<byte[],RegionLoad> entry : sl.getRegionsLoad().entrySet()) {
      loads.put(Bytes.toString(entry.getKey()),entry.getValue());
    }
  }
}
