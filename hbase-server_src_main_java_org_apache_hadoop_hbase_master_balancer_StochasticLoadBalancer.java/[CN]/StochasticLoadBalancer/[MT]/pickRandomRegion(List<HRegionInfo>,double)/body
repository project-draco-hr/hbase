{
  if (regions.isEmpty() || RANDOM.nextFloat() < chanceOfNoSwap) {
    return null;
  }
  int count=0;
  HRegionInfo r=null;
  while (count < 10 && r == null) {
    count++;
    r=regions.get(RANDOM.nextInt(regions.size()));
    if (r.isMetaRegion()) {
      r=null;
    }
  }
  if (r != null) {
    regions.remove(r);
  }
  return r;
}
