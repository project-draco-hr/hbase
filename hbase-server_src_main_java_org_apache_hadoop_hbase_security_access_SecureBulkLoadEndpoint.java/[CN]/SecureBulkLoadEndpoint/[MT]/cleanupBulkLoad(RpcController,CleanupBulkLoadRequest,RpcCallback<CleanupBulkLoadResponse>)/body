{
  try {
    getAccessController().preCleanupBulkLoad(env);
    fs.delete(createStagingDir(baseStagingDir,getActiveUser(),env.getRegion().getTableDesc().getTableName(),new Path(request.getBulkToken()).getName()),true);
    done.run(CleanupBulkLoadResponse.newBuilder().build());
  }
 catch (  IOException e) {
    ResponseConverter.setControllerException(controller,e);
  }
  done.run(null);
}
