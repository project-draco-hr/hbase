{
  try {
    getAccessController().prePrepareBulkLoad(env);
    String bulkToken=createStagingDir(baseStagingDir,getActiveUser(),request.getTableName().toByteArray()).toString();
    done.run(PrepareBulkLoadResponse.newBuilder().setBulkToken(bulkToken).build());
  }
 catch (  IOException e) {
    ResponseConverter.setControllerException(controller,e);
  }
  done.run(null);
}
