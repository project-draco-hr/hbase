{
  if (this.superUsers.contains(user.getShortName())) {
    return true;
  }
  String[] groups=user.getGroupNames();
  if (groups != null && groups.length > 0) {
    for (    String group : groups) {
      if (this.superGroups.contains(group)) {
        return true;
      }
    }
  }
  return false;
}
