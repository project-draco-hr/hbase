{
  if (rowI != null && rowI.hasNext()) {
    return true;
  }
  if (cached != null) {
    return true;
  }
  try {
    Result result=scanner.next();
    if (result != null && !result.isEmpty()) {
      cached=result;
    }
  }
 catch (  UnknownScannerException e) {
    throw new IllegalArgumentException(e);
  }
catch (  IOException e) {
    LOG.error(StringUtils.stringifyException(e));
  }
  return cached != null;
}
