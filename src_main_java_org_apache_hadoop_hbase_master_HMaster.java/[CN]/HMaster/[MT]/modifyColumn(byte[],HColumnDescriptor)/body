{
  checkInitialized();
  if (cpHost != null) {
    if (cpHost.preModifyColumn(tableName,descriptor)) {
      return;
    }
  }
  new TableModifyFamilyHandler(tableName,descriptor,this,this,this,supportInstantSchemaChanges).process();
  if (cpHost != null) {
    cpHost.postModifyColumn(tableName,descriptor);
  }
}
