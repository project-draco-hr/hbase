{
  if (LOG.isDebugEnabled()) {
    LOG.debug("Stopping service threads");
  }
  this.rpcServer.stop();
  if (this.infoServer != null) {
    LOG.info("Stopping infoServer");
    try {
      this.infoServer.stop();
    }
 catch (    Exception ex) {
      ex.printStackTrace();
    }
  }
  this.executorService.shutdown();
}
