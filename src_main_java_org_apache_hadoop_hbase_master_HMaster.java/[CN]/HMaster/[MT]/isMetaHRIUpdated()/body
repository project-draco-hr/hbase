{
  boolean metaUpdated=false;
  Get get=new Get(HRegionInfo.FIRST_META_REGIONINFO.getRegionName());
  get.addColumn(HConstants.CATALOG_FAMILY,HConstants.META_MIGRATION_QUALIFIER);
  Result r=catalogTracker.waitForRootServerConnectionDefault().get(HRegionInfo.ROOT_REGIONINFO.getRegionName(),get);
  if (r != null && r.getBytes() != null) {
    byte[] metaMigrated=r.getValue(HConstants.CATALOG_FAMILY,HConstants.META_MIGRATION_QUALIFIER);
    String migrated=Bytes.toString(metaMigrated);
    metaUpdated=new Boolean(migrated).booleanValue();
  }
 else {
    LOG.info("metaUpdated = NULL.");
  }
  LOG.info("Meta updated status = " + metaUpdated);
  return metaUpdated;
}
