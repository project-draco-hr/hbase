{
  InetAddress ia=getRemoteInetAddress(port,serverStartCode);
  ServerName rs=this.serverManager.regionServerStartup(ia,port,serverStartCode,serverCurrentTime);
  MapWritable mw=createConfigurationSubset();
  mw.put(new Text(HConstants.KEY_FOR_HOSTNAME_SEEN_BY_MASTER),new Text(rs.getHostname()));
  return mw;
}
