{
  try {
    super.setUp();
    HTableDescriptor desc=new HTableDescriptor(getName());
    desc.addFamily(new HColumnDescriptor(HConstants.CATALOG_FAMILY));
    HBaseAdmin admin=new HBaseAdmin(conf);
    admin.createTable(desc);
  }
 catch (  Exception e) {
    e.printStackTrace();
    throw e;
  }
}
