{
  return new RpcCallback<ParameterType>(){
    private boolean alreadyCalled=false;
    @Override public void run(    final ParameterType parameter){
synchronized (this) {
        if (alreadyCalled) {
          throw new AlreadyCalledException();
        }
        alreadyCalled=true;
      }
      originalCallback.run(parameter);
    }
  }
;
}
