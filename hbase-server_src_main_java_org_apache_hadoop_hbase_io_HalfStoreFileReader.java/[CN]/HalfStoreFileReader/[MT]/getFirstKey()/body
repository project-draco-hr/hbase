{
  if (!firstKeySeeked) {
    HFileScanner scanner=getScanner(true,true,false);
    try {
      if (scanner.seekTo()) {
        this.firstKey=new KeyValue.KeyOnlyKeyValue(Bytes.toBytes(scanner.getKey()));
      }
      firstKeySeeked=true;
    }
 catch (    IOException e) {
      LOG.warn("Failed seekTo first KV in the file",e);
    }
  }
  return this.firstKey;
}
