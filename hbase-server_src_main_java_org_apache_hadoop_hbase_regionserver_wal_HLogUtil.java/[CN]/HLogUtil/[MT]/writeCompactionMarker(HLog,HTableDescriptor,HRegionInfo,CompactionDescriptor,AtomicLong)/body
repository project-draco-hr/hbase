{
  WALEdit e=WALEdit.createCompaction(c);
  long now=EnvironmentEdgeManager.currentTimeMillis();
  TableName tn=TableName.valueOf(c.getTableName().toByteArray());
  log.appendNoSync(info,tn,e,new ArrayList<UUID>(),now,htd,sequenceId,false,HConstants.NO_NONCE,HConstants.NO_NONCE);
  log.sync();
  if (LOG.isTraceEnabled()) {
    LOG.trace("Appended compaction marker " + TextFormat.shortDebugString(c));
  }
}
