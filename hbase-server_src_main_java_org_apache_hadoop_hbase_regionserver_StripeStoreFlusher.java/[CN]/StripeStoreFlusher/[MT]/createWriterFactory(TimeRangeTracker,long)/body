{
  return new StripeMultiFileWriter.WriterFactory(){
    @Override public StoreFileWriter createWriter() throws IOException {
      StoreFileWriter writer=store.createWriterInTmp(kvCount,store.getFamily().getCompressionType(),false,true,true,false);
      writer.setTimeRangeTracker(tracker);
      return writer;
    }
  }
;
}
