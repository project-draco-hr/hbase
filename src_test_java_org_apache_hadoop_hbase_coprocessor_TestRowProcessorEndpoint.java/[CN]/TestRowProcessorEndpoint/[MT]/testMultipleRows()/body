{
  prepareTestData();
  failures.set(0);
  int numThreads=1000;
  concurrentExec(new SwapRowsRunner(),numThreads);
  LOG.debug("row keyvalues:" + stringifyKvs(table.get(new Get(ROW)).list()));
  LOG.debug("row2 keyvalues:" + stringifyKvs(table.get(new Get(ROW2)).list()));
  assertEquals(rowSize,table.get(new Get(ROW)).list().size());
  assertEquals(row2Size,table.get(new Get(ROW2)).list().size());
  assertEquals(0,failures.get());
}
