{
  try {
    HColumnDescriptor.isLegalFamilyName(Bytes.toBytes(rd.getName()));
  }
 catch (  IllegalArgumentException iae) {
    return false;
  }
  try {
    return fs.getFileStatus(rd).isDir();
  }
 catch (  IOException ioe) {
    LOG.warn("Skipping file " + rd + " due to IOException",ioe);
    return false;
  }
}
