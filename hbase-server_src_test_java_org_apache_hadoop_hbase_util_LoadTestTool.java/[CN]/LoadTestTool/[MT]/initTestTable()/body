{
  Durability durability=Durability.USE_DEFAULT;
  if (deferredLogFlush) {
    durability=Durability.ASYNC_WAL;
  }
  HBaseTestingUtility.createPreSplitLoadTestTable(conf,tableName,COLUMN_FAMILY,compressAlgo,dataBlockEncodingAlgo,numRegionsPerServer,regionReplication,durability);
  applyColumnFamilyOptions(tableName,COLUMN_FAMILIES);
}
