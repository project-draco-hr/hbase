{
  if (masterCoprocessorsBuilder_ == null) {
    if (value == null) {
      throw new NullPointerException();
    }
    ensureMasterCoprocessorsIsMutable();
    masterCoprocessors_.set(index,value);
    onChanged();
  }
 else {
    masterCoprocessorsBuilder_.setMessage(index,value);
  }
  return this;
}
