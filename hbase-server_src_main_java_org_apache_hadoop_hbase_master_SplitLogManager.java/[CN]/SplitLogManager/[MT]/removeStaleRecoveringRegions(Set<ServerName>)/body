{
  Set<String> knownFailedServers=new HashSet<String>();
  if (failedServers != null) {
    for (    ServerName tmpServerName : failedServers) {
      knownFailedServers.add(tmpServerName.getServerName());
    }
  }
  this.recoveringRegionLock.lock();
  try {
    ((BaseCoordinatedStateManager)server.getCoordinatedStateManager()).getSplitLogManagerCoordination().removeStaleRecoveringRegions(knownFailedServers);
  }
  finally {
    this.recoveringRegionLock.unlock();
  }
}
