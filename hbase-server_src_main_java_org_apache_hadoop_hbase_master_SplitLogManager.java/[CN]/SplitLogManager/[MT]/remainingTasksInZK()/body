{
  int count=0;
  try {
    List<String> tasks=ZKUtil.listChildrenNoWatch(watcher,watcher.splitLogZNode);
    if (tasks != null) {
      for (      String t : tasks) {
        if (!ZKSplitLog.isRescanNode(watcher,t)) {
          count++;
        }
      }
    }
  }
 catch (  KeeperException ke) {
    LOG.warn("Failed to check remaining tasks",ke);
    count=-1;
  }
  return count;
}
