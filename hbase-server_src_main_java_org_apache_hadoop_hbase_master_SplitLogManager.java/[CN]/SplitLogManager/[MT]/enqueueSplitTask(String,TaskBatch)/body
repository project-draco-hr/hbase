{
  lastTaskCreateTime=EnvironmentEdgeManager.currentTime();
  String task=((BaseCoordinatedStateManager)server.getCoordinatedStateManager()).getSplitLogManagerCoordination().prepareTask(taskname);
  Task oldtask=createTaskIfAbsent(task,batch);
  if (oldtask == null) {
    ((BaseCoordinatedStateManager)server.getCoordinatedStateManager()).getSplitLogManagerCoordination().submitTask(task);
    return true;
  }
  return false;
}
