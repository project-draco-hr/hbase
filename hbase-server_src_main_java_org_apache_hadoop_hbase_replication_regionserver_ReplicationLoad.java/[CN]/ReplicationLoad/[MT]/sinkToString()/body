{
  if (this.replicationLoadSink == null)   return null;
  StringBuilder sb=new StringBuilder();
  sb=Strings.appendKeyValue(sb,"AgeOfLastAppliedOp",this.replicationLoadSink.getAgeOfLastAppliedOp());
  sb=Strings.appendKeyValue(sb,"TimeStampsOfLastAppliedOp",(new Date(this.replicationLoadSink.getTimeStampsOfLastAppliedOp()).toString()));
  return sb.toString();
}
