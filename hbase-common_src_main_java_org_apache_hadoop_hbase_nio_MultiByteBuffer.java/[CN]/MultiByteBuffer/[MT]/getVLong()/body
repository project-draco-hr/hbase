{
  byte firstByte=get();
  int len=WritableUtils.decodeVIntSize(firstByte);
  if (len == 1) {
    return firstByte;
  }
  long i=0;
  byte b;
  for (int idx=0; idx < len - 1; idx++) {
    b=get();
    i=i << 8;
    i=i | (b & 0xFF);
  }
  return (WritableUtils.isNegativeVInt(firstByte) ? (i ^ -1L) : i);
}
