{
  if (this.singleItem) {
    this.curItem.position(this.curItem.position() + length);
    return;
  }
  int jump=0;
  while (true) {
    jump=this.curItem.remaining();
    if (jump >= length) {
      this.curItem.position(this.curItem.position() + length);
      break;
    }
    this.curItem.position(this.curItem.position() + jump);
    length-=jump;
    this.curItemIndex++;
    this.curItem=this.items[this.curItemIndex];
  }
}
