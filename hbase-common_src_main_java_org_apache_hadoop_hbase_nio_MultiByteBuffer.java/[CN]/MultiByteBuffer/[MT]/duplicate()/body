{
  if (this.singleItem) {
    return new MultiByteBuffer(new ByteBuffer[]{curItem.duplicate()},this.itemBeginPos,this.limit,this.limitedItemIndex,this.curItemIndex,this.markedItemIndex);
  }
  ByteBuffer[] itemsCopy=new ByteBuffer[this.items.length];
  for (int i=0; i < this.items.length; i++) {
    itemsCopy[i]=items[i].duplicate();
  }
  return new MultiByteBuffer(itemsCopy,this.itemBeginPos,this.limit,this.limitedItemIndex,this.curItemIndex,this.markedItemIndex);
}
