{
  CompactionDescriptor.Builder builder=CompactionDescriptor.newBuilder().setTableName(ByteStringer.wrap(info.getTableName())).setEncodedRegionName(ByteStringer.wrap(info.getEncodedNameAsBytes())).setFamilyName(ByteStringer.wrap(family)).setStoreHomeDir(storeDir.getName());
  for (  Path inputPath : inputPaths) {
    builder.addCompactionInput(inputPath.getName());
  }
  for (  Path outputPath : outputPaths) {
    builder.addCompactionOutput(outputPath.getName());
  }
  builder.setRegionName(ByteStringer.wrap(info.getRegionName()));
  return builder.build();
}
