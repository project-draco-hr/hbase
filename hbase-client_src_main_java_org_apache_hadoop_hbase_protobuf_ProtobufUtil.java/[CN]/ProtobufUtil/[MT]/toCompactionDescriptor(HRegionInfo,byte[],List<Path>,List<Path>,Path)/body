{
  CompactionDescriptor.Builder builder=CompactionDescriptor.newBuilder().setTableName(ByteString.copyFrom(info.getTableName())).setEncodedRegionName(ByteString.copyFrom(info.getEncodedNameAsBytes())).setFamilyName(ByteString.copyFrom(family)).setStoreHomeDir(storeDir.getName());
  for (  Path inputPath : inputPaths) {
    builder.addCompactionInput(inputPath.getName());
  }
  for (  Path outputPath : outputPaths) {
    builder.addCompactionOutput(outputPath.getName());
  }
  return builder.build();
}
