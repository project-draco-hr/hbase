{
  t.setAutoFlush(false,true);
  byte[] k=new byte[3];
  int rowCount=0;
  for (byte b1='a'; b1 <= 'z'; b1++) {
    for (byte b2='a'; b2 <= 'z'; b2++) {
      for (byte b3='a'; b3 <= 'z'; b3++) {
        k[0]=b1;
        k[1]=b2;
        k[2]=b3;
        Put put=new Put(k);
        for (int i=0; i < f.length; i++) {
          put.add(f[i],null,k);
        }
        t.put(put);
        rowCount++;
      }
    }
  }
  t.flushCommits();
  return rowCount;
}
