{
  t.setAutoFlush(false);
  int rowCount=0;
  for (  byte[] row : HBaseTestingUtility.ROWS) {
    Put put=new Put(row);
    put.setDurability(writeToWAL ? Durability.USE_DEFAULT : Durability.SKIP_WAL);
    for (int i=0; i < f.length; i++) {
      put.add(f[i],null,value != null ? value : row);
    }
    t.put(put);
    rowCount++;
  }
  t.flushCommits();
  return rowCount;
}
