{
  if (hbaseAdmin != null) {
    hbaseAdmin.close();
    hbaseAdmin=null;
  }
  if (zooKeeperWatcher != null) {
    zooKeeperWatcher.close();
    zooKeeperWatcher=null;
  }
  if (this.hbaseCluster != null) {
    this.hbaseCluster.shutdown();
    this.hbaseCluster.join();
    this.hbaseCluster=null;
  }
}
