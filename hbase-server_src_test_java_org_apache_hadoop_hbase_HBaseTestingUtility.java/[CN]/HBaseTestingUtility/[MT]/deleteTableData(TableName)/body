{
  HTable table=(HTable)getConnection().getTable(tableName);
  Scan scan=new Scan();
  ResultScanner resScan=table.getScanner(scan);
  for (  Result res : resScan) {
    Delete del=new Delete(res.getRow());
    table.delete(del);
  }
  resScan=table.getScanner(scan);
  resScan.close();
  return table;
}
