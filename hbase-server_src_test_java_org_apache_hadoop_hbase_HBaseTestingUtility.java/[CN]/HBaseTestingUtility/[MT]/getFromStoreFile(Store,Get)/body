{
  MultiVersionConsistencyControl.resetThreadReadPoint();
  Scan scan=new Scan(get);
  InternalScanner scanner=(InternalScanner)store.getScanner(scan,scan.getFamilyMap().get(store.getFamily().getName()));
  List<KeyValue> result=new ArrayList<KeyValue>();
  scanner.next(result);
  if (!result.isEmpty()) {
    KeyValue kv=result.get(0);
    if (!Bytes.equals(kv.getRow(),get.getRow())) {
      result.clear();
    }
  }
  scanner.close();
  return result;
}
