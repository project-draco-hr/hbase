{
  if (field == null) {
    throw new IllegalArgumentException();
  }
switch (field) {
case FAMILY:
    return isSetFamily();
case QUALIFIER:
  return isSetQualifier();
case VALUE:
return isSetValue();
case TIMESTAMP:
return isSetTimestamp();
case TAGS:
return isSetTags();
}
throw new IllegalStateException();
}
