{
  int begin=minCellIdx;
  int end=maxCellIdx - 1;
  while (begin <= end) {
    int mid=(begin + end) >>> 1;
    Cell midCell=getCell(mid);
    int compareRes=comparator.compare(midCell,needle);
    if (compareRes == 0) {
      return mid;
    }
    if (compareRes < 0) {
      begin=mid + 1;
    }
 else {
      end=mid - 1;
    }
  }
  return (-1 * begin) - 1;
}
