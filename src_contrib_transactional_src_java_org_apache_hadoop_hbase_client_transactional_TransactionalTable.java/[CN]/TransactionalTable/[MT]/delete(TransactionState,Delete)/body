{
  super.getConnection().getRegionServerWithRetries(new TransactionalServerCallable<Object>(super.getConnection(),super.getTableName(),delete.getRow(),transactionState){
    public Object call() throws IOException {
      recordServer();
      getTransactionServer().delete(transactionState.getTransactionId(),location.getRegionInfo().getRegionName(),delete);
      return null;
    }
  }
);
}
