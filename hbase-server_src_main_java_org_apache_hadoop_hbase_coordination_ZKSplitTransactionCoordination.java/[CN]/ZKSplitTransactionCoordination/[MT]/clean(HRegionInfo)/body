{
  try {
    if (!ZKAssign.deleteNode(coordinationManager.getServer().getZooKeeper(),hri.getEncodedName(),RS_ZK_REQUEST_REGION_SPLIT,coordinationManager.getServer().getServerName())) {
      ZKAssign.deleteNode(coordinationManager.getServer().getZooKeeper(),hri.getEncodedName(),RS_ZK_REGION_SPLITTING,coordinationManager.getServer().getServerName());
    }
  }
 catch (  KeeperException.NoNodeException e) {
    LOG.info("Failed cleanup zk node of " + hri.getRegionNameAsString(),e);
  }
catch (  KeeperException e) {
    coordinationManager.getServer().abort("Failed cleanup of " + hri.getRegionNameAsString(),e);
  }
}
