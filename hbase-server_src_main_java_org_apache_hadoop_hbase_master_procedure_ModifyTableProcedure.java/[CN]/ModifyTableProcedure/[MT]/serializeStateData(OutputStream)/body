{
  super.serializeStateData(stream);
  MasterProcedureProtos.ModifyTableStateData.Builder modifyTableMsg=MasterProcedureProtos.ModifyTableStateData.newBuilder().setUserInfo(MasterProcedureUtil.toProtoUserInfo(user)).setModifiedTableSchema(modifiedHTableDescriptor.convert()).setDeleteColumnFamilyInModify(deleteColumnFamilyInModify);
  if (unmodifiedHTableDescriptor != null) {
    modifyTableMsg.setUnmodifiedTableSchema(unmodifiedHTableDescriptor.convert());
  }
  modifyTableMsg.build().writeDelimitedTo(stream);
}
