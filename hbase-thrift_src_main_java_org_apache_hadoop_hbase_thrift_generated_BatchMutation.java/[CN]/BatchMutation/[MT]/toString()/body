{
  StringBuilder sb=new StringBuilder("BatchMutation(");
  boolean first=true;
  sb.append("row:");
  if (this.row == null) {
    sb.append("null");
  }
 else {
    org.apache.thrift.TBaseHelper.toString(this.row,sb);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("mutations:");
  if (this.mutations == null) {
    sb.append("null");
  }
 else {
    sb.append(this.mutations);
  }
  first=false;
  sb.append(")");
  return sb.toString();
}
