{
  Map<byte[],byte[]> colCriteria=new TreeMap<byte[],byte[]>(Bytes.BYTES_COMPARATOR);
  for (int i=0; i < colKeys.length; i++) {
    colCriteria.put(colKeys[i],GOOD_BYTES);
  }
  RowFilterInterface filter=new RegExpRowFilter("[^aeiou]",colCriteria);
  Scanner scanner=table.getScanner(colKeys,new byte[]{FIRST_ROWKEY},filter);
  iterateOnScanner(scanner,"[^aei-qu]");
}
