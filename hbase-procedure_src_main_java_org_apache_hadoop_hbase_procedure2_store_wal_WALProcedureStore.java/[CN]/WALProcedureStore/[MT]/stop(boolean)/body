{
  if (!setRunning(false)) {
    return;
  }
  LOG.info("Stopping the WAL Procedure Store");
  sendStopSignal();
  if (!abort) {
    try {
      while (syncThread.isAlive()) {
        sendStopSignal();
        syncThread.join(250);
      }
    }
 catch (    InterruptedException e) {
      LOG.warn("join interrupted",e);
      Thread.currentThread().interrupt();
    }
  }
  closeStream();
  for (  ProcedureWALFile log : logs) {
    log.close();
  }
  logs.clear();
}
