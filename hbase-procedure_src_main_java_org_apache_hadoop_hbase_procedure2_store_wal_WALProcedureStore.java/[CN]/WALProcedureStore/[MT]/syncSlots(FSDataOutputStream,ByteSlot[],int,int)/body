{
  long totalSynced=0;
  for (int i=0; i < count; ++i) {
    ByteSlot data=slots[offset + i];
    data.writeTo(stream);
    totalSynced+=data.size();
  }
  stream.hsync();
  if (LOG.isTraceEnabled()) {
    LOG.trace("Sync slots=" + count + '/'+ slots.length+ " flushed="+ StringUtils.humanSize(totalSynced));
  }
  return totalSynced;
}
