{
  try {
    if (stream != null) {
      try {
        ProcedureWALFormat.writeTrailer(stream,storeTracker);
      }
 catch (      IOException e) {
        LOG.warn("Unable to write the trailer: " + e.getMessage());
      }
      stream.close();
    }
  }
 catch (  IOException e) {
    LOG.error("Unable to close the stream",e);
  }
 finally {
    stream=null;
  }
}
