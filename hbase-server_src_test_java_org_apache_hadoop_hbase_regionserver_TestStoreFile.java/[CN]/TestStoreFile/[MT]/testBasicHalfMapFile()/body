{
  Path outputDir=new Path(new Path(this.testDir,"regionname"),"familyname");
  StoreFile.Writer writer=new StoreFile.WriterBuilder(conf,cacheConf,this.fs,2 * 1024).withOutputDir(outputDir).build();
  writeStoreFile(writer);
  checkHalfHFile(new StoreFile(this.fs,writer.getPath(),conf,cacheConf,BloomType.NONE,NoOpDataBlockEncoder.INSTANCE));
}
