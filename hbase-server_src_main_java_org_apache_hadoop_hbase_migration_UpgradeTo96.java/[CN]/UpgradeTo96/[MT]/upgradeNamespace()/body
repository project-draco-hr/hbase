{
  LOG.info("Upgrading Namespace");
  try {
    int res=ToolRunner.run(getConf(),new NamespaceUpgrade(),new String[]{"--upgrade"});
    LOG.info("Successfully Upgraded NameSpace.");
    return res;
  }
 catch (  Exception e) {
    LOG.error("Got exception while upgrading Namespace",e);
    throw e;
  }
}
