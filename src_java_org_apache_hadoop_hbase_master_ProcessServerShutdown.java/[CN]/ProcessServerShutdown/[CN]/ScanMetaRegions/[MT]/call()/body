{
  if (LOG.isDebugEnabled()) {
    LOG.debug("process server shutdown scanning " + m.getRegionName() + " on "+ m.getServer());
  }
  long scannerId=server.openScanner(m.getRegionName(),COLUMN_FAMILY_ARRAY,EMPTY_START_ROW,System.currentTimeMillis(),null);
  scanMetaRegion(server,scannerId,m.getRegionName());
  return true;
}
