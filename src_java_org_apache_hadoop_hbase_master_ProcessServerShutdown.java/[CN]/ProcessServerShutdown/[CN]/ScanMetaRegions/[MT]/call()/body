{
  if (LOG.isDebugEnabled()) {
    LOG.debug("process server shutdown scanning " + Bytes.toString(m.getRegionName()) + " on "+ m.getServer());
  }
  Scan scan=new Scan();
  scan.addFamily(CATALOG_FAMILY);
  long scannerId=server.openScanner(HRegionInfo.ROOT_REGIONINFO.getRegionName(),scan);
  scanMetaRegion(server,scannerId,m.getRegionName());
  return true;
}
