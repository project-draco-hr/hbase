{
  if (LOG.isDebugEnabled()) {
    LOG.debug("process server shutdown scanning root region on " + master.getRegionManager().getRootRegionLocation().getBindAddress());
  }
  Scan scan=new Scan();
  scan.addFamily(CATALOG_FAMILY);
  long scannerId=server.openScanner(HRegionInfo.ROOT_REGIONINFO.getRegionName(),scan);
  scanMetaRegion(server,scannerId,HRegionInfo.ROOT_REGIONINFO.getRegionName());
  return true;
}
