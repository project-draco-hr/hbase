{
  validateParameters(scan);
  final AggregateArgument.Builder requestBuilder=AggregateArgument.newBuilder();
  requestBuilder.setInterpreterClassName(ci.getClass().getCanonicalName());
  if (ci.columnInterpreterSpecificData() != null) {
    requestBuilder.setInterpreterSpecificBytes(ci.columnInterpreterSpecificData());
  }
  requestBuilder.setScan(ProtobufUtil.toScan(scan));
  return requestBuilder.build();
}
