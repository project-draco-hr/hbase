{
  if (cell instanceof ByteBufferedCell) {
    return comparator.compareTo(((ByteBufferedCell)cell).getFamilyByteBuffer(),((ByteBufferedCell)cell).getFamilyPositionInByteBuffer(),cell.getFamilyLength());
  }
  return comparator.compareTo(cell.getFamilyArray(),cell.getFamilyOffset(),cell.getFamilyLength());
}
