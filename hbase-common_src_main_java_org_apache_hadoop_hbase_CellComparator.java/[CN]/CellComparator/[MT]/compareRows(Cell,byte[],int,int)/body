{
  if (left instanceof ByteBufferedCell) {
    return ByteBufferUtils.compareTo(((ByteBufferedCell)left).getRowByteBuffer(),((ByteBufferedCell)left).getRowPositionInByteBuffer(),left.getRowLength(),right,roffset,rlength);
  }
  return Bytes.compareTo(left.getRowArray(),left.getRowOffset(),left.getRowLength(),right,roffset,rlength);
}
