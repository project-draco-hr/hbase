{
  if (left instanceof ByteBufferedCell && right instanceof ByteBufferedCell) {
    return ByteBufferUtils.compareTo(((ByteBufferedCell)left).getFamilyByteBuffer(),((ByteBufferedCell)left).getFamilyPositionInByteBuffer(),left.getFamilyLength(),((ByteBufferedCell)right).getFamilyByteBuffer(),((ByteBufferedCell)right).getFamilyPositionInByteBuffer(),right.getFamilyLength());
  }
  if (left instanceof ByteBufferedCell) {
    return ByteBufferUtils.compareTo(((ByteBufferedCell)left).getFamilyByteBuffer(),((ByteBufferedCell)left).getFamilyPositionInByteBuffer(),left.getFamilyLength(),right.getFamilyArray(),right.getFamilyOffset(),right.getFamilyLength());
  }
  if (right instanceof ByteBufferedCell) {
    return -(ByteBufferUtils.compareTo(((ByteBufferedCell)right).getFamilyByteBuffer(),((ByteBufferedCell)right).getFamilyPositionInByteBuffer(),right.getFamilyLength(),left.getFamilyArray(),left.getFamilyOffset(),left.getFamilyLength()));
  }
  return Bytes.compareTo(left.getFamilyArray(),left.getFamilyOffset(),left.getFamilyLength(),right.getFamilyArray(),right.getFamilyOffset(),right.getFamilyLength());
}
