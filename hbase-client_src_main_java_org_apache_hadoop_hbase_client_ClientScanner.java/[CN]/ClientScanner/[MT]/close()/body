{
  if (!scanMetricsPublished)   writeScanMetrics();
  if (callable != null) {
    callable.setClose();
    try {
      call(callable,caller,scannerTimeout);
    }
 catch (    UnknownScannerException e) {
    }
catch (    IOException e) {
      LOG.warn("scanner failed to close. Exception follows: " + e);
    }
    callable=null;
  }
  closed=true;
}
