{
  this.locker.readLock().lock();
  try {
    MapFile.Reader[] maparray=maps.values().toArray(new MapFile.Reader[maps.size()]);
    for (int i=maparray.length - 1; i >= 0; i--) {
      MapFile.Reader map=maparray[i];
synchronized (map) {
        BytesWritable readval=new BytesWritable();
        map.reset();
        HStoreKey readkey=(HStoreKey)map.getClosest(key,readval);
        do {
          Text readcol=readkey.getColumn();
          if (results.get(readcol) == null && key.matchesWithoutColumn(readkey)) {
            results.put(new Text(readcol),readval);
            readval=new BytesWritable();
          }
 else           if (key.getRow().compareTo(readkey.getRow()) > 0) {
            break;
          }
        }
 while (map.next(readkey,readval));
      }
    }
  }
  finally {
    this.locker.readLock().unlock();
  }
}
