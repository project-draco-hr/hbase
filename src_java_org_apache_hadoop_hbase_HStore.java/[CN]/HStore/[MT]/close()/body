{
  LOG.info("closing HStore for " + this.regionName + "/"+ this.colFamily);
  this.lock.writeLock().lock();
  try {
    for (    MapFile.Reader map : maps.values()) {
      map.close();
    }
    maps.clear();
    mapFiles.clear();
    LOG.info("HStore closed for " + this.regionName + "/"+ this.colFamily);
  }
  finally {
    this.lock.writeLock().unlock();
  }
}
