{
  StringBuilder s=new StringBuilder(k.getRow().toString());
  s.append(k.getColumn().toString());
  byte[] bytes=null;
  try {
    bytes=s.toString().getBytes(HConstants.UTF8_ENCODING);
  }
 catch (  UnsupportedEncodingException e) {
    e.printStackTrace();
    assert(false);
  }
  return new Key(bytes);
}
