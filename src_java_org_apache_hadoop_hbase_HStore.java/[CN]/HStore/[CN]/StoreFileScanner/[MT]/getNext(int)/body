{
  boolean result=false;
  ImmutableBytesWritable ibw=new ImmutableBytesWritable();
  while (true) {
    if (!readers[i].next(keys[i],ibw)) {
      closeSubScanner(i);
      break;
    }
    if (keys[i].getTimestamp() <= this.timestamp) {
      vals[i]=ibw.get();
      result=true;
      break;
    }
  }
  return result;
}
