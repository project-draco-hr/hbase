{
  StringBuilder sb=new StringBuilder();
  sb.append("keyvalues=");
  if (isEmpty()) {
    sb.append("NONE");
    return sb.toString();
  }
  sb.append("{");
  boolean moreThanOne=false;
  for (  KeyValue kv : this.kvs) {
    if (moreThanOne) {
      sb.append(", ");
    }
 else {
      moreThanOne=true;
    }
    sb.append(kv.toString());
  }
  sb.append("}");
  return sb.toString();
}
