{
  if (isEmpty()) {
    out.writeInt(0);
  }
 else {
    int totalLen=0;
    for (    KeyValue kv : kvs) {
      totalLen+=kv.getLength() + Bytes.SIZEOF_INT;
    }
    out.writeInt(totalLen);
    for (    KeyValue kv : kvs) {
      out.writeInt(kv.getLength());
      out.write(kv.getBuffer(),kv.getOffset(),kv.getLength());
    }
  }
}
