{
  super.tearDown();
  try {
    HConnectionManager.deleteConnectionInfo(conf,true);
    if (this.cluster != null) {
      try {
        this.cluster.shutdown();
      }
 catch (      Exception e) {
        LOG.warn("Closing mini dfs",e);
      }
    }
    if (startDfs) {
      shutdownDfs(dfsCluster);
    }
  }
 catch (  Exception e) {
    LOG.error(e);
  }
}
