{
synchronized (tasks) {
    if (result != null) {
      return result;
    }
    if (exeEx != null) {
      throw exeEx;
    }
    unit.timedWait(tasks,timeout);
  }
  if (result != null) {
    return result;
  }
  if (exeEx != null) {
    throw exeEx;
  }
  throw new TimeoutException("timeout=" + timeout + ", "+ unit);
}
