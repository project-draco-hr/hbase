{
  List<Cell> cells=new ArrayList<Cell>();
  try (HFile.Reader reader=HFile.createReader(this.fs,hfilePath,cacheConfig,this.conf);HFileScanner scanner=reader.getScanner(true,true)){
    scanner.seekTo();
    do {
      cells.add(scanner.getCell());
      LOG.info(scanner.getKey());
    }
 while (scanner.next());
    scanner.seekTo(reader.getFirstKey());
    scanner.next();
    LOG.info(scanner.getCell());
  }
   return cells;
}
