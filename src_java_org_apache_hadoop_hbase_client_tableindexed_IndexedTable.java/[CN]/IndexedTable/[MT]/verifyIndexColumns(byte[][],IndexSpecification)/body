{
  if (requestedColumns == null) {
    return;
  }
  for (  byte[] requestedColumn : requestedColumns) {
    boolean found=false;
    for (    byte[] indexColumn : indexSpec.getAllColumns()) {
      if (Bytes.equals(requestedColumn,indexColumn)) {
        found=true;
        break;
      }
    }
    if (!found) {
      throw new RuntimeException("Column [" + Bytes.toString(requestedColumn) + "] not in index "+ indexSpec.getIndexId());
    }
  }
}
