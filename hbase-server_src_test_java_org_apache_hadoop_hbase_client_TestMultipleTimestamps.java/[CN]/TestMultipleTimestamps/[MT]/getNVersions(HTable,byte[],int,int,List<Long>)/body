{
  byte row[]=Bytes.toBytes("row:" + rowIdx);
  byte column[]=Bytes.toBytes("column:" + colIdx);
  Get get=new Get(row);
  get.addColumn(cf,column);
  get.setMaxVersions();
  get.setTimeRange(Collections.min(versions),Collections.max(versions) + 1);
  Result result=ht.get(get);
  return result.raw();
}
