{
  String res="Store segment of type " + this.getClass().getName() + "; ";
  res+="isEmpty " + (isEmpty() ? "yes" : "no") + "; ";
  res+="cellCount " + getCellsCount() + "; ";
  res+="cellsSize " + keySize() + "; ";
  res+="heapSize " + size() + "; ";
  res+="Min ts " + getMinTimestamp() + "; ";
  return res;
}
