{
  StoreFile storeFile=null;
  try {
    createStoreFileAndReader(path,NoOpDataBlockEncoder.INSTANCE);
  }
 catch (  IOException e) {
    LOG.error("Failed to open store file : " + path + ", keeping it in tmp location",e);
    throw e;
  }
 finally {
    if (storeFile != null) {
      storeFile.closeReader(false);
    }
  }
}
