{
  for (  StoreFile sf : this.storeEngine.getStoreFileManager().getStorefiles()) {
    if (sf.getReader() == null) {
      LOG.debug("StoreFile " + sf + " has null Reader");
      return false;
    }
  }
  return storeEngine.getCompactionPolicy().isMajorCompaction(this.storeEngine.getStoreFileManager().getStorefiles());
}
