{
  try {
    Class<? extends CompactionManager> managerClass=(Class<? extends CompactionManager>)Class.forName(managerClassName);
    compactionManager=managerClass.getDeclaredConstructor(new Class[]{Configuration.class,Store.class}).newInstance(new Object[]{conf,this});
  }
 catch (  ClassNotFoundException e) {
    throw new UnsupportedOperationException("Unable to find region server interface " + managerClassName,e);
  }
catch (  IllegalAccessException e) {
    throw new UnsupportedOperationException("Unable to access specified class " + managerClassName,e);
  }
catch (  InstantiationException e) {
    throw new UnsupportedOperationException("Unable to instantiate specified class " + managerClassName,e);
  }
catch (  InvocationTargetException e) {
    throw new UnsupportedOperationException("Unable to invoke specified target class constructor " + managerClassName,e);
  }
catch (  NoSuchMethodException e) {
    throw new UnsupportedOperationException("Unable to find suitable constructor for class " + managerClassName,e);
  }
}
