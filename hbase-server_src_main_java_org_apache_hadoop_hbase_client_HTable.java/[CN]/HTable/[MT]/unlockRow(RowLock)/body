{
  new ServerCallable<Boolean>(connection,tableName,rl.getRow(),operationTimeout){
    public Boolean call() throws IOException {
      try {
        UnlockRowRequest request=RequestConverter.buildUnlockRowRequest(location.getRegionInfo().getRegionName(),rl.getLockId());
        server.unlockRow(null,request);
        return Boolean.TRUE;
      }
 catch (      ServiceException se) {
        throw ProtobufUtil.getRemoteException(se);
      }
    }
  }
.withRetries();
}
