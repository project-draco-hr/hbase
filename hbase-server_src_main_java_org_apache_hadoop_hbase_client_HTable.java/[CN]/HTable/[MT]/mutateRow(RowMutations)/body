{
  new ServerCallable<Void>(connection,tableName,rm.getRow(),operationTimeout){
    public Void call() throws IOException {
      try {
        MultiRequest request=RequestConverter.buildMultiRequest(location.getRegionInfo().getRegionName(),rm);
        server.multi(null,request);
      }
 catch (      ServiceException se) {
        throw ProtobufUtil.getRemoteException(se);
      }
      return null;
    }
  }
.withRetries();
}
