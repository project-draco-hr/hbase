{
  while (v.hasNext()) {
    r.setStatus("Reducer committing " + k);
    BatchUpdate bu=new BatchUpdate(k.get());
    while (v.hasNext()) {
      HbaseMapWritable<byte[],byte[]> hmw=v.next();
      for (      Entry<byte[],byte[]> e : hmw.entrySet()) {
        bu.put(e.getKey(),e.getValue());
      }
    }
    output.collect(k,bu);
  }
}
