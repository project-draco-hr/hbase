{
  String table1="testFixByTable1";
  String table2="testFixByTable2";
  try {
    setupTable(table1);
    TEST_UTIL.getHBaseAdmin().flush(table1);
    deleteRegion(conf,tbl.getTableDescriptor(),Bytes.toBytes("B"),Bytes.toBytes("C"),false,false,true);
    setupTable(table2);
    TEST_UTIL.getHBaseAdmin().flush(table2);
    deleteRegion(conf,tbl.getTableDescriptor(),Bytes.toBytes("B"),Bytes.toBytes("C"),false,false,true);
    HBaseFsck hbck=doFsck(conf,false);
    assertErrors(hbck,new ERROR_CODE[]{ERROR_CODE.NOT_IN_HDFS,ERROR_CODE.NOT_IN_HDFS});
    doFsck(conf,true,table1);
    assertNoErrors(doFsck(conf,false,table1));
    assertErrors(doFsck(conf,false,table2),new ERROR_CODE[]{ERROR_CODE.NOT_IN_HDFS});
    doFsck(conf,true,table2);
    assertNoErrors(doFsck(conf,false));
    assertEquals(ROWKEYS.length - 2,countRows());
  }
  finally {
    deleteTable(table1);
    deleteTable(table2);
  }
}
