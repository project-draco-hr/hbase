{
  HBaseFsck hbck=doFsck(conf,false);
  assertNoErrors(hbck);
  deleteMetaRegion(conf,true,false,false);
  hbck=doFsck(conf,false);
  assertErrors(hbck,new ERROR_CODE[]{ERROR_CODE.NO_META_REGION,ERROR_CODE.UNKNOWN});
  hbck=doFsck(conf,true);
  assertErrors(hbck,new ERROR_CODE[]{ERROR_CODE.NO_META_REGION,ERROR_CODE.UNKNOWN});
  hbck=doFsck(conf,false);
  assertNoErrors(hbck);
}
