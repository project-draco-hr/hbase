{
  if (Bytes.compareTo(keyValue.getColumn(),this.columnName) != 0) {
    return ReturnCode.INCLUDE;
  }
  LOG.info("Found column [" + Bytes.toString(columnName) + "] in row ["+ Bytes.toString(keyValue.getRow())+ "]");
  foundColValue=true;
  boolean filtered=filterColumnValue(keyValue.getBuffer(),keyValue.getValueOffset(),keyValue.getValueLength());
  if (filtered) {
    LOG.info("filtered it");
    filterThisRow=true;
    return ReturnCode.NEXT_ROW;
  }
  return ReturnCode.INCLUDE;
}
