{
  int tier=0;
  if (start == -1) {
    while (endInTier[tier] >= endInTier[0]) {
      tier++;
    }
    return endInTier[tier];
  }
  tier=tierOf[start];
  if (endInTier[tier] == start + 1) {
    tier++;
    start=endInTier[tier];
    while (endInTier[tier] >= start) {
      tier++;
    }
    return endInTier[tier];
  }
  return start + 1;
}
