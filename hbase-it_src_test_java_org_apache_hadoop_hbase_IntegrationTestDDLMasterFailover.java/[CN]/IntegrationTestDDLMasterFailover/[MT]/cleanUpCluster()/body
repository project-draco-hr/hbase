{
  if (!keepTableAtTheEnd) {
    Admin admin=util.getHBaseAdmin();
    admin.disableTables("ittable-\\d+");
    admin.deleteTables("ittable-\\d+");
  }
  enabledTables.clear();
  disabledTables.clear();
  deletedTables.clear();
  namespaceMap.clear();
  Connection connection=getConnection();
  connection.close();
  super.cleanUpCluster();
}
