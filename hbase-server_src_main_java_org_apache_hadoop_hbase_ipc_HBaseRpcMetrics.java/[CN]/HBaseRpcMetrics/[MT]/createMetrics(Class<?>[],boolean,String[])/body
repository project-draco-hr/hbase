{
  for (  Class<?> iface : ifaces) {
    Method[] methods=iface.getMethods();
    for (    Method method : methods) {
      String attrName=prefixWithClass ? getMetricName(iface,method.getName()) : method.getName();
      if (get(attrName) == null)       create(attrName);
      if (suffixes != null) {
        for (        String s : suffixes) {
          String metricName=attrName + s;
          if (get(metricName) == null)           create(metricName);
        }
      }
    }
  }
}
