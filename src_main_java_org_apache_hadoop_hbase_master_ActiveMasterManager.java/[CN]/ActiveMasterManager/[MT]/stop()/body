{
  try {
    HServerAddress zkAddress=ZKUtil.getDataAsAddress(watcher,watcher.masterAddressZNode);
    if (zkAddress != null && zkAddress.equals(address)) {
      ZKUtil.deleteNode(watcher,watcher.masterAddressZNode);
    }
  }
 catch (  KeeperException e) {
    watcher.error("Error deleting our own master address node",e);
  }
}
