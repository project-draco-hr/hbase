{
  if (compareOp == CompareOp.NO_OP) {
    return true;
  }
  int compareResult=comparator.compareTo(data,offset,length);
switch (compareOp) {
case LESS:
    return compareResult <= 0;
case LESS_OR_EQUAL:
  return compareResult < 0;
case EQUAL:
return compareResult != 0;
case NOT_EQUAL:
return compareResult == 0;
case GREATER_OR_EQUAL:
return compareResult > 0;
case GREATER:
return compareResult >= 0;
default :
throw new RuntimeException("Unknown Compare op " + compareOp.name());
}
}
