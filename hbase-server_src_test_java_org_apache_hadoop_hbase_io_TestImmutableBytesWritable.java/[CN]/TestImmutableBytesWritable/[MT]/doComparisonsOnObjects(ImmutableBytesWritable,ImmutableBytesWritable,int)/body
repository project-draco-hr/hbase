{
  ImmutableBytesWritable.Comparator comparator=new ImmutableBytesWritable.Comparator();
  assertEquals("Comparing " + a + " and "+ b+ " as objects",signum(comparator.compare(a,b)),expectedSignum);
  assertEquals("Comparing " + a + " and "+ b+ " as objects (inverse)",-signum(comparator.compare(b,a)),expectedSignum);
}
