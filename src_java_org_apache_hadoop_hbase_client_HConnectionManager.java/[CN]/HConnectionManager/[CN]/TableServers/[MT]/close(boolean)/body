{
  if (master != null) {
    if (stopProxy) {
      HBaseRPC.stopProxy(master);
    }
    master=null;
    masterChecked=false;
  }
  resetZooKeeper();
  if (stopProxy) {
synchronized (servers) {
      for (      HRegionInterface i : servers.values()) {
        HBaseRPC.stopProxy(i);
      }
    }
  }
}
