{
  ExecutorService pool=null;
  try {
    pool=createExecutorService();
    Multimap<ByteBuffer,LoadQueueItem> regionGroups=groupOrSplitPhase(table,pool,queue,startEndKeys);
    bulkLoadPhase(table,conn,pool,queue,regionGroups);
  }
  finally {
    if (pool != null) {
      pool.shutdown();
    }
  }
}
