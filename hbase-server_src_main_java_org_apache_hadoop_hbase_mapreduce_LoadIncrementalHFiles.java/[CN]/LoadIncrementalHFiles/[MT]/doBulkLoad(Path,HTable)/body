{
  Admin admin=null;
  try {
    try {
      admin=table.getConnection().getAdmin();
    }
 catch (    NeedUnmanagedConnectionException ex) {
      admin=new HBaseAdmin(table.getConfiguration());
    }
    doBulkLoad(hfofDir,admin,table,table.getRegionLocator());
  }
  finally {
    admin.close();
  }
}
