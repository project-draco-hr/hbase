{
  conf=new Configuration();
  HBaseRPC.setProtocolEngine(conf,TestRpcService.class,ProtobufRpcEngine.class);
  PBServerImpl serverImpl=new PBServerImpl();
  server=HBaseRPC.getServer(TestRpcService.class,serverImpl,new Class[]{TestRpcService.class},ADDRESS,PORT,10,10,true,conf,0);
  addr=server.getListenerAddress();
  server.start();
}
