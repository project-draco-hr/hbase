{
  if (valueMap.size() == 1) {
    return "timestamp=" + getTimestamp() + ", value="+ Bytes.toString(getValue());
  }
  StringBuilder s=new StringBuilder("{ ");
  int i=0;
  for (  Map.Entry<Long,byte[]> entry : valueMap.entrySet()) {
    if (i > 0) {
      s.append(", ");
    }
    s.append("[timestamp=");
    s.append(entry.getKey());
    s.append(", value=");
    s.append(Bytes.toString(entry.getValue()));
    s.append("]");
    i++;
  }
  s.append(" }");
  return s.toString();
}
