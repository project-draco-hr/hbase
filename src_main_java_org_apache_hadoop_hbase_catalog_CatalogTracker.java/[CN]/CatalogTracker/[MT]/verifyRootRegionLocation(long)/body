{
  HRegionInterface connection=null;
  try {
    connection=waitForRootServerConnection(timeout);
  }
 catch (  NotAllMetaRegionsOnlineException e) {
  }
catch (  org.apache.hadoop.hbase.ipc.ServerNotRunningException e) {
  }
catch (  IOException e) {
    throw e;
  }
  return (connection == null) ? false : verifyRegionLocation(connection,this.rootRegionTracker.getRootRegionLocation(),HRegionInfo.ROOT_REGIONINFO.getRegionName());
}
