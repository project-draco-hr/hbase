{
  HRegionInterface connection=null;
  try {
    connection=waitForRootServerConnection(timeout);
  }
 catch (  NotAllMetaRegionsOnlineException e) {
  }
catch (  ServerNotRunningYetException e) {
  }
  return (connection == null) ? false : verifyRegionLocation(connection,this.rootRegionTracker.getRootRegionLocation(),ROOT_REGION_NAME);
}
