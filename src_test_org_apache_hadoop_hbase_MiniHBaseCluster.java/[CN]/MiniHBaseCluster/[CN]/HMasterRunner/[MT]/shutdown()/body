{
  isRunning=false;
  if (this.master != null) {
    try {
      this.master.shutdown();
    }
 catch (    IOException e) {
      LOG.error("Master crashed during stop",e);
    }
 finally {
      try {
        masterThread.join();
      }
 catch (      InterruptedException e) {
      }
      master=null;
    }
  }
}
