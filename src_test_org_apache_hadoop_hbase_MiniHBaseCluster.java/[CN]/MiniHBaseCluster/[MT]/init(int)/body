{
  try {
    this.parentdir=new Path(conf.get(HBASE_DIR,DEFAULT_HBASE_DIR));
    fs.mkdirs(parentdir);
    FSUtils.setVersion(fs,parentdir);
    this.hbaseCluster=new LocalHBaseCluster(this.conf,nRegionNodes);
    this.hbaseCluster.startup();
  }
 catch (  IOException e) {
    shutdown();
    throw e;
  }
}
