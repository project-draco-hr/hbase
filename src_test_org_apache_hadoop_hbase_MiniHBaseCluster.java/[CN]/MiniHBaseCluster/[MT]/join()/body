{
  if (regionThreads != null) {
synchronized (regionThreads) {
      for (      Thread t : regionThreads) {
        if (t.isAlive()) {
          try {
            t.join();
          }
 catch (          InterruptedException e) {
          }
        }
      }
    }
  }
  if (masterThread != null && masterThread.isAlive()) {
    try {
      masterThread.join();
    }
 catch (    InterruptedException e) {
    }
  }
}
