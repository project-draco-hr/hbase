{
  this.conf=conf;
  this.deleteOnExit=deleteOnExit;
  if (miniHdfsFilesystem) {
    try {
      this.cluster=new MiniDFSCluster(this.conf,2,format,(String[])null);
    }
 catch (    Throwable t) {
      LOG.error("Failed setup of mini dfs cluster",t);
      t.printStackTrace();
      return;
    }
  }
  init(nRegionNodes);
}
