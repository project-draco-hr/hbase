{
  if (scannerId != -1L && closed) {
    server.close(scannerId);
    scannerId=-1L;
  }
 else   if (scannerId == -1L && !closed) {
    scannerId=openScanner();
  }
 else {
    Result[] rrs=server.next(scannerId,caching);
    return rrs == null || rrs.length == 0 ? null : rrs;
  }
  return null;
}
