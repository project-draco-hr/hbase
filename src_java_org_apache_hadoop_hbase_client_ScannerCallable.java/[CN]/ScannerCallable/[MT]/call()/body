{
  if (scannerId != -1L && closed) {
    server.close(scannerId);
    scannerId=-1L;
  }
 else   if (scannerId == -1L && !closed) {
    scannerId=server.openScanner(this.location.getRegionInfo().getRegionName(),columns,row,timestamp,filter);
  }
 else {
    return server.next(scannerId);
  }
  return null;
}
