{
  byte[][] keyVals=null;
  ArrayList<byte[]> foundList=new ArrayList<byte[]>();
  int numCols=columns.length;
  if (numCols > 0) {
    for (    Cell value : r.listCells()) {
      byte[] column=KeyValue.makeColumn(CellUtil.getFamilyArray(value),CellUtil.getQualifierArray(value));
      for (int i=0; i < numCols; i++) {
        if (Bytes.equals(column,columns[i])) {
          foundList.add(CellUtil.getValueArray(value));
          break;
        }
      }
    }
    if (foundList.size() == numCols) {
      keyVals=foundList.toArray(new byte[numCols][]);
    }
  }
  return keyVals;
}
