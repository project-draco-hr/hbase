{
  HFileArchiver.archiveFamily(fs,conf,regionInfo,tableDir,Bytes.toBytes(familyName));
  Path familyDir=getStoreDir(familyName);
  if (fs.exists(familyDir) && !deleteDir(familyDir))   throw new IOException("Could not delete family " + familyName + " from FileSystem for region "+ regionInfo.getRegionNameAsString()+ "("+ regionInfo.getEncodedName()+ ")");
}
