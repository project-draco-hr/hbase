{
  if (load == null)   return;
synchronized (this.loadToServers) {
    Set<String> servers=this.loadToServers.get(load);
    if (servers != null) {
      servers.remove(serverName);
      if (servers.size() > 0)       this.loadToServers.put(load,servers);
 else       this.loadToServers.remove(load);
    }
  }
}
