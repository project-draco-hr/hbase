{
  int numServers=numServers();
  int numDeadServers=deadservers.size();
  double averageLoad=getAverageLoad();
  String deadServersList=deadservers.toString();
  LOG.info(numServers + " region servers, " + numDeadServers+ " dead, average load "+ averageLoad+ ((deadServersList != null && deadServersList.length() > 0) ? deadServersList : ""));
}
