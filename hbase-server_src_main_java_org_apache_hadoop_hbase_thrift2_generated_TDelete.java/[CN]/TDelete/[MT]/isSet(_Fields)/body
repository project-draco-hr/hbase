{
  if (field == null) {
    throw new IllegalArgumentException();
  }
switch (field) {
case ROW:
    return isSetRow();
case COLUMNS:
  return isSetColumns();
case TIMESTAMP:
return isSetTimestamp();
case DELETE_TYPE:
return isSetDeleteType();
case ATTRIBUTES:
return isSetAttributes();
case DURABILITY:
return isSetDurability();
}
throw new IllegalStateException();
}
