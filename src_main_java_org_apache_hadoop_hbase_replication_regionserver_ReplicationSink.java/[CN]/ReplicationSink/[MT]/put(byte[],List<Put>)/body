{
  if (puts.isEmpty()) {
    return;
  }
  HTableInterface table=null;
  try {
    table=this.pool.getTable(tableName);
    table.put(puts);
    this.metrics.appliedOpsRate.inc(puts.size());
    this.pool.putTable(table);
    puts.clear();
  }
  finally {
    if (table != null) {
      this.pool.putTable(table);
    }
  }
}
