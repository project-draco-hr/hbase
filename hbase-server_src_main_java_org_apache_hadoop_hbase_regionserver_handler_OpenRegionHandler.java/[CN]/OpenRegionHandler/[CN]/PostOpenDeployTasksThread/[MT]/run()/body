{
  try {
    this.services.postOpenDeployTasks(this.region,this.server.getCatalogTracker());
  }
 catch (  KeeperException e) {
    server.abort("Exception running postOpenDeployTasks; region=" + this.region.getRegionInfo().getEncodedName(),e);
  }
catch (  Exception e) {
    LOG.warn("Exception running postOpenDeployTasks; region=" + this.region.getRegionInfo().getEncodedName(),e);
    this.exception=e;
  }
  this.signaller.set(true);
synchronized (this.signaller) {
    this.signaller.notify();
  }
}
