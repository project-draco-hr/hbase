{
  Delete d=new Delete(tableName.getName());
  if (LOG.isDebugEnabled()) {
    LOG.debug("Removing permissions of removed table " + tableName);
  }
  HTable acls=null;
  try {
    acls=new HTable(conf,ACL_TABLE_NAME);
    acls.delete(d);
  }
  finally {
    if (acls != null)     acls.close();
  }
}
