{
  master.createTable(new HTableDescriptor(ACL_TABLE_NAME).addFamily(new HColumnDescriptor(ACL_LIST_FAMILY).setMaxVersions(1).setInMemory(true).setBlockCacheEnabled(true).setBlocksize(8 * 1024).setBloomFilterType(BloomType.NONE).setScope(HConstants.REPLICATION_SCOPE_LOCAL).setCacheDataInL1(true)),null);
