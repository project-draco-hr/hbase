{
  ListMultimap<String,TablePermission> allPerms=getTablePermissions(conf,tableName);
  List<UserPermission> perms=new ArrayList<UserPermission>();
  for (  Map.Entry<String,TablePermission> entry : allPerms.entries()) {
    UserPermission up=new UserPermission(Bytes.toBytes(entry.getKey()),entry.getValue().getTable(),entry.getValue().getFamily(),entry.getValue().getQualifier(),entry.getValue().getActions());
    perms.add(up);
  }
  return perms;
}
