{
  int numberOfRegions=this.regionLoad.size();
  StringBuilder sb=new StringBuilder();
  sb=Strings.appendKeyValue(sb,"requestsPerSecond",Integer.valueOf(numberOfRequests / msgInterval));
  sb=Strings.appendKeyValue(sb,"numberOfOnlineRegions",Integer.valueOf(numberOfRegions));
  sb=Strings.appendKeyValue(sb,"usedHeapMB",Integer.valueOf(this.usedHeapMB));
  sb=Strings.appendKeyValue(sb,"maxHeapMB",Integer.valueOf(maxHeapMB));
  return sb.toString();
}
