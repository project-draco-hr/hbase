{
  int numberOfRegions=this.regionLoad.size();
  StringBuilder sb=new StringBuilder();
  sb=Strings.appendKeyValue(sb,"requests",Integer.valueOf(numberOfRequests / msgInterval));
  sb=Strings.appendKeyValue(sb,"regions",Integer.valueOf(numberOfRegions));
  sb=Strings.appendKeyValue(sb,"usedHeap",Integer.valueOf(this.usedHeapMB));
  sb=Strings.appendKeyValue(sb,"maxHeap",Integer.valueOf(maxHeapMB));
  return sb.toString();
}
