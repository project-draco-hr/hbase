{
  try {
    ArrayList<RowResult> a=new ArrayList<RowResult>();
    HTable table=new HTable(tableName);
    Scanner s=table.getScanner(columnNames);
    RowResult r;
    while ((r=s.next()) != null) {
      a.add(r);
    }
    return a.toArray(new RowResult[0]);
  }
 catch (  Exception e) {
    throw new HBaseRestException(e);
  }
}
