{
  if (transactionState.addRegion(location)) {
    ((TransactionalRegionInterface)server).beginTransaction(transactionState.getTransactionId(),location.getRegionInfo().getRegionName());
  }
  return ((TransactionalRegionInterface)server).openScanner(transactionState.getTransactionId(),this.location.getRegionInfo().getRegionName(),getScan());
}
