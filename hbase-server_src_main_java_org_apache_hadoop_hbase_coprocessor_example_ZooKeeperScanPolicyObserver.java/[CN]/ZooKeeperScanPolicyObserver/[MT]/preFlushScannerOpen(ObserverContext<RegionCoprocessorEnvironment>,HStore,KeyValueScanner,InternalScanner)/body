{
  HStore.ScanInfo scanInfo=getScanInfo(store,c.getEnvironment());
  if (scanInfo == null) {
    return null;
  }
  Scan scan=new Scan();
  scan.setMaxVersions(scanInfo.getMaxVersions());
  return new StoreScanner(store,scanInfo,scan,Collections.singletonList(memstoreScanner),ScanType.MINOR_COMPACT,store.getHRegion().getSmallestReadPoint(),HConstants.OLDEST_TIMESTAMP);
}
