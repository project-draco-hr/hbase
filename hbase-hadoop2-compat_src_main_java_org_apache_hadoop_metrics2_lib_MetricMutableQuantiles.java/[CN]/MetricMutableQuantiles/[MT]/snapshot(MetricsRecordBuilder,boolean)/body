{
  if (all || changed()) {
    builder.addGauge(numInfo,previousCount);
    for (int i=0; i < quantiles.length; i++) {
      long newValue=0;
      if (previousSnapshot != null) {
        newValue=previousSnapshot.get(quantiles[i]);
      }
      builder.addGauge(quantileInfos[i],newValue);
    }
    if (changed()) {
      clearChanged();
    }
  }
}
