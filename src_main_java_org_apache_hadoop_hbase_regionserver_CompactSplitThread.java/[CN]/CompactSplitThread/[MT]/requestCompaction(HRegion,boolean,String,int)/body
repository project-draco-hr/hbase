{
  if (this.server.isStopped()) {
    return;
  }
  if (force) {
    r.setForceMajorCompaction(force);
  }
  if (compactionQueue.add(r,priority) && LOG.isDebugEnabled()) {
    LOG.debug("Compaction " + (force ? "(major) " : "") + "requested for "+ r.getRegionNameAsString()+ (why != null && !why.isEmpty() ? " because " + why : "")+ "; priority="+ priority+ ", compaction queue size="+ compactionQueue.size());
  }
}
