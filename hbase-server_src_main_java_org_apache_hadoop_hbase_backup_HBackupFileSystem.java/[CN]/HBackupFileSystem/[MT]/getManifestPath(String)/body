{
  Path manifestPath=new Path(getTableBackupPath(tableName),BackupManifest.FILE_NAME);
  LOG.debug("Looking for " + manifestPath.toString());
  if (!fs.exists(manifestPath)) {
    manifestPath=new Path(getLogBackupDir(this.backupRootPath.toString(),this.backupId) + File.separator + BackupManifest.FILE_NAME);
    LOG.debug("Looking for " + manifestPath.toString());
    if (!fs.exists(manifestPath)) {
      String errorMsg="Could not find backup manifest for " + backupId + " in "+ backupRootPath.toString();
      throw new IOException(errorMsg);
    }
  }
  return manifestPath;
}
