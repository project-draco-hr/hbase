{
  String tableName=context.getConfiguration().get(OUTPUT_TABLE);
  HTable table=null;
  try {
    table=new HTable(new HBaseConfiguration(context.getConfiguration()),tableName);
  }
 catch (  IOException e) {
    LOG.error(e);
    throw e;
  }
  table.setAutoFlush(false);
  return new TableRecordWriter(table);
}
