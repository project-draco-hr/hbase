{
  Set<String> allKeys=new TreeSet<String>(a.keySet());
  allKeys.addAll(b.keySet());
  Map<String,Long> diff=new TreeMap<String,Long>();
  for (  String k : allKeys) {
    long aVal=getLong(a,k);
    long bVal=getLong(b,k);
    if (aVal != bVal) {
      diff.put(k,bVal - aVal);
    }
  }
  return diff;
}
