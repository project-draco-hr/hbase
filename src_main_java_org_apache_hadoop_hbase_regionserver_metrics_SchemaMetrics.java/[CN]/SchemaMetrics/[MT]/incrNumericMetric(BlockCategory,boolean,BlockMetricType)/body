{
  if (blockCategory == null) {
    blockCategory=BlockCategory.UNKNOWN;
  }
  HRegion.incrNumericMetric(getBlockMetricName(blockCategory,isCompaction,metricType),1);
  if (blockCategory != BlockCategory.ALL_CATEGORIES) {
    incrNumericMetric(BlockCategory.ALL_CATEGORIES,isCompaction,metricType);
  }
}
