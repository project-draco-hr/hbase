{
  final AtomicInteger taken=new AtomicInteger();
  Thread consumer=new Thread(){
    @Override public void run(){
      try {
        Integer n=stealJobQueue.take();
        taken.set(n);
      }
 catch (      InterruptedException e) {
        e.printStackTrace();
      }
    }
  }
;
  consumer.start();
  stealFromQueue.offer(3);
  consumer.join(1000);
  assertEquals(3,taken.get());
  consumer.interrupt();
}
