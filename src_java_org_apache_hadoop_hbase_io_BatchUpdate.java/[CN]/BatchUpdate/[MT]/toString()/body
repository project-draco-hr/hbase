{
  StringBuilder sb=new StringBuilder();
  sb.append("row => ");
  sb.append(row == null ? "" : Bytes.toString(row));
  sb.append(", {");
  boolean morethanone=false;
  for (  BatchOperation bo : this.operations) {
    if (morethanone) {
      sb.append(", ");
    }
    morethanone=true;
    sb.append(bo.toString());
  }
  sb.append("}");
  return sb.toString();
}
