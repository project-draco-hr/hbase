{
  int nOps=in.readInt();
  for (int i=0; i < nOps; i++) {
    Text row=new Text();
    row.readFields(in);
    int nRowOps=in.readInt();
    ArrayList<BatchOperation> rowOps=new ArrayList<BatchOperation>();
    for (int j=0; j < nRowOps; j++) {
      BatchOperation op=new BatchOperation();
      op.readFields(in);
      rowOps.add(op);
    }
    operations.put(row,rowOps);
  }
}
