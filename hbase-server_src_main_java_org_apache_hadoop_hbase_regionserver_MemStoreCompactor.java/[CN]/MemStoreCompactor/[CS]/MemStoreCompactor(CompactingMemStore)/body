{
  this.compactingMemStore=compactingMemStore;
  this.compactionKVMax=compactingMemStore.getConfiguration().getInt(HConstants.COMPACTION_KV_MAX,HConstants.COMPACTION_KV_MAX_DEFAULT);
  this.fraction=1 - compactingMemStore.getConfiguration().getDouble(COMPACTION_THRESHOLD_REMAIN_FRACTION,COMPACTION_THRESHOLD_REMAIN_FRACTION_DEFAULT);
}
