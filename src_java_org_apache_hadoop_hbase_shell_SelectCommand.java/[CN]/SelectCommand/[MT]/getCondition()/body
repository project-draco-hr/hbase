{
  int type=0;
  if (this.condition == null) {
    type=0;
  }
 else   if (this.condition.containsKey("row")) {
    if (getConditionSize() == 1) {
      type=1;
    }
 else     if (this.condition.containsKey("column")) {
      if (getConditionSize() == 2) {
        if (this.limit == 0) {
          type=3;
        }
 else {
          type=4;
        }
      }
 else {
        type=5;
      }
    }
  }
 else   if (this.condition.containsKey("column")) {
    type=2;
  }
  return type;
}
