{
  this.cowType=cowType;
  this.compress=compress;
  this.encoderType=encoderType;
  this.encoder=encoderType.getEncoder();
  this.cacheCompressedData=cacheCompressedData;
  this.blockCache=blockCache;
  testDescription="[cacheOnWrite=" + cowType + ", compress="+ compress+ ", encoderType="+ encoderType+ ", cacheCompressedData="+ cacheCompressedData+ "]";
  LOG.info(testDescription);
