{
  if (user != null) {
    if (!user.isAdmin()) {
      throw new WebApplicationException(Response.Status.FORBIDDEN);
    }
    this.user=user;
  }
  this.table=table;
  cacheControl=new CacheControl();
  cacheControl.setNoCache(true);
  cacheControl.setNoTransform(false);
  servlet=RESTServlet.getInstance();
}
