{
  if (authorizeUser(user.getShortName(),table,family,qualifier,action)) {
    return true;
  }
  String[] groups=user.getGroupNames();
  if (groups != null) {
    for (    String group : groups) {
      if (authorizeGroup(group,table,family,action)) {
        return true;
      }
    }
  }
  return false;
}
