{
  try {
    Properties properties=parseZooKeeperConfig();
    QuorumPeerConfig.parseProperties(properties);
  }
 catch (  Exception e) {
    e.printStackTrace();
    System.exit(-1);
  }
  if (ServerConfig.isStandalone()) {
    ZooKeeperServerMain.main(args);
  }
 else {
    QuorumPeerMain.runPeerFromConfig();
  }
}
