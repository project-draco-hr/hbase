{
  Result r;
  Get get=new Get(row);
  get.addColumn(family,quality);
  int nbTry=0;
  do {
    assertTrue("Fail to get from " + htable.getName() + " after "+ nbTry+ " tries",nbTry < 50);
    nbTry++;
    Thread.sleep(100);
    r=htable.get(get);
  }
 while (r == null || r.getValue(family,quality) == null);
  assertEquals("value",Bytes.toStringBinary(value),Bytes.toStringBinary(r.getValue(family,quality)));
}
