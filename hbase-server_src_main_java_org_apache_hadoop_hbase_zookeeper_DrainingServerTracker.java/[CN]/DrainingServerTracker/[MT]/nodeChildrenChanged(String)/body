{
  if (path.equals(watcher.drainingZNode)) {
    try {
      final List<String> newNodes=ZKUtil.listChildrenAndWatchThem(watcher,watcher.drainingZNode);
      add(newNodes);
    }
 catch (    KeeperException e) {
      abortable.abort("Unexpected zk exception getting RS nodes",e);
    }
catch (    IOException e) {
      abortable.abort("Unexpected zk exception getting RS nodes",e);
    }
  }
}
