{
  final HalfStoreFileReader halfreader=new HalfStoreFileReader(fs,p,cacheConf,bottom,DataBlockEncoding.NONE);
  halfreader.loadFileInfo();
  final HFileScanner scanner=halfreader.getScanner(false,false);
  scanner.seekTo();
  KeyValue curr;
  do {
    curr=scanner.getKeyValue();
    KeyValue reseekKv=getLastOnCol(curr);
    int ret=scanner.reseekTo(reseekKv.getKey());
    assertTrue("reseek to returned: " + ret,ret > 0);
  }
 while (scanner.next());
  int ret=scanner.reseekTo(getLastOnCol(curr).getKey());
  assertTrue(ret > 0);
  halfreader.close(true);
}
