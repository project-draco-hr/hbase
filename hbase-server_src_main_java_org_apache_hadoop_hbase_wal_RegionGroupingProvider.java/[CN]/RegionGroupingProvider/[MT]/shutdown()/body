{
  IOException failure=null;
synchronized (logs) {
    for (    FSHLog wal : logs) {
      try {
        wal.shutdown();
      }
 catch (      IOException exception) {
        LOG.error("Problem shutting down log '" + wal + "': "+ exception.getMessage());
        LOG.debug("Details of problem shutting down log '" + wal + "'",exception);
        failure=exception;
      }
    }
  }
  if (failure != null) {
    throw failure;
  }
}
