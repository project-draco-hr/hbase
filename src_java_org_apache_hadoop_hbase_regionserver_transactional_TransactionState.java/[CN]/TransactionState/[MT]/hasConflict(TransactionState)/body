{
  if (checkAgainst.getStatus().equals(TransactionState.Status.ABORTED)) {
    return false;
  }
  for (  BatchUpdate otherUpdate : checkAgainst.getWriteSet()) {
    if (this.hasScan) {
      LOG.info("Transaction" + this.toString() + " has a scan read. Meanwile a write occured. "+ "Conservitivly reporting conflict");
      return true;
    }
    if (this.getReadSet().contains(otherUpdate.getRow())) {
      LOG.trace("Transaction " + this.toString() + " conflicts with "+ checkAgainst.toString());
      return true;
    }
  }
  return false;
}
