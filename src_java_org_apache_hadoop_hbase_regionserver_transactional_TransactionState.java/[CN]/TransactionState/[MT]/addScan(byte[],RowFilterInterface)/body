{
  ScanRange scanRange=new ScanRange(firstRow,getEndRow(filter));
  LOG.trace(String.format("Adding scan for transcaction [%s], from [%s] to [%s]",transactionId,scanRange.startRow == null ? "null" : Bytes.toString(scanRange.startRow),scanRange.endRow == null ? "null" : Bytes.toString(scanRange.endRow)));
  scanSet.add(scanRange);
}
