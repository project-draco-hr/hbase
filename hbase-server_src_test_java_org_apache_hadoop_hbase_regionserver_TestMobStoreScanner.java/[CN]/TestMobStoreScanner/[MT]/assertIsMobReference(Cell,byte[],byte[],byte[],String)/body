{
  Assert.assertEquals(Bytes.toString(row),Bytes.toString(CellUtil.cloneRow(cell)));
  Assert.assertEquals(Bytes.toString(family),Bytes.toString(CellUtil.cloneFamily(cell)));
  Assert.assertFalse(Bytes.toString(value).equals(Bytes.toString(CellUtil.cloneValue(cell))));
  byte[] referenceValue=CellUtil.cloneValue(cell);
  String fileName=Bytes.toString(referenceValue,8,referenceValue.length - 8);
  Path mobFamilyPath;
  mobFamilyPath=new Path(MobUtils.getMobRegionPath(TEST_UTIL.getConfiguration(),TableName.valueOf(TN)),hcd.getNameAsString());
  Path targetPath=new Path(mobFamilyPath,fileName);
  FileSystem fs=FileSystem.get(TEST_UTIL.getConfiguration());
  Assert.assertTrue(fs.exists(targetPath));
}
