{
  if (dfsCluster != null)   try {
    FileSystem fs=dfsCluster.getFileSystem();
    if (fs != null) {
      LOG.info("Shutting down FileSystem");
      fs.close();
    }
    FileSystem.closeAll();
    dfsCluster=null;
  }
 catch (  Exception e) {
    LOG.warn(StringUtils.stringifyException(e));
  }
}
