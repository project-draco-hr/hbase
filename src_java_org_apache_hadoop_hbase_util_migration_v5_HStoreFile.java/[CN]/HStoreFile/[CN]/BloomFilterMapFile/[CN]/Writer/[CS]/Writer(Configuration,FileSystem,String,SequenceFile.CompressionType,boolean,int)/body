{
  super(conf,fs,dirName,compression);
  this.dirName=dirName;
  this.fs=fs;
  if (filter) {
    this.bloomFilter=new BloomFilter((int)DEFAULT_NUMBER_OF_HASH_FUNCTIONS,(int)Math.ceil((DEFAULT_NUMBER_OF_HASH_FUNCTIONS * (1.0 * nrows)) / Math.log(2.0)),Hash.JENKINS_HASH);
  }
 else {
    this.bloomFilter=null;
  }
}
