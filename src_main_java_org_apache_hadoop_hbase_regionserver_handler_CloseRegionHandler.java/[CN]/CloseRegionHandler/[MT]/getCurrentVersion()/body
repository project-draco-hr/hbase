{
  int expectedVersion=FAILED;
  try {
    if ((expectedVersion=ZKAssign.getVersion(server.getZooKeeper(),regionInfo)) == FAILED) {
      LOG.warn("Error getting node's version in CLOSING state," + " aborting close of " + regionInfo.getRegionNameAsString());
    }
  }
 catch (  KeeperException e) {
    LOG.warn("Error creating node in CLOSING state, aborting close of " + regionInfo.getRegionNameAsString(),e);
  }
  return expectedVersion;
}
