{
  ByteBuffer keyValues=RedundantKVGenerator.convertKvToByteBuffer(generator.generateTestKeyValues(60),includesMemstoreTS);
  int size=keyValues.limit();
  ByteBuffer buf=ByteBuffer.allocate(size + HFileBlock.HEADER_SIZE);
  buf.position(HFileBlock.HEADER_SIZE);
  keyValues.rewind();
  buf.put(keyValues);
  HFileBlock b=new HFileBlock(BlockType.DATA,size,size,-1,buf,HFileBlock.FILL_HEADER,0,includesMemstoreTS);
  UNKNOWN_TABLE_AND_CF.passSchemaMetricsTo(b);
  return b;
}
