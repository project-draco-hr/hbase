{
  if (isSystemTable) {
    return ReturnCode.INCLUDE;
  }
  if (prevFam.getBytes() == null || (Bytes.compareTo(prevFam.getBytes(),prevFam.getOffset(),prevFam.getLength(),cell.getFamilyArray(),cell.getFamilyOffset(),cell.getFamilyLength()) != 0)) {
    prevFam.set(cell.getFamilyArray(),cell.getFamilyOffset(),cell.getFamilyLength());
    familyMaxVersions=cfVsMaxVersions.get(prevFam);
    prevQual.unset();
  }
  if (prevQual.getBytes() == null || (Bytes.compareTo(prevQual.getBytes(),prevQual.getOffset(),prevQual.getLength(),cell.getQualifierArray(),cell.getQualifierOffset(),cell.getQualifierLength()) != 0)) {
    prevQual.set(cell.getQualifierArray(),cell.getQualifierOffset(),cell.getQualifierLength());
    currentVersions=0;
  }
  currentVersions++;
  if (currentVersions > familyMaxVersions) {
    return ReturnCode.SKIP;
  }
  if (authManager.authorize(user,table,cell,cellFirstStrategy,Permission.Action.READ)) {
    return ReturnCode.INCLUDE;
  }
  return ReturnCode.SKIP;
}
