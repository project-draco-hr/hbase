{
  List<String> list=new ArrayList<String>();
  SequenceFile.Reader reader=new SequenceFile.Reader(conf,SequenceFile.Reader.file(path));
  String next=(String)reader.next((String)null);
  while (next != null) {
    list.add(next);
    next=(String)reader.next((String)null);
  }
  reader.close();
  return list;
}
