{
  final Map<HRegionInfo,HServerAddress> regions=new TreeMap<HRegionInfo,HServerAddress>();
  Visitor v=new Visitor(){
    @Override public boolean visit(    Result r) throws IOException {
      if (r == null || r.isEmpty())       return true;
      Pair<HRegionInfo,HServerAddress> region=metaRowToRegionPair(r);
      if (region == null)       return true;
      regions.put(region.getFirst(),region.getSecond());
      return true;
    }
  }
;
  fullScan(catalogTracker,v);
  return regions;
}
