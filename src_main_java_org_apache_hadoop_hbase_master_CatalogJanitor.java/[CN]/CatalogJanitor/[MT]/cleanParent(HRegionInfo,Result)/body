{
  boolean result=false;
  Pair<Boolean,Boolean> a=checkDaughter(parent,rowContent,HConstants.SPLITA_QUALIFIER);
  Pair<Boolean,Boolean> b=checkDaughter(parent,rowContent,HConstants.SPLITB_QUALIFIER);
  if ((a.getFirst() && !a.getSecond()) && (b.getFirst() && !b.getSecond())) {
    LOG.debug("Deleting region " + parent.getRegionNameAsString() + " because daughter splits no longer hold references");
    if (this.services.getAssignmentManager() != null) {
      this.services.getAssignmentManager().regionOffline(parent);
    }
    FileSystem fs=this.services.getMasterFileSystem().getFileSystem();
    Path rootdir=this.services.getMasterFileSystem().getRootDir();
    HRegion.deleteRegion(fs,rootdir,parent);
    MetaEditor.deleteRegion(this.server.getCatalogTracker(),parent);
    result=true;
  }
  return result;
}
