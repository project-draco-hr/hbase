{
  List<LoadBalancer.ServerAndLoad> result=new ArrayList<LoadBalancer.ServerAndLoad>(list.size());
  if (plans == null)   return result;
  Map<ServerName,LoadBalancer.ServerAndLoad> map=new HashMap<ServerName,LoadBalancer.ServerAndLoad>(list.size());
  for (  RegionPlan plan : plans) {
    ServerName source=plan.getSource();
    updateLoad(map,source,-1);
    ServerName destination=plan.getDestination();
    updateLoad(map,destination,+1);
  }
  result.clear();
  result.addAll(map.values());
  return result;
}
