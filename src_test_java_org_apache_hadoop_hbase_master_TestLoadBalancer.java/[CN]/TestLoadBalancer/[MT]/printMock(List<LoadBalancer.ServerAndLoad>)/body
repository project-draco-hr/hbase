{
  SortedSet<LoadBalancer.ServerAndLoad> sorted=new TreeSet<LoadBalancer.ServerAndLoad>(balancedCluster);
  LoadBalancer.ServerAndLoad[] arr=sorted.toArray(new LoadBalancer.ServerAndLoad[sorted.size()]);
  StringBuilder sb=new StringBuilder(sorted.size() * 4 + 4);
  sb.append("{ ");
  for (int i=0; i < arr.length; i++) {
    if (i != 0) {
      sb.append(" , ");
    }
    sb.append(arr[i].getLoad());
  }
  sb.append(" }");
  return sb.toString();
}
