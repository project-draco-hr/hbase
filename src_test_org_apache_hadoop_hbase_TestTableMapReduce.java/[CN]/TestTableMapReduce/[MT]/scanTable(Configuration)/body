{
  HClient client=new HClient(conf);
  client.openTable(new Text(TABLE_NAME));
  Text[] columns={TEXT_INPUT_COLUMN,TEXT_OUTPUT_COLUMN};
  HScannerInterface scanner=client.obtainScanner(columns,HClient.EMPTY_START_ROW);
  try {
    HStoreKey key=new HStoreKey();
    TreeMap<Text,byte[]> results=new TreeMap<Text,byte[]>();
    while (scanner.next(key,results)) {
      System.out.print("row: " + key.getRow());
      for (      Map.Entry<Text,byte[]> e : results.entrySet()) {
        System.out.print(" column: " + e.getKey() + " value: "+ new String(e.getValue()));
      }
      System.out.println();
    }
  }
  finally {
    scanner.close();
  }
}
