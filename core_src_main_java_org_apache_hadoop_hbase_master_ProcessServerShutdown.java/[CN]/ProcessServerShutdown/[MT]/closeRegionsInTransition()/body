{
  Map<String,RegionState> inTransition=master.getRegionManager().getRegionsInTransitionOnServer(deadServer);
  for (  Map.Entry<String,RegionState> entry : inTransition.entrySet()) {
    String regionName=entry.getKey();
    RegionState state=entry.getValue();
    LOG.info("Region " + regionName + " was in transition "+ state+ " on dead server "+ deadServer+ " - marking unassigned");
    master.getRegionManager().setUnassigned(state.getRegionInfo(),true);
  }
}
