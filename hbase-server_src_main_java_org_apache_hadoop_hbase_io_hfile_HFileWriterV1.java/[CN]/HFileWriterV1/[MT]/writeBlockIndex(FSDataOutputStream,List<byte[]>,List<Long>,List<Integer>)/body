{
  long pos=out.getPos();
  if (keys.size() > 0) {
    BlockType.INDEX_V1.write(out);
    for (int i=0; i < keys.size(); ++i) {
      out.writeLong(offsets.get(i).longValue());
      out.writeInt(uncompressedSizes.get(i).intValue());
      byte[] key=keys.get(i);
      Bytes.writeByteArray(out,key);
    }
  }
  return pos;
}
