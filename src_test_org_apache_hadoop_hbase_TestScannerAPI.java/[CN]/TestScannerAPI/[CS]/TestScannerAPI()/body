{
  super();
  try {
    TreeMap<byte[],Cell> columns=new TreeMap<byte[],Cell>(Bytes.BYTES_COMPARATOR);
    columns.put(Bytes.toBytes("a:1"),new Cell(Bytes.toBytes("1"),HConstants.LATEST_TIMESTAMP));
    values.put(Bytes.toBytes("1"),columns);
    columns=new TreeMap<byte[],Cell>(Bytes.BYTES_COMPARATOR);
    columns.put(Bytes.toBytes("a:2"),new Cell(Bytes.toBytes("2"),HConstants.LATEST_TIMESTAMP));
    columns.put(Bytes.toBytes("b:2"),new Cell(Bytes.toBytes("2"),HConstants.LATEST_TIMESTAMP));
  }
 catch (  Exception e) {
    e.printStackTrace();
    throw e;
  }
}
