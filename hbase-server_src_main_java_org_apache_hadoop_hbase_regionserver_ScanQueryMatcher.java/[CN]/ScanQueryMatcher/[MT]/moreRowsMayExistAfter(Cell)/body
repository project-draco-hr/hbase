{
  if (this.get) {
    return false;
  }
  if (this.stopRow == null || this.stopRow == HConstants.EMPTY_BYTE_ARRAY) {
    return true;
  }
  return this.isReversed ? rowComparator.compareRows(kv,stopRow,0,stopRow.length) > 0 : Bytes.equals(stopRow,HConstants.EMPTY_END_ROW) || rowComparator.compareRows(kv,stopRow,0,stopRow.length) < 0;
}
