{
  if (this.isReversed) {
    if (rowComparator.compareRows(kv,stopRow,0,stopRow.length) <= 0) {
      return false;
    }
 else {
      return true;
    }
  }
  if (!Bytes.equals(stopRow,HConstants.EMPTY_END_ROW) && rowComparator.compareRows(kv,stopRow,0,stopRow.length) >= 0) {
    return false;
  }
 else {
    return true;
  }
}
