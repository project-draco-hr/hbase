{
synchronized (grabTaskLock) {
    if (workerInGrabTask) {
      String taskpath=currentTask;
      if (taskpath != null && taskpath.equals(path)) {
        if (!TaskState.TASK_OWNED.equals(data,serverName) && !TaskState.TASK_DONE.equals(data,serverName) && !TaskState.TASK_ERR.equals(data,serverName)&& !TaskState.TASK_RESIGNED.equals(data,serverName)) {
          LOG.info("task " + taskpath + " preempted from server "+ serverName+ " ... current task state and owner - "+ new String(data));
          stopTask();
        }
      }
    }
  }
}
