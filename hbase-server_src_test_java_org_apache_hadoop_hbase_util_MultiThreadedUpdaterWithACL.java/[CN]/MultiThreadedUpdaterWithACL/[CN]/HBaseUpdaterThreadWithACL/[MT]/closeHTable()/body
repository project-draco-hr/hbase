{
  try {
    if (table != null) {
      table.close();
    }
    for (    HTableInterface table : userVsTable.values()) {
      try {
        table.close();
      }
 catch (      Exception e) {
        LOG.error("Error while closing the table " + table.getName(),e);
      }
    }
  }
 catch (  Exception e) {
    LOG.error("Error while closing the HTable " + table.getName(),e);
  }
}
