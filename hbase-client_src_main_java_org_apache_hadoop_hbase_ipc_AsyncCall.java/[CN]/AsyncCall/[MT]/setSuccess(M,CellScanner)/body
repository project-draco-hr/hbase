{
  if (LOG.isTraceEnabled()) {
    long callTime=EnvironmentEdgeManager.currentTime() - startTime;
    LOG.trace("Call: " + method.getName() + ", callTime: "+ callTime+ "ms");
  }
  try {
    this.setSuccess(this.messageConverter.convert(value,cellBlockScanner));
  }
 catch (  IOException e) {
    this.setFailed(e);
  }
}
