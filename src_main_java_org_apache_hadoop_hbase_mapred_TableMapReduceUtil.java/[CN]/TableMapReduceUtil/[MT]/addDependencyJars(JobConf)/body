{
  org.apache.hadoop.hbase.mapreduce.TableMapReduceUtil.addDependencyJars(job,org.apache.zookeeper.ZooKeeper.class,com.google.common.base.Function.class,job.getMapOutputKeyClass(),job.getMapOutputValueClass(),job.getOutputKeyClass(),job.getOutputValueClass(),job.getPartitionerClass(),job.getClass("mapred.input.format.class",TextInputFormat.class,InputFormat.class),job.getClass("mapred.output.format.class",TextOutputFormat.class,OutputFormat.class),job.getCombinerClass());
}
