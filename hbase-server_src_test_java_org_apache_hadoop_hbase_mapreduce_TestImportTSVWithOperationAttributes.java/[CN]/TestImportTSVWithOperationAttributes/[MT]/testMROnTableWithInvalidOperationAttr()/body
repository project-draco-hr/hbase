{
  String tableName="test-" + UUID.randomUUID();
  String[] args=new String[]{"-D" + ImportTsv.MAPPER_CONF_KEY + "=org.apache.hadoop.hbase.mapreduce.TsvImporterCustomTestMapperForOprAttr","-D" + ImportTsv.COLUMNS_CONF_KEY + "=HBASE_ROW_KEY,FAM:A,FAM:B,HBASE_ATTRIBUTES_KEY","-D" + ImportTsv.SEPARATOR_CONF_KEY + "=\u001b",tableName};
  String data="KEY\u001bVALUE1\u001bVALUE2\u001btest1=>myvalue\n";
  util.createTable(TableName.valueOf(tableName),FAMILY);
  doMROnTableTest(util,FAMILY,data,args,1,false);
  util.deleteTable(tableName);
}
