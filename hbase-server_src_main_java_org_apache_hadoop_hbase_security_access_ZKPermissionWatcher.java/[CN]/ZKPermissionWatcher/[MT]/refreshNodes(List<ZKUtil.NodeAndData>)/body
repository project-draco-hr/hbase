{
  for (  ZKUtil.NodeAndData n : nodes) {
    if (n.isEmpty())     continue;
    String path=n.getNode();
    TableName table=TableName.valueOf(ZKUtil.getNodeName(path));
    try {
      byte[] nodeData=n.getData();
      if (LOG.isDebugEnabled()) {
        LOG.debug("Updating permissions cache from node " + table + " with data: "+ Bytes.toStringBinary(nodeData));
      }
      authManager.refreshCacheFromWritable(table,nodeData);
    }
 catch (    IOException ioe) {
      LOG.error("Failed parsing permissions for table '" + table + "' from zk",ioe);
    }
  }
}
