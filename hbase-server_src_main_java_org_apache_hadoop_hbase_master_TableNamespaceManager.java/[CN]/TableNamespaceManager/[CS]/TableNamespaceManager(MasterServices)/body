{
  this.masterServices=masterServices;
  this.conf=masterServices.getConfiguration();
  this.exclusiveLockTimeoutMs=conf.getLong(TABLE_WRITE_LOCK_TIMEOUT_MS,DEFAULT_TABLE_WRITE_LOCK_TIMEOUT_MS);
  this.sharedLockTimeoutMs=conf.getLong(TABLE_READ_LOCK_TIMEOUT_MS,DEFAULT_TABLE_READ_LOCK_TIMEOUT_MS);
}
