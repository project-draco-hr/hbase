{
  Result res=table.get(new Get(Bytes.toBytes(name)));
  if (res.isEmpty()) {
    return null;
  }
  return ProtobufUtil.toNamespaceDescriptor(HBaseProtos.NamespaceDescriptor.parseFrom(res.getColumnLatest(HTableDescriptor.NAMESPACE_FAMILY_INFO_BYTES,HTableDescriptor.NAMESPACE_COL_DESC_BYTES).getValue()));
}
