{
  callOnce=new Expectations(){
{
      oneOf(log).append(with(any(HTableDescriptor.class)),with(any(HRegionInfo.class)),with(any(WALKey.class)),with(bulkLogWalEditType(WALEdit.BULK_LOAD)),with(any(boolean.class)));
      will(AppendAction.append());
      oneOf(log).sync(with(any(long.class)));
    }
  }
;
}
