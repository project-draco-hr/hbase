{
  Put put=null;
  Delete delete=null;
  if (LOG.isTraceEnabled()) {
    LOG.trace("Considering the row." + Bytes.toString(key.get(),key.getOffset(),key.getLength()));
  }
  if (filter == null || !filter.filterRowKey(KeyValueUtil.createFirstOnRow(key.get(),key.getOffset(),(short)key.getLength()))) {
    processKV(key,result,context,put,delete);
  }
}
