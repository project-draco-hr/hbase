{
  prepareTest("FlippingEncodeOnDisk");
  DataBlockEncoding[] encodings=new DataBlockEncoding[]{DataBlockEncoding.NONE,DataBlockEncoding.FAST_DIFF};
  for (  DataBlockEncoding encoding : encodings) {
    boolean[] flagValues;
    if (encoding == DataBlockEncoding.NONE) {
      flagValues=new boolean[]{HColumnDescriptor.DEFAULT_ENCODE_ON_DISK};
    }
 else {
      flagValues=new boolean[]{false,true,false,true};
    }
    for (    boolean encodeOnDisk : flagValues) {
      setEncodingConf(encoding,encodeOnDisk);
      writeSomeNewData();
      verifyAllData();
      compactAndWait();
      verifyAllData();
    }
  }
}
