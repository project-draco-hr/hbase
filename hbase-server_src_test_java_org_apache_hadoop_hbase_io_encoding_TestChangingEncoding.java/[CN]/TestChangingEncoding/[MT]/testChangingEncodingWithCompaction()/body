{
  prepareTest("ChangingEncodingWithCompaction");
  for (  boolean encodeOnDisk : new boolean[]{false,true}) {
    for (    DataBlockEncoding encoding : ENCODINGS_TO_ITERATE) {
      setEncodingConf(encoding,encodeOnDisk);
      writeSomeNewData();
      verifyAllData();
      compactAndWait();
      verifyAllData();
    }
  }
}
