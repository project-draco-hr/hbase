{
  for (  String serverName : servers) {
    SchemaChangeTracker.SchemaAlterStatus sas=getRSSchemaAlterStatus(tableName,serverName);
    if (sas != null) {
      mas.update(sas);
      LOG.debug("processTableNodeWithState:Updated Master Alter Status = " + mas + " for server = "+ serverName);
    }
 else {
      LOG.debug("SchemaAlterStatus is NULL for table = " + tableName);
    }
  }
}
