{
  String childPath=getSchemaChangeNodePathForTableAndServer(tableName,serverName);
  byte[] childData=ZKUtil.getData(this.watcher,childPath);
  if (childData == null || childData.length <= 0) {
    return null;
  }
  SchemaChangeTracker.SchemaAlterStatus sas=new SchemaChangeTracker.SchemaAlterStatus();
  Writables.getWritable(childData,sas);
  LOG.debug("Schema Status data for server = " + serverName + " table = "+ tableName+ " == "+ sas);
  return sas;
}
