{
switch (timeUnit) {
case NANOSECONDS:
    return "nsec";
case MICROSECONDS:
  return "usec";
case MILLISECONDS:
return "msec";
case SECONDS:
return "sec";
case MINUTES:
return "min";
case HOURS:
return "hour";
case DAYS:
return "day";
}
throw new RuntimeException("Invalid TimeUnit " + timeUnit);
}
