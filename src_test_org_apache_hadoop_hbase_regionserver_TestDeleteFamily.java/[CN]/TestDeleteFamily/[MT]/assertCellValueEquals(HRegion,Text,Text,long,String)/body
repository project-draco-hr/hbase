{
  Map<Text,byte[]> result=region.getFull(row,timestamp);
  byte[] cell_value=result.get(column);
  if (value == null) {
    assertEquals(column.toString() + " at timestamp " + timestamp,null,cell_value);
  }
 else {
    if (cell_value == null) {
      fail(column.toString() + " at timestamp " + timestamp+ "\" was expected to be \""+ value+ " but was null");
    }
    assertEquals(column.toString() + " at timestamp " + timestamp,value,new String(cell_value));
  }
}
