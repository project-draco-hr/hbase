{
  if (writer != null) {
    writer.appendMetadata(maxSeqId,false);
    writer.appendFileInfo(StoreFile.BULKLOAD_TIME_KEY,Bytes.toBytes(bulkloadTime));
    try {
      writer.close();
    }
 catch (    IOException e) {
      LOG.error("Failed to close the writer of the ref file " + writer.getPath(),e);
    }
  }
}
