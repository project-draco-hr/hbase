{
  Cell startKV=cur;
  while (enforceMVCC && cur != null && (cur.getMvccVersion() > readPt)) {
    hfs.next();
    setCurrentCell(hfs.getKeyValue());
    if (this.stopSkippingKVsIfNextRow && getComparator().compareRows(cur,startKV) > 0) {
      return false;
    }
  }
  if (cur == null) {
    close();
    return false;
  }
  return true;
}
