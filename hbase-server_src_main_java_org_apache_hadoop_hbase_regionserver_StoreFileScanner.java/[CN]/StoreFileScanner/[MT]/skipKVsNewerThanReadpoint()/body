{
  long readPoint=MultiVersionConsistencyControl.getThreadReadPoint();
  while (enforceMVCC && cur != null && (cur.getMvccVersion() > readPoint)) {
    hfs.next();
    cur=hfs.getKeyValue();
  }
  if (cur == null) {
    close();
    return false;
  }
  if (cur.getMvccVersion() <= readPoint) {
    cur.setMvccVersion(0);
  }
  return true;
}
