{
  KeyValue startKV=cur;
  while (enforceMVCC && cur != null && (cur.getMvccVersion() > readPt)) {
    hfs.next();
    cur=hfs.getKeyValue();
    if (this.stopSkippingKVsIfNextRow && Bytes.compareTo(cur.getRowArray(),cur.getRowOffset(),cur.getRowLength(),startKV.getRowArray(),startKV.getRowOffset(),startKV.getRowLength()) > 0) {
      return false;
    }
  }
  if (cur == null) {
    close();
    return false;
  }
  if (cur.getMvccVersion() <= readPt) {
    cur.setMvccVersion(0);
  }
  return true;
}
