{
  if (seekCount != null)   seekCount.incrementAndGet();
  try {
    try {
      if (!reseekAtOrAfter(hfs,key)) {
        close();
        return false;
      }
      cur=hfs.getKeyValue();
      return skipKVsNewerThanReadpoint();
    }
  finally {
      realSeekDone=true;
    }
  }
 catch (  IOException ioe) {
    throw new IOException("Could not reseek " + this + " to key "+ key,ioe);
  }
}
