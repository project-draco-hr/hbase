{
  Cell retKey=cur;
  try {
    if (cur != null) {
      hfs.next();
      setCurrentCell(hfs.getKeyValue());
      if (hasMVCCInfo || this.reader.isBulkLoaded()) {
        skipKVsNewerThanReadpoint();
      }
    }
  }
 catch (  IOException e) {
    throw new IOException("Could not iterate " + this,e);
  }
  return retKey;
}
