{
  LinkedList<HTableInterface> queue=tables.get(tableName);
  if (queue == null) {
    queue=new LinkedList<HTableInterface>();
    tables.putIfAbsent(tableName,queue);
    return createHTable(tableName);
  }
  HTableInterface table;
synchronized (queue) {
    table=queue.poll();
  }
  if (table == null) {
    return createHTable(tableName);
  }
  return table;
}
