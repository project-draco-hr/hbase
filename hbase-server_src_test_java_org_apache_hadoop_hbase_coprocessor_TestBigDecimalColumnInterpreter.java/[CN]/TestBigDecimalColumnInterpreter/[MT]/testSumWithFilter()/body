{
  AggregationClient aClient=new AggregationClient(conf);
  Filter f=new PrefixFilter(Bytes.toBytes("foo:bar"));
  Scan scan=new Scan();
  scan.addFamily(TEST_FAMILY);
  scan.setFilter(f);
  final ColumnInterpreter<BigDecimal,BigDecimal,EmptyMsg,BigDecimalMsg,BigDecimalMsg> ci=new BigDecimalColumnInterpreter();
  BigDecimal sum=null;
  sum=aClient.sum(TEST_TABLE,ci,scan);
  assertEquals(null,sum);
}
