{
  short lrowlength=left.getRowLength();
  short rrowlength=right.getRowLength();
  return ((left.getTimestampOffset() - left.getOffset()) == (right.getTimestampOffset() - right.getOffset())) && matchingRows(left,lrowlength,right,rrowlength) && compareColumns(left,lrowlength,right,rrowlength) == 0;
}
