{
  try {
    setup();
    locks();
    badPuts();
    basic();
    scan();
    batchWrite();
    splitAndMerge();
    read();
    cleanup();
  }
  finally {
    if (r != null) {
      r.close();
    }
    if (log != null) {
      log.closeAndDelete();
    }
    StaticTestEnvironment.shutdownDfs(cluster);
  }
}
