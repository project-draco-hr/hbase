{
  init();
  HTable ht=createTable(TABLE,FAMILY);
  Put put=new Put(ROW);
  put.add(FAMILY,QUALIFIER,VALUE);
  ht.put(put);
  admin.disableTable(TABLE);
  Get get=new Get(ROW);
  get.addColumn(FAMILY,QUALIFIER);
  boolean ok=false;
  try {
    ht.get(get);
  }
 catch (  RetriesExhaustedException e) {
    ok=true;
  }
  assertEquals(true,ok);
  admin.enableTable(TABLE);
  try {
    ht.get(get);
  }
 catch (  RetriesExhaustedException e) {
    ok=false;
  }
  assertEquals(true,ok);
}
