{
  ServerName sn=new ServerName(ia.getHostName(),port,serverStartcode);
  checkClockSkew(sn,serverCurrentTime);
  checkIsDead(sn,"STARTUP");
  if (!checkAlreadySameHostPortAndRecordNewServer(sn,ServerLoad.EMPTY_SERVERLOAD)) {
    LOG.warn("THIS SHOULD NOT HAPPEN, RegionServerStartup" + " could not record the server: " + sn);
  }
  return sn;
}
