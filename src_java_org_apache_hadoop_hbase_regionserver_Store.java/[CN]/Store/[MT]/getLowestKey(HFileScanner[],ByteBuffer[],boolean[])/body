{
  int lowestKey=-1;
  for (int i=0; i < rdrs.length; i++) {
    if (done[i]) {
      continue;
    }
    if (lowestKey < 0) {
      lowestKey=i;
    }
 else {
      RawComparator<byte[]> c=rdrs[i].getReader().getComparator();
      if (c.compare(keys[i].array(),keys[i].arrayOffset(),keys[i].limit(),keys[lowestKey].array(),keys[lowestKey].arrayOffset(),keys[lowestKey].limit()) < 0) {
        lowestKey=i;
      }
    }
  }
  return lowestKey;
}
