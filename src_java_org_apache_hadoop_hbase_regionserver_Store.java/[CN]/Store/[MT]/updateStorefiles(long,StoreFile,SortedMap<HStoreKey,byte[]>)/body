{
  int count=0;
  this.lock.writeLock().lock();
  try {
    this.storefiles.put(Long.valueOf(logCacheFlushId),sf);
    count=this.storefiles.size();
    notifyChangedReadersObservers();
    this.memcache.clearSnapshot(cache);
    return count;
  }
  finally {
    this.lock.writeLock().unlock();
  }
}
