{
  try {
    doReconstructionLog(reconstructionLog,msid,reporter);
  }
 catch (  EOFException e) {
    LOG.warn("Exception processing reconstruction log " + reconstructionLog + " opening "+ Bytes.toString(this.storeName)+ " -- continuing.  Probably lack-of-HADOOP-1700 causing DATA LOSS!",e);
  }
catch (  IOException e) {
    LOG.warn("Exception processing reconstruction log " + reconstructionLog + " opening "+ Bytes.toString(this.storeName),e);
    throw e;
  }
}
