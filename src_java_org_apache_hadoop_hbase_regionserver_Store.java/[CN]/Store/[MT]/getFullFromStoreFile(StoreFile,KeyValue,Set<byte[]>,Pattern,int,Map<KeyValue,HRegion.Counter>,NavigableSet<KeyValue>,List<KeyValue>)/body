{
  long now=System.currentTimeMillis();
  HFileScanner scanner=f.getReader().getScanner();
  if (!getClosest(scanner,target)) {
    return false;
  }
  boolean hasEnough=false;
  do {
    KeyValue kv=scanner.getKeyValue();
    if (target.isEmptyColumn() ? !this.comparator.matchingRows(target,kv) : !this.comparator.matchingRowColumn(target,kv)) {
      break;
    }
    if (!Store.getFullCheck(this.comparator,target,kv,columns,columnPattern)) {
      continue;
    }
    if (Store.doKeyValue(kv,versions,versionCounter,columns,deletes,now,this.ttl,keyvalues,null)) {
      hasEnough=true;
      break;
    }
  }
 while (scanner.next());
  return hasEnough;
}
