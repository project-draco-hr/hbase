{
  boolean foundCandidate=false;
  do {
    KeyValue kv=scanner.getKeyValue();
    if (this.comparator.compareRows(kv,firstOnRow) < 0)     continue;
    if (state.isTooFar(kv,firstOnRow))     break;
    if (state.isExpired(kv)) {
      continue;
    }
    if (state.handle(kv)) {
      foundCandidate=true;
      break;
    }
  }
 while (scanner.next());
  return foundCandidate;
}
