{
  if (!Store.matchingColumns(candidate,columns)) {
    return false;
  }
  if (columnPattern != null) {
    if (!(columnPattern.matcher(candidate.getColumnString()).matches())) {
      return false;
    }
  }
  if (c.compareTimestamps(target,candidate) > 0) {
    return false;
  }
  return true;
}
