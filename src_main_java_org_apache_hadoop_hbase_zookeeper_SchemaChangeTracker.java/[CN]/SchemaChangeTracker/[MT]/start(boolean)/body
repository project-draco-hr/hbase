{
  try {
    watcher.registerListener(this);
    ZKUtil.listChildrenAndWatchThem(watcher,node);
    return true;
  }
 catch (  KeeperException e) {
    if (allowAbort && (abortable != null)) {
      abortable.abort("RegionServer SchemaChangeTracker startup failed",e);
    }
 else {
      LOG.error("RegionServer SchemaChangeTracker startup failed with " + "KeeperException.",e);
    }
    return false;
  }
}
