{
  byte[] statusBytes=ZKUtil.getData(this.watcher,getSchemaChangeNodePathForTableAndServer(tableName,regionServer.getServerName().getServerName()));
  if (statusBytes == null || statusBytes.length <= 0) {
    return null;
  }
  SchemaAlterStatus sas=new SchemaAlterStatus();
  Writables.getWritable(statusBytes,sas);
  return sas;
}
