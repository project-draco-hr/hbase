{
  Map<String,Long> currentMetricsSnapshot=SchemaMetrics.getMetricsSnapshot();
  Map<String,Long> diffs=SchemaMetrics.diffMetrics(previousMetricSnapshot,currentMetricsSnapshot);
  long dataBlockRead=SchemaMetrics.getLong(diffs,schemaMetrics.getBlockMetricName(BlockCategory.DATA,false,BlockMetricType.READ_COUNT));
  long indexBlockRead=SchemaMetrics.getLong(diffs,schemaMetrics.getBlockMetricName(BlockCategory.INDEX,false,BlockMetricType.READ_COUNT));
  Assert.assertEquals(expectDataBlockRead,dataBlockRead);
  Assert.assertEquals(expectedIndexBlockRead,indexBlockRead);
}
