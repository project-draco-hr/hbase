{
  boolean scheduled=false;
  HRegion r=this.server.getOnlineRegion(region);
  FlushRequester requester=null;
  if (r != null) {
    requester=this.server.getFlushRequester();
    if (requester != null) {
      requester.request(r);
      scheduled=true;
    }
  }
  if (!scheduled) {
    LOG.warn("Failed to schedule flush of " + Bytes.toString(region) + "r="+ r+ ", requester="+ requester);
  }
}
