{
  LOG.info("Creating on-heap cache of size " + StringUtils.humanReadableInt(onHeapSize) + "bytes with an average block size of "+ StringUtils.humanReadableInt(blockSizeLru)+ " bytes.");
  onHeapCache=new LruBlockCache(onHeapSize,blockSizeLru);
  LOG.info("Creating off-heap cache of size " + StringUtils.humanReadableInt(offHeapSize) + "bytes with an average block size of "+ StringUtils.humanReadableInt(blockSizeSlab)+ " bytes.");
  offHeapCache=new SlabCache(offHeapSize,blockSizeSlab);
  this.stats=new CacheStats();
}
