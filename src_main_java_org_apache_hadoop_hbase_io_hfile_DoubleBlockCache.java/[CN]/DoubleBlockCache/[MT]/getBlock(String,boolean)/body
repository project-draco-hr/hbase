{
  Cacheable cachedBlock;
  if ((cachedBlock=onHeapCache.getBlock(blockName,caching)) != null) {
    stats.hit(caching);
    return cachedBlock;
  }
 else   if ((cachedBlock=offHeapCache.getBlock(blockName,caching)) != null) {
    if (caching) {
      onHeapCache.cacheBlock(blockName,cachedBlock);
    }
    stats.hit(caching);
    return cachedBlock;
  }
  stats.miss(caching);
  return null;
}
