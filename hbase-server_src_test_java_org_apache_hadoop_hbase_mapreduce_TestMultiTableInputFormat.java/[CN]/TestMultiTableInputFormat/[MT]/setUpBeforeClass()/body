{
  TEST_UTIL.enableDebug(MultiTableInputFormat.class);
  TEST_UTIL.enableDebug(MultiTableInputFormatBase.class);
  TEST_UTIL.startMiniCluster(3);
  for (int i=0; i < 3; i++) {
    try (HTable table=TEST_UTIL.createMultiRegionTable(TableName.valueOf(TABLE_NAME + String.valueOf(i)),INPUT_FAMILY,4)){
      TEST_UTIL.loadTable(table,INPUT_FAMILY,false);
    }
   }
  TEST_UTIL.startMiniMapReduceCluster();
}
