{
  if ((buf.position() + extra) > buf.limit()) {
    int newSize=(int)Math.min((((long)buf.capacity()) * 2),(long)(Integer.MAX_VALUE));
    newSize=Math.max(newSize,buf.position() + extra);
    ByteBuffer newBuf=ByteBuffer.allocate(newSize);
    buf.flip();
    newBuf.put(buf);
    buf=newBuf;
  }
}
