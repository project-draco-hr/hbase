{
  for (  JVMClusterUtil.RegionServerThread rst : miniCluster.getRegionServerThreads()) {
    for (    HRegion hri : rst.getRegionServer().getOnlineRegions(tableName)) {
      assertEquals("The bloom filter did not match expected value " + expectedType + " on RS "+ rst.getName()+ " region "+ hri.getRegionNameAsString(),expectedType,hri.getTableDesc().getColumnFamilies()[0].getBloomFilterType());
    }
  }
}
