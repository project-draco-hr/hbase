{
  if (length != this.value.length) {
    return 1;
  }
  int b=0;
  for (int i=value.length - 1; i >= 0 && b == 0; i--) {
switch (bitOperator) {
case AND:
      b=(this.value[i] & value[i + offset]) & 0xff;
    break;
case OR:
  b=(this.value[i] | value[i + offset]) & 0xff;
break;
case XOR:
b=(this.value[i] ^ value[i + offset]) & 0xff;
break;
}
}
return b == 0 ? 1 : 0;
}
