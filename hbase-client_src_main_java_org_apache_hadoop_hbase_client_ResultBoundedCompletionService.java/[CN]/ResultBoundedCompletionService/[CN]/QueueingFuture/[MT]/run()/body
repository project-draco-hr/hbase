{
  try {
    if (!cancelled) {
      result=this.retryingCaller.callWithRetries(future,callTimeout);
      resultObtained=true;
    }
  }
 catch (  Throwable t) {
    exeEx=new ExecutionException(t);
  }
 finally {
synchronized (tasks) {
      if (!cancelled && completed == null) {
        completed=(QueueingFuture<V>)QueueingFuture.this;
      }
      tasks.notify();
    }
  }
}
