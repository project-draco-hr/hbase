{
  TreeSet<HRegionInfo> regions=new TreeSet<HRegionInfo>();
  if (latestRegion == null) {
    latestRegion=nextRegion();
  }
  if (latestRegion != null) {
    regions.add(latestRegion);
  }
  latestRegion=nextRegion();
  if (latestRegion != null) {
    regions.add(latestRegion);
  }
  return regions;
}
