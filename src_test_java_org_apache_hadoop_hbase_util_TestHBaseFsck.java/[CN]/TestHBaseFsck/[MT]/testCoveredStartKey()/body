{
  String table="tableCoveredStartKey";
  try {
    setupTable(table);
    HRegionInfo hriOverlap=createRegion(conf,tbl.getTableDescriptor(),Bytes.toBytes("A2"),Bytes.toBytes("B2"));
    TEST_UTIL.getHBaseCluster().getMaster().assignRegion(hriOverlap);
    TEST_UTIL.getHBaseCluster().getMaster().getAssignmentManager().waitForAssignment(hriOverlap);
    HBaseFsck hbck=doFsck(false);
    assertErrors(hbck,new ERROR_CODE[]{ERROR_CODE.OVERLAP_IN_REGION_CHAIN,ERROR_CODE.OVERLAP_IN_REGION_CHAIN});
    assertEquals(3,hbck.getOverlapGroups(table).size());
  }
  finally {
    deleteTable(table);
  }
}
