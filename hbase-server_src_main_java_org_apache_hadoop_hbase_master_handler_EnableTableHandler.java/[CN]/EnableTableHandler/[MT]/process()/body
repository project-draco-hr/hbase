{
  try {
    LOG.info("Attempting to enable the table " + this.tableName);
    MasterCoprocessorHost cpHost=((HMaster)this.server).getMasterCoprocessorHost();
    if (cpHost != null) {
      cpHost.preEnableTableAction(this.tableName);
    }
    handleEnableTable();
    if (cpHost != null) {
      cpHost.postCompletedEnableTableAction(this.tableName);
    }
  }
 catch (  IOException|InterruptedException e) {
    LOG.error("Error trying to enable the table " + this.tableName,e);
  }
 finally {
    releaseTableLock();
  }
}
