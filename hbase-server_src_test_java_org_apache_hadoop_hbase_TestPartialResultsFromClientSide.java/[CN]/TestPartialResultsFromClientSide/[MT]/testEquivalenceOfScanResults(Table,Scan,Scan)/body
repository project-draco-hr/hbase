{
  ResultScanner scanner1=table.getScanner(scan1);
  ResultScanner scanner2=table.getScanner(scan2);
  Result r1=null;
  Result r2=null;
  int count=0;
  while ((r1=scanner1.next()) != null) {
    r2=scanner2.next();
    assertTrue(r2 != null);
    compareResults(r1,r2,"Comparing result #" + count);
    count++;
  }
  assertTrue(scanner2.next() == null);
  scanner1.close();
  scanner2.close();
}
