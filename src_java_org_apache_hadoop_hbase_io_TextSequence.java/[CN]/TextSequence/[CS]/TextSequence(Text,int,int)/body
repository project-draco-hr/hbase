{
  this.delegatee=d;
  if (s < 0 || s >= d.getLength()) {
    throw new IllegalArgumentException("Nonsensical start position " + s);
  }
  this.start=s;
  if (e == -1) {
    this.end=this.delegatee.getLength();
  }
 else   if (e <= 0 || e > d.getLength()) {
    throw new IllegalArgumentException("Nonsensical start position " + s);
  }
 else {
    this.end=e;
  }
}
