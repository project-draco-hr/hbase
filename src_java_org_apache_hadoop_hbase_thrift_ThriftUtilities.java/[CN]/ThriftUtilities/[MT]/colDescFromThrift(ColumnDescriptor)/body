{
  CompressionType comp=CompressionType.valueOf(in.compression);
  boolean bloom=false;
  if (in.bloomFilterType.compareTo("NONE") != 0) {
    bloom=true;
  }
  if (in.name == null || in.name.length <= 0) {
    throw new IllegalArgument("column name is empty");
  }
  HColumnDescriptor col=new HColumnDescriptor(in.name,in.maxVersions,comp,in.inMemory,in.blockCacheEnabled,in.maxValueLength,in.timeToLive,bloom);
  return col;
}
