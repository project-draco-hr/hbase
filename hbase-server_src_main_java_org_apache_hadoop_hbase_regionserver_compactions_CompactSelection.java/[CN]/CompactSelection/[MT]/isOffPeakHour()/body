{
  int currentHour=(new GregorianCalendar()).get(Calendar.HOUR_OF_DAY);
  if (this.offPeakStartHour == this.offPeakEndHour) {
    return false;
  }
  if (this.offPeakStartHour < this.offPeakEndHour) {
    return (currentHour >= this.offPeakStartHour && currentHour < this.offPeakEndHour);
  }
  return (currentHour >= this.offPeakStartHour || currentHour < this.offPeakEndHour);
}
