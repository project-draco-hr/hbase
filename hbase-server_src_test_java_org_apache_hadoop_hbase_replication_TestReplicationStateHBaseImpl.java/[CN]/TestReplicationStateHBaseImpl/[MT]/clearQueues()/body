{
  rq1.removeAllQueues();
  rq2.removeAllQueues();
  rq3.removeAllQueues();
  assertEquals(0,rq1.getAllQueues().size());
  assertEquals(0,rq2.getAllQueues().size());
  assertEquals(0,rq3.getAllQueues().size());
  ds0.resetAbortCount();
  ds1.resetAbortCount();
  ds2.resetAbortCount();
  ds3.resetAbortCount();
}
