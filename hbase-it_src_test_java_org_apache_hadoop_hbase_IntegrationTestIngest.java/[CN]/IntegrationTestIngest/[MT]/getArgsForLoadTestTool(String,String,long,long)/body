{
  List<String> args=new ArrayList<String>();
  args.add("-tn");
  args.add(getTablename());
  args.add(mode);
  args.add(modeSpecificArg);
  args.add("-start_key");
  args.add(String.valueOf(startKey));
  args.add("-num_keys");
  args.add(String.valueOf(numKeys));
  args.add("-skip_init");
  return args.toArray(new String[args.size()]);
}
