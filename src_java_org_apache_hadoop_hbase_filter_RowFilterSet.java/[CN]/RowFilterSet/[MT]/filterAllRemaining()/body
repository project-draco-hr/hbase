{
  boolean result=operator == Operator.OR;
  for (  RowFilterInterface filter : filters) {
    if (operator == Operator.AND) {
      if (filter.filterAllRemaining()) {
        return true;
      }
    }
 else     if (operator == Operator.OR) {
      if (!filter.filterAllRemaining()) {
        return false;
      }
    }
  }
  return result;
}
