{
  boolean result=operator == Operator.MUST_PASS_ONE;
  for (  RowFilterInterface filter : filters) {
    if (operator == Operator.MUST_PASS_ALL) {
      if (filter.filterAllRemaining()) {
        return true;
      }
    }
 else     if (operator == Operator.MUST_PASS_ONE) {
      if (!filter.filterAllRemaining()) {
        return false;
      }
    }
  }
  return result;
}
