{
  if (checkCommitsSinceFlush()) {
    return;
  }
  LOG.warn("Blocking updates for '" + Thread.currentThread().getName() + "': Memcache size "+ StringUtils.humanReadableInt(this.memcache.getSize())+ " is >= than blocking "+ StringUtils.humanReadableInt(this.blockingMemcacheSize)+ " size");
  while (!checkCommitsSinceFlush()) {
    try {
      wait();
    }
 catch (    InterruptedException e) {
    }
  }
  LOG.warn("Unblocking updates for '" + Thread.currentThread().getName() + "'");
}
