{
  lock.obtainReadLock();
  try {
    BytesWritable[] result=memcache.get(key,numVersions);
    if (result != null) {
      return result;
    }
    Text colFamily=HStoreKey.extractFamily(key.getColumn());
    HStore targetStore=stores.get(colFamily);
    if (targetStore == null) {
      return null;
    }
    return targetStore.get(key,numVersions);
  }
  finally {
    lock.releaseReadLock();
  }
}
