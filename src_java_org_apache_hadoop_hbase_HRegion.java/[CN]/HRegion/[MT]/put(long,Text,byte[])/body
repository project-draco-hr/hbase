{
  if (val.length == HStoreKey.DELETE_BYTES.length) {
    boolean matches=true;
    for (int i=0; i < val.length; i++) {
      if (val[i] != HStoreKey.DELETE_BYTES[i]) {
        matches=false;
        break;
      }
    }
    if (matches) {
      throw new IOException("Cannot insert value: " + val);
    }
  }
  localput(lockid,targetCol,val);
}
