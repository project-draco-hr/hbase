{
  if (writestate.closed) {
    throw new IOException("HRegion is closed.");
  }
  checkRow(row);
  Text colFamily=HStoreKey.extractFamily(column);
  checkFamily(colFamily);
  obtainLock(row);
  try {
    return get(new HStoreKey(row,column,timestamp),numVersions);
  }
  finally {
    releaseLock(row);
  }
}
