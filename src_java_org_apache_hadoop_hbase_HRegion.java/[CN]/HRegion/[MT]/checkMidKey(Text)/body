{
  if (((this.regionInfo.getStartKey().getLength() != 0) && (this.regionInfo.getStartKey().compareTo(midKey) > 0)) || ((this.regionInfo.getEndKey().getLength() != 0) && (this.regionInfo.getEndKey().compareTo(midKey) < 0))) {
    throw new IOException("Region splitkey must lie within region " + "boundaries.");
  }
}
