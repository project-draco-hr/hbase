{
  boolean needsCompaction=false;
  for (  HStore store : stores.values()) {
    if (store.needsCompaction()) {
      needsCompaction=true;
      if (LOG.isDebugEnabled()) {
        LOG.debug(store.toString() + " needs compaction");
      }
      break;
    }
  }
  if (!needsCompaction) {
    return false;
  }
  return compactStores();
}
