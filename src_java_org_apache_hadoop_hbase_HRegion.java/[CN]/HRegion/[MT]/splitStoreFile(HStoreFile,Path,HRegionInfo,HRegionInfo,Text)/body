{
  if (LOG.isDebugEnabled()) {
    LOG.debug("Started splitting HStore " + hsf.getRegionName() + "/"+ hsf.getColFamily()+ "/"+ hsf.fileId());
  }
  HStoreFile dstA=new HStoreFile(conf,splits,a.regionName,hsf.getColFamily(),Math.abs(rand.nextLong()));
  HStoreFile dstB=new HStoreFile(conf,splits,b.regionName,hsf.getColFamily(),Math.abs(rand.nextLong()));
  hsf.splitStoreFile(midKey,dstA,dstB,fs,conf);
  if (LOG.isDebugEnabled()) {
    LOG.debug("Finished splitting HStore " + hsf.getRegionName() + "/"+ hsf.getColFamily()+ "/"+ hsf.fileId());
  }
  return hsf;
}
