{
  if (((regionInfo.startKey.getLength() == 0) || (regionInfo.startKey.compareTo(row) <= 0)) && ((regionInfo.endKey.getLength() == 0) || (regionInfo.endKey.compareTo(row) > 0))) {
  }
 else {
    throw new WrongRegionException("Requested row out of range for " + "HRegion " + regionInfo.regionName + ", startKey='"+ regionInfo.startKey+ "', endKey='"+ regionInfo.endKey+ "', row='"+ row+ "'");
  }
}
