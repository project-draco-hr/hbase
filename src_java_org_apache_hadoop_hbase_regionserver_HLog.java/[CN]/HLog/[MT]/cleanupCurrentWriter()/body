{
  Path oldFile=null;
  if (this.writer != null) {
    try {
      this.writer.close();
    }
 catch (    IOException e) {
      FailedLogCloseException flce=new FailedLogCloseException("#" + this.filenum);
      flce.initCause(e);
      throw e;
    }
    oldFile=computeFilename(old_filenum);
    if (filenum > 0) {
synchronized (this.sequenceLock) {
        this.outputfiles.put(Long.valueOf(this.logSeqNum - 1),oldFile);
      }
    }
  }
  return oldFile;
}
