{
switch (fieldID) {
case ROW:
    if (value == null) {
      unsetRow();
    }
 else {
      setRow((byte[])value);
    }
  break;
case COLUMNS:
if (value == null) {
  unsetColumns();
}
 else {
  setColumns((Map<byte[],TCell>)value);
}
break;
default :
throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
}
}
