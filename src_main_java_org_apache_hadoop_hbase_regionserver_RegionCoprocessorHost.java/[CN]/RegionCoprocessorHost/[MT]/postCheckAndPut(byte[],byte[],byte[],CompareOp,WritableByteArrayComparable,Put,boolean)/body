{
  for (  RegionEnvironment env : coprocessors) {
    if (env.getInstance() instanceof RegionObserver) {
      result=((RegionObserver)env.getInstance()).postCheckAndPut(env,row,family,qualifier,compareOp,comparator,put,result);
      if (env.shouldComplete()) {
        break;
      }
    }
  }
  return result;
}
