{
  boolean bypass=false;
  for (  RegionEnvironment env : coprocessors) {
    if (env.getInstance() instanceof RegionObserver) {
      ((RegionObserver)env.getInstance()).preScannerClose(env,s);
      bypass|=env.shouldBypass();
      if (env.shouldComplete()) {
        break;
      }
    }
  }
  return bypass;
}
