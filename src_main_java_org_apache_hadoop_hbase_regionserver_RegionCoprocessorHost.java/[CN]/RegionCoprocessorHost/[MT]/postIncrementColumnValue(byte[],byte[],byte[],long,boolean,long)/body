{
  for (  RegionEnvironment env : coprocessors) {
    if (env.getInstance() instanceof RegionObserver) {
      result=((RegionObserver)env.getInstance()).postIncrementColumnValue(env,row,family,qualifier,amount,writeToWAL,result);
      if (env.shouldComplete()) {
        break;
      }
    }
  }
  return result;
}
