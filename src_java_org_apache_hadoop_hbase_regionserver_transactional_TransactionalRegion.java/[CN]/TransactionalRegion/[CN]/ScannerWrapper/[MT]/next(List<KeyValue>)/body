{
  boolean result=scanner.next(results);
  TransactionState state=getTransactionState(transactionId);
  if (result) {
    byte[] row=results.get(0).getRow();
    Map<byte[],Cell> localWrites=state.localGetFull(row,null,Integer.MAX_VALUE);
    if (localWrites != null) {
      LOG.info("Scanning over row that has been writen to " + transactionId);
      for (      Entry<byte[],Cell> entry : localWrites.entrySet()) {
        results.add(new KeyValue(row,entry.getKey(),entry.getValue().getTimestamp(),entry.getValue().getValue()));
      }
    }
  }
  return result;
}
