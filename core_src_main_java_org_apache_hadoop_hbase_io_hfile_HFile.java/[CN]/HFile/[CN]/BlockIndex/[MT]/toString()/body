{
  StringBuilder sb=new StringBuilder();
  sb.append("size=" + count);
  for (int i=0; i < count; i++) {
    sb.append(", ");
    sb.append("key=").append(Bytes.toString(blockKeys[i])).append(", offset=").append(blockOffsets[i]).append(", dataSize=" + blockDataSizes[i]);
  }
  return sb.toString();
}
