{
  try {
    HColumnDescriptor column=new HColumnDescriptor(COLUMN_FAMILY.toString());
    HBaseAdmin admin=new HBaseAdmin(conf);
    HTableDescriptor testTableADesc=new HTableDescriptor("table");
    testTableADesc.addFamily(column);
    admin.createTable(testTableADesc);
    new HTable(conf,new Text("notATable"));
    fail("Should have thrown a TableNotFoundException");
  }
 catch (  TableNotFoundException e) {
  }
catch (  IOException e) {
    e.printStackTrace();
    fail("Should have thrown a TableNotFoundException instead of a " + e.getClass());
  }
}
