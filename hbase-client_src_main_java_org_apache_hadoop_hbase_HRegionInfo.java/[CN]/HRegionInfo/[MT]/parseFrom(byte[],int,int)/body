{
  if (ProtobufUtil.isPBMagicPrefix(bytes,offset,len)) {
    int pblen=ProtobufUtil.lengthOfPBMagic();
    try {
      HBaseProtos.RegionInfo ri=HBaseProtos.RegionInfo.newBuilder().mergeFrom(bytes,pblen + offset,len - pblen).build();
      return convert(ri);
    }
 catch (    InvalidProtocolBufferException e) {
      throw new DeserializationException(e);
    }
  }
 else {
    throw new DeserializationException("PB encoded HRegionInfo expected");
  }
}
