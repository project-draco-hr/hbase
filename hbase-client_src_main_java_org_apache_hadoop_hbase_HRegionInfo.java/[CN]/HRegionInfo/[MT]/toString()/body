{
  return "{ENCODED => " + getEncodedName() + ", "+ HConstants.NAME+ " => '"+ this.regionNameStr+ "', STARTKEY => '"+ Bytes.toStringBinary(this.startKey)+ "', ENDKEY => '"+ Bytes.toStringBinary(this.endKey)+ "'"+ (isOffline() ? ", OFFLINE => true" : "")+ (isSplit() ? ", SPLIT => true" : "")+ "}";
}
