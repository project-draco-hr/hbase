{
  if (!this.unassigned) {
    LOG.warn("Cannot assign a region that is not currently unassigned. " + "FIX!! State: " + toString());
  }
  this.unassigned=false;
  this.pendingOpen=true;
  this.open=false;
  this.closing=false;
  this.pendingClose=false;
  this.closed=false;
  this.offlined=false;
  this.serverName=serverName;
}
