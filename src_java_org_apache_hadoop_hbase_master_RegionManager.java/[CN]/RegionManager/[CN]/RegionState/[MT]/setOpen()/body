{
  if (!pendingOpen) {
    throw new IllegalStateException("Cannot set a region as open if it has not been pending. State: " + toString());
  }
  this.unassigned=false;
  this.pendingOpen=false;
  this.open=true;
  this.closing=false;
  this.pendingClose=false;
  this.closed=false;
  this.offlined=false;
}
