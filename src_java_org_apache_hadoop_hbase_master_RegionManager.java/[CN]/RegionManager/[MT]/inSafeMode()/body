{
  if (safeMode) {
    if (isInitialMetaScanComplete() && regionsInTransition.size() == 0 && tellZooKeeperOutOfSafeMode()) {
      master.getServerConnection().unsetRootRegionLocation();
      safeMode=false;
      LOG.info("exiting safe mode");
    }
 else {
      LOG.info("in safe mode");
    }
  }
  return safeMode;
}
