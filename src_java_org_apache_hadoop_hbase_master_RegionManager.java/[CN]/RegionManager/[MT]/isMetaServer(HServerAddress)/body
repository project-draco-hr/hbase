{
  for (  MetaRegion region : onlineMetaRegions.values()) {
    if (server.equals(region.getServer())) {
      return true;
    }
  }
  for (  RegionState s : regionsInTransition.values()) {
    if (s.getRegionInfo().isMetaRegion() && !s.isUnassigned() && s.getServerName() != null && s.getServerName().equals(server.toString())) {
      return true;
    }
  }
  return false;
}
