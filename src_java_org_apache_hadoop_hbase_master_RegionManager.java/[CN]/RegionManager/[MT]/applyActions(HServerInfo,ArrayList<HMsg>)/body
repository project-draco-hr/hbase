{
  applyActions(serverInfo,returnMsgs,this.regionsToCompact,HMsg.Type.MSG_REGION_COMPACT);
  applyActions(serverInfo,returnMsgs,this.regionsToSplit,HMsg.Type.MSG_REGION_SPLIT);
  applyActions(serverInfo,returnMsgs,this.regionsToFlush,HMsg.Type.MSG_REGION_FLUSH);
  applyActions(serverInfo,returnMsgs,this.regionsToMajorCompact,HMsg.Type.MSG_REGION_MAJOR_COMPACT);
}
