{
  util.getConfiguration().set(CoprocessorHost.REGION_COPROCESSOR_CONF_KEY,PingHandler.class.getName());
  util.startMiniCluster(1);
  cluster=util.getMiniHBaseCluster();
  HTable table=util.createTable(TEST_TABLE,TEST_FAMILY);
  util.createMultiRegions(util.getConfiguration(),table,TEST_FAMILY,new byte[][]{HConstants.EMPTY_BYTE_ARRAY,ROW_B,ROW_C});
  Put puta=new Put(ROW_A);
  puta.add(TEST_FAMILY,Bytes.toBytes("col1"),Bytes.toBytes(1));
  table.put(puta);
  Put putb=new Put(ROW_B);
  putb.add(TEST_FAMILY,Bytes.toBytes("col1"),Bytes.toBytes(1));
  table.put(putb);
  Put putc=new Put(ROW_C);
  putc.add(TEST_FAMILY,Bytes.toBytes("col1"),Bytes.toBytes(1));
  table.put(putc);
}
