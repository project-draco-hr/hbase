{
  if (rc != 0) {
    LOG.warn("rc != 0 for " + path + " -- retryable connectionloss -- "+ "FIX see http://wiki.apache.org/hadoop/ZooKeeper/FAQ#A2");
    this.counter.addAndGet(1);
    return;
  }
  if (LOG.isDebugEnabled()) {
    LOG.debug("rs=" + ctx + ", server="+ destination);
  }
  HRegionInfo region=((RegionState)ctx).getRegion();
  offlineNodesVersions.put(region.getEncodedName(),Integer.valueOf(stat.getVersion()));
  this.counter.addAndGet(1);
}
