{
  if (key == null)   key=new ImmutableBytesWritable();
  if (value == null)   value=new Result();
  try {
    value=this.scanner.next();
  }
 catch (  IOException e) {
    LOG.debug("recovered from " + StringUtils.stringifyException(e));
    restart(lastRow);
    scanner.next();
    value=scanner.next();
  }
  if (value != null && value.size() > 0) {
    key.set(value.getRow());
    lastRow=key.get();
    return true;
  }
  return false;
}
