{
  try {
    String clusterZNode=this.zookeeperWrapper.getZNode(this.rsServerNameZnode,clusterId);
    this.zookeeperWrapper.writeZNode(clusterZNode,filename,Long.toString(position));
  }
 catch (  InterruptedException e) {
    LOG.error(e);
  }
catch (  KeeperException e) {
    LOG.error(e);
  }
}
