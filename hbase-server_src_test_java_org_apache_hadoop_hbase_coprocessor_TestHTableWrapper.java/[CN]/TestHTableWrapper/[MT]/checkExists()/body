{
  boolean ex=hTableInterface.exists(new Get(ROW_A).addColumn(TEST_FAMILY,qualifierCol1));
  assertTrue(ex);
  Boolean[] exArray=hTableInterface.exists(Arrays.asList(new Get[]{new Get(ROW_A).addColumn(TEST_FAMILY,qualifierCol1),new Get(ROW_B).addColumn(TEST_FAMILY,qualifierCol1),new Get(ROW_C).addColumn(TEST_FAMILY,qualifierCol1),new Get(Bytes.toBytes("does not exist")).addColumn(TEST_FAMILY,qualifierCol1)}));
  assertArrayEquals(new Boolean[]{Boolean.TRUE,Boolean.TRUE,Boolean.TRUE,Boolean.FALSE},exArray);
}
