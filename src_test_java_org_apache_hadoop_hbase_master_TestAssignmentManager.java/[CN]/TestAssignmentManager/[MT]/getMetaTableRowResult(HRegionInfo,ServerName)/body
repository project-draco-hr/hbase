{
  List<KeyValue> kvs=new ArrayList<KeyValue>();
  kvs.add(new KeyValue(HConstants.EMPTY_BYTE_ARRAY,HConstants.CATALOG_FAMILY,HConstants.REGIONINFO_QUALIFIER,Writables.getBytes(hri)));
  kvs.add(new KeyValue(HConstants.EMPTY_BYTE_ARRAY,HConstants.CATALOG_FAMILY,HConstants.SERVER_QUALIFIER,Bytes.toBytes(sn.getHostAndPort())));
  kvs.add(new KeyValue(HConstants.EMPTY_BYTE_ARRAY,HConstants.CATALOG_FAMILY,HConstants.STARTCODE_QUALIFIER,Bytes.toBytes(sn.getStartcode())));
  return new Result(kvs);
}
