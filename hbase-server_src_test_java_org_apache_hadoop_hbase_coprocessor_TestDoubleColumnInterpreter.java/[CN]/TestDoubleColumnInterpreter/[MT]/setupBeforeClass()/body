{
  conf.set(CoprocessorHost.REGION_COPROCESSOR_CONF_KEY,"org.apache.hadoop.hbase.coprocessor.AggregateImplementation");
  util.startMiniCluster(2);
  final byte[][] SPLIT_KEYS=new byte[][]{ROWS[rowSeperator1],ROWS[rowSeperator2]};
  Table table=util.createTable(TEST_TABLE,TEST_FAMILY,SPLIT_KEYS);
  for (int i=0; i < ROWSIZE; i++) {
    Put put=new Put(ROWS[i]);
    put.setDurability(Durability.SKIP_WAL);
    Double d=new Double(i);
    put.addColumn(TEST_FAMILY,TEST_QUALIFIER,Bytes.toBytes(d));
    table.put(put);
    Put p2=new Put(ROWS[i]);
    put.setDurability(Durability.SKIP_WAL);
    p2.addColumn(TEST_FAMILY,Bytes.add(TEST_MULTI_CQ,Bytes.toBytes(d)),Bytes.toBytes(d * 0.10));
    table.put(p2);
  }
  table.close();
}
