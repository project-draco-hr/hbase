{
  AggregationClient aClient=new AggregationClient(conf);
  Scan scan=new Scan();
  scan.addColumn(TEST_FAMILY,TEST_QUALIFIER);
  scan.setStartRow(ROWS[5]);
  scan.setStopRow(ROWS[15]);
  final ColumnInterpreter<Double,Double,EmptyMsg,DoubleMsg,DoubleMsg> ci=new DoubleColumnInterpreter();
  double sum=aClient.sum(TEST_TABLE,ci,scan);
  assertEquals(95.00,sum,0.00);
}
