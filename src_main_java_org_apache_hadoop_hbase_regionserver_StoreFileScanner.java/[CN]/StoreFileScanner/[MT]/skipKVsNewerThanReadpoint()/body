{
  long readPoint=MultiVersionConsistencyControl.getThreadReadPoint();
  while (enforceMVCC && cur != null && (cur.getMemstoreTS() > readPoint)) {
    hfs.next();
    cur=hfs.getKeyValue();
  }
  if (cur == null) {
    close();
    return false;
  }
  if (cur.getMemstoreTS() <= readPoint) {
    cur.setMemstoreTS(0);
  }
  return true;
}
