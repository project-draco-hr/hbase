{
  long readPoint=ReadWriteConsistencyControl.getThreadReadPoint();
  while (enforceRWCC && cur != null && (cur.getMemstoreTS() > readPoint)) {
    hfs.next();
    cur=hfs.getKeyValue();
  }
  if (cur == null) {
    close();
    return false;
  }
  if (cur.getMemstoreTS() <= readPoint) {
    cur.setMemstoreTS(0);
  }
  return true;
}
