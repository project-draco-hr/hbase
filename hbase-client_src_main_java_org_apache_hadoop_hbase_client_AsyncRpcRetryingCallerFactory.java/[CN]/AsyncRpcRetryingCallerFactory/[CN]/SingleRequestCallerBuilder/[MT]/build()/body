{
  return new AsyncSingleRequestRpcRetryingCaller<>(retryTimer,conn,Preconditions.checkNotNull(tableName,"tableName is null"),Preconditions.checkNotNull(row,"row is null"),Preconditions.checkNotNull(callable,"action is null"),conn.connConf.getPauseNs(),conn.connConf.getMaxRetries(),operationTimeoutNs,rpcTimeoutNs,conn.connConf.getStartLogErrorsCnt());
}
