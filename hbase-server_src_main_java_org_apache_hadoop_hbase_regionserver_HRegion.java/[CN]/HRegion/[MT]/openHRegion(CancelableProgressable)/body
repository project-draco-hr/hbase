{
  checkCompressionCodecs();
  checkEncryption();
  checkClassLoading();
  this.openSeqNum=initialize(reporter);
  this.setSequenceId(openSeqNum);
  if (wal != null && getRegionServerServices() != null && !writestate.readOnly && !isRecovering) {
    writeRegionOpenMarker(wal,openSeqNum);
  }
  return this;
}
