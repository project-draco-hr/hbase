{
  WriteEntry writeEntry=null;
  WALKey walKey=new WALKey(this.getRegionInfo().getEncodedNameAsBytes(),this.htableDescriptor.getTableName(),WALKey.NO_SEQUENCE_ID,now,clusterIds,nonceGroup,nonce,mvcc);
  try {
    long txid=this.wal.append(this.htableDescriptor,this.getRegionInfo(),walKey,walEdit,true);
    if (txid != 0)     sync(txid,durability);
    writeEntry=walKey.getWriteEntry();
  }
 catch (  IOException ioe) {
    if (walKey != null)     mvcc.complete(walKey.getWriteEntry());
    throw ioe;
  }
  return writeEntry;
}
