{
  @SuppressWarnings("deprecation") WALKey key=new HLogKey(getRegionInfo().getEncodedNameAsBytes(),getRegionInfo().getTable(),WALKey.NO_SEQUENCE_ID,0,null,HConstants.NO_NONCE,HConstants.NO_NONCE,getMVCC());
  try {
    wal.append(getTableDesc(),getRegionInfo(),key,WALEdit.EMPTY_WALEDIT,false);
  }
 catch (  Throwable t) {
    getMVCC().complete(key.getWriteEntry());
  }
  return key;
}
