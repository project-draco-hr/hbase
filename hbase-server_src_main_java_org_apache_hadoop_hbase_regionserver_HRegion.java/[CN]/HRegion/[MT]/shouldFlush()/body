{
  if (flushCheckInterval <= 0) {
    return false;
  }
  long now=EnvironmentEdgeManager.currentTimeMillis();
  if ((now - getLastFlushTime() < flushCheckInterval)) {
    return false;
  }
  for (  Store s : this.getStores().values()) {
    if (s.timeOfOldestEdit() < now - flushCheckInterval) {
      return true;
    }
  }
  return false;
}
