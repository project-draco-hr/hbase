{
  if (this.maxFlushedSeqId > 0 && (this.maxFlushedSeqId + this.flushPerChanges < this.sequenceId.get())) {
    return true;
  }
  if (flushCheckInterval <= 0) {
    return false;
  }
  long now=EnvironmentEdgeManager.currentTime();
  if ((now - getEarliestFlushTimeForAllStores() < flushCheckInterval)) {
    return false;
  }
  for (  Store s : this.getStores().values()) {
    if (s.timeOfOldestEdit() < now - flushCheckInterval) {
      return true;
    }
  }
  return false;
}
