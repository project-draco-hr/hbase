{
  if (!isAvailable()) {
    LOG.debug("Region " + this.getRegionNameAsString() + " is not mergeable because it is closing or closed");
    return false;
  }
  if (hasReferences()) {
    LOG.debug("Region " + this.getRegionNameAsString() + " is not mergeable because it has references");
    return false;
  }
  return true;
}
