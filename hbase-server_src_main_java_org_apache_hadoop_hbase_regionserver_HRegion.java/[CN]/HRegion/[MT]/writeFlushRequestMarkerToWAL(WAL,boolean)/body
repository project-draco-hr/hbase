{
  if (writeFlushWalMarker && wal != null && !writestate.readOnly) {
    FlushDescriptor desc=ProtobufUtil.toFlushDescriptor(FlushAction.CANNOT_FLUSH,getRegionInfo(),-1,new TreeMap<byte[],List<Path>>());
    try {
      WALUtil.writeFlushMarker(wal,this.htableDescriptor,getRegionInfo(),desc,sequenceId,true);
      return true;
    }
 catch (    IOException e) {
      LOG.warn(getRegionInfo().getEncodedName() + " : " + "Received exception while trying to write the flush request to wal",e);
    }
  }
  return false;
}
