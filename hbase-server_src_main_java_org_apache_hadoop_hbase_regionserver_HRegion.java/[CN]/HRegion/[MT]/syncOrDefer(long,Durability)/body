{
  if (this.getRegionInfo().isMetaRegion()) {
    this.log.sync(txid);
  }
 else {
switch (durability) {
case USE_DEFAULT:
      if (!isDeferredLogSyncEnabled()) {
        this.log.sync(txid);
      }
    break;
case SKIP_WAL:
  break;
case ASYNC_WAL:
if (this.deferredLogSyncDisabled) {
  this.log.sync(txid);
}
break;
case SYNC_WAL:
case FSYNC_WAL:
this.log.sync(txid);
break;
}
}
}
