{
  if (!rowIsInRange(getRegionInfo(),row)) {
    throw new WrongRegionException("Requested row out of range for " + op + " on HRegion "+ this+ ", startKey='"+ Bytes.toStringBinary(getStartKey())+ "', getEndKey()='"+ Bytes.toStringBinary(getEndKey())+ "', row='"+ Bytes.toStringBinary(row)+ "'");
  }
}
