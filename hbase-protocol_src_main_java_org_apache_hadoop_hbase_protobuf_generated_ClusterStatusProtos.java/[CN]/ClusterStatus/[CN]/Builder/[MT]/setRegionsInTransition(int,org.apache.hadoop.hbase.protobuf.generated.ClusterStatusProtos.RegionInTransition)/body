{
  if (regionsInTransitionBuilder_ == null) {
    if (value == null) {
      throw new NullPointerException();
    }
    ensureRegionsInTransitionIsMutable();
    regionsInTransition_.set(index,value);
    onChanged();
  }
 else {
    regionsInTransitionBuilder_.setMessage(index,value);
  }
  return this;
}
