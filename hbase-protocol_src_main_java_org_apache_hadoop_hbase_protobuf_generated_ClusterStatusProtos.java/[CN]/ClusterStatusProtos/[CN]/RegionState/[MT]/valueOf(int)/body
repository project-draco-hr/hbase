{
switch (value) {
case 0:
    return OFFLINE;
case 1:
  return PENDING_OPEN;
case 2:
return OPENING;
case 3:
return OPEN;
case 4:
return PENDING_CLOSE;
case 5:
return CLOSING;
case 6:
return CLOSED;
case 7:
return SPLITTING;
case 8:
return SPLIT;
case 9:
return FAILED_OPEN;
case 10:
return FAILED_CLOSE;
case 11:
return MERGING;
case 12:
return MERGED;
default :
return null;
}
}
