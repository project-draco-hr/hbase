{
  String className=conf.get(COMPACTION_POLICY_KEY,DEFAULT_COMPACTION_POLICY_CLASS.getName());
  try {
    Class<? extends CompactionPolicy> clazz=Class.forName(className).asSubclass(CompactionPolicy.class);
    return clazz;
  }
 catch (  Exception e) {
    throw new IOException("Unable to load configured region compaction policy '" + className + "' for column '"+ family.getNameAsString()+ "'",e);
  }
}
