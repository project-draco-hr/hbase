{
  p.addImmutable(HConstants.CATALOG_FAMILY,MetaReader.getServerColumn(replicaId),Bytes.toBytes(sn.getHostAndPort()));
  p.addImmutable(HConstants.CATALOG_FAMILY,MetaReader.getStartCodeColumn(replicaId),Bytes.toBytes(sn.getStartcode()));
  p.addImmutable(HConstants.CATALOG_FAMILY,MetaReader.getSeqNumColumn(replicaId),Bytes.toBytes(openSeqNum));
  return p;
}
