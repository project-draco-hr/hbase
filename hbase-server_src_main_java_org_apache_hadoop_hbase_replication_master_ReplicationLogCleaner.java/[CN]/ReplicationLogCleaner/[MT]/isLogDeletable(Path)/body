{
  try {
    if (!zkHelper.getReplication()) {
      return false;
    }
  }
 catch (  KeeperException e) {
    abort("Cannot get the state of replication",e);
    return false;
  }
  if (this.conf == null) {
    return true;
  }
  String log=filePath.getName();
  if (this.hlogs.contains(log)) {
    return false;
  }
  return !refreshHLogsAndSearch(log);
}
