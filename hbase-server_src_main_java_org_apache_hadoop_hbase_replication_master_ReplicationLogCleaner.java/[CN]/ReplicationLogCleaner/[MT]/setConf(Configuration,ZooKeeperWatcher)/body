{
  super.setConf(conf);
  try {
    this.zkw=zk;
    this.replicationQueues=ReplicationFactory.getReplicationQueuesClient(zkw,conf,new WarnOnlyAbortable());
    this.replicationQueues.init();
  }
 catch (  ReplicationException e) {
    LOG.error("Error while configuring " + this.getClass().getName(),e);
  }
}
