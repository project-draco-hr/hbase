{
  if (LOG.isDebugEnabled()) {
    LOG.debug("Stopping async HBase RPC client");
  }
synchronized (connections) {
    if (closed) {
      return;
    }
    closed=true;
    for (    AsyncRpcChannel conn : connections.values()) {
      conn.close(null);
    }
  }
  if (!useGlobalEventLoopGroup) {
    bootstrap.config().group().shutdownGracefully();
  }
}
