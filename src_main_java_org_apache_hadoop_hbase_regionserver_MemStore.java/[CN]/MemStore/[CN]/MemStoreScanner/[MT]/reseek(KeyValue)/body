{
  numIterReseek=reseekNumKeys;
  while (kvsetNextRow != null && comparator.compare(kvsetNextRow,key) < 0) {
    kvsetNextRow=getNext(kvsetIt);
    if (kvsetNextRow == null && numIterReseek == 0) {
      return seek(key);
    }
  }
  while (snapshotNextRow != null && comparator.compare(snapshotNextRow,key) < 0) {
    snapshotNextRow=getNext(snapshotIt);
    if (snapshotNextRow == null && numIterReseek == 0) {
      return seek(key);
    }
  }
  return (kvsetNextRow != null || snapshotNextRow != null);
}
