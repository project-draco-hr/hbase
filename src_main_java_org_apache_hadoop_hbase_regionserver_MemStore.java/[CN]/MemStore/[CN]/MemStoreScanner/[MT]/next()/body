{
  if (theNext == null) {
    return null;
  }
  KeyValue ret=theNext;
  long readPoint=ReadWriteConsistencyControl.getThreadReadPoint();
  if (theNext == kvsetNextRow) {
    kvsetNextRow=getNext(kvsetIt,readPoint);
  }
 else {
    snapshotNextRow=getNext(snapshotIt,readPoint);
  }
  theNext=getLowest();
  return ret;
}
