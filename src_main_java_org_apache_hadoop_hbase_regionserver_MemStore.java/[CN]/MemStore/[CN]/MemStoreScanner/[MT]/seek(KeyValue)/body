{
  if (key == null) {
    close();
    return false;
  }
  numIterReseek=0;
  SortedSet<KeyValue> kvTail=kvset.tailSet(key);
  SortedSet<KeyValue> snapshotTail=snapshot.tailSet(key);
  kvsetIt=kvTail.iterator();
  snapshotIt=snapshotTail.iterator();
  long readPoint=ReadWriteConsistencyControl.getThreadReadPoint();
  kvsetNextRow=getNext(kvsetIt,readPoint);
  snapshotNextRow=getNext(snapshotIt,readPoint);
  theNext=getLowest();
  return (theNext != null);
}
