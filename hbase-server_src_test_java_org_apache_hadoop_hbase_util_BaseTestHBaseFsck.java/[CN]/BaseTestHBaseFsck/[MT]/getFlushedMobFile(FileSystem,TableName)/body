{
  Path regionDir=MobUtils.getMobRegionPath(conf,table);
  Path famDir=new Path(regionDir,FAM_STR);
  while (true) {
    FileStatus[] hfFss=fs.listStatus(famDir);
    if (hfFss.length == 0) {
      continue;
    }
    for (    FileStatus hfs : hfFss) {
      if (!hfs.isDirectory()) {
        return hfs.getPath();
      }
    }
  }
}
