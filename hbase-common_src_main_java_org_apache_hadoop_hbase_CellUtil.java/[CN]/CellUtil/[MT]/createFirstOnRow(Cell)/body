{
  if (cell instanceof ByteBufferedCell) {
    return new FirstOnRowByteBufferedCell(((ByteBufferedCell)cell).getRowByteBuffer(),((ByteBufferedCell)cell).getRowPositionInByteBuffer(),cell.getRowLength());
  }
  return new FirstOnRowCell(cell.getRowArray(),cell.getRowOffset(),cell.getRowLength());
}
