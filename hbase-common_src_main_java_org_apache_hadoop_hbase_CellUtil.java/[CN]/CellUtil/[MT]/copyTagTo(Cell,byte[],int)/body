{
  int tlen=cell.getTagsLength();
  if (cell instanceof ByteBufferedCell) {
    ByteBufferUtils.copyFromBufferToArray(destination,((ByteBufferedCell)cell).getTagsByteBuffer(),((ByteBufferedCell)cell).getTagsPositionInByteBuffer(),destinationOffset,tlen);
  }
 else {
    System.arraycopy(cell.getTagsArray(),cell.getTagsOffset(),destination,destinationOffset,tlen);
  }
  return destinationOffset + tlen;
}
