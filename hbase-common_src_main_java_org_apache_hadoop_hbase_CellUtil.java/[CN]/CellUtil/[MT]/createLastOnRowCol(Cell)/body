{
  if (cell instanceof ByteBufferedCell) {
    return new LastOnRowColByteBufferedCell(((ByteBufferedCell)cell).getRowByteBuffer(),((ByteBufferedCell)cell).getRowPositionInByteBuffer(),cell.getRowLength(),((ByteBufferedCell)cell).getFamilyByteBuffer(),((ByteBufferedCell)cell).getFamilyPositionInByteBuffer(),cell.getFamilyLength(),((ByteBufferedCell)cell).getQualifierByteBuffer(),((ByteBufferedCell)cell).getQualifierPositionInByteBuffer(),cell.getQualifierLength());
  }
  return new LastOnRowColCell(cell.getRowArray(),cell.getRowOffset(),cell.getRowLength(),cell.getFamilyArray(),cell.getFamilyOffset(),cell.getFamilyLength(),cell.getQualifierArray(),cell.getQualifierOffset(),cell.getQualifierLength());
}
