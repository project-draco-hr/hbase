{
  int qlen=cell.getQualifierLength();
  if (cell instanceof ByteBufferedCell) {
    ByteBufferUtils.copyFromBufferToArray(destination,((ByteBufferedCell)cell).getQualifierByteBuffer(),((ByteBufferedCell)cell).getQualifierPositionInByteBuffer(),destinationOffset,qlen);
  }
 else {
    System.arraycopy(cell.getQualifierArray(),cell.getQualifierOffset(),destination,destinationOffset,qlen);
  }
  return destinationOffset + qlen;
}
