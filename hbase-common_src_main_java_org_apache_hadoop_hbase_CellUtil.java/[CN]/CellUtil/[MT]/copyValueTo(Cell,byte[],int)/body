{
  int vlen=cell.getValueLength();
  if (cell instanceof ByteBufferedCell) {
    ByteBufferUtils.copyFromBufferToArray(destination,((ByteBufferedCell)cell).getValueByteBuffer(),((ByteBufferedCell)cell).getValuePositionInByteBuffer(),destinationOffset,vlen);
  }
 else {
    System.arraycopy(cell.getValueArray(),cell.getValueOffset(),destination,destinationOffset,vlen);
  }
  return destinationOffset + vlen;
}
