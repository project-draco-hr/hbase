{
  if (commonPrefix == 0) {
    out.writeShort(rLen);
  }
 else   if (commonPrefix == 1) {
    out.writeByte((byte)rLen);
    commonPrefix--;
  }
 else {
    commonPrefix-=KeyValue.ROW_LENGTH_SIZE;
  }
  if (rLen > commonPrefix) {
    out.write(cell.getRowArray(),cell.getRowOffset() + commonPrefix,rLen - commonPrefix);
  }
}
