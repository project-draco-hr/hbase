{
  if (buf == null) {
    return left.getQualifierLength() == 0;
  }
  if (left instanceof ByteBufferedCell) {
    return ByteBufferUtils.compareTo(((ByteBufferedCell)left).getQualifierByteBuffer(),((ByteBufferedCell)left).getQualifierPositionInByteBuffer(),left.getQualifierLength(),buf,offset,length) == 0;
  }
  return Bytes.equals(left.getQualifierArray(),left.getQualifierOffset(),left.getQualifierLength(),buf,offset,length);
}
