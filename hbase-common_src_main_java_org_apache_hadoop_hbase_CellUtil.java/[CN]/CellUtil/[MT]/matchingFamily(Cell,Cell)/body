{
  byte lfamlength=left.getFamilyLength();
  byte rfamlength=right.getFamilyLength();
  if (left instanceof ByteBufferedCell && right instanceof ByteBufferedCell) {
    return ByteBufferUtils.equals(((ByteBufferedCell)left).getFamilyByteBuffer(),((ByteBufferedCell)left).getFamilyPositionInByteBuffer(),lfamlength,((ByteBufferedCell)right).getFamilyByteBuffer(),((ByteBufferedCell)right).getFamilyPositionInByteBuffer(),rfamlength);
  }
  if (left instanceof ByteBufferedCell) {
    return ByteBufferUtils.equals(((ByteBufferedCell)left).getFamilyByteBuffer(),((ByteBufferedCell)left).getFamilyPositionInByteBuffer(),lfamlength,right.getFamilyArray(),right.getFamilyOffset(),rfamlength);
  }
  if (right instanceof ByteBufferedCell) {
    return ByteBufferUtils.equals(((ByteBufferedCell)right).getFamilyByteBuffer(),((ByteBufferedCell)right).getFamilyPositionInByteBuffer(),rfamlength,left.getFamilyArray(),left.getFamilyOffset(),lfamlength);
  }
  return Bytes.equals(left.getFamilyArray(),left.getFamilyOffset(),left.getFamilyLength(),right.getFamilyArray(),right.getFamilyOffset(),right.getFamilyLength());
}
