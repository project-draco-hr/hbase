{
  if (left instanceof ByteBufferedCell) {
    return ByteBufferUtils.compareTo(((ByteBufferedCell)left).getRowByteBuffer(),((ByteBufferedCell)left).getRowPositionInByteBuffer(),left.getRowLength(),buf,offset,length) == 0;
  }
  return Bytes.equals(left.getRowArray(),left.getRowOffset(),left.getRowLength(),buf,offset,length);
}
