{
  if (left instanceof ByteBufferedCell) {
    return ByteBufferUtils.equals(((ByteBufferedCell)left).getRowByteBuffer(),((ByteBufferedCell)left).getRowPositionInByteBuffer(),left.getRowLength(),buf,offset,length);
  }
  return Bytes.equals(left.getRowArray(),left.getRowOffset(),left.getRowLength(),buf,offset,length);
}
