{
  if (cell instanceof ByteBufferedCell) {
    return ByteBufferUtils.toBigDecimal(((ByteBufferedCell)cell).getValueByteBuffer(),((ByteBufferedCell)cell).getValuePositionInByteBuffer(),cell.getValueLength());
  }
  return Bytes.toBigDecimal(cell.getValueArray(),cell.getValueOffset(),cell.getValueLength());
}
