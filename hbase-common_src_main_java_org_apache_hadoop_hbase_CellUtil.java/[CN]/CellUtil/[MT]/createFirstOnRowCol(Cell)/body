{
  if (cell instanceof ByteBufferedCell) {
    return new FirstOnRowColByteBufferedCell(((ByteBufferedCell)cell).getRowByteBuffer(),((ByteBufferedCell)cell).getRowPositionInByteBuffer(),cell.getRowLength(),((ByteBufferedCell)cell).getQualifierByteBuffer(),((ByteBufferedCell)cell).getQualifierPositionInByteBuffer(),cell.getQualifierLength());
  }
  return new FirstOnRowColCell(cell.getRowArray(),cell.getRowOffset(),cell.getRowLength(),HConstants.EMPTY_BYTE_ARRAY,0,(byte)0,cell.getQualifierArray(),cell.getQualifierOffset(),cell.getQualifierLength());
}
