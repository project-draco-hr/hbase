{
  AdminService.BlockingInterface service=null;
  try {
    service=waitForMetaServerConnection(timeout);
  }
 catch (  NotAllMetaRegionsOnlineException e) {
  }
catch (  ServerNotRunningYetException e) {
  }
catch (  UnknownHostException e) {
  }
  return (service == null) ? false : verifyRegionLocation(service,this.metaRegionTracker.getMetaRegionLocation(),META_REGION_NAME);
}
