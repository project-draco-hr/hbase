{
  while (!this.stopped) {
    try {
      if (waitForMeta(100) != null)       break;
    }
 catch (    NotAllMetaRegionsOnlineException e) {
      if (LOG.isTraceEnabled()) {
        LOG.trace("hbase:meta still not available, sleeping and retrying." + " Reason: " + e.getMessage());
      }
    }
  }
}
