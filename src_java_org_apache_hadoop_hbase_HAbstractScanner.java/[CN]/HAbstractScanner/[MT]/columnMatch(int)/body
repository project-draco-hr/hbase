{
  Text column=keys[i].getColumn();
  Text family=HStoreKey.extractFamily(column);
  Vector<ColumnMatcher> matchers=okCols.get(family);
  if (matchers == null) {
    return false;
  }
  for (int m=0; m < matchers.size(); m++) {
    if (matchers.get(m).matches(column)) {
      return true;
    }
  }
  return false;
}
