{
  String result=null;
  if (Bytes.equals(columnName,HConstants.COL_REGIONINFO) || Bytes.equals(columnName,HConstants.COL_SPLITA) || Bytes.equals(columnName,HConstants.COL_SPLITB)) {
    result=Writables.getHRegionInfoOrNull(cell).toString();
  }
 else   if (Bytes.equals(columnName,HConstants.COL_STARTCODE)) {
    result=Long.toString(Bytes.toLong(cell));
  }
 else {
    result=Bytes.toString(cell);
  }
  return result;
}
