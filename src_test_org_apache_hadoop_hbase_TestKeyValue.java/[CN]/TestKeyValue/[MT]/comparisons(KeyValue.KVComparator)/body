{
  long now=System.currentTimeMillis();
  assertTrue(c.compare(new KeyValue(".META.,,1",now),new KeyValue(".META.,,1",now)) == 0);
  assertTrue(c.compare(new KeyValue(".META.,,1",now),new KeyValue(".META.,,2",now)) < 0);
  assertTrue(c.compare(new KeyValue(".META.,,2",now),new KeyValue(".META.,,1",now)) > 0);
}
