{
  long now=System.currentTimeMillis();
  assertTrue(c.compare(new KeyValue(".META.,a,,0,1",now),new KeyValue(".META.,a,,0,1",now)) == 0);
  KeyValue a=new KeyValue(".META.,a,,0,1",now);
  KeyValue b=new KeyValue(".META.,a,,0,2",now);
  assertTrue(c.compare(a,b) < 0);
  assertTrue(c.compare(new KeyValue(".META.,a,,0,2",now),new KeyValue(".META.,a,,0,1",now)) > 0);
}
