{
  Set<String> disabledTables=new HashSet<String>();
  List<String> children=ZKUtil.listChildrenNoWatch(zkw,zkw.tableZNode);
  for (  String child : children) {
    TableState state=getTableState(zkw,child);
    if (state == TableState.DISABLED || state == TableState.DISABLING)     disabledTables.add(child);
  }
  return disabledTables;
}
