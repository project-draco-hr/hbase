{
  this.conf=arg0;
  if (rcvr != null) {
    try {
      rcvr.close();
    }
 catch (    IOException e) {
      LOG.warn("Error closing LocalFileSpanReceiver.",e);
    }
  }
  try {
    rcvr=new LocalFileSpanReceiver(conf.get(FILE_NAME_CONF_KEY));
  }
 catch (  IOException e) {
    Trace.removeReceiver(this);
    rcvr=null;
    LOG.warn("Unable to initialize LocalFileSpanReceiver, removing owner (HBaseLocalFileSpanReceiver) from receiver list.",e);
  }
}
