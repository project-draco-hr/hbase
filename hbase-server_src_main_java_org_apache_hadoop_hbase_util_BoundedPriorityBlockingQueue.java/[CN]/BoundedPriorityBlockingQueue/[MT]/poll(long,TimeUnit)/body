{
  long nanos=unit.toNanos(timeout);
  lock.lockInterruptibly();
  E result=null;
  try {
    while (queue.size() == 0 && nanos > 0) {
      notEmpty.awaitNanos(nanos);
    }
    if (queue.size() > 0) {
      result=queue.poll();
    }
    notFull.signal();
  }
  finally {
    lock.unlock();
  }
  return result;
}
