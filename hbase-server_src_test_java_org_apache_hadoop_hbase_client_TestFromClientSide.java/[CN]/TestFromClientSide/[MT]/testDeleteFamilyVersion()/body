{
  HBaseAdmin admin=new HBaseAdmin(TEST_UTIL.getConfiguration());
  byte[] TABLE=Bytes.toBytes("testDeleteFamilyVersion");
  byte[][] QUALIFIERS=makeNAscii(QUALIFIER,1);
  byte[][] VALUES=makeN(VALUE,5);
  long[] ts={1000,2000,3000,4000,5000};
  Table ht=TEST_UTIL.createTable(TABLE,FAMILY,5);
  Put put=new Put(ROW);
  for (int q=0; q < 1; q++)   for (int t=0; t < 5; t++)   put.add(FAMILY,QUALIFIERS[q],ts[t],VALUES[t]);
  ht.put(put);
  admin.flush(TABLE);
  Delete delete=new Delete(ROW);
  delete.deleteFamilyVersion(FAMILY,ts[1]);
  delete.deleteFamilyVersion(FAMILY,ts[3]);
  ht.delete(delete);
  admin.flush(TABLE);
  for (int i=0; i < 1; i++) {
    Get get=new Get(ROW);
    get.addColumn(FAMILY,QUALIFIERS[i]);
    get.setMaxVersions(Integer.MAX_VALUE);
    Result result=ht.get(get);
    assertNResult(result,ROW,FAMILY,QUALIFIERS[i],new long[]{ts[0],ts[2],ts[4]},new byte[][]{VALUES[0],VALUES[2],VALUES[4]},0,2);
  }
  ht.close();
  admin.close();
}
