{
  assertTrue("Expected row [" + Bytes.toString(row) + "] "+ "Got row ["+ Bytes.toString(CellUtil.getRowArray(key))+ "]",equals(row,CellUtil.getRowArray(key)));
  assertTrue("Expected family [" + Bytes.toString(family) + "] "+ "Got family ["+ Bytes.toString(CellUtil.getFamilyArray(key))+ "]",equals(family,CellUtil.getFamilyArray(key)));
  assertTrue("Expected qualifier [" + Bytes.toString(qualifier) + "] "+ "Got qualifier ["+ Bytes.toString(CellUtil.getQualifierArray(key))+ "]",equals(qualifier,CellUtil.getQualifierArray(key)));
  assertTrue("Expected value [" + value + "] "+ "Got value ["+ Bytes.toLong(CellUtil.getValueArray(key))+ "]",Bytes.toLong(CellUtil.getValueArray(key)) == value);
}
