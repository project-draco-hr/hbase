{
  setup();
  startMiniDFSCluster();
  createRegions();
  try {
    HMerge.merge(conf,fs,HConstants.META_TABLE_NAME);
    hCluster=new MiniHBaseCluster(conf,1,dfsCluster);
    try {
      HMerge.merge(conf,fs,desc.getName());
    }
  finally {
      hCluster.shutdown();
    }
  }
 catch (  Throwable t) {
    t.printStackTrace();
    fail();
  }
 finally {
    dfsCluster.shutdown();
  }
}
