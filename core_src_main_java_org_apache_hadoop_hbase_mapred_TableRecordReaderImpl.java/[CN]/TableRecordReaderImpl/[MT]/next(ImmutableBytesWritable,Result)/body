{
  Result result;
  try {
    result=this.scanner.next();
  }
 catch (  UnknownScannerException e) {
    LOG.debug("recovered from " + StringUtils.stringifyException(e));
    restart(lastRow);
    this.scanner.next();
    result=this.scanner.next();
  }
  if (result != null && result.size() > 0) {
    key.set(result.getRow());
    lastRow=key.get();
    Writables.copyWritable(result,value);
    return true;
  }
  return false;
}
