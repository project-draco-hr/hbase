{
  String row=Bytes.toStringBinary(rowArray,rowOffset,rowLength);
  String family=(familyLength == 0) ? "" : Bytes.toStringBinary(familyArray,familyOffset,familyLength);
  String qualifier=(qualifierLength == 0) ? "" : Bytes.toStringBinary(qualifierArray,qualifierOffset,qualifierLength);
  return row + "/" + family+ (family != null && family.length() > 0 ? ":" : "")+ qualifier+ "/"+ KeyValue.humanReadableTimestamp(ts)+ "/"+ Type.codeToType(type);
}
