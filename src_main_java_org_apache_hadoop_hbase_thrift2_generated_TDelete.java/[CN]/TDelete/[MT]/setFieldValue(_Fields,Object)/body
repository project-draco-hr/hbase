{
switch (field) {
case ROW:
    if (value == null) {
      unsetRow();
    }
 else {
      setRow((ByteBuffer)value);
    }
  break;
case COLUMNS:
if (value == null) {
  unsetColumns();
}
 else {
  setColumns((List<TColumn>)value);
}
break;
case TIMESTAMP:
if (value == null) {
unsetTimestamp();
}
 else {
setTimestamp((Long)value);
}
break;
case DELETE_TYPE:
if (value == null) {
unsetDeleteType();
}
 else {
setDeleteType((TDeleteType)value);
}
break;
}
}
