{
  boolean available=true;
  if ((master.regionManager.numMetaRegions() != master.regionManager.numOnlineMetaRegions()) || master.regionManager.metaRegionsInTransition()) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("numberOfMetaRegions: " + master.regionManager.numMetaRegions() + ", onlineMetaRegions.size(): "+ master.regionManager.numOnlineMetaRegions());
      LOG.debug("Requeuing because not all meta regions are online");
    }
    available=false;
    requeue();
  }
  return available;
}
