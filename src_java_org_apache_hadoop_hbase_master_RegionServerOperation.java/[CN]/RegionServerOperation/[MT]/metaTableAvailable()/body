{
  boolean available=true;
  if ((master.getRegionManager().numMetaRegions() != master.getRegionManager().numOnlineMetaRegions()) || master.getRegionManager().metaRegionsInTransition()) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("numberOfMetaRegions: " + master.getRegionManager().numMetaRegions() + ", onlineMetaRegions.size(): "+ master.getRegionManager().numOnlineMetaRegions());
      LOG.debug("Requeuing because not all meta regions are online");
    }
    available=false;
    requeue();
  }
  return available;
}
