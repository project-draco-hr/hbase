{
  boolean available=true;
  if (LOG.isDebugEnabled()) {
    LOG.debug("numberOfMetaRegions: " + master.numberOfMetaRegions.get() + ", onlineMetaRegions.size(): "+ master.onlineMetaRegions.size());
  }
  if (master.numberOfMetaRegions.get() != master.onlineMetaRegions.size()) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("Requeuing because not all meta regions are online");
    }
    available=false;
    requeue();
  }
  return available;
}
