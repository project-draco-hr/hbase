{
  DataInputStream dis=new DataInputStream(new ByteArrayInputStream(onDiskBlock,offset,block.getOnDiskSizeWithoutHeader()));
  ByteBuffer buffer=block.getBufferWithoutHeader();
  Compression.decompress(buffer.array(),buffer.arrayOffset(),(InputStream)dis,block.getOnDiskSizeWithoutHeader(),block.getUncompressedSizeWithoutHeader(),compressAlgo);
}
