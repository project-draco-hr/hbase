{
  try {
    HTable table=new HTable(tableName);
    Put put=new Put(rowName);
    put.setTimeStamp(timestamp);
    byte[][] famAndQf=KeyValue.parseColumn(columnName);
    put.add(famAndQf[0],famAndQf[1],value);
    table.put(put);
  }
 catch (  IOException e) {
    throw new HBaseRestException(e);
  }
}
