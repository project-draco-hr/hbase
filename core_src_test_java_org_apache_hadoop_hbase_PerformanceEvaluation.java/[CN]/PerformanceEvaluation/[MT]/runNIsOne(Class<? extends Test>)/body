{
  Status status=new Status(){
    public void setStatus(    String msg) throws IOException {
      LOG.info(msg);
    }
  }
;
  HBaseAdmin admin=null;
  try {
    admin=new HBaseAdmin(this.conf);
    checkTable(admin);
    runOneClient(cmd,0,this.R,this.R,this.flushCommits,this.writeToWAL,status);
  }
 catch (  Exception e) {
    LOG.error("Failed",e);
  }
}
