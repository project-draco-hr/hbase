{
  if (heapSize < maxSize) {
    queue.add(cb);
    heapSize+=cb.heapSize();
  }
 else {
    CachedBlock head=queue.peek();
    if (cb.compareTo(head) > 0) {
      heapSize+=cb.heapSize();
      heapSize-=head.heapSize();
      if (heapSize > maxSize) {
        queue.poll();
      }
 else {
        heapSize+=head.heapSize();
      }
      queue.add(cb);
    }
  }
}
