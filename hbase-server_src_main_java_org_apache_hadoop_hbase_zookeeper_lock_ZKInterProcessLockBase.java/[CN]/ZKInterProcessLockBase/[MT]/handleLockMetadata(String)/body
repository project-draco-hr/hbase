{
  byte[] metadata=null;
  try {
    metadata=ZKUtil.getData(zkWatcher,lockZNode);
  }
 catch (  KeeperException ex) {
    LOG.warn("Cannot getData for znode:" + lockZNode,ex);
  }
  if (metadata == null) {
    return false;
  }
  if (handler != null) {
    handler.handleMetadata(metadata);
  }
  return true;
}
