{
switch (field) {
case TABLE:
    if (value == null) {
      unsetTable();
    }
 else {
      setTable((ByteBuffer)value);
    }
  break;
case ROW:
if (value == null) {
  unsetRow();
}
 else {
  setRow((ByteBuffer)value);
}
break;
case RELOAD:
if (value == null) {
unsetReload();
}
 else {
setReload((Boolean)value);
}
break;
}
}
