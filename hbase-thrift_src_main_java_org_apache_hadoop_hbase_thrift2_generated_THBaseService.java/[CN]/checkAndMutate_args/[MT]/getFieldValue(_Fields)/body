{
switch (field) {
case TABLE:
    return getTable();
case ROW:
  return getRow();
case FAMILY:
return getFamily();
case QUALIFIER:
return getQualifier();
case COMPARE_OP:
return getCompareOp();
case VALUE:
return getValue();
case ROW_MUTATIONS:
return getRowMutations();
}
throw new IllegalStateException();
}
