{
  StringBuilder sb=new StringBuilder("increment_args(");
  boolean first=true;
  sb.append("table:");
  if (this.table == null) {
    sb.append("null");
  }
 else {
    org.apache.thrift.TBaseHelper.toString(this.table,sb);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("tincrement:");
  if (this.tincrement == null) {
    sb.append("null");
