{
switch (field) {
case TABLE:
    if (value == null) {
      unsetTable();
    }
 else {
      setTable((ByteBuffer)value);
    }
  break;
case SCAN:
if (value == null) {
  unsetScan();
}
 else {
  setScan((TScan)value);
}
break;
case NUM_ROWS:
if (value == null) {
unsetNumRows();
}
 else {
setNumRows((Integer)value);
}
break;
}
}
