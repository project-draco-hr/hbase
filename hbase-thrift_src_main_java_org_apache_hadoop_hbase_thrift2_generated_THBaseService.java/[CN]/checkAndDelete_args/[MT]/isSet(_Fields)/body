{
  if (field == null) {
    throw new IllegalArgumentException();
  }
switch (field) {
case TABLE:
    return isSetTable();
case ROW:
  return isSetRow();
case FAMILY:
return isSetFamily();
case QUALIFIER:
return isSetQualifier();
case VALUE:
return isSetValue();
case TDELETE:
return isSetTdelete();
}
throw new IllegalStateException();
}
