{
  this.blockName=blockName;
  this.buf=buf;
  this.accessTime=accessTime;
  this.size=ClassSize.align(2 * blockName.length()) + ClassSize.align(buf.heapSize()) + PER_BLOCK_OVERHEAD;
  if (inMemory) {
    this.priority=BlockPriority.MEMORY;
  }
 else {
    this.priority=BlockPriority.SINGLE;
  }
}
