{
  HRegion region=null;
  this.lock.readLock().lock();
  try {
    region=onlineRegions.get(Integer.valueOf(Bytes.hashCode(regionName)));
    if (region == null) {
      throw new NotServingRegionException(regionName);
    }
    return region;
  }
  finally {
    this.lock.readLock().unlock();
  }
}
