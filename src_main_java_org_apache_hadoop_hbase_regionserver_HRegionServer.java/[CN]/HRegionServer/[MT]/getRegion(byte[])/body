{
  HRegion region=null;
  this.lock.readLock().lock();
  try {
    region=getOnlineRegion(regionName);
    if (region == null) {
      throw new NotServingRegionException("Region is not online: " + Bytes.toStringBinary(regionName));
    }
    return region;
  }
  finally {
    this.lock.readLock().unlock();
  }
}
