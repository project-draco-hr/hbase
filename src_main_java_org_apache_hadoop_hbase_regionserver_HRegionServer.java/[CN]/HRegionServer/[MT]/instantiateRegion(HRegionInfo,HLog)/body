{
  Path dir=HTableDescriptor.getTableDir(rootDir,regionInfo.getTableDesc().getName());
  HRegion r=HRegion.newHRegion(dir,this.hlog,this.fs,conf,regionInfo,this.cacheFlusher);
  long seqid=r.initialize(new Progressable(){
    public void progress(){
      addProcessingMessage(regionInfo);
    }
  }
);
  if (wal != null) {
    if (seqid > wal.getSequenceNumber())     wal.setSequenceNumber(seqid);
  }
  return r;
}
