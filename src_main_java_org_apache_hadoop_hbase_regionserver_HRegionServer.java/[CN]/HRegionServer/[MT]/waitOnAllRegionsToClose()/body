{
  int lastCount=-1;
  while (!this.onlineRegions.isEmpty()) {
    int count=this.onlineRegions.size();
    if (count != lastCount) {
      lastCount=count;
      LOG.info("Waiting on " + count + " regions to close");
      if (count < 10) {
        LOG.debug(this.onlineRegions);
      }
    }
    Threads.sleep(1000);
  }
}
