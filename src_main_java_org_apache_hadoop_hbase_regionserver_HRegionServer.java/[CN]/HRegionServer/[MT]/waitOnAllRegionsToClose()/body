{
  int lastCount=-1;
  while (!isOnlineRegionsEmpty()) {
    int count=getNumberOfOnlineRegions();
    if (count != lastCount) {
      lastCount=count;
      LOG.info("Waiting on " + count + " regions to close");
      if (count < 10 && LOG.isDebugEnabled()) {
        LOG.debug(this.onlineRegions);
      }
    }
    Threads.sleep(1000);
  }
}
