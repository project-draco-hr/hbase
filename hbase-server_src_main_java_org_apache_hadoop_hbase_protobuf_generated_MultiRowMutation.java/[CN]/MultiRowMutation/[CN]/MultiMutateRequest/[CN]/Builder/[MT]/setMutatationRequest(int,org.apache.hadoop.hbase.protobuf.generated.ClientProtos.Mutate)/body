{
  if (mutatationRequestBuilder_ == null) {
    if (value == null) {
      throw new NullPointerException();
    }
    ensureMutatationRequestIsMutable();
    mutatationRequest_.set(index,value);
    onChanged();
  }
 else {
    mutatationRequestBuilder_.setMessage(index,value);
  }
  return this;
}
