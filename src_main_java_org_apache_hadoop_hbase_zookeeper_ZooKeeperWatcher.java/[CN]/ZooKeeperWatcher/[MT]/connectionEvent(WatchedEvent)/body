{
switch (event.getState()) {
case SyncConnected:
    break;
case Disconnected:
  info("Received Disconnected from ZooKeeper, ignoring");
break;
case Expired:
error("Received Expired from ZooKeeper, aborting server");
if (abortable != null) {
abortable.abort("Received Expired from ZooKeeper, aborting server",null);
}
break;
}
}
