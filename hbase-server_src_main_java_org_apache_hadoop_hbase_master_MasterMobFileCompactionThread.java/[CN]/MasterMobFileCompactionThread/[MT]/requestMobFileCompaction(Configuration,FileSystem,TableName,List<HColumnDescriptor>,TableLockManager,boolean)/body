{
  master.reportMobFileCompactionStart(tableName);
  try {
    masterMobPool.execute(new CompactionRunner(fs,tableName,hcds,tableLockManager,isForceAllFiles,mobFileCompactorPool));
  }
 catch (  RejectedExecutionException e) {
    try {
      master.reportMobFileCompactionEnd(tableName);
    }
 catch (    IOException e1) {
      LOG.error("Failed to mark end of mob file compation",e1);
    }
    throw e;
  }
  if (LOG.isDebugEnabled()) {
    LOG.debug("The mob file compaction is requested for the columns " + hcds + " of the table "+ tableName.getNameAsString());
  }
}
