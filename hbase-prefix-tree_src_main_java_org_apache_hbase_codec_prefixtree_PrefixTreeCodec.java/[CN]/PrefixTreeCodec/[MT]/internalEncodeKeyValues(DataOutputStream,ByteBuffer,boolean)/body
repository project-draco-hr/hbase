{
  rawKeyValues.rewind();
  PrefixTreeEncoder builder=EncoderFactory.checkOut(encodedOutputStream,includesMvccVersion);
  try {
    KeyValue kv;
    while ((kv=KeyValueTool.nextShallowCopy(rawKeyValues,includesMvccVersion)) != null) {
      builder.write(kv);
    }
    builder.flush();
  }
  finally {
    EncoderFactory.checkIn(builder);
  }
}
