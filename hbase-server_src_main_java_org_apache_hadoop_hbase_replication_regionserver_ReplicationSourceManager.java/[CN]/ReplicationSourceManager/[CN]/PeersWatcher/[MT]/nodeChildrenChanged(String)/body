{
  List<String> peers=refreshPeersList(path);
  if (peers == null) {
    return;
  }
  for (  String id : peers) {
    try {
      boolean added=zkHelper.connectToPeer(id);
      if (added) {
        addSource(id);
      }
    }
 catch (    IOException e) {
      LOG.error("Error while adding a new peer",e);
    }
catch (    KeeperException e) {
      LOG.error("Error while adding a new peer",e);
    }
  }
}
