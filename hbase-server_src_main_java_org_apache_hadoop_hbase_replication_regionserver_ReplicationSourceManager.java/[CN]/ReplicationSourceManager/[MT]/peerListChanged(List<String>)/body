{
  for (  String id : peerIds) {
    try {
      boolean added=this.replicationPeers.connectToPeer(id);
      if (added) {
        addSource(id);
      }
    }
 catch (    IOException e) {
      LOG.error("Error while adding a new peer",e);
    }
catch (    KeeperException e) {
      LOG.error("Error while adding a new peer",e);
    }
  }
}
