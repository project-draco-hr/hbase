{
  WorkItemRegion[] work=new WorkItemRegion[regionServerList.size()];
  int num=0;
  for (  HServerInfo rsinfo : regionServerList) {
    work[num]=new WorkItemRegion(this,rsinfo,errors,connection);
    executor.execute(work[num]);
    num++;
  }
  for (int i=0; i < num; i++) {
synchronized (work[i]) {
      while (!work[i].isDone()) {
        work[i].wait();
      }
    }
  }
}
