{
  HTableDescriptor desc=new HTableDescriptor(tableName);
  for (  byte[] family : families) {
    HColumnDescriptor hcd=new HColumnDescriptor(family,numVersions,HColumnDescriptor.DEFAULT_COMPRESSION,HColumnDescriptor.DEFAULT_IN_MEMORY,HColumnDescriptor.DEFAULT_BLOCKCACHE,HColumnDescriptor.DEFAULT_BLOCKSIZE,HColumnDescriptor.DEFAULT_TTL,HColumnDescriptor.DEFAULT_BLOOMFILTER,HColumnDescriptor.DEFAULT_REPLICATION_SCOPE);
    desc.addFamily(hcd);
  }
  getHBaseAdmin().createTable(desc);
  return new HTable(c,tableName);
}
