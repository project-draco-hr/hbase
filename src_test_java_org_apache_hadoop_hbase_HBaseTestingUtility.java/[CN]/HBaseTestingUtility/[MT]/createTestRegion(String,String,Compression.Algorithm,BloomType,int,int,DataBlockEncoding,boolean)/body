{
  HColumnDescriptor hcd=new HColumnDescriptor(Bytes.toBytes(cfName),maxVersions,comprAlgo.getName(),HColumnDescriptor.DEFAULT_IN_MEMORY,HColumnDescriptor.DEFAULT_BLOCKCACHE,HColumnDescriptor.DEFAULT_TTL,bloomType.toString());
  hcd.setBlocksize(blockSize);
  hcd.setDataBlockEncoding(encoding);
  hcd.setEncodeOnDisk(encodeOnDisk);
  return createTestRegion(tableName,hcd);
}
