{
  LOG.trace("forget [" + xid.toString() + "] ");
  threadLocalTransactionState.remove();
  TransactionState state=xidToTransactionState.remove(xid);
  if (state != null) {
    try {
      transactionManager.abort(state);
    }
 catch (    IOException e) {
      throw new RuntimeException(e);
    }
  }
}
