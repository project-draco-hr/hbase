{
  ZkRegionMergeDetails zrmd=(ZkRegionMergeDetails)rmd;
  byte[] payload=HRegionInfo.toDelimitedByteArray(merged,a,b);
  try {
    zrmd.setZnodeVersion(ZKAssign.transitionNode(watcher,merged,serverName,beginState,endState,zrmd.getZnodeVersion(),payload));
  }
 catch (  KeeperException e) {
    throw new IOException(e);
  }
}
