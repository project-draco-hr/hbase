{
  openDaughters(server,services,regions.getFirst(),regions.getSecond());
  if (useCoordinatedStateManager(server)) {
    ((BaseCoordinatedStateManager)server.getCoordinatedStateManager()).getSplitTransactionCoordination().completeSplitTransaction(services,regions.getFirst(),regions.getSecond(),std,parent);
  }
  if (parent.getCoprocessorHost() != null) {
    parent.getCoprocessorHost().postSplit(regions.getFirst(),regions.getSecond());
  }
  return regions;
}
