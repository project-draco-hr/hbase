{
  try {
    if (!ZKAssign.deleteNode(server.getZooKeeper(),hri.getEncodedName(),RS_ZK_REQUEST_REGION_SPLIT,server.getServerName())) {
      ZKAssign.deleteNode(server.getZooKeeper(),hri.getEncodedName(),RS_ZK_REGION_SPLITTING,server.getServerName());
    }
  }
 catch (  KeeperException.NoNodeException e) {
    LOG.info("Failed cleanup zk node of " + hri.getRegionNameAsString(),e);
  }
catch (  KeeperException e) {
    server.abort("Failed cleanup of " + hri.getRegionNameAsString(),e);
  }
}
