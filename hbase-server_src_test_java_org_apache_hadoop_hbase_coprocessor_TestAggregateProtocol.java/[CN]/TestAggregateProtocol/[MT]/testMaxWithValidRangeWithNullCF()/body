{
  AggregationClient aClient=new AggregationClient(conf);
  final ColumnInterpreter<Long,Long,EmptyMsg,LongMsg,LongMsg> ci=new LongColumnInterpreter();
  Scan scan=new Scan();
  Long max=null;
  try {
    max=aClient.max(TEST_TABLE,ci,scan);
  }
 catch (  Throwable e) {
    max=null;
  }
  assertEquals(null,max);
}
