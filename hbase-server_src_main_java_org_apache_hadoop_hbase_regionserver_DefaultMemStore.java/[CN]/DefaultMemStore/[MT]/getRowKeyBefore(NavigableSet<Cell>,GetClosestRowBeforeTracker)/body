{
  Cell firstOnRow=state.getTargetKey();
  for (Member p=memberOfPreviousRow(set,state,firstOnRow); p != null; p=memberOfPreviousRow(p.set,state,firstOnRow)) {
    if (!state.isTargetTable(p.cell))     break;
    if (!state.isBetterCandidate(p.cell))     break;
    firstOnRow=new KeyValue(p.cell.getRowArray(),p.cell.getRowOffset(),p.cell.getRowLength(),HConstants.LATEST_TIMESTAMP);
    if (walkForwardInSingleRow(p.set,firstOnRow,state))     break;
  }
}
