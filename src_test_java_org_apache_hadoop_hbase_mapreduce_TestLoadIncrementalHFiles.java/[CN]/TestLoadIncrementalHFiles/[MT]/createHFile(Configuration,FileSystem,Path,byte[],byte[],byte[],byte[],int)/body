{
  HFile.Writer writer=HFile.getWriterFactory(conf,new CacheConfig(conf)).withPath(fs,path).withBlockSize(BLOCKSIZE).withCompression(COMPRESSION).withComparator(KeyValue.KEY_COMPARATOR).create();
  long now=System.currentTimeMillis();
  try {
    for (    byte[] key : Bytes.iterateOnSplits(startKey,endKey,numRows - 2)) {
      KeyValue kv=new KeyValue(key,family,qualifier,now,key);
      writer.append(kv);
    }
  }
  finally {
    writer.close();
  }
}
