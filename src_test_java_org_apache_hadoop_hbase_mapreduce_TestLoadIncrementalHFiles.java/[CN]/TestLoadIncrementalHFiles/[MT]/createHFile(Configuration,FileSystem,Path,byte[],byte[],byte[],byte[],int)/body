{
  HFile.Writer writer=HFile.getWriterFactory(conf,new CacheConfig(conf)).createWriter(fs,path,BLOCKSIZE,COMPRESSION,KeyValue.KEY_COMPARATOR);
  long now=System.currentTimeMillis();
  try {
    for (    byte[] key : Bytes.iterateOnSplits(startKey,endKey,numRows - 2)) {
      KeyValue kv=new KeyValue(key,family,qualifier,now,key);
      writer.append(kv);
    }
  }
  finally {
    writer.close();
  }
}
