{
  HFile.Writer writer=new HFile.Writer(fs,path,BLOCKSIZE,COMPRESSION,KeyValue.KEY_COMPARATOR);
  try {
    for (    byte[] key : Bytes.iterateOnSplits(startKey,endKey,numRows - 2)) {
      KeyValue kv=new KeyValue(key,family,qualifier,key);
      writer.append(kv);
    }
  }
  finally {
    writer.close();
  }
}
