{
  Configuration conf=util.getConfiguration();
  HFile.Reader reader=HFile.createReader(p.getFileSystem(conf),p,new CacheConfig(conf));
  reader.loadFileInfo();
  HFileScanner scanner=reader.getScanner(false,false);
  scanner.seekTo();
  int count=0;
  do {
    count++;
  }
 while (scanner.next());
  assertTrue(count > 0);
  reader.close();
  return count;
}
