{
  Configuration conf=util.getConfiguration();
  HFile.Reader reader=new HFile.Reader(p.getFileSystem(conf),p,null,false);
  reader.loadFileInfo();
  HFileScanner scanner=reader.getScanner(false,false);
  scanner.seekTo();
  int count=0;
  do {
    count++;
  }
 while (scanner.next());
  assertTrue(count > 0);
  return count;
}
