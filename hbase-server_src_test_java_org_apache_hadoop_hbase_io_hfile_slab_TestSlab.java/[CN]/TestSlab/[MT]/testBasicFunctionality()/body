{
  for (int i=0; i < NUMBLOCKS; i++) {
    buffers[i]=testSlab.alloc(BLOCKSIZE);
    assertEquals(BLOCKSIZE,buffers[i].limit());
  }
  for (int i=0; i < NUMBLOCKS; i++) {
    buffers[i].putInt(i);
  }
  for (int i=0; i < NUMBLOCKS; i++) {
    buffers[i].putInt(i);
  }
  for (int i=0; i < NUMBLOCKS; i++) {
    testSlab.free(buffers[i]);
  }
  for (int i=0; i < NUMBLOCKS; i++) {
    buffers[i]=testSlab.alloc(BLOCKSIZE);
    assertEquals(BLOCKSIZE,buffers[i].limit());
  }
}
