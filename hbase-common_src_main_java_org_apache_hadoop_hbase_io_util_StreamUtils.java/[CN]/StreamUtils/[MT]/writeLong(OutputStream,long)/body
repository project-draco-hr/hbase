{
  out.write((byte)(0xff & (v >> 56)));
  out.write((byte)(0xff & (v >> 48)));
  out.write((byte)(0xff & (v >> 40)));
  out.write((byte)(0xff & (v >> 32)));
  out.write((byte)(0xff & (v >> 24)));
  out.write((byte)(0xff & (v >> 16)));
  out.write((byte)(0xff & (v >> 8)));
  out.write((byte)(0xff & v));
}
