{
  for (int i=0; i < getColumnCount(); i++) {
    if (!getColumn(i).isInitialized()) {
      return false;
    }
  }
  for (int i=0; i < getAttributeCount(); i++) {
    if (!getAttribute(i).isInitialized()) {
      return false;
    }
  }
  if (hasFilter()) {
    if (!getFilter().isInitialized()) {
      return false;
    }
  }
  return true;
}
