{
  if (isLazy) {
    for (    KeyValueScanner scanner : scanners) {
      scanner.requestSeek(seekKey,false,true);
    }
  }
 else {
    if (!isParallelSeek) {
      for (      KeyValueScanner scanner : scanners) {
        scanner.seek(seekKey);
      }
    }
 else {
      parallelSeek(scanners,seekKey);
    }
  }
}
