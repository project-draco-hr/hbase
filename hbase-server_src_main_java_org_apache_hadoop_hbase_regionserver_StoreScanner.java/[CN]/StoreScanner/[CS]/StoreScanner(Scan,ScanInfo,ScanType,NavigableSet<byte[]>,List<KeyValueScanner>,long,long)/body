{
  this(null,scan.getCacheBlocks(),scan,columns,scanInfo.getTtl(),scanInfo.getMinVersions(),readPt);
  this.matcher=new ScanQueryMatcher(scan,scanInfo,columns,scanType,Long.MAX_VALUE,earliestPutTs,oldestUnexpiredTS);
  if (this.store != null) {
    this.store.addChangedReaderObserver(this);
  }
  if (!isParallelSeekEnabled) {
    for (    KeyValueScanner scanner : scanners) {
      scanner.seek(matcher.getStartKey());
    }
  }
 else {
    parallelSeek(scanners,matcher.getStartKey());
  }
  heap=new KeyValueHeap(scanners,scanInfo.getComparator());
}
