{
  if (filesUser == null && filesGroup == null) {
    outputFs.mkdirs(path);
  }
 else {
    Path parent=path.getParent();
    if (!outputFs.exists(parent) && !parent.isRoot()) {
      createOutputPath(parent);
    }
    outputFs.mkdirs(path);
    outputFs.setOwner(path,filesUser,filesGroup);
  }
}
