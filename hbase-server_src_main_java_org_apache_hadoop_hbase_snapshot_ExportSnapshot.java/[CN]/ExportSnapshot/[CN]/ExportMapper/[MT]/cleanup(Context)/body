{
  if (outputFs != null) {
    try {
      outputFs.close();
    }
 catch (    IOException e) {
      LOG.error("Error closing output FileSystem",e);
    }
  }
  if (inputFs != null) {
    try {
      inputFs.close();
    }
 catch (    IOException e) {
      LOG.error("Error closing input FileSystem",e);
    }
  }
}
