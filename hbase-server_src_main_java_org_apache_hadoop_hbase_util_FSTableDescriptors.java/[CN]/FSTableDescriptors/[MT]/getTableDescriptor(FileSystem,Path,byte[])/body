{
  HTableDescriptor htd=null;
  try {
    TableDescriptorModtime tdmt=getTableDescriptorModtime(fs,hbaseRootDir,Bytes.toString(tableName));
    htd=tdmt == null ? null : tdmt.getTableDescriptor();
  }
 catch (  NullPointerException e) {
    LOG.debug("Exception during readTableDecriptor. Current table name = " + Bytes.toString(tableName),e);
  }
  return htd;
}
