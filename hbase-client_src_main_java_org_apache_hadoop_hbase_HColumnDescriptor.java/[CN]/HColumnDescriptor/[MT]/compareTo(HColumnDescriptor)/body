{
  int result=Bytes.compareTo(this.name,o.getName());
  if (result == 0) {
    if (!this.values.equals(o.values)) {
      return 1;
    }
  }
  if (result == 0) {
    result=this.configuration.hashCode() - o.configuration.hashCode();
    if (result < 0)     result=-1;
 else     if (result > 0)     result=1;
  }
  return result;
}
