{
  boolean isSuccessful=false;
  List<Path> result=null;
  try {
    isSuccessful=finishWriting();
  }
  finally {
    result=close();
    List<IOException> thrown=closeLogWriters(null);
    if (thrown != null && !thrown.isEmpty()) {
      throw MultipleIOException.createIOException(thrown);
    }
  }
  if (isSuccessful) {
    splits=result;
  }
  return splits;
}
