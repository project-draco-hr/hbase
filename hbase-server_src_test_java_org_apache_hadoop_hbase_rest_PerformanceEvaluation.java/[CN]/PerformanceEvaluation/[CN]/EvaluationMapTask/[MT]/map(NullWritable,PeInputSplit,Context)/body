{
  Status status=new Status(){
    public void setStatus(    String msg){
      context.setStatus(msg);
    }
  }
;
  pe.tableName=value.getTableName();
  long elapsedTime=this.pe.runOneClient(this.cmd,value.getStartRow(),value.getRows(),value.getTotalRows(),value.isFlushCommits(),value.isWriteToWAL(),value.isUseTags(),value.getNoOfTags(),HConnectionManager.createConnection(context.getConfiguration()),status);
  context.getCounter(Counter.ELAPSED_TIME).increment(elapsedTime);
  context.getCounter(Counter.ROWS).increment(value.rows);
  context.write(new LongWritable(value.startRow),new LongWritable(elapsedTime));
  context.progress();
}
