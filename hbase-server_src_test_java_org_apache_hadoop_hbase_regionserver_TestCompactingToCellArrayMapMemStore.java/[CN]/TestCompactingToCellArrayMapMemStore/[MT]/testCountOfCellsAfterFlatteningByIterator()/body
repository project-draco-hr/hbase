{
  String[] keys1={"A","B","C"};
  addRowsByKeysWith50Cols(memstore,keys1);
  ((CompactingMemStore)memstore).flushInMemory();
  while (((CompactingMemStore)memstore).isMemStoreFlushingInMemory()) {
    Threads.sleep(10);
  }
  MemStoreCompactorIterator itr=new MemStoreCompactorIterator(((CompactingMemStore)memstore).getImmutableSegments().getStoreSegments(),CellComparator.COMPARATOR,10,((CompactingMemStore)memstore).getStore());
  int cnt=0;
  try {
    while (itr.next() != null) {
      cnt++;
    }
  }
  finally {
    itr.close();
  }
  assertEquals("the count should be ",cnt,150);
}
