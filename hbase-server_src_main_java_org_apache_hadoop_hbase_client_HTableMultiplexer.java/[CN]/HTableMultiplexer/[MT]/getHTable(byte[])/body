{
  HTable htable=this.tableNameToHTableMap.get(table);
  if (htable == null) {
synchronized (this.tableNameToHTableMap) {
      htable=this.tableNameToHTableMap.get(table);
      if (htable == null) {
        htable=new HTable(conf,table);
        this.tableNameToHTableMap.put(table,htable);
      }
    }
  }
  return htable;
}
