{
  while (!master.closed.get()) {
    if (regionManager.isInitialRootScanComplete() && regionManager.numMetaRegions() == regionManager.numOnlineMetaRegions()) {
      break;
    }
    try {
      wait(master.threadWakeFrequency);
    }
 catch (    InterruptedException e) {
    }
  }
  return master.closed.get();
}
