{
  MetaRegion region=null;
  while (!this.master.isClosed() && (region == null && metaRegionsToScan.size() > 0) && !metaRegionsScanned()) {
    try {
      region=metaRegionsToScan.poll(this.master.getThreadWakeFrequency(),TimeUnit.MILLISECONDS);
    }
 catch (    InterruptedException e) {
    }
    if (region == null && metaRegionsToRescan.size() != 0) {
      region=metaRegionsToRescan.remove(0);
    }
    if (region != null) {
      if (!scanOneMetaRegion(region)) {
        metaRegionsToRescan.add(region);
      }
    }
  }
  initialScanComplete=true;
  return true;
}
