{
  List<Put> puts=new ArrayList<Put>(rows);
  byte[] qualifier=Bytes.toBytes("val");
  for (int i=0; i < flushes; i++) {
    for (int k=0; k < rows; k++) {
      byte[] row=Bytes.toBytes(random.nextLong());
      Put p=new Put(row);
      p.add(family,qualifier,row);
      puts.add(p);
    }
    ht.put(puts);
    ht.flushCommits();
    TEST_UTIL.flush();
    puts.clear();
  }
}
